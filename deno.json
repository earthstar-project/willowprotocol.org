{
  "imports": {
    "macromaniajsx/jsx-dev-runtime": "https://raw.githubusercontent.com/worm-blossom/macromania/main/mod.ts",
    "macromaniajsx/jsx-runtime": "https://raw.githubusercontent.com/worm-blossom/macromania/main/mod.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsxdev",
    "jsxImportSource": "macromaniajsx",
    "lib": [
      "deno.ns",
      "dom"
    ],
    "strict": true
  },
  "tasks": {
    "build": "deno run -A ./src/main.tsx",
    "watch": "deno run --watch -A ./src/main.tsx",
    "serve": "deno run -A ./server.ts"
  },
  "include": ["src", "deps.ts", "devServer.ts"],
  "exclude": ["build"]
}