  "display": "Deno",

  "imports": {
    "macromaniajsx/jsx-dev-runtime": "./deps.ts",
    "macromaniajsx/jsx-runtime": "./deps.ts"
  },

  "compilerOptions": {
    "jsx": "react-jsxdev",
