"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"note\" default-locale=\"de-AT\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Austrian Legal (Deutsch - Österreich)</title>\n    <id>http://www.zotero.org/styles/austrian-legal</id>\n    <link href=\"http://www.zotero.org/styles/austrian-legal\" rel=\"self\"/>\n    <link href=\"http://www.lexisnexis.at/service/training-und-hilfe/zitieren.aspx\" rel=\"documentation\"/>\n    <author>\n      <name>LexisNexis</name>\n      <email>andreas.geyrecker@lexisnexis.at</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"law\"/>\n    <summary>The Austrian Legal Citation style (based on AZR - Abkürzungs- und Zitierregeln)</summary>\n    <published>2012-05-31T00:00:00+02:00</published>\n    <updated>2015-07-25T13:49:55+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de-AT\">\n    <terms>\n      <term name=\"et-al\">et al</term>\n    </terms>\n  </locale>\n  <macro name=\"authority-sort\">\n    <choose>\n      <if type=\"legal_case\">\n        <choose>\n          <if variable=\"authority\">\n            <text variable=\"authority\"/>\n          </if>\n          <else>\n            <text variable=\"title\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <!-- move statutes to the bottom -->\n        <text value=\"ZZZZ\"/>\n      </else-if>\n      <else>\n        <!-- move all but cases to the top -->\n        <text value=\"AAAA\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"contributors-sort\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"verb-short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"author\"/>\n        <text macro=\"noauthor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"sort-key\">\n    <text macro=\"authority-sort\"/>\n    <text macro=\"contributors-sort\" suffix=\" \"/>\n  </macro>\n  <macro name=\"noauthor\">\n    <text value=\"OV\"/>\n  </macro>\n  <macro name=\"authors\">\n    <names variable=\"author\" font-style=\"italic\">\n      <name delimiter=\"/\" name-as-sort-order=\"all\" form=\"short\" delimiter-precedes-last=\"always\" initialize-with=\".\"/>\n      <et-al term=\"et-al\" font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\" font-style=\"italic\"/>\n        <names variable=\"translator\" font-style=\"italic\"/>\n        <text macro=\"noauthor\" font-style=\"italic\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editors\">\n    <names variable=\"editor\" font-style=\"italic\">\n      <name delimiter=\"/\" name-as-sort-order=\"all\" form=\"short\" delimiter-precedes-last=\"always\" initialize-with=\".\"/>\n      <et-al term=\"et-al\" font-style=\"italic\"/>\n    </names>\n    <text value=\" (Hrsg)\" font-style=\"normal\"/>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\" givenname-disambiguation-rule=\"all-names-with-initials\" disambiguate-add-givenname=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if type=\"legal_case\">\n          <choose>\n            <if variable=\"authority issued number\" match=\"all\">\n              <text variable=\"authority\" suffix=\" \"/>\n              <date variable=\"issued\" suffix=\" \">\n                <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                <date-part name=\"year\" form=\"long\" suffix=\",\"/>\n              </date>\n              <text variable=\"number\"/>\n            </if>\n            <else>\n              <!-- fallback: since RIS format does not have a court (authority) variable -->\n              <text variable=\"title\"/>\n            </else>\n          </choose>\n        </if>\n        <else-if type=\"bill legislation\" match=\"any\">\n          <choose>\n            <if variable=\"container-title section\" match=\"all\">\n              <text variable=\"section\" suffix=\" \"/>\n              <text variable=\"container-title\"/>\n              <choose>\n                <if position=\"first\">\n                  <choose>\n                    <if variable=\"references\">\n                      <text variable=\"references\" prefix=\" \"/>\n                    </if>\n                  </choose>\n                </if>\n              </choose>\n            </if>\n            <else>\n              <text variable=\"title\"/>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <text macro=\"authors\" suffix=\", \"/>\n          <choose>\n            <if position=\"first\">\n              <text variable=\"title\" suffix=\", \"/>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"page issued\" match=\"all\">\n              <text variable=\"container-title\" suffix=\" \"/>\n              <date variable=\"issued\" suffix=\", \">\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n              <text variable=\"page\"/>\n              <text variable=\"locator\" prefix=\" (\" suffix=\")\"/>\n            </if>\n            <else>\n              <text variable=\"container-title\"/>\n              <!-- if no page info is available, assume container-title (publication) holds article number citation -->\n              <text variable=\"locator\" prefix=\", \"/>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"book\">\n          <text macro=\"authors\" font-style=\"italic\" suffix=\", \"/>\n          <text variable=\"title\"/>\n          <text variable=\"volume\" prefix=\" \"/>\n          <choose>\n            <if variable=\"edition note\" match=\"any\">\n              <choose>\n                <if variable=\"edition\">\n                  <text variable=\"edition\" vertical-align=\"sup\"/>\n                </if>\n                <else-if variable=\"note\">\n                  <!-- RIS workaround -->\n                  <text variable=\"note\" vertical-align=\"sup\"/>\n                </else-if>\n              </choose>\n            </if>\n          </choose>\n          <group delimiter=\" \" prefix=\" (\" suffix=\")\">\n            <text variable=\"collection-title\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\" form=\"long\"/>\n            </date>\n          </group>\n          <text variable=\"locator\" prefix=\" \"/>\n        </else-if>\n        <else-if type=\"chapter\">\n          <text macro=\"authors\" font-style=\"italic\"/>\n          <choose>\n            <if variable=\"page\">\n              <text variable=\"title\" prefix=\", \"/>\n              <choose>\n                <if position=\"first\">\n                  <choose>\n                    <if variable=\"editor\">\n                      <text macro=\"editors\" prefix=\", in \" suffix=\", \"/>\n                    </if>\n                    <else>\n                      <text value=\", \"/>\n                    </else>\n                  </choose>\n                  <text variable=\"container-title\"/>\n                  <text variable=\"volume\" prefix=\" \"/>\n                  <choose>\n                    <if variable=\"edition\">\n                      <text variable=\"edition\" vertical-align=\"sup\"/>\n                    </if>\n                    <else-if variable=\"note\">\n                      <text variable=\"note\" vertical-align=\"sup\"/>\n                    </else-if>\n                  </choose>\n                  <group delimiter=\" \" prefix=\" (\" suffix=\")\">\n                    <text variable=\"collection-title\"/>\n                    <date variable=\"issued\">\n                      <date-part name=\"year\" form=\"long\"/>\n                    </date>\n                  </group>\n                </if>\n              </choose>\n              <text variable=\"page\" prefix=\" \"/>\n              <text variable=\"locator\" prefix=\" (\" suffix=\")\"/>\n            </if>\n            <else>\n              <!-- no page info, classic legal commentary -->\n              <choose>\n                <if position=\"first\">\n                  <choose>\n                    <if variable=\"editor\">\n                      <text macro=\"editors\" prefix=\" in \" suffix=\", \"/>\n                    </if>\n                    <else>\n                      <text value=\", \"/>\n                    </else>\n                  </choose>\n                  <text variable=\"container-title\"/>\n                  <text variable=\"volume\" prefix=\" \"/>\n                  <choose>\n                    <if variable=\"edition\">\n                      <text variable=\"edition\" vertical-align=\"sup\"/>\n                    </if>\n                    <else-if variable=\"note\">\n                      <!-- RIS workaround -->\n                      <text variable=\"note\" vertical-align=\"sup\"/>\n                    </else-if>\n                  </choose>\n                  <group delimiter=\" \" prefix=\" (\" suffix=\") \">\n                    <text variable=\"collection-title\"/>\n                    <date variable=\"issued\">\n                      <date-part name=\"year\" form=\"long\"/>\n                    </date>\n                  </group>\n                </if>\n                <else>\n                  <text value=\",\"/>\n                </else>\n              </choose>\n              <text variable=\"title\" prefix=\" \"/>\n              <text variable=\"locator\" prefix=\" \"/>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"authors\" suffix=\", \"/>\n          <text variable=\"title\"/>\n          <choose>\n            <if position=\"first\">\n              <text variable=\"genre\" prefix=\". \"/>\n              <text variable=\"publisher\" prefix=\", \"/>\n              <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n            </if>\n          </choose>\n          <text variable=\"locator\" prefix=\" \"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\", \">\n            <text macro=\"authors\"/>\n            <text variable=\"title\"/>\n            <group delimiter=\" \">\n              <date form=\"numeric\" variable=\"issued\"/>\n              <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"authors\" font-style=\"italic\"/>\n            <text variable=\"title\"/>\n            <choose>\n              <if variable=\"container-title\">\n                <text variable=\"container-title\"/>\n              </if>\n            </choose>\n            <date form=\"numeric\" variable=\"issued\"/>\n            <text variable=\"locator\" prefix=\" \"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&#8212;&#8212;\" hanging-indent=\"true\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"sort-key\"/>\n      <key variable=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <choose>\n        <if type=\"legal_case\">\n          <choose>\n            <if variable=\"authority issued number\" match=\"all\">\n              <text variable=\"authority\" suffix=\" \"/>\n              <date variable=\"issued\" suffix=\" \">\n                <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                <date-part name=\"year\" form=\"long\" suffix=\",\"/>\n              </date>\n              <text variable=\"number\"/>\n            </if>\n            <else>\n              <!-- fallback: since RIS format does not have a court (authority) variable -->\n              <text variable=\"title\"/>\n              <text variable=\"title\"/>\n            </else>\n          </choose>\n        </if>\n        <else-if type=\"bill legislation\" match=\"any\">\n          <text value=\" \"/>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <text macro=\"authors\"/>\n          <text variable=\"title\" prefix=\", \" suffix=\", \"/>\n          <choose>\n            <if variable=\"page issued\" match=\"all\">\n              <text variable=\"container-title\" suffix=\" \"/>\n              <date variable=\"issued\" suffix=\", \">\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n              <text variable=\"page\"/>\n            </if>\n            <else>\n              <!-- if no page info is available, assume container-title (publication) holds article number citation -->\n              <text variable=\"container-title\" suffix=\" \"/>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"book\">\n          <choose>\n            <if variable=\"editor\">\n              <text macro=\"editors\" suffix=\", \" font-style=\"italic\"/>\n            </if>\n            <else>\n              <text macro=\"authors\" suffix=\", \"/>\n            </else>\n          </choose>\n          <text variable=\"title\"/>\n          <choose>\n            <if variable=\"volume\">\n              <text variable=\"volume\" prefix=\" \"/>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"edition note\" match=\"any\">\n              <choose>\n                <if variable=\"edition\">\n                  <text variable=\"edition\" vertical-align=\"sup\"/>\n                </if>\n                <else-if variable=\"note\">\n                  <!-- RIS workaround -->\n                  <text variable=\"note\" vertical-align=\"sup\"/>\n                </else-if>\n              </choose>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"publisher\">\n              <text variable=\"publisher\" prefix=\", \"/>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"publisher-place\">\n              <text variable=\"publisher-place\" prefix=\", \"/>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"publisher publisher-place\" match=\"any\">\n              <date variable=\"issued\" prefix=\" \">\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n            </if>\n            <else>\n              <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"chapter\">\n          <choose>\n            <if variable=\"editor\">\n              <text macro=\"editors\" suffix=\", \" font-style=\"italic\"/>\n            </if>\n            <else>\n              <text macro=\"authors\" suffix=\", \"/>\n            </else>\n          </choose>\n          <text variable=\"container-title\"/>\n          <choose>\n            <if variable=\"volume\">\n              <text variable=\"volume\" prefix=\" \"/>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"edition note\" match=\"any\">\n              <choose>\n                <if variable=\"edition\">\n                  <text variable=\"edition\" vertical-align=\"sup\"/>\n                </if>\n                <else-if variable=\"note\">\n                  <!-- RIS workaround -->\n                  <text variable=\"note\" vertical-align=\"sup\"/>\n                </else-if>\n              </choose>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"publisher\">\n              <text variable=\"publisher\" prefix=\", \"/>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"publisher-place\">\n              <text variable=\"publisher-place\" prefix=\", \"/>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"publisher publisher-place\" match=\"any\">\n              <date variable=\"issued\" prefix=\" \">\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n            </if>\n            <else>\n              <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"authors\" suffix=\", \"/>\n          <text variable=\"title\" suffix=\".\"/>\n          <text variable=\"genre\" prefix=\" \"/>\n          <text variable=\"publisher\" prefix=\", \"/>\n          <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n            <date-part name=\"year\" form=\"long\"/>\n          </date>\n        </else-if>\n        <else-if type=\"webpage\">\n          <text macro=\"authors\"/>\n          <text value=\", \"/>\n          <text variable=\"title\" suffix=\", \"/>\n          <date variable=\"issued\" suffix=\" \" form=\"numeric\"/>\n          <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n          <choose>\n            <if variable=\"accessed\">\n              <text value=\", aufgerufen am \"/>\n              <date variable=\"accessed\" suffix=\" \" form=\"numeric\"/>\n            </if>\n          </choose>\n        </else-if>\n        <else>\n          <text macro=\"authors\" font-style=\"italic\"/>\n          <text variable=\"title\" prefix=\", \"/>\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"container-title\" prefix=\", \"/>\n            </if>\n          </choose>\n          <date variable=\"issued\" prefix=\", \" form=\"numeric\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"