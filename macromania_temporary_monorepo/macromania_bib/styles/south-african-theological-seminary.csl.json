"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>South African Theological Seminary</title>\n    <title-short>SATS</title-short>\n    <id>http://www.zotero.org/styles/south-african-theological-seminary</id>\n    <link href=\"http://www.zotero.org/styles/south-african-theological-seminary\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/friedrich-schiller-universitat-jena-medizinische-fakultat\" rel=\"template\"/>\n    <link href=\"https://sats.ac.za/wp-content/uploads/2019/10/SmithHowtocitesources.pdf\" rel=\"documentation\"/>\n    <link href=\"https://sats.ac.za/wp-content/uploads/2019/10/Howtocompileabibliography.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <issn>1996-8167</issn>\n    <updated>2018-05-29T11:18:57+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"accessed\">accessed from</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <label form=\"verb\" text-case=\"capitalize-first\" strip-periods=\"false\" suffix=\" \"/>\n      <name and=\"text\" initialize-with=\"\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" et-al-min=\"11\" et-al-use-first=\"10\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\" suffix=\".\"/>\n        <text variable=\"publisher\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" initialize-with=\"\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n        <text variable=\"publisher\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n      <number variable=\"edition\" form=\"ordinal\"/>\n      <label variable=\"edition\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n      <number variable=\"volume\"/>\n      <label variable=\"volume\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if match=\"any\" variable=\"issued\">\n        <choose>\n          <if type=\"article-newspaper\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n              <date-part name=\"day\" prefix=\" (\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\")\"/>\n            </date>\n          </if>\n          <else>\n            <text macro=\"issued\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\":\">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\" line-spacing=\"2\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author-short\"/>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\" \"/>\n      <text macro=\"date\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n          <group delimiter=\". \">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <text macro=\"title\" prefix=\" \"/>\n                <text macro=\"edition\"/>\n              </group>\n              <text macro=\"editor-translator\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"report\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\".\">\n            <group>\n              <group suffix=\".\">\n                <text variable=\"genre\" prefix=\" \"/>\n                <text variable=\"number\" prefix=\" No. \"/>\n              </group>\n              <group suffix=\".\">\n                <text variable=\"publisher\" prefix=\" \"/>\n                <text variable=\"publisher-place\" prefix=\", \"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\" prefix=\" \"/>\n            <group delimiter=\", \" prefix=\" \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\" prefix=\" \"/>\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <names variable=\"editor\">\n                  <name delimiter-precedes-last=\"always\" et-al-min=\"8\" et-al-use-first=\"3\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n                  <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n                </names>\n              </group>\n              <group delimiter=\" \">\n                <text variable=\"container-title\"/>\n                <text macro=\"volume\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"publisher-place\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"post-weblog webpage\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                <text variable=\"URL\"/>\n              </group>\n              <date variable=\"accessed\">\n                <date-part name=\"year\"/>\n                <date-part name=\"month\" form=\"numeric\" prefix=\"-\"/>\n                <date-part name=\"day\" prefix=\"-\"/>\n              </date>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"title\" prefix=\" \"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group prefix=\" \">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                <text variable=\"page\" prefix=\":\"/>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"