"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-US\">\n  <info>\n    <title>Multidisciplinary Digital Publishing Institute</title>\n    <title-short>MDPI</title-short>\n    <id>http://www.zotero.org/styles/multidisciplinary-digital-publishing-institute</id>\n    <link href=\"http://www.zotero.org/styles/multidisciplinary-digital-publishing-institute\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-chemical-society\" rel=\"template\"/>\n    <link href=\"http://www.mdpi.com/authors/references\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>  \n    <contributor>  \n      <name>Johan van der Molen Moris</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <summary>Style for the MDPI Open access journals</summary>\n    <updated>2020-12-17T13:36:58+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available online</term>\n      <term name=\"accessed\">accessed on</term>\n      <term name=\"presented at\">presented at the</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\", \" et-al-min=\"11\" et-al-use-first=\"10\" initialize-with=\".\" name-as-sort-order=\"all\" delimiter=\"; \" delimiter-precedes-last=\"always\"/>\n      <et-al term=\"et-al\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <text variable=\"collection-title\"/>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"book-container\">\n    <group delimiter=\" \">\n      <text variable=\"title\" text-case=\"title\" suffix=\".\"/>\n      <text term=\"in\" text-case=\"capitalize-first\"/>\n      <text variable=\"container-title\" font-style=\"italic\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"full-issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"month\" form=\"long\"/>\n      <date-part name=\"day\"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <text variable=\"ISBN\" prefix=\" ISBN \"/>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"DOI\" prefix=\", doi:\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\" \"/>\n      <choose>\n        <if type=\"article-magazine\">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"full-issued\" suffix=\",\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text variable=\"title\" text-case=\"title\"/>\n              <text variable=\"genre\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\"; \" suffix=\";\">\n            <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n            <text macro=\"editor\" prefix=\" \"/>\n            <text macro=\"series\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group delimiter=\"; \">\n            <text macro=\"book-container\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"series\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <group delimiter=\"; \">\n            <group delimiter=\" \">\n              <text variable=\"title\" text-case=\"title\" suffix=\".\"/>\n              <text variable=\"container-title\" prefix=\"In Proceedings of the \"/>\n            </group>\n            <text macro=\"editor\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"full-issued\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\" \">\n            <text variable=\"title\" text-case=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"available at\" text-case=\"capitalize-first\"/>\n              <text variable=\"URL\"/>\n            </group>\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <text term=\"accessed\"/>\n              <date delimiter=\" \" variable=\"accessed\">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <group delimiter=\" \">\n            <text variable=\"title\" text-case=\"title\" suffix=\".\"/>\n             <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"presented at\" text-case=\"capitalize-first\"/>\n                <text variable=\"event\"/>\n              </group>\n              <text variable=\"event-place\"/>\n              <text macro=\"issued\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text variable=\"title\" text-case=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            <group delimiter=\", \">\n              <text macro=\"issued\" font-weight=\"bold\"/>\n              <text variable=\"volume\" font-style=\"italic\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <group delimiter=\". \">\n              <text variable=\"title\" text-case=\"title\"/>\n              <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"issued\"/>\n              <text variable=\"volume\" font-style=\"italic\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"