"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"fr-FR\">\n  <info>\n    <title>Anthropologie et Sociétés (Français)</title>\n    <id>http://www.zotero.org/styles/anthropologie-et-societes</id>\n    <link href=\"http://www.zotero.org/styles/anthropologie-et-societes\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/limnetica\" rel=\"template\"/>\n    <link href=\"http://www.anthropologie-societes.ant.ulaval.ca/regles-de-publication/normes-editoriales\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0702-8997</issn>\n    <eissn>1703-7921</eissn>\n    <updated>2020-07-31T08:06:37+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"internet\">site internet</term>\n      <term name=\"editor\" form=\"short\">\n        <single>dir.</single>\n        <multiple>dir.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" vertical-align=\"baseline\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"first\" sort-separator=\" \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <choose>\n          <if variable=\"URL\">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"internet\"/>\n                <text variable=\"URL\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <group delimiter=\" \">\n                <text term=\"accessed\"/>\n                <date variable=\"accessed\" form=\"text\"/>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"legal_case\">\n    <group prefix=\" \" delimiter=\" \">\n      <text variable=\"volume\"/>\n      <text variable=\"container-title\"/>\n    </group>\n    <text variable=\"authority\" prefix=\" (\" suffix=\")\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\" match=\"none\">\n        <group delimiter=\", \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n        <text variable=\"genre\" prefix=\". \"/>\n      </if>\n      <else>\n        <group delimiter=\",  \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <group delimiter=\" \">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <date date-parts=\"year\" form=\"text\" variable=\"original-date\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"issue\" suffix=\":\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if locator=\"page\">\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation and=\"text\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" givenname-disambiguation-rule=\"primary-name\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout delimiter=\" ; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\" : \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text macro=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" text-case=\"uppercase\" strip-periods=\"false\" suffix=\", \"/>\n      <text macro=\"year-date\"/>\n      <choose>\n        <if type=\"book\" match=\"any\">\n          <text macro=\"legal_case\"/>\n          <group delimiter=\". \">\n            <group delimiter=\", \" prefix=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group delimiter=\" \" prefix=\" \" suffix=\".\">\n            <text term=\"in\" font-style=\"italic\"/>\n            <text macro=\"editor\" suffix=\":\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text variable=\"page\" suffix=\".\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill graphic legal_case legislation manuscript motion_picture report song thesis\" match=\"any\">\n          <text macro=\"legal_case\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" suffix=\".\"/>\n          </group>\n          <group prefix=\" \" delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text variable=\"page\" prefix=\" \" suffix=\"pp.\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \" prefix=\" \" suffix=\",\">\n            <text macro=\"title\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <group delimiter=\" : \" prefix=\" \" suffix=\".\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"