"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>Genetics</title>\n    <id>http://www.zotero.org/styles/genetics</id>\n    <link href=\"http://www.zotero.org/styles/genetics\" rel=\"self\"/>\n    <link href=\"http://www.genetics.org/content/prep-manuscript#references\" rel=\"documentation\"/>\n    <author>\n      <name>Mark Budde</name>\n      <email>markbudde@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0016-6731</issn>\n    <eissn>1943-2631</eissn>\n    <updated>2018-06-11T07:52:51+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\" sort-separator=\" \"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"first\" sort-separator=\" \"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"year-suffix\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" line-spacing=\"2\" et-al-min=\"6\" et-al-use-first=\"5\">\n    <sort>\n      <key macro=\"author-short\" names-min=\"1\" names-use-first=\"1\"/>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\" suffix=\",\"/>\n      <date variable=\"issued\" prefix=\" \">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group suffix=\".\" prefix=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"editor-translator\"/>\n          </group>\n          <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group delimiter=\" \" prefix=\", \" suffix=\".\">\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n            <text term=\"in\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\",\"/>\n              <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n            </group>\n            <group delimiter=\". \">\n              <names variable=\"editor translator\">\n                <label form=\"verb\" suffix=\" \"/>\n                <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\" prefix=\" \"/>\n                <et-al font-style=\"italic\"/>\n              </names>\n              <text macro=\"publisher\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <group delimiter=\", \" prefix=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"editor-translator\"/>\n            </group>\n            <group delimiter=\" \" prefix=\" \" suffix=\".\">\n              <text variable=\"container-title\" form=\"short\"/>\n              <group delimiter=\": \">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"