"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Helvetica Chimica Acta</title>\n    <id>http://www.zotero.org/styles/helvetica-chimica-acta</id>\n    <link href=\"http://www.zotero.org/styles/helvetica-chimica-acta\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/angewandte-chemie\" rel=\"template\"/>\n    <link href=\"http://www.helvchimacta.ch/downloads/Instructions_for_Authors.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Daniel Probst</name>\n      <email>daenuprobst@gmail.com</email>\n      <uri>http://danielprobst.net</uri>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <category field=\"chemistry\"/>\n    <issn>0018-019X</issn>\n    <eissn>1522-2675</eissn>\n    <summary>A style for Wiley-VCH's journal \"Helvetica Chimica Acta\" based on \"Angewandte Chemie International Edition\". The style incorrectly cites two items cited together as [1,2] instead of [1-2]</summary>\n    <updated>2019-07-03T16:34:22+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\" suffix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" text-case=\"capitalize-first\" suffix=\": \"/>\n      <name initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <group font-weight=\"bold\">\n          <choose>\n            <if variable=\"issued\">\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n            </if>\n            <else>\n              <text term=\"no date\" form=\"short\"/>\n            </else>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"DOI \"/>\n          </if>\n        </choose>\n        <choose>\n          <if type=\"webpage\">\n            <text variable=\"URL\" prefix=\"can be found under \"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text term=\"in\"/>\n              <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n              <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"year-date\"/>\n            <group delimiter=\" \">\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n            <text macro=\"year-date\"/>\n            <text variable=\"number\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\" text-case=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text macro=\"access\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <!-- <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/> -->\n            <group delimiter=\", \">\n              <text variable=\"title\" quotes=\"true\"/>\n              <text macro=\"container-title\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"year-date\"/>\n              <group>\n                <text variable=\"volume\" font-style=\"italic\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n          <text macro=\"access\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"