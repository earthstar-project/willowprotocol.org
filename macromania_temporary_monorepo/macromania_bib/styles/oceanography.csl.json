"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Oceanography</title>\n    <id>http://www.zotero.org/styles/oceanography</id>\n    <link href=\"http://www.zotero.org/styles/oceanography\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/palaeovertebrata\" rel=\"template\"/>\n    <link href=\"http://tos.org/oceanography/guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <issn>1042-8275</issn>\n    <updated>2017-02-08T16:29:47+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\".\">\n      <name font-variant=\"normal\" and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n      <label form=\"long\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name suffix=\",\" and=\"text\" initialize-with=\". \" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if type=\"book chapter webpage\" variable=\"container-title volume page\" match=\"none\">\n        <text term=\"forthcoming\"/>\n      </else-if>\n      <else-if type=\"book chapter webpage\" variable=\"volume page\" match=\"none\">\n        <text term=\"in press\"/>\n      </else-if>\n      <else>\n        <choose>\n          <if type=\"webpage\" match=\"none\">\n            <text term=\"no date\" form=\"short\"/>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if variable=\"edition\">\n        <text variable=\"container-title\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"website-title\">\n    <choose>\n      <if type=\"webpage\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"false\" disambiguate-add-givenname=\"false\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"website-title\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <text variable=\"locator\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <group suffix=\". \">\n          <choose>\n            <if type=\"webpage\" match=\"none\">\n              <choose>\n                <if variable=\"issued\">\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                </if>\n                <else-if type=\"book chapter webpage\" variable=\"container-title volume page\" match=\"none\">\n                  <text term=\"forthcoming\" text-case=\"capitalize-first\"/>\n                </else-if>\n                <else-if type=\"book chapter webpage\" variable=\"volume page\" match=\"none\">\n                  <text term=\"in press\" text-case=\"capitalize-first\"/>\n                </else-if>\n                <else>\n                  <text term=\"no date\" form=\"short\"/>\n                </else>\n              </choose>\n            </if>\n            <else>\n              <choose>\n                <if match=\"any\" variable=\"author\">\n                  <text macro=\"year-date\"/>\n                </if>\n              </choose>\n            </else>\n          </choose>\n        </group>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group suffix=\".\">\n              <group delimiter=\", \">\n                <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n                <text macro=\"edition\"/>\n              </group>\n              <text macro=\"editor\" prefix=\" (\" suffix=\")\"/>\n            </group>\n            <text prefix=\" \" suffix=\", \" macro=\"publisher\"/>\n            <choose>\n              <if variable=\"number-of-pages\">\n                <text variable=\"number-of-pages\"/>\n              </if>\n              <else>\n                <text variable=\"page\"/>\n              </else>\n            </choose>\n            <text term=\"page\" form=\"short\" plural=\"true\"/>\n          </if>\n          <else-if type=\"thesis\" match=\"any\">\n            <text variable=\"title\" suffix=\".\"/>\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n              <text variable=\"number-of-pages\"/>\n            </group>\n            <text term=\"page\" form=\"short\"/>\n          </else-if>\n          <else-if type=\"chapter\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"title\" suffix=\"; \"/>\n              <text term=\"page\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\". \"/>\n              <text variable=\"page\" suffix=\". \"/>\n              <text term=\"in\" text-case=\"capitalize-first\" font-style=\"normal\"/>\n              <group delimiter=\", \">\n                <text macro=\"container-title\" text-case=\"title\"/>\n                <text variable=\"collection-title\" prefix=\". \"/>\n                <text macro=\"edition\"/>\n              </group>\n              <text macro=\"editor\" suffix=\",\"/>\n              <group delimiter=\". \">\n                <choose>\n                  <if is-numeric=\"volume\">\n                    <group delimiter=\" \">\n                      <text value=\"Vol. \"/>\n                      <number variable=\"volume\" suffix=\".\"/>\n                    </group>\n                  </if>\n                  <else>\n                    <text variable=\"volume\" suffix=\".\"/>\n                  </else>\n                </choose>\n                <text macro=\"publisher\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <choose>\n                <if match=\"none\" variable=\"author\">\n                  <text macro=\"year-date\"/>\n                </if>\n              </choose>\n              <text variable=\"container-title\" form=\"short\"/>\n              <date variable=\"accessed\" prefix=\"Retrieved \">\n                <date-part name=\"month\" suffix=\" \"/>\n                <date-part name=\"day\" suffix=\", \"/>\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"URL\"/>\n            </group>\n            <text prefix=\" \" macro=\"publisher\"/>\n          </else-if>\n          <else>\n            <text variable=\"title\" suffix=\". \"/>\n            <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\" suffix=\",\"/>\n            <group delimiter=\":\">\n              <group>\n                <text variable=\"volume\" prefix=\" \"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\" suffix=\",\"/>\n            </group>\n          </else>\n        </choose>\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"