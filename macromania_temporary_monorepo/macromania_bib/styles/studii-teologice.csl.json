"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <!-- Polyglot; journal publishes in English, French, German, and Romanian -->\n  <!-- This style was edited with the Visual CSL Editor (https://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Studii Teologice</title>\n    <id>http://www.zotero.org/styles/studii-teologice</id>\n    <link href=\"http://www.zotero.org/styles/studii-teologice\" rel=\"self\"/>\n    <link href=\"http://www.studiiteologice.ro/index.php?option=com_content&amp;view=article&amp;id=24\" rel=\"documentation\"/>\n    <author>\n      <name>Nicolae Turcan</name>\n      <email>nturcan@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"theology\"/>\n    <issn>1011-8845</issn>\n    <summary>Studii Teologice Review, full notes</summary>\n    <updated>2023-07-02T18:43:58+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"ro\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"et-al\">et alii</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <label form=\"verb\" text-case=\"lowercase\"/>\n      <name delimiter=\", \" prefix=\" \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <choose>\n      <if variable=\"author\">\n        <names variable=\"editor\" delimiter=\", \">\n          <name delimiter=\", \" delimiter-precedes-last=\"always\">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n          <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\" delimiter=\", \">\n      <label form=\"short\" text-case=\"lowercase\"/>\n      <name prefix=\" \" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if variable=\"author\">\n        <names variable=\"author\">\n          <name delimiter=\", \" delimiter-precedes-last=\"always\" sort-separator=\", \">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n        </names>\n      </if>\n    </choose>\n    <choose>\n      <if variable=\"author\" match=\"none\">\n        <choose>\n          <if variable=\"editor\">\n            <names variable=\"editor\">\n              <name delimiter=\", \" delimiter-precedes-last=\"always\" sort-separator=\", \">\n                <name-part name=\"family\" font-variant=\"small-caps\"/>\n              </name>\n              <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n            </names>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author-subsequent\">\n    <choose>\n      <if variable=\"author\">\n        <names variable=\"author\">\n          <name delimiter=\", \" delimiter-precedes-last=\"always\" sort-separator=\", \" initialize-with=\".\">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n        </names>\n      </if>\n    </choose>\n    <choose>\n      <if variable=\"author\" match=\"none\">\n        <choose>\n          <if variable=\"editor\">\n            <names variable=\"editor\">\n              <name delimiter=\", \" delimiter-precedes-last=\"always\" sort-separator=\", \" initialize-with=\".\">\n                <name-part name=\"family\" font-variant=\"small-caps\"/>\n              </name>\n              <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n            </names>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"book-author\">\n    <choose>\n      <if variable=\"container-author\">\n        <names variable=\"container-author\">\n          <name delimiter=\", \" delimiter-precedes-last=\"always\" sort-separator=\", \">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"recipient\">\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"lowercase\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n        <date variable=\"accessed\" prefix=\", \">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \" text-case=\"lowercase\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"graphic report\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \" text-case=\"lowercase\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \" text-case=\"lowercase\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\" \" prefix=\"coll. \">\n      <text variable=\"collection-title\" font-style=\"italic\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"author-bib\">\n    <choose>\n      <if match=\"all\" variable=\"author\">\n        <names variable=\"author\">\n          <name delimiter=\"; \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n        </names>\n      </if>\n    </choose>\n    <choose>\n      <if match=\"none\" variable=\"author\">\n        <names variable=\"editor\">\n          <name delimiter=\"; \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n          <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"false\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <group>\n              <text macro=\"author-subsequent\"/>\n              <text macro=\"recipient\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"title-short\"/>\n              <choose>\n                <if type=\"book\" match=\"any\">\n                  <text macro=\"volume\"/>\n                </if>\n              </choose>\n            </group>\n            <choose>\n              <if type=\"interview\">\n                <text term=\"interview\" text-case=\"lowercase\"/>\n              </if>\n              <else-if variable=\"recipient\">\n                <text macro=\"issued\"/>\n              </else-if>\n            </choose>\n            <text macro=\"locators\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <group delimiter=\", \">\n            <group>\n              <text macro=\"author-subsequent\"/>\n              <text macro=\"recipient\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"title-short\"/>\n              <choose>\n                <if type=\"book\" match=\"any\">\n                  <text macro=\"volume\"/>\n                </if>\n              </choose>\n            </group>\n            <choose>\n              <if type=\"interview\">\n                <text term=\"interview\" text-case=\"lowercase\"/>\n              </if>\n              <else-if variable=\"recipient\">\n                <text macro=\"issued\"/>\n              </else-if>\n            </choose>\n            <text macro=\"locators\"/>\n          </group>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <group>\n              <text macro=\"author-subsequent\"/>\n              <text macro=\"recipient\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"title-short\"/>\n              <choose>\n                <if type=\"book\" match=\"any\">\n                  <text macro=\"volume\"/>\n                </if>\n              </choose>\n            </group>\n            <choose>\n              <if type=\"interview\">\n                <text term=\"interview\" text-case=\"lowercase\"/>\n              </if>\n              <else-if variable=\"recipient\">\n                <text macro=\"issued\"/>\n              </else-if>\n            </choose>\n            <text macro=\"locators\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"recipient\"/>\n          </group>\n          <text macro=\"title\"/>\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <group>\n                <text term=\"in\" text-case=\"lowercase\" prefix=\", \" suffix=\":\"/>\n                <group delimiter=\", \" prefix=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <text macro=\"editor-short\"/>\n                  <text macro=\"book-author\"/>\n                </group>\n                <text macro=\"volume\" prefix=\", \"/>\n                <text macro=\"collection\" prefix=\", \"/>\n              </group>\n              <text macro=\"translator\" prefix=\", \"/>\n              <text variable=\"edition\" prefix=\", \"/>\n              <text macro=\"publisher\" prefix=\", \"/>\n              <text macro=\"issued\" prefix=\", \"/>\n            </if>\n            <else-if type=\"article-journal\" match=\"any\">\n              <group>\n                <text term=\"in\" text-case=\"lowercase\" prefix=\", \" suffix=\":\"/>\n                <group prefix=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <group delimiter=\", \" prefix=\", \">\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\"/>\n                  </group>\n                  <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n                </group>\n                <text macro=\"editor\" prefix=\", \"/>\n                <text macro=\"translator\" prefix=\", \"/>\n                <text variable=\"edition\" prefix=\", \"/>\n                <text macro=\"publisher\" prefix=\", \"/>\n              </group>\n            </else-if>\n            <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n              <group>\n                <text term=\"in\" text-case=\"lowercase\" prefix=\", \" suffix=\":\"/>\n                <group prefix=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <group delimiter=\", \" prefix=\", \">\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\"/>\n                  </group>\n                  <text macro=\"issued\" prefix=\", \"/>\n                </group>\n                <text macro=\"editor\" prefix=\", \"/>\n                <text macro=\"translator\" prefix=\", \"/>\n                <text variable=\"edition\" prefix=\", \"/>\n                <text macro=\"publisher\" prefix=\", \"/>\n              </group>\n            </else-if>\n            <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <text macro=\"volume\" prefix=\", \"/>\n              <text macro=\"collection\" prefix=\", \"/>\n              <text macro=\"translator\" prefix=\", \"/>\n              <choose>\n                <if variable=\"author\">\n                  <text macro=\"editor\" prefix=\", \"/>\n                </if>\n              </choose>\n              <text variable=\"edition\" prefix=\", \"/>\n              <text macro=\"publisher\" prefix=\", \"/>\n              <text macro=\"issued\" prefix=\", \"/>\n            </else-if>\n            <else>\n              <group>\n                <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n                <text variable=\"volume\" prefix=\" \"/>\n                <text variable=\"issue\" prefix=\", \"/>\n              </group>\n            </else>\n          </choose>\n          <text macro=\"locators\" prefix=\", \"/>\n          <text macro=\"access\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography>\n    <layout suffix=\".\">\n      <text macro=\"author-bib\" suffix=\", \"/>\n      <text macro=\"title\"/>\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <group>\n            <text term=\"in\" text-case=\"lowercase\" prefix=\", \" suffix=\":\"/>\n            <group delimiter=\", \" prefix=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"editor-short\"/>\n              <text macro=\"book-author\"/>\n            </group>\n            <text macro=\"volume\" prefix=\", \"/>\n            <text macro=\"collection\" prefix=\", \"/>\n          </group>\n          <text macro=\"translator\" prefix=\", \"/>\n          <text variable=\"edition\" prefix=\", \"/>\n          <text macro=\"publisher\" prefix=\", \"/>\n          <text macro=\"issued\" prefix=\", \"/>\n        </if>\n        <else-if type=\"article-journal\" match=\"any\">\n          <group>\n            <text term=\"in\" text-case=\"lowercase\" prefix=\", \" suffix=\":\"/>\n            <group prefix=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group delimiter=\", \" prefix=\", \">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\"/>\n              </group>\n              <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n            </group>\n            <text macro=\"editor\" prefix=\", \"/>\n            <text macro=\"translator\" prefix=\", \"/>\n            <text variable=\"edition\" prefix=\", \"/>\n            <text macro=\"publisher\" prefix=\", \"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n          <group>\n            <text term=\"in\" text-case=\"lowercase\" prefix=\", \" suffix=\":\"/>\n            <group prefix=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group delimiter=\", \" prefix=\", \">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\"/>\n              </group>\n              <text macro=\"issued\" prefix=\", \"/>\n            </group>\n            <text macro=\"editor\" prefix=\", \"/>\n            <text macro=\"translator\" prefix=\", \"/>\n            <text variable=\"edition\" prefix=\", \"/>\n            <text macro=\"publisher\" prefix=\", \"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"volume\" prefix=\", \"/>\n          <text macro=\"collection\" prefix=\", \"/>\n          <text macro=\"translator\" prefix=\", \"/>\n          <choose>\n            <if match=\"any\" variable=\"author\">\n              <text macro=\"editor\" prefix=\", \"/>\n            </if>\n          </choose>\n          <text variable=\"edition\" prefix=\", \"/>\n          <text macro=\"publisher\" prefix=\", \"/>\n          <text macro=\"issued\" prefix=\", \"/>\n        </else-if>\n        <else>\n          <group>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n            <text variable=\"volume\" prefix=\" \"/>\n            <text variable=\"issue\" prefix=\", \"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\", \"/>\n    </layout>\n  </bibliography>\n</style>\n"