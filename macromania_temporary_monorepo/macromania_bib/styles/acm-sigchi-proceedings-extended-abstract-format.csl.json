"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>ACM SIGCHI Proceedings - Extended Abstract Format</title>\n    <title-short>CHI Extended Abstract Format</title-short>\n    <id>http://www.zotero.org/styles/acm-sigchi-proceedings-extended-abstract-format</id>\n    <link href=\"http://www.zotero.org/styles/acm-sigchi-proceedings-extended-abstract-format\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/acm-sigchi-proceedings\" rel=\"template\"/>\n    <link href=\"http://www.sigchi.org/publications/chipubform/sigchi-extended-abstracts-format-2016/view\" rel=\"documentation\"/>\n    <author>\n      <name>Jo Vermeulen</name>\n      <email>jo.vermeulen@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <updated>2014-04-08T11:45:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name delimiter=\", \" and=\"text\" sort-separator=\", \"/>\n        <label form=\"short\" prefix=\", \"/>\n        <substitute>\n          <names variable=\"editor\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"journal\">\n    <group delimiter=\" \">\n      <text variable=\"container-title\" font-style=\"italic\"/>\n      <group delimiter=\", \">\n        <text variable=\"volume\"/>\n        <text variable=\"issue\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"conference\">\n    <group delimiter=\", \">\n      <text variable=\"container-title\" font-style=\"italic\"/>\n      <group delimiter=\" \">\n        <text variable=\"publisher\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"book-publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \">\n          <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n          <text term=\"from\"/>\n          <text variable=\"URL\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation>\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <group delimiter=\":\">\n        <text variable=\"citation-number\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\" suffix=\". \"/>\n      <text macro=\"year\" suffix=\". \"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"book-publisher\" suffix=\".\"/>\n        </if>\n        <else-if type=\"paper-conference\">\n          <group suffix=\".\" delimiter=\", \">\n            <text macro=\"conference\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <text macro=\"editor\" suffix=\", \"/>\n          <text variable=\"container-title\" font-style=\"italic\" suffix=\". \"/>\n          <group suffix=\".\" delimiter=\", \">\n            <text macro=\"book-publisher\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group suffix=\".\" delimiter=\": \">\n            <text macro=\"journal\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\" delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n          <text macro=\"access\" prefix=\" \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"