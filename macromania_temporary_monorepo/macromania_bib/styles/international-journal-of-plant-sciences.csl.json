"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>International Journal of Plant Sciences</title>\n    <title-short>IJPS</title-short>\n    <id>http://www.zotero.org/styles/international-journal-of-plant-sciences</id>\n    <link href=\"http://www.zotero.org/styles/international-journal-of-plant-sciences\" rel=\"self\"/>\n    <link href=\"http://www.press.uchicago.edu/journals/ijps/instruct.html?journal=ijps\" rel=\"documentation\"/>\n    <author>\n      <name>Ekaphan Kraichak</name>\n      <email>ekraichak@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <issn>1058-5893</issn>\n    <eissn>1537-5315</eissn>\n    <updated>2014-11-12T06:04:21+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editortranslator\" form=\"long\">\n        <single>editor and translator</single>\n        <multiple>editors and translators</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\" delimiter=\"; \" suffix=\".\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"first\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" strip-periods=\"false\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" initialize-with=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title-short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"review\">\n    <group delimiter=\". \">\n      <text variable=\"reviewed-title\"/>\n      <text variable=\"container-title\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"accessed\" match=\"any\">\n        <group delimiter=\". \">\n          <group prefix=\" [\" suffix=\"]\" delimiter=\" \">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\" delimiter=\" \">\n              <date-part name=\"year\"/>\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"day\"/>\n            </date>\n          </group>\n          <text variable=\"URL\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"thesis\" match=\"any\">\n          <text variable=\"genre\" form=\"long\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <label plural=\"never\" suffix=\" \" variable=\"page\" form=\"short\"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <group delimiter=\" \">\n      <date delimiter=\" \" variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"patent article-newspaper webpage\" match=\"any\">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n        </if>\n        <else-if type=\"article-journal article-magazine\" match=\"any\">\n          <choose>\n            <if variable=\"volume issue\" match=\"none\">\n              <date variable=\"issued\" delimiter=\" \">\n                <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n                <date-part name=\"day\"/>\n              </date>\n            </if>\n          </choose>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <text variable=\"locator\" prefix=\": \"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\" hanging-indent=\"true\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n        <text macro=\"title\"/>\n      </group>\n      <group suffix=\".\">\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n            <group prefix=\" \" suffix=\".\" delimiter=\" \">\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text prefix=\" \" macro=\"publisher\"/>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group prefix=\" \" delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\" suffix=\".\"/>\n              <text variable=\"volume\" prefix=\"Vol. \" suffix=\".\"/>\n              <text macro=\"edition\"/>\n              <text variable=\"collection-title\" suffix=\".\"/>\n              <group suffix=\".\" delimiter=\". \">\n                <text macro=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"review review-book\" match=\"any\">\n            <text macro=\"editor\" prefix=\" \" suffix=\".\"/>\n            <group prefix=\" \" suffix=\".\">\n              <text macro=\"review\" suffix=\".\"/>\n              <group prefix=\" \">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </else-if>\n          <else>\n            <text macro=\"editor\" suffix=\".\" prefix=\" \"/>\n            <group prefix=\" \" suffix=\".\">\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n              <text variable=\"volume\" prefix=\" \"/>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"