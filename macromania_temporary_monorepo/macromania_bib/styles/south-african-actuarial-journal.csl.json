"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>South African Actuarial Journal</title>\n    <title-short>SAAJ</title-short>\n    <id>http://www.zotero.org/styles/south-african-actuarial-journal</id>\n    <link href=\"http://www.zotero.org/styles/south-african-actuarial-journal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/university-of-south-africa-harvard\" rel=\"template\"/>\n    <link href=\"https://www.ajol.info/index.php/saaj/about/submissions\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <issn>1680-2179</issn>\n    <updated>2020-01-28T14:46:23+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"volume\" form=\"short\">v.</term>\n      <term name=\"available at\">available</term>\n      <term name=\"open-quote\">“</term>\n      <term name=\"close-quote\">”</term>\n      <term name=\"open-inner-quote\">‘</term>\n      <term name=\"close-inner-quote\">’</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <choose>\n      <if variable=\"editor\">\n        <names variable=\"editor\">\n          <name and=\"symbol\" initialize-with=\".\" delimiter=\", \"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n    <choose>\n      <if variable=\"translator\">\n        <names variable=\"translator\">\n          <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <name and=\"symbol\" delimiter=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\"\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song webpage\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" quotes=\"true\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n    </names>\n  </macro>\n  <macro name=\"date-issued\">\n    <choose>\n      <if type=\"article-journal\" match=\"none\">\n        <text macro=\"date-issued-year\"/>\n      </if>\n      <else-if variable=\"page\" match=\"none\">\n        <text macro=\"date-issued-year\"/>\n      </else-if>\n      <else-if is-numeric=\"page\">\n        <text macro=\"date-issued-year\"/>\n      </else-if>\n      <else>\n        <text term=\"in press\" prefix=\"(\" suffix=\")\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date-issued-no-parentheses\">\n    <choose>\n      <if type=\"article-journal\" match=\"none\">\n        <text macro=\"date-issued-year\"/>\n      </if>\n      <else-if variable=\"page\" match=\"none\">\n        <text macro=\"date-issued-year\"/>\n      </else-if>\n      <else-if is-numeric=\"page\">\n        <text macro=\"date-issued-year\"/>\n      </else-if>\n      <else>\n        <text term=\"in press\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date-issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi.org/\"/>\n      </if>\n      <else-if variable=\"URL\">\n        <group delimiter=\" \">\n          <group>\n            <text term=\"available at\" text-case=\"capitalize-first\" suffix=\" from: \"/>\n            <text variable=\"URL\"/>\n          </group>\n          <text macro=\"date-access\"/>\n        </group>\n      </else-if>\n      <else-if variable=\"source accessed\" match=\"all\">\n        <group delimiter=\" \">\n          <text variable=\"source\"/>\n          <text macro=\"date-access\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"date-access\">\n    <date variable=\"accessed\" prefix=\"[Accessed \" suffix=\"]\">\n      <date-part name=\"day\" suffix=\" \"/>\n      <date-part name=\"month\"/>\n      <date-part name=\"year\" prefix=\" \"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song webpage\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"book-details\">\n    <group delimiter=\". \">\n      <group delimiter=\" \">\n        <number variable=\"edition\" form=\"ordinal\"/>\n        <label variable=\"edition\"/>\n      </group>\n      <group delimiter=\" \">\n        <label variable=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n        <text variable=\"volume\"/>\n      </group>\n      <text macro=\"editor-translator\"/>\n      <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n        <text variable=\"collection-title\"/>\n        <group delimiter=\" \">\n          <label variable=\"issue\" form=\"short\"/>\n          <text variable=\"collection-number\"/>\n        </group>\n      </group>\n      <text macro=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"6\" et-al-use-first=\"1\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" disambiguate-add-names=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\" year-suffix-delimiter=\",\">\n    <sort>\n      <key macro=\"date-issued-no-parentheses\" sort=\"descending\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"date-issued-no-parentheses\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" et-al-min=\"9\" et-al-use-first=\"8\">\n    <sort>\n      <key macro=\"author-short\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author\" names-min=\"3\" names-use-first=\"1\"/>\n      <key macro=\"date-issued-no-parentheses\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"date-issued\" prefix=\"(\" suffix=\")\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture post-weblog song webpage\" match=\"any\">\n            <text macro=\"title\"/>\n            <text macro=\"book-details\"/>\n          </if>\n          <else-if type=\"article-journal article-magazine\" match=\"any\">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group delimiter=\", \">\n              <choose>\n                <if variable=\"volume issue\" match=\"any\">\n                  <group>\n                    <text variable=\"volume\"/>\n                  </group>\n                </if>\n                <else>\n                  <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n                    <date-part name=\"month\" form=\"long\"/>\n                    <date-part name=\"day\" prefix=\", \"/>\n                  </date>\n                </else>\n              </choose>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-newspaper\" match=\"any\">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"publisher-place\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group delimiter=\": \">\n              <date variable=\"issued\">\n                <date-part name=\"day\" suffix=\" \"/>\n                <date-part name=\"month\" form=\"long\"/>\n              </date>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n          </else-if>\n          <else-if type=\"chapter paper-conference entry-encyclopedia entry-dictionary\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor-translator\"/>\n                <group delimiter=\": \" prefix=\"(\" suffix=\")\">\n                  <text macro=\"date-issued-year\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"patent\" match=\"any\">\n            <group font-style=\"italic\">\n              <text variable=\"number\" prefix=\"Patent No. \"/>\n            </group>\n            <group delimiter=\": \">\n              <text macro=\"publisher\"/>\n              <text variable=\"authority\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <text macro=\"title\"/>\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              <text variable=\"number\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </else-if>\n        </choose>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"