"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Duale Hochschule Baden-WÃ¼rttemberg - Department of International Business</title>\n    <id>http://www.zotero.org/styles/duale-hochschule-baden-wurttemberg-department-of-international-business</id>\n    <link href=\"http://www.zotero.org/styles/duale-hochschule-baden-wurttemberg-department-of-international-business\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/wiesbaden-business-school\" rel=\"template\"/>\n    <link href=\"https://www.dhbw-stuttgart.de/studierendenportale/bwl-ib/Zitierrichtlinien/Guidelines_for_Academic_Writing_v1.7.1.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"humanities\"/>\n    <summary>Based on the Guidelines for Academic Writing as of December 2022 (version 1.7).</summary>\n    <updated>2023-08-08T10:40:03+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\" form=\"long\">status as of</term>\n      <term name=\"on\">of</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"volume\" form=\"short\">Jg.</term>\n      <term name=\"issue\" form=\"long\">Heft</term>\n      <term name=\"retrieved\">zugegriffen am</term>\n      <term name=\"anonymous\" form=\"short\">o.V.</term>\n      <term name=\"accessed\">Stand</term>\n      <term name=\"section\" form=\"short\">\n        <single>Abs.</single>\n        <multiple>Abs.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\"; \" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <choose>\n          <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n            <text variable=\"container-title\"/>\n          </if>\n        </choose>\n        <text macro=\"anonymous\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"anonymous\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\" delimiter=\", \">\n      <name delimiter=\"; \" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\"/>\n      <substitute>\n        <names variable=\"editor translator\"/>\n        <text macro=\"anonymous\" strip-periods=\"false\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\"; \">\n      <name delimiter=\"; \" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \">\n      <text variable=\"URL\"/>\n      <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n        <text term=\"accessed\"/>\n        <date variable=\"accessed\">\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"day\" suffix=\", \"/>\n          <date-part form=\"long\" name=\"year\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part form=\"numeric\" name=\"day\" suffix=\".\"/>\n          <date-part form=\"numeric\" name=\"month\" suffix=\".\"/>\n          <date-part form=\"long\" name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"accessed\">\n          <date-part form=\"numeric\" name=\"day\" suffix=\".\"/>\n          <date-part form=\"numeric\" name=\"month\" suffix=\".\"/>\n          <date-part form=\"long\" name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if match=\"any\" is-uncertain-date=\"issued\">\n        <text term=\"no date\"/>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n    <choose>\n      <if type=\"interview\" match=\"any\">\n        <text variable=\"medium\" prefix=\", \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label strip-periods=\"false\" variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"report\" match=\"any\">\n          <text variable=\"publisher\"/>\n        </if>\n      </choose>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if match=\"any\" is-numeric=\"edition\">\n        <group delimiter=\". \">\n          <number variable=\"edition\"/>\n          <label text-case=\"capitalize-first\" variable=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"type-sort\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <text value=\"X\"/>\n      </if>\n      <else-if type=\"legal_case legislation\" match=\"any\">\n        <text value=\"Y\"/>\n      </else-if>\n      <else-if type=\"interview\" match=\"any\">\n        <text value=\"Z\"/>\n      </else-if>\n      <else>\n        <text value=\"A\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <layout delimiter=\"; \" suffix=\".\">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n        </group>\n        <choose>\n          <if type=\"post post-weblog webpage\" match=\"any\">\n            <text macro=\"access\"/>\n          </if>\n          <else-if type=\"interview\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n            </group>\n          </else-if>\n          <else-if type=\"entry-dictionary\" match=\"any\">\n            <text variable=\"title\"/>\n          </else-if>\n        </choose>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"type-sort\"/>\n      <key macro=\"author\"/>\n      <key macro=\"date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"legal_case\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"title\" font-weight=\"bold\"/>\n              <text term=\"from\"/>\n              <date form=\"numeric\" variable=\"issued\"/>\n            </group>\n            <text variable=\"authority\"/>\n            <text variable=\"number\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page-first\"/>\n            </group>\n          </group>\n        </if>\n        <else>\n          <group>\n            <group delimiter=\" \">\n              <group font-weight=\"bold\">\n                <text macro=\"author\"/>\n                <text macro=\"year-date\" prefix=\" (\" suffix=\"):\"/>\n              </group>\n              <text variable=\"title\"/>\n            </group>\n            <choose>\n              <if type=\"webpage post-weblog post\" match=\"any\">\n                <group delimiter=\". \" prefix=\", \">\n                  <text variable=\"container-title\"/>\n                  <text macro=\"access\"/>\n                </group>\n              </if>\n              <else-if type=\"speech\" match=\"any\">\n                <group delimiter=\", \" prefix=\". \">\n                  <text variable=\"publisher-place\"/>\n                  <text macro=\"date\" prefix=\", \"/>\n                  <text macro=\"access\" prefix=\", \"/>\n                </group>\n              </else-if>\n              <else-if type=\"article-journal\" match=\"any\">\n                <group delimiter=\", \" prefix=\". \">\n                  <text variable=\"container-title\"/>\n                  <group delimiter=\". \">\n                    <label variable=\"volume\" form=\"short\"/>\n                    <text variable=\"volume\"/>\n                  </group>\n                  <group delimiter=\" \">\n                    <label variable=\"issue\" form=\"short\"/>\n                    <text variable=\"issue\"/>\n                  </group>\n                  <text macro=\"pages\"/>\n                </group>\n              </else-if>\n              <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n                <group delimiter=\", \" prefix=\". \">\n                  <text variable=\"container-title\"/>\n                  <group delimiter=\" \">\n                    <group delimiter=\" \">\n                      <label text-case=\"capitalize-first\" variable=\"issue\" form=\"short\"/>\n                      <text variable=\"issue\"/>\n                    </group>\n                    <text term=\"on\"/>\n                    <date form=\"text\" variable=\"issued\"/>\n                  </group>\n                  <text macro=\"pages\"/>\n                </group>\n              </else-if>\n              <else-if type=\"chapter paper-conference\" match=\"any\">\n                <group delimiter=\". \" prefix=\". \">\n                  <group delimiter=\": \">\n                    <text term=\"in\" text-case=\"capitalize-first\"/>\n                    <text macro=\"editor\"/>\n                    <text variable=\"container-title\" font-style=\"normal\"/>\n                  </group>\n                  <group delimiter=\", \">\n                    <text macro=\"edition\"/>\n                    <text macro=\"publisher\"/>\n                    <text macro=\"pages\"/>\n                  </group>\n                </group>\n              </else-if>\n              <else-if type=\"thesis\" match=\"any\">\n                <group delimiter=\", \" prefix=\". \">\n                  <text variable=\"genre\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n              </else-if>\n              <else-if type=\"interview\" match=\"any\">\n                <group delimiter=\" \" prefix=\", \">\n                  <text variable=\"genre\"/>\n                  <date form=\"text\" variable=\"issued\" prefix=\" on \"/>\n                  <group delimiter=\" \">\n                    <text term=\"in\"/>\n                    <text variable=\"publisher-place\"/>\n                  </group>\n                </group>\n              </else-if>\n              <else-if type=\"report\" match=\"any\">\n                <group delimiter=\", \" prefix=\". \">\n                  <group delimiter=\" \">\n                    <text variable=\"genre\"/>\n                    <group delimiter=\" \">\n                      <label text-case=\"capitalize-first\" variable=\"issue\" form=\"short\"/>\n                      <text variable=\"number\"/>\n                    </group>\n                  </group>\n                  <text macro=\"publisher\"/>\n                </group>\n              </else-if>\n              <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n                <group delimiter=\", \" prefix=\", \">\n                  <text macro=\"edition\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n              </else-if>\n              <else>\n                <group delimiter=\", \" prefix=\". \">\n                  <text macro=\"edition\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"