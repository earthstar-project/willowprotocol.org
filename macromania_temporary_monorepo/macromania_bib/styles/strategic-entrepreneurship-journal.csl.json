"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Strategic Entrepreneurship Journal</title>\n    <id>http://www.zotero.org/styles/strategic-entrepreneurship-journal</id>\n    <link href=\"http://www.zotero.org/styles/strategic-entrepreneurship-journal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/strategic-management-journal\" rel=\"template\"/>\n    <link href=\"https://onlinelibrary.wiley.com/page/journal/1932443x/homepage/forauthors.html\" rel=\"documentation\"/>\n    <contributor>\n      <name>Fabio Motoki</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <issn>1932-4391</issn>\n    <eissn>1932-443X</eissn>\n    <updated>2019-01-05T01:36:46+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"retrieved\">available</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name name-as-sort-order=\"all\" initialize-with=\"\" delimiter=\", \" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"long\" font-style=\"normal\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"long\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n        </group>\n        <choose>\n          <if type=\"webpage post post-weblog\" match=\"any\">\n            <date variable=\"accessed\" form=\"text\" prefix=\" [\" suffix=\"]\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" font-style=\"normal\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"genre\" suffix=\", \"/>\n    <choose>\n      <if type=\"report thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\": \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <group>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <choose>\n            <if type=\"bill book graphic legal_case motion_picture report song article-journal chapter paper-conference\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part prefix=\", \" name=\"month\"/>\n                <date-part prefix=\" \" name=\"day\"/>\n              </date>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text variable=\"container-title\" font-style=\"normal\"/>\n          </group>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"normal\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group prefix=\" \">\n          <group>\n            <text variable=\"volume\" font-weight=\"normal\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <group prefix=\": \">\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture report song chapter paper-conference\" match=\"any\">\n        <group>\n          <group prefix=\", \">\n            <text term=\"volume\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1-\"/>\n          </group>\n          <number variable=\"volume\" form=\"numeric\" prefix=\", \"/>\n          <text variable=\"page\" prefix=\": \"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" collapse=\"year\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group>\n        <text macro=\"author-short\" suffix=\", \"/>\n        <text macro=\"issued-year\"/>\n        <text macro=\"citation-locator\" prefix=\": \"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"6\" et-al-use-first=\"1\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"issued\"/>\n        <text macro=\"title\"/>\n      </group>\n      <group delimiter=\", \" prefix=\". \">\n        <group delimiter=\", \">\n          <text macro=\"container-title\"/>\n          <text variable=\"collection-title\"/>\n          <text macro=\"container-contributors\"/>\n        </group>\n      </group>\n      <group delimiter=\". \" prefix=\". \">\n        <text macro=\"publisher\"/>\n      </group>\n      <text macro=\"locators\"/>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"