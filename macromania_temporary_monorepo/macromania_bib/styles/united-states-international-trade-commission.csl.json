"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"display-and-sort\" page-range-format=\"chicago\">\n  <info>\n    <title>United States International Trade Commission</title>\n    <title-short>USITC</title-short>\n    <id>http://www.zotero.org/styles/united-states-international-trade-commission</id>\n    <link href=\"http://www.zotero.org/styles/united-states-international-trade-commission\" rel=\"self\"/>\n    <link href=\"http://www.chicagomanualofstyle.org/tools_citationguide.html\" rel=\"documentation\"/>\n    <link href=\"https://www.zotero.org/groups/5338814/usitc_referencing/library\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"generic-base\"/>\n    <summary>A bibliographical style file for the United States International Trade Commission</summary>\n    <updated>2023-12-25T18:06:05+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"container-author\" form=\"verb\">by</term>\n      <term name=\"translator\" form=\"verb-short\">trans.</term>\n      <term name=\"editortranslator\" form=\"verb\">edited and translated by</term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"interviewer\" form=\"verb-short\">by</term>\n      <term name=\"recipient\" form=\"verb-short\">to</term>\n    </terms>\n  </locale>\n  <macro name=\"creator\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"first\" and=\"text\" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <choose>\n          <if type=\"broadcast entry-encyclopedia article-magazine article-newspaper post-weblog\" match=\"any\">\n            <text macro=\"container-title\"/>\n          </if>\n          <else>\n            <text macro=\"title\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"creator-note\">\n    <choose>\n      <if variable=\"citation-label\" match=\"any\">\n        <text variable=\"citation-label\"/>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name form=\"short\" and=\"text\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <choose>\n              <if type=\"entry-encyclopedia\" match=\"any\">\n                <text variable=\"container-title\"/>\n              </if>\n              <else-if type=\"article-magazine article-newspaper broadcast post-weblog\">\n                <text macro=\"container-title\"/>\n              </else-if>\n              <else>\n                <text macro=\"title-short\"/>\n              </else>\n            </choose>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"chapter-secondary-creators\">\n    <choose>\n      <if type=\"chapter\" match=\"any\">\n        <group delimiter=\", \">\n          <names variable=\"container-author\">\n            <label form=\"verb\" suffix=\" \"/>\n            <name and=\"text\" delimiter-precedes-last=\"always\"/>\n          </names>\n          <names variable=\"editor translator\">\n            <label form=\"verb\" suffix=\" \"/>\n            <name and=\"text\" delimiter-precedes-last=\"always\"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"other-secondary-creators\">\n    <choose>\n      <if type=\"chapter\" variable=\"part-title\" match=\"none\">\n        <text variable=\"part-title\"/>\n        <group delimiter=\", \">\n          <names variable=\"editor translator\" delimiter=\", \">\n            <label form=\"verb\" suffix=\" \" text-case=\"capitalize-first\"/>\n            <name and=\"text\" delimiter-precedes-last=\"always\"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"part-creators\">\n    <choose>\n      <if variable=\"part-title\">\n        <group delimiter=\" \" suffix=\",\">\n          <text variable=\"part-title\"/>\n          <names variable=\"container-author\">\n            <label form=\"verb\" suffix=\" \"/>\n            <name and=\"text\" delimiter-precedes-last=\"always\"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\">\n      <label form=\"verb-short\" suffix=\" \"/>\n      <name and=\"text\" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"identifier\">\n    <choose>\n      <if type=\"dataset\" match=\"any\">\n        <text variable=\"number\"/>\n      </if>\n      <else-if type=\"manuscript\">\n        <text variable=\"genre\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book report\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"interview\">\n        <group delimiter=\", \">\n          <text variable=\"title\"/>\n          <text macro=\"interviewer\"/>\n        </group>\n      </else-if>\n      <else-if type=\"manuscript map\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"book report\" match=\"any\">\n        <text variable=\"title\" form=\"short\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"interview\">\n        <group delimiter=\", \">\n          <text variable=\"title\"/>\n          <text macro=\"interviewer\"/>\n        </group>\n      </else-if>\n      <else-if type=\"manuscript map\">\n        <text variable=\"title\" form=\"short\"/>\n      </else-if>\n      <else>\n        <group delimiter=\" \">\n          <choose>\n            <if type=\"entry-encyclopedia\">\n              <choose>\n                <if variable=\"author editor\" match=\"none\">\n                  <text term=\"sub-verbo\" form=\"short\"/>\n                </if>\n              </choose>\n            </if>\n          </choose>\n          <text variable=\"title\" form=\"short\" quotes=\"true\" text-case=\"title\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"letter\">\n    <choose>\n      <if type=\"personal_communication\">\n        <group delimiter=\" \">\n          <choose>\n            <if variable=\"genre\" match=\"any\">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            </if>\n            <else>\n              <text term=\"letter\" text-case=\"capitalize-first\"/>\n            </else>\n          </choose>\n          <names variable=\"recipient\">\n            <label form=\"verb-short\" suffix=\" \"/>\n            <name and=\"text\" delimiter-precedes-last=\"always\"/>\n          </names>\n          <text variable=\"annote\" prefix=\"(\" suffix=\")\"/>\n          <text variable=\"source\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"letter-note\">\n    <choose>\n      <if type=\"personal_communication\">\n        <group delimiter=\" \">\n          <choose>\n            <if variable=\"genre\" match=\"any\">\n              <text variable=\"genre\"/>\n            </if>\n            <else>\n              <text term=\"letter\"/>\n            </else>\n          </choose>\n          <names variable=\"recipient\">\n            <label form=\"verb-short\" suffix=\" \"/>\n            <name and=\"text\"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"chapter\">\n          <text term=\"in\" text-case=\"capitalize-first\"/>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"document webpage\" match=\"none\">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"post-weblog\" match=\"any\">\n          <text variable=\"genre\" prefix=\"(\" suffix=\")\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"document-container\">\n    <choose>\n      <if type=\"document\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <text variable=\"volume\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"broadcast-container\">\n    <choose>\n      <if type=\"broadcast\" match=\"any\">\n        <text variable=\"collection-title\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"book-series\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"collection-title\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if type=\"paper-conference speech\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <choose>\n              <if variable=\"genre\" match=\"any\">\n                <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              </if>\n              <else>\n                <text term=\"speech\" text-case=\"capitalize-first\"/>\n              </else>\n            </choose>\n            <text term=\"presented at\"/>\n            <text variable=\"event\"/>\n          </group>\n          <date variable=\"event-date\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"web-genre\">\n    <choose>\n      <if type=\"webpage\" match=\"any\">\n        <text variable=\"genre\" text-case=\"capitalize-first\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages-chapter\">\n    <choose>\n      <if type=\"chapter\" match=\"any\">\n        <text variable=\"page\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition-other\">\n    <choose>\n      <if type=\"book\" match=\"none\">\n        <text macro=\"edition\" font-style=\"italic\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition-book\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text macro=\"edition\" font-style=\"italic\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume-book\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <group delimiter=\" \" font-style=\"italic\">\n          <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n          <text variable=\"volume\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-info\">\n    <choose>\n      <if type=\"article-journal\">\n        <group delimiter=\": \">\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text variable=\"volume\"/>\n              <group delimiter=\" \">\n                <text term=\"issue\" form=\"short\"/>\n                <text variable=\"issue\"/>\n              </group>\n            </group>\n            <date variable=\"issued\" form=\"text\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"magazine-date\">\n    <choose>\n      <if type=\"article-magazine article-newspaper\" match=\"any\">\n        <date variable=\"issued\" form=\"text\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"report-details\">\n    <choose>\n      <if type=\"report thesis\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"genre\"/>\n          <text variable=\"number\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"medium-other\">\n    <choose>\n      <if type=\"book chapter\" match=\"none\">\n        <group delimiter=\", \">\n          <text variable=\"medium\" text-case=\"capitalize-first\"/>\n          <text variable=\"dimensions\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"medium-book\">\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <text variable=\"medium\" text-case=\"capitalize-first\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"none\">\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <choose>\n            <if type=\"webpage\" match=\"any\">\n              <text variable=\"container-title\"/>\n            </if>\n          </choose>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\", \">\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"none\">\n        <date variable=\"issued\" form=\"text\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"date-note\">\n    <date variable=\"issued\" form=\"text\"/>\n  </macro>\n  <macro name=\"access-date\">\n    <choose>\n      <if variable=\"issued\" match=\"none\">\n        <group delimiter=\" \">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access-date-note\">\n    <choose>\n      <if variable=\"issued\" match=\"none\">\n        <group delimiter=\" \">\n          <text term=\"accessed\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"URL\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else>\n        <text variable=\"URL\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"cite-locator\">\n    <choose>\n      <if locator=\"page\" match=\"any\">\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"legal-cites\">\n    <choose>\n      <if type=\"bill\">\n        <group delimiter=\". \">\n          <text variable=\"authority\"/>\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n            <text variable=\"chapter-number\"/>\n          </group>\n          <date variable=\"issued\" form=\"text\"/>\n          <text variable=\"URL\"/>\n        </group>\n      </if>\n      <else-if type=\"legislation\">\n        <group delimiter=\". \">\n          <names variable=\"author contributor\" delimiter=\". \">\n            <name and=\"text\" name-as-sort-order=\"first\"/>\n          </names>\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text variable=\"title\"/>\n              <group delimiter=\" \">\n                <text variable=\"volume\"/>\n                <group delimiter=\", \">\n                  <text variable=\"container-title\"/>\n                  <text variable=\"page\" suffix=\",\"/>\n                </group>\n                <group delimiter=\" \">\n                  <label variable=\"section\" form=\"symbol\"/>\n                  <text variable=\"section\"/>\n                </group>\n              </group>\n              <text variable=\"number\" prefix=\"Pub. L. No. \"/>\n            </group>\n            <text variable=\"references\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <choose>\n            <if variable=\"issued\">\n              <date variable=\"issued\" form=\"text\"/>\n            </if>\n            <else>\n              <text macro=\"access-date\"/>\n            </else>\n          </choose>\n          <text variable=\"URL\"/>\n        </group>\n      </else-if>\n      <else-if type=\"legal_case\">\n        <group delimiter=\". \">\n          <text variable=\"authority\"/>\n          <text variable=\"title\"/>\n          <group delimiter=\": \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"volume\"/>\n              <date variable=\"issued\" form=\"text\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"hearing\">\n        <group delimiter=\". \">\n          <names variable=\"author contributor\" delimiter=\". \">\n            <name and=\"text\" name-as-sort-order=\"first\"/>\n          </names>\n          <text variable=\"authority\"/>\n          <text variable=\"section\"/>\n          <text variable=\"title\"/>\n          <group delimiter=\", \">\n            <text variable=\"chapter-number\"/>\n            <date variable=\"issued\" form=\"text\"/>\n          </group>\n          <text variable=\"URL\"/>\n        </group>\n      </else-if>\n      <else-if type=\"regulation\">\n        <group delimiter=\". \">\n          <text macro=\"creator\"/>\n          <text macro=\"title\"/>\n          <group delimiter=\": \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n          <text macro=\"date\"/>\n          <text variable=\"URL\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"legal-cites-note\">\n    <choose>\n      <if type=\"bill\">\n        <group delimiter=\" \">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n            <text variable=\"chapter-number\"/>\n          </group>\n          <date variable=\"issued\" form=\"text\" prefix=\"(\" suffix=\")\"/>\n        </group>\n        <text macro=\"cite-locator\" prefix=\", \"/>\n      </if>\n      <else-if type=\"legislation\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text variable=\"title-short\"/>\n              <group delimiter=\" \">\n                <text variable=\"volume\"/>\n                <text variable=\"container-title\"/>\n                <choose>\n                  <if locator=\"section\">\n                    <group delimiter=\" \">\n                      <label variable=\"locator\" form=\"symbol\"/>\n                      <text variable=\"locator\"/>\n                    </group>\n                  </if>\n                  <else>\n                    <group delimiter=\" \">\n                      <label variable=\"section\" form=\"symbol\"/>\n                      <text variable=\"section\"/>\n                    </group>\n                  </else>\n                </choose>\n              </group>\n              <text variable=\"number\" prefix=\"Pub. L. No. \"/>\n            </group>\n            <text variable=\"references\" prefix=\"(\" suffix=\")\"/>\n            <date variable=\"issued\" form=\"text\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <choose>\n            <if locator=\"section\" match=\"none\">\n              <text macro=\"cite-locator\"/>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n      <else-if type=\"legal_case\">\n        <group delimiter=\". \">\n          <text variable=\"title\" form=\"short\"/>\n          <group delimiter=\" \">\n            <text variable=\"volume\"/>\n            <text variable=\"container-title\" form=\"short\"/>\n            <group delimiter=\", \">\n              <text variable=\"page-first\"/>\n              <text variable=\"locator\"/>\n            </group>\n            <date variable=\"issued\" form=\"text\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"hearing\">\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"citation-label\" match=\"any\">\n              <text variable=\"citation-label\"/>\n            </if>\n            <else>\n              <names variable=\"author\">\n                <name and=\"text\" form=\"short\" name-as-sort-order=\"first\"/>\n                <substitute>\n                  <names variable=\"contributor\"/>\n                </substitute>\n              </names>\n              <text variable=\"authority\"/>\n            </else>\n          </choose>\n          <text variable=\"section\"/>\n          <text variable=\"title\" form=\"short\"/>\n          <group delimiter=\", \">\n            <text variable=\"chapter-number\"/>\n            <date variable=\"issued\" form=\"text\"/>\n          </group>\n          <text macro=\"cite-locator\"/>\n        </group>\n      </else-if>\n      <else-if type=\"regulation\">\n        <group delimiter=\", \">\n          <text variable=\"title\" form=\"short\"/>\n          <group delimiter=\" \">\n            <text variable=\"volume\"/>\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            <text variable=\"page-first\"/>\n            <text macro=\"date\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text macro=\"cite-locator\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" givenname-disambiguation-rule=\"primary-name\" collapse=\"year\" after-collapse-delimiter=\"; \">\n    <layout delimiter=\"; \" suffix=\".\">\n      <choose>\n        <if type=\"bill legal_case hearing legislation regulation\" match=\"any\">\n          <text macro=\"legal-cites-note\"/>\n        </if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"creator-note\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"volume-book\"/>\n            <text macro=\"edition-book\"/>\n            <text macro=\"letter-note\"/>\n            <text macro=\"medium-other\"/>\n            <text variable=\"archive_collection\"/>\n            <text macro=\"date-note\"/>\n            <text macro=\"access-date-note\"/>\n            <text macro=\"cite-locator\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"11\" et-al-use-first=\"7\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"creator\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"bill legal_case hearing legislation regulation\" match=\"any\">\n          <text macro=\"legal-cites\"/>\n        </if>\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"creator\"/>\n            <text macro=\"identifier\"/>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"volume-book\"/>\n              <text macro=\"edition-book\"/>\n            </group>\n            <text macro=\"other-secondary-creators\"/>\n            <text macro=\"broadcast-container\"/>\n            <text macro=\"book-series\"/>\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <group delimiter=\", \">\n                  <text macro=\"container-title\"/>\n                  <text macro=\"edition-other\"/>\n                  <text macro=\"part-creators\"/>\n                </group>\n                <text macro=\"journal-info\"/>\n              </group>\n              <text macro=\"magazine-date\"/>\n              <text macro=\"chapter-secondary-creators\"/>\n              <text macro=\"pages-chapter\"/>\n            </group>\n            <text macro=\"report-details\"/>\n            <text macro=\"medium-other\"/>\n            <group delimiter=\", \">\n              <text macro=\"letter\"/>\n              <text macro=\"event\"/>\n              <text macro=\"web-genre\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"document-container\"/>\n              <text variable=\"archive_collection\"/>\n              <text macro=\"date\"/>\n            </group>\n            <text macro=\"medium-book\"/>\n            <text macro=\"archive\"/>\n            <text macro=\"access-date\"/>\n            <text macro=\"URL\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"