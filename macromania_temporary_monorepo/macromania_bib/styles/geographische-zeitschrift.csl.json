"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\">\n  <!-- Polyglot; journal publishes in German and English -->\n  <info>\n    <title>Geographische Zeitschrift</title>\n    <id>http://www.zotero.org/styles/geographische-zeitschrift</id>\n    <link href=\"http://www.zotero.org/styles/geographische-zeitschrift\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/suburban-zeitschrift-fur-kritische-stadtforschung\" rel=\"template\"/>\n    <link href=\"http://www.steiner-verlag.de/uploads/media/2019-08_MB_Zitierrichtlinien_GZ_engl.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geography\"/>\n    <issn>0016-7479</issn>\n    <eissn>2365-3124</eissn>\n    <updated>2020-07-01T08:57:18+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"accessed\">letzter Zugriff</term>\n      <term name=\"presented at\">Paper pr√§sentiert bei der</term>\n      <term name=\"anonymous\" form=\"long\">Ohne Verfasser</term>\n      <term name=\"anonymous\" form=\"short\">O.V.</term>\n    </terms>\n  </locale>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\": \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <names variable=\"editor translator\" delimiter=\", \">\n              <name delimiter-precedes-last=\"always\" et-al-min=\"3\" et-al-use-first=\"1\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n              <label form=\"short\" text-case=\"capitalize-first\" prefix=\" (\" suffix=\")\"/>\n            </names>\n          </group>\n          <group delimiter=\", \" suffix=\". \">\n            <text variable=\"container-title\" text-case=\"title\"/>\n            <text variable=\"collection-title\" text-case=\"title\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"book graphic legal_case motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"container-title\"/>\n          <text variable=\"collection-title\"/>\n        </group>\n      </else-if>\n      <else-if type=\"article-journal article-magazine article\" match=\"any\">\n        <group delimiter=\" \">\n          <group delimiter=\": \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text variable=\"collection-title\"/>\n            </group>\n          </group>\n          <group delimiter=\", \">\n            <group delimiter=\"/\">\n              <text variable=\"volume\"/>\n              <text variable=\"issue\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <group delimiter=\" \">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\"/>\n              <text variable=\"volume\"/>\n              <group delimiter=\"/\">\n                <text variable=\"number\"/>\n                <date date-parts=\"year\" form=\"numeric\" variable=\"issued\"/>\n              </group>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n          <group delimiter=\" \">\n            <text value=\"in der Fassung\"/>\n            <text variable=\"references\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\": \">\n            <text term=\"in\"/>\n            <text variable=\"container-title\"/>\n          </group>\n          <date variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\" form=\"numeric\" prefix=\".\"/>\n            <date-part name=\"year\" prefix=\".\"/>\n          </date>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"container-title\"/>\n          <text variable=\"collection-title\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\"/\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <group>\n          <text term=\"anonymous\" form=\"short\"/>\n        </group>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <date variable=\"accessed\" prefix=\"(\" suffix=\")\">\n            <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n            <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <text variable=\"title\"/>\n        <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n          <text variable=\"genre\"/>\n          <text variable=\"number\" prefix=\"Nr. \"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song speech\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\"/>\n          <text variable=\"medium\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <text variable=\"event\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </if>\n        <else>\n          <text variable=\"edition\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"pages\"/>\n        </group>\n      </else-if>\n      <else-if type=\"patent\">\n        <text variable=\"number\"/>\n      </else-if>\n      <else-if type=\"thesis\" match=\"any\">\n        <group delimiter=\". \">\n          <text macro=\"publisher\"/>\n          <text variable=\"genre\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"author-year-title\">\n    <group delimiter=\": \">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <group prefix=\"(\" suffix=\")\">\n          <text macro=\"issued\"/>\n          <date date-parts=\"year\" form=\"text\" variable=\"original-date\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </group>\n      <text macro=\"title\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"descending\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <choose>\n            <if type=\"legislation\" match=\"any\">\n              <text variable=\"title-short\"/>\n            </if>\n            <else>\n              <text macro=\"author-short\"/>\n            </else>\n          </choose>\n          <text macro=\"issued\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"3\" et-al-use-first=\"1\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"descending\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author-year-title\"/>\n        <group>\n          <text macro=\"container\"/>\n          <text macro=\"locators\"/>\n        </group>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"