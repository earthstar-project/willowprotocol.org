   <text macro=\"anon\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text value=\"Available from:\" suffix=\" \"/>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"year\"/>\n      <date-part name=\"month\" prefix=\" \"/>\n      <date-part name=\"day\" prefix=\" \"/>\n    </date>\n    <group prefix=\" [\" suffix=\"]\" delimiter=\" \">\n      <text term=\"cited\"/>\n      <date variable=\"accessed\">\n        <date-part name=\"year\"/>\n        <date-part name=\"month\" prefix=\" \"/>\n        <date-part name=\"day\" prefix=\" \"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <label strip-periods=\"true\" suffix=\" \" variable=\"page\" form=\"short\"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"journal\">\n    <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n    <choose>\n      <if variable=\"URL\">\n        <text term=\"internet\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"baseline\" delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\" second-field-align=\"margin\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" suffix=\".\" delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text prefix=\" \" macro=\"publisher\"/>\n          <group suffix=\".\" prefix=\"; \" delimiter=\" \">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n            <text macro=\"pages\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" suffix=\".\"/>\n            <text variable=\"volume\" prefix=\"Vol. \" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <group suffix=\".\">\n              <text macro=\"publisher\"/>\n              <group suffix=\".\" prefix=\"; \" delimiter=\". \">\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix