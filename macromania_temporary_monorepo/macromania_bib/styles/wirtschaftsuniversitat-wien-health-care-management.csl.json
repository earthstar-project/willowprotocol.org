"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"symbol\" initialize=\"false\" initialize-with=\".\" demote-non-dropping-particle=\"never\">\n  <info>\n    <title>Wirtschaftsuniversität Wien - Health Care Management</title>\n    <title-short>WU HCM</title-short>\n    <id>http://www.zotero.org/styles/wirtschaftsuniversitat-wien-health-care-management</id>\n    <link href=\"http://www.zotero.org/styles/wirtschaftsuniversitat-wien-health-care-management\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/wirtschaftsuniversitat-wien-author-date\" rel=\"template\"/>\n    <link href=\"https://www.complexity-research.com/pdf/WissArb.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Daniel Kufner</name>\n      <email>daniel.kufner@wu.ac.at</email>\n      <uri>http://wu.ac.at/library</uri>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <summary>Citation style for WU Vienna’s MBA for Health Care Management</summary>\n    <updated>2020-11-17T15:37:30+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editortranslator\" form=\"short\">\n        <single>ed. &amp; trans.</single>\n        <multiple>eds. &amp; trans.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"anonymous\">No author</term>\n      <term name=\"ad\">of</term>\n      <term name=\"presented at\">Paper presented at the</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"editortranslator\" form=\"short\">\n        <single>ed. &amp; trans.</single>\n        <multiple>eds. &amp; trans.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"accessed\">Abgefragt am</term>\n      <term name=\"ad\">vom</term>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"presented at\">Paper präsentiert bei der</term>\n      <term name=\"anonymous\">Anonym</term>\n      <term name=\"no date\">o.J.</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\", \" suffix=\" \">\n          <names variable=\"container-author\" delimiter=\", \">\n            <name delimiter-precedes-last=\"always\" initialize=\"false\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n            <label form=\"short\" prefix=\" (\" text-case=\"title\" suffix=\")\"/>\n          </names>\n          <names variable=\"editor translator\" delimiter=\", \">\n            <name delimiter-precedes-last=\"always\" initialize=\"false\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n            <label form=\"short\" prefix=\" (\" text-case=\"title\" suffix=\")\"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"article-journal chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"none\">\n        <group delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <names variable=\"container-author\" delimiter=\", \">\n            <name delimiter-precedes-last=\"always\" initialize=\"false\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n            <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n          </names>\n          <names variable=\"editor translator\" delimiter=\", \">\n            <name delimiter-precedes-last=\"always\" initialize=\"false\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n            <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize=\"false\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <group>\n          <text term=\"anonymous\"/>\n        </group>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter-precedes-et-al=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\">\n            <text variable=\"publisher\"/>\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else-if type=\"legal_case\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else-if>\n          <else-if type=\"bill book graphic legislation motion_picture song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </else-if>\n          <else-if variable=\"reviewed-author\">\n            <choose>\n              <if variable=\"reviewed-title\" match=\"none\">\n                <text variable=\"title\" form=\"short\" font-style=\"italic\" prefix=\"Review of \"/>\n              </if>\n              <else>\n                <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n              </else>\n            </choose>\n          </else-if>\n          <else>\n            <group>\n              <text term=\"anonymous\"/>\n            </group>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" - \">\n      <choose>\n        <if type=\"thesis report\" match=\"any\">\n          <choose>\n            <if variable=\"archive\" match=\"any\">\n              <group>\n                <text variable=\"archive\" suffix=\".\"/>\n                <text variable=\"archive_location\" prefix=\" (\" suffix=\")\"/>\n              </group>\n            </if>\n            <else>\n              <text variable=\"URL\"/>\n            </else>\n          </choose>\n        </if>\n        <else>\n          <choose>\n            <if variable=\"DOI\">\n              <text variable=\"DOI\" prefix=\"DOI: \"/>\n            </if>\n            <else>\n              <choose>\n                <if type=\"webpage\">\n                  <group delimiter=\" \">\n                    <text variable=\"URL\"/>\n                  </group>\n                </if>\n                <else>\n                  <group>\n                    <text variable=\"URL\"/>\n                  </group>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </else>\n      </choose>\n      <group>\n        <text term=\"accessed\" suffix=\" \"/>\n        <date form=\"numeric\" variable=\"accessed\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book graphic manuscript motion_picture report song speech thesis webpage post-weblog paper-conference\" match=\"any\">\n        <choose>\n          <if variable=\"version\" type=\"book\" match=\"all\">\n            <text variable=\"title\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if variable=\"reviewed-author\">\n        <choose>\n          <if variable=\"reviewed-title\">\n            <group delimiter=\" \">\n              <text variable=\"title\"/>\n              <group delimiter=\", \" prefix=\"[\" suffix=\"]\">\n                <text variable=\"reviewed-title\" font-style=\"italic\" prefix=\"Review of \"/>\n                <names variable=\"reviewed-author\" delimiter=\", \">\n                  <label form=\"verb-short\" suffix=\" \"/>\n                  <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n                </names>\n              </group>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\", \" prefix=\"[\" suffix=\"]\">\n              <text variable=\"title\" font-style=\"italic\" prefix=\"Review of \"/>\n              <names variable=\"reviewed-author\" delimiter=\", \">\n                <label form=\"verb-short\" suffix=\" \"/>\n                <name initialize-with=\". \"/>\n              </names>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-plus-extra\">\n    <text macro=\"title\" suffix=\".\"/>\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <group delimiter=\", \" prefix=\" (\" suffix=\").\">\n          <group delimiter=\" \">\n            <text variable=\"genre\"/>\n            <text variable=\"number\" prefix=\"No. \"/>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"version\" text-case=\"capitalize-first\"/>\n            <text variable=\"version\"/>\n          </group>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else-if type=\"post-weblog webpage\" match=\"any\">\n        <text variable=\"genre\" prefix=\" [\" suffix=\"]\"/>\n      </else-if>\n      <else-if variable=\"version\">\n        <group delimiter=\" \" prefix=\" (\" suffix=\")\">\n          <text term=\"version\" text-case=\"capitalize-first\"/>\n          <text variable=\"version\"/>\n        </group>\n      </else-if>\n    </choose>\n    <text macro=\"format\" prefix=\" [\" suffix=\"]\"/>\n  </macro>\n  <macro name=\"format\">\n    <choose>\n      <if match=\"any\" variable=\"medium\">\n        <text variable=\"medium\" text-case=\"capitalize-first\"/>\n      </if>\n      <else-if type=\"dataset\" match=\"any\">\n        <text value=\"Data set\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n      <else-if type=\"thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"genre\"/>\n          </group>\n          <group delimiter=\", \">\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"post-weblog webpage\" match=\"none\">\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"event version\" type=\"speech\" match=\"none\">\n              <text variable=\"genre\"/>\n            </if>\n          </choose>\n          <choose>\n            <if type=\"article-journal article-magazine article-newspaper\" match=\"none\">\n              <group delimiter=\": \">\n                <choose>\n                  <if variable=\"publisher-place\">\n                    <text variable=\"publisher-place\"/>\n                  </if>\n                  <else>\n                    <text variable=\"event-place\"/>\n                  </else>\n                </choose>\n                <text variable=\"publisher\"/>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"container-title\" match=\"none\">\n        <choose>\n          <if variable=\"event\">\n            <choose>\n              <if variable=\"genre\" match=\"none\">\n                <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <text variable=\"event\" quotes=\"true\"/>\n              </if>\n              <else>\n                <group delimiter=\" \">\n                  <text variable=\"genre\" text-case=\"capitalize-first\"/>\n                  <text term=\"presented at\"/>\n                  <text variable=\"event\" quotes=\"true\"/>\n                </group>\n              </else>\n            </choose>\n          </if>\n          <else-if type=\"speech\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"bill legal_case legislation\" match=\"none\">\n        <choose>\n          <if variable=\"issued\">\n            <group delimiter=\" \" prefix=\" \">\n              <choose>\n                <if variable=\"author\"/>\n                <else-if variable=\"editor\">\n                  <text term=\"editor\" form=\"short\" prefix=\"(\" suffix=\")\"/>\n                </else-if>\n                <else-if variable=\"translator\">\n                  <text term=\"translator\" form=\"short\"/>\n                </else-if>\n              </choose>\n              <group>\n                <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n                  <date-part name=\"year\"/>\n                </date>\n                <text variable=\"year-suffix\"/>\n                <choose>\n                  <if type=\"speech\" match=\"any\">\n                    <date variable=\"issued\">\n                      <date-part prefix=\", \" name=\"month\"/>\n                    </date>\n                  </if>\n                  <else-if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song dataset article-newspaper\" match=\"none\">\n                    <date variable=\"issued\">\n                      <date-part prefix=\", \" name=\"month\"/>\n                      <date-part prefix=\" \" name=\"day\"/>\n                    </date>\n                  </else-if>\n                </choose>\n              </group>\n            </group>\n          </if>\n          <else-if variable=\"status\">\n            <group prefix=\" (\" suffix=\")\">\n              <text variable=\"status\"/>\n              <text variable=\"year-suffix\" prefix=\"-\"/>\n            </group>\n          </else-if>\n          <else>\n            <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n              <group>\n                <text term=\"no date\" form=\"short\"/>\n                <text variable=\"year-suffix\" prefix=\"-\"/>\n              </group>\n              <choose>\n                <if variable=\"author\"/>\n                <else-if variable=\"editor\">\n                  <text term=\"editor\" form=\"short\"/>\n                </else-if>\n                <else-if variable=\"translator\">\n                  <text term=\"translator\" form=\"short\"/>\n                </else-if>\n              </choose>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued-sort\">\n    <choose>\n      <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song dataset\" match=\"none\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <group delimiter=\"/\">\n          <date variable=\"original-date\" form=\"text\"/>\n          <group>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n            <text variable=\"year-suffix\"/>\n          </group>\n        </group>\n      </if>\n      <else-if variable=\"status\">\n        <text variable=\"status\"/>\n        <text variable=\"year-suffix\" prefix=\"-\"/>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n        <text variable=\"year-suffix\" prefix=\"-\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"volume\" font-style=\"italic\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n        <choose>\n          <if variable=\"issued\">\n            <choose>\n              <if variable=\"page issue\" match=\"none\">\n                <text variable=\"status\" prefix=\". \"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"book graphic motion_picture report song chapter paper-conference entry-encyclopedia entry-dictionary\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\" \">\n            <choose>\n              <if type=\"report\" match=\"none\">\n                <text macro=\"edition\"/>\n              </if>\n            </choose>\n            <choose>\n              <if variable=\"volume\" match=\"any\">\n                <group>\n                  <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                  <number variable=\"volume\" form=\"numeric\"/>\n                </group>\n              </if>\n              <else>\n                <group>\n                  <text term=\"volume\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n                  <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1&#8211;\"/>\n                </group>\n              </else>\n            </choose>\n          </group>\n          <text variable=\"page\" prefix=\"S. \"/>\n        </group>\n      </else-if>\n      <else-if type=\"legal_case\">\n        <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n          <text variable=\"authority\"/>\n          <date variable=\"issued\" form=\"text\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <text variable=\"locator\" prefix=\"S. \"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"post-weblog webpage\" match=\"none\">\n        <group>\n          <choose>\n            <if type=\"chapter paper-conference entry-encyclopedia\" match=\"any\">\n              <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            </if>\n          </choose>\n          <text macro=\"container-contributors\"/>\n          <text macro=\"secondary-contributors\"/>\n          <text macro=\"container-title\"/>\n          <choose>\n            <if type=\"article-newspaper\">\n              <group prefix=\" \" delimiter=\" \">\n                <date form=\"text\" variable=\"issued\" suffix=\",\"/>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\" suffix=\",\"/>\n      </if>\n      <else-if type=\"bill legal_case legislation\" match=\"none\">\n        <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"legal-cites\">\n    <choose>\n      <if type=\"bill legal_case legislation\" match=\"any\">\n        <group delimiter=\" \" prefix=\", \">\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"volume\"/>\n              <text variable=\"container-title\"/>\n              <group delimiter=\" \">\n                <text term=\"section\" form=\"symbol\"/>\n                <text variable=\"section\"/>\n              </group>\n              <text variable=\"page\"/>\n            </if>\n            <else>\n              <choose>\n                <if type=\"legal_case\">\n                  <text variable=\"number\" prefix=\"No. \"/>\n                </if>\n                <else>\n                  <text variable=\"number\" prefix=\"Pub. L. No. \"/>\n                  <group delimiter=\" \">\n                    <text term=\"section\" form=\"symbol\"/>\n                    <text variable=\"section\"/>\n                  </group>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"original-date\">\n    <choose>\n      <if variable=\"original-date\">\n        <group prefix=\"(\" suffix=\")\" delimiter=\" \">\n          <text value=\"Original work published\"/>\n          <date variable=\"original-date\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation and=\"symbol\" et-al-min=\"3\" et-al-use-first=\"1\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" givenname-disambiguation-rule=\"primary-name\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-sort\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued-year\"/>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography and=\"symbol\" et-al-use-last=\"true\" initialize-with=\".\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-sort\" sort=\"ascending\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \">\n        <group delimiter=\" \">\n          <group delimiter=\" \">\n            <text macro=\"author\"/>\n            <text macro=\"issued\"/>\n          </group>\n          <group delimiter=\". \">\n            <text macro=\"title-plus-extra\"/>\n            <text macro=\"container\"/>\n          </group>\n        </group>\n        <text macro=\"legal-cites\"/>\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <text macro=\"event\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text macro=\"locators\"/>\n        </group>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n      <text macro=\"original-date\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"