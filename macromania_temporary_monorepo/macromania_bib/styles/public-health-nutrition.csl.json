"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Public Health Nutrition</title>\n    <id>http://www.zotero.org/styles/public-health-nutrition</id>\n    <link href=\"http://www.zotero.org/styles/public-health-nutrition\" rel=\"self\"/>\n    <link href=\"http://assets.cambridge.org/PHN/PHN_ifc.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Michael Berkowitz</name>\n      <email>mberkowi@gmu.edu</email>\n    </author>\n    <contributor>\n      <name>Sean Takats</name>\n      <email>stakats@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1368-9800</issn>\n    <eissn>1475-2727</eissn>\n    <summary>Style for the Cambridge UP Journal \"Public Health Nutrition\"</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"never\" and=\"symbol\"/>\n      <label form=\"long\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <group prefix=\"[\" suffix=\"]\">\n      <names variable=\"editor\">\n        <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"long\" prefix=\", \"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"date-year\">\n    <group prefix=\"(\" suffix=\")\">\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"publisher-place\" suffix=\": \"/>\n        <text variable=\"publisher\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <text variable=\"URL\"/>\n        <choose>\n          <if variable=\"URL\">\n            <group prefix=\" (accessed\" suffix=\")\" delimiter=\" \">\n              <date variable=\"accessed\">\n                <date-part name=\"month\" prefix=\" \"/>\n                <date-part name=\"year\" prefix=\" \"/>\n              </date>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n          <text variable=\"volume\" font-weight=\"bold\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <label variable=\"page\" form=\"short\" suffix=\" \"/>\n        <text variable=\"page\"/>\n      </if>\n      <else>\n        <text variable=\"page\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"book-volume\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <text variable=\"volume\" prefix=\"vol. \"/>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" prefix=\"(\" suffix=\")\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\" \" suffix=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"date-year\"/>\n        <text macro=\"title\"/>\n      </group>\n      <group suffix=\".\">\n        <group delimiter=\", \">\n          <text macro=\"container-title\"/>\n          <text macro=\"book-volume\"/>\n          <text macro=\"edition\"/>\n          <text macro=\"pages\"/>\n        </group>\n        <text macro=\"editor\" prefix=\" \"/>\n      </group>\n      <group delimiter=\";\">\n        <text macro=\"publisher\" prefix=\" \"/>\n        <text macro=\"access\" prefix=\" \"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"