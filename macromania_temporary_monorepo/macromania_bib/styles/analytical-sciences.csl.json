"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Analytical Sciences</title>\n    <title-short>Anal. Sci.</title-short>\n    <id>http://www.zotero.org/styles/analytical-sciences</id>\n    <link href=\"http://www.zotero.org/styles/analytical-sciences\" rel=\"self\"/>\n    <link href=\"http://www.jsac.or.jp/analsci/data/instr/InstAuthor-13-1.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Shoji Takahashi</name>\n      <email>s.takahashi@elsevier.com</email>\n      <uri>http://www.mendeley.com/profiles/shoji-takahashi3/</uri>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <issn>0910-6340</issn>\n    <eissn>1348-2246</eissn>\n    <updated>2015-12-11T12:15:05+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <style-options punctuation-in-quote=\"false\"/>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"chapter\" match=\"any\">\n        <text variable=\"container-title\" quotes=\"true\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <text term=\"in\" suffix=\" \"/>\n        <text variable=\"container-title\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" font-style=\"normal\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <number suffix=\" \" variable=\"edition\" form=\"ordinal\"/>\n    <label plural=\"never\" variable=\"edition\" form=\"short\"/>\n  </macro>\n  <macro name=\"event\">\n    <text variable=\"event-place\" suffix=\", \"/>\n    <date variable=\"event-date\">\n      <date-part name=\"month\" range-delimiter=\"\" suffix=\" \"/>\n      <date-part name=\"day\" range-delimiter=\"\" suffix=\", \"/>\n      <date-part name=\"year\" range-delimiter=\"\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date date-parts=\"year\" form=\"numeric\" variable=\"issued\" font-weight=\"normal\">\n      <date-part name=\"year\" font-weight=\"bold\"/>\n    </date>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" plural=\"never\" suffix=\" \"/>\n      <name and=\"text\" initialize=\"false\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"volume\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"chapter\" match=\"any\">\n        <group delimiter=\", \">\n          <group>\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" font-style=\"normal\" suffix=\" \"/>\n            <text variable=\"volume\"/>\n          </group>\n          <group>\n            <text term=\"chapter\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <text variable=\"chapter-number\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"article-journal chapter\" match=\"any\">\n        <text variable=\"page-first\"/>\n      </if>\n      <else>\n        <text variable=\"page\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"article-journal\">\n          <group delimiter=\", \">\n            <text macro=\"container-title\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"page\"/>\n          </group>\n        </if>\n        <else-if type=\"book\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"edition\" suffix=\" \"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"container-title\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"container-title\"/>\n            <text macro=\"event\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text macro=\"issued\"/>\n            <text variable=\"number\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"