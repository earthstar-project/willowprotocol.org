"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Acta Universitatis Agriculturae Sueciae (Swedish University of Agricultural Sciences)</title>\n    <id>http://www.zotero.org/styles/acta-universitatis-agriculturae-sueciae</id>\n    <link href=\"http://www.zotero.org/styles/acta-universitatis-agriculturae-sueciae\" rel=\"self\"/>\n    <link href=\"https://www.slu.se/en/subweb/library/write-and-cite/writing-references/referenslista-enligt-harvard/\" rel=\"documentation\"/>\n    <link href=\"https://www.slu.se/en/subweb/library/write-and-cite/writing-references/in-text-citations-using-the-harvard-system/\" rel=\"documentation\"/>\n    <author>\n      <name>Rintze Zelle</name>\n      <uri>http://twitter.com/rintzezelle</uri>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <summary>Style as per the May 2019 guidelines.</summary>\n    <updated>2023-10-02T14:12:49+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"edition\" form=\"short\">ed</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"sv\">\n    <terms>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"page\" form=\"short\">\n        <single>s.</single>\n        <multiple>ss.</multiple>\n      </term>\n      <term name=\"available at\">tillg√§nglig</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name and=\"symbol\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <date variable=\"accessed\" prefix=\"[\" suffix=\"]\">\n            <date-part name=\"year\"/>\n            <date-part name=\"month\" form=\"numeric-leading-zeros\" prefix=\"-\"/>\n            <date-part name=\"day\" form=\"numeric-leading-zeros\" prefix=\"-\"/>\n          </date>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture patent report song thesis webpage software\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <choose>\n          <if type=\"software\" variable=\"version\" match=\"any\">\n            <text variable=\"version\" prefix=\" (\" suffix=\")\"/>\n            <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n          </if>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"report thesis\" match=\"any\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <group prefix=\" (\" suffix=\").\">\n      <choose>\n        <if match=\"any\" variable=\"issued\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </if>\n        <else>\n          <text term=\"no date\" form=\"short\"/>\n        </else>\n      </choose>\n      <text variable=\"year-suffix\"/>\n    </group>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n    <text variable=\"year-suffix\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"numeric\" suffix=\".\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group prefix=\" \" delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group prefix=\" \" delimiter=\", \">\n          <text macro=\"edition\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"type\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <text variable=\"genre\" prefix=\"(\" suffix=\")\"/>\n      </if>\n      <else-if type=\"graphic\" match=\"any\">\n        <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"collection-title\">\n    <choose>\n      <if variable=\"URL\"/>\n      <else>\n        <text variable=\"collection-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"issued-year\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\":\">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\" line-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-year\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <choose>\n        <if type=\"paper-conference\">\n          <text macro=\"issued\" suffix=\" \"/>\n          <group delimiter=\". \" suffix=\".\">\n            <text macro=\"title\"/>\n            <text macro=\"type\"/>\n          </group>\n          <text macro=\"container-contributors\" prefix=\" \"/>\n          <group prefix=\" \">\n            <text value=\"Proceedings of \" font-style=\"italic\"/>\n            <text variable=\"event\" font-style=\"italic\"/>\n          </group>\n          <text variable=\"publisher-place\" prefix=\", \"/>\n          <date variable=\"issued\" prefix=\", \" suffix=\".\">\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n          <text macro=\"locators\"/>\n          <text macro=\"publisher\" prefix=\". \"/>\n        </if>\n        <else-if type=\"chapter entry-encyclopedia\" match=\"any\">\n          <group delimiter=\". \" suffix=\".\">\n            <text macro=\"issued\"/>\n            <text macro=\"title\"/>\n            <group delimiter=\": \">\n              <choose>\n                <if type=\"chapter paper-conference\" match=\"any\">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                </if>\n              </choose>\n              <group delimiter=\" \" suffix=\".\">\n                <text macro=\"container-contributors\"/>\n                <text macro=\"secondary-contributors\"/>\n                <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\".\"/>\n              </group>\n            </group>\n            <group prefix=\" (\" suffix=\").\" delimiter=\"; \">\n              <text macro=\"collection-title\"/>\n              <text variable=\"collection-number\"/>\n            </group>\n            <text macro=\"edition\" prefix=\" \"/>\n            <text macro=\"publisher\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\". \">\n            <group delimiter=\" \">\n              <text macro=\"issued\"/>\n              <text macro=\"title\"/>\n            </group>\n            <group delimiter=\", \" prefix=\" (\" suffix=\")\">\n              <text variable=\"collection-title\"/>\n              <text variable=\"number\"/>\n            </group>\n            <text macro=\"publisher\" prefix=\" \" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\". \" suffix=\".\">\n            <text macro=\"title\" prefix=\", \"/>\n            <text variable=\"number\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog post\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"issued\"/>\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <text macro=\"issued\" suffix=\" \"/>\n          <text macro=\"title\" suffix=\".\"/>\n          <text macro=\"container-contributors\" prefix=\" \"/>\n          <text macro=\"secondary-contributors\"/>\n          <group prefix=\" \" suffix=\",\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"type\"/>\n          </group>\n          <text macro=\"locators\"/>\n        </else-if>\n        <else-if type=\"map\">\n          <text macro=\"issued\" suffix=\" \"/>\n          <text macro=\"title\" suffix=\" \"/>\n          <text variable=\"note\" prefix=\"[cartographic material]: \" suffix=\".\"/>\n          <text macro=\"container-contributors\" prefix=\" \"/>\n          <text macro=\"secondary-contributors\"/>\n          <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n          <text macro=\"type\" suffix=\",\"/>\n          <text macro=\"locators\"/>\n          <text macro=\"edition\" prefix=\" \"/>\n          <text macro=\"publisher\" prefix=\" \" suffix=\".\"/>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song software\" match=\"any\">\n          <choose>\n            <if variable=\"URL\">\n              <text macro=\"issued\" suffix=\" \"/>\n              <group suffix=\".\">\n                <text macro=\"title\"/>\n                <text macro=\"type\"/>\n              </group>\n              <text macro=\"container-contributors\" prefix=\" \"/>\n              <text macro=\"secondary-contributors\"/>\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n              <text macro=\"locators\"/>\n              <text macro=\"publisher\" prefix=\" \" suffix=\".\"/>\n            </if>\n            <else>\n              <text macro=\"issued\" suffix=\" \"/>\n              <group suffix=\".\">\n                <text macro=\"title\"/>\n                <text macro=\"type\"/>\n              </group>\n              <text macro=\"container-contributors\" prefix=\" \"/>\n              <text macro=\"secondary-contributors\"/>\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n              <text macro=\"locators\"/>\n              <text macro=\"publisher\" prefix=\". \" suffix=\".\"/>\n              <group prefix=\" (\" suffix=\")\" delimiter=\"; \">\n                <text macro=\"collection-title\"/>\n                <text variable=\"collection-number\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text macro=\"issued\"/>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"type\"/>\n              <text macro=\"container-contributors\"/>\n              <text macro=\"secondary-contributors\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"locators\"/>\n              <text macro=\"publisher\" suffix=\".\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"