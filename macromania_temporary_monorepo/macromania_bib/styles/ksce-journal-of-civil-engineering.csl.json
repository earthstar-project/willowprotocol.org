"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>KSCE Journal of Civil Engineering</title>\n    <id>http://www.zotero.org/styles/ksce-journal-of-civil-engineering</id>\n    <link href=\"http://www.zotero.org/styles/ksce-journal-of-civil-engineering\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/environmental-and-engineering-geoscience\" rel=\"template\"/>\n    <link href=\"http://www.springer.com/engineering/civil+engineering/journal/12205\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geology\"/>\n    <issn>1226-7988</issn>\n    <eissn>1976-3808</eissn>\n    <updated>2017-06-16T13:32:58+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography delimiter-precedes-last=\"never\" et-al-min=\"16\" et-al-use-first=\"15\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <date variable=\"issued\" prefix=\"(\" suffix=\").\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n      <choose>\n        <if type=\"report\">\n          <text variable=\"title\" prefix=\" \" suffix=\":\"/>\n          <text variable=\"publisher\" prefix=\" \"/>\n          <text variable=\"collection-title\" prefix=\" \"/>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \" suffix=\".\">\n            <group>\n              <text variable=\"title\" font-style=\"italic\" prefix=\" \"/>\n              <text macro=\"editor-translator\" prefix=\" \"/>\n            </group>\n            <group delimiter=\" \">\n              <number vertical-align=\"baseline\" variable=\"edition\" form=\"ordinal\"/>\n              <label variable=\"edition\" form=\"short\" text-case=\"capitalize-first\"/>\n            </group>\n            <text macro=\"publisher\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <text variable=\"title\" quotes=\"true\" prefix=\" \" suffix=\".\"/>\n            <group>\n              <text variable=\"container-title\" font-style=\"italic\" suffix=\",\"/>\n              <names variable=\"editor translator\" prefix=\" \" suffix=\",\" delimiter=\", \">\n                <name and=\"text\" sort-separator=\", \" initialize-with=\".\" name-as-sort-order=\"all\"/>\n                <label form=\"short\" prefix=\" \"/>\n              </names>\n              <text variable=\"collection-title\" prefix=\" \" suffix=\",\"/>\n              <group suffix=\".\">\n                <text macro=\"publisher\" prefix=\" \"/>\n                <group prefix=\", \">\n                  <text variable=\"page\" prefix=\"p. \"/>\n                </group>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"map\">\n          <text variable=\"title\" prefix=\" \"/>\n          <text variable=\"collection-title\" prefix=\": \"/>\n          <text variable=\"note\" prefix=\", scale \"/>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \" prefix=\" \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" plural=\"never\" form=\"short\"/>\n              <number variable=\"number-of-pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article\" match=\"any\">\n          <group suffix=\":\">\n            <text variable=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" suffix=\",\" font-style=\"italic\"/>\n            <group delimiter=\", \" prefix=\" \">\n              <group delimiter=\" \">\n                <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <group delimiter=\" \">\n                <text term=\"issue\" form=\"short\" text-case=\"capitalize-first\"/>\n                <text variable=\"issue\"/>\n              </group>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n              <text variable=\"DOI\" prefix=\"DOI: \"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\":\">\n            <text variable=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" suffix=\",\" font-style=\"italic\"/>\n            <group delimiter=\", \" prefix=\" \">\n              <group delimiter=\" \">\n                <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <group delimiter=\" \">\n                <text term=\"issue\" form=\"short\" text-case=\"capitalize-first\"/>\n                <text variable=\"issue\"/>\n              </group>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"