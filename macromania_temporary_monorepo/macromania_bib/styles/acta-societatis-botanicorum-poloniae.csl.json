"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Acta Societatis Botanicorum Poloniae</title>\n    <title-short>ASBP</title-short>\n    <id>http://www.zotero.org/styles/acta-societatis-botanicorum-poloniae</id>\n    <link href=\"http://www.zotero.org/styles/acta-societatis-botanicorum-poloniae\" rel=\"self\"/>\n    <link href=\"https://pbsociety.org.pl/journals/index.php/asbp/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Piotr OtrÄ™ba</name>\n      <email>p.otreba@pbsociety.org.pl</email>\n    </author>\n    <contributor>\n      <name>Sean Takats</name>\n      <email>stakats@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"botany\"/>\n    <issn>0001-6977</issn>\n    <eissn>2083-9480</eissn>\n    <summary>Modified default Vancouver style available at Zotero, for Acta Societatis Botanicorum Poloniae Journal (published by Polish Botanical Society)</summary>\n    <updated>2012-10-15T22:46:45+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\" suffix=\"; \"/>\n    <group suffix=\". \">\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text value=\"Available from: \"/>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group suffix=\". \">\n          <text variable=\"container-title\" form=\"short\"/>\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"internet\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"article-journal article-magazine\" match=\"none\">\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <group prefix=\"(\" suffix=\"). \">\n            <text variable=\"collection-title\" suffix=\"; \"/>\n            <text variable=\"volume\" prefix=\"vol \"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\". \">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\"/>\n          </group>\n          <text macro=\"publisher\" prefix=\" \"/>\n          <text variable=\"page\" prefix=\"p. \" suffix=\". \"/>\n          <group prefix=\"(\" suffix=\"). \">\n            <text variable=\"collection-title\" suffix=\"; \"/>\n            <text variable=\"volume\" prefix=\"vol \"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group suffix=\";\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n          <group suffix=\". \">\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            <text variable=\"page\" prefix=\":\"/>\n            <text variable=\"DOI\" prefix=\". doi:\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"