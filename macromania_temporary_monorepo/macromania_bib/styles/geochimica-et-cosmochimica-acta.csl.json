"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Geochimica et Cosmochimica Acta</title>\n    <id>http://www.zotero.org/styles/geochimica-et-cosmochimica-acta</id>\n    <link href=\"http://www.zotero.org/styles/geochimica-et-cosmochimica-acta\" rel=\"self\"/>\n    <link href=\"http://www.elsevier.com/journals/geochimica-et-cosmochimica-acta/0016-7037/guide-for-authors\" rel=\"documentation\"/>\n    <author>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Nate Boland</name>\n      <email>neboland@gmail.com</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"chemistry\"/>\n    <issn>0016-7037</issn>\n    <summary>Geochimica et Cosmochimica Acta style</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <label form=\"short\" suffix=\" \"/>\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\" \" initialize-with=\". \" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation and=\"text\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"false\" disambiguate-add-givenname=\"true\" cite-group-delimiter=\", \" collapse=\"year-suffix\" year-suffix-delimiter=\",\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \" suffix=\",\">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"editor\" prefix=\" (\" suffix=\").\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text variable=\"event\" suffix=\".\"/>\n            <group suffix=\".\" delimiter=\". \">\n              <text macro=\"publisher\" prefix=\" \"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\" \" suffix=\".\">\n            <text macro=\"title\" suffix=\". \"/>\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            <group prefix=\" \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n            </group>\n            <group prefix=\", \">\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"