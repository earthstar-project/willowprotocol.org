"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Journal of Sleep Research</title>\n    <id>http://www.zotero.org/styles/journal-of-sleep-research</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-sleep-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/springer-mathphys-author-date\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/(ISSN)1365-2869/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Omar Harb</name>\n      <email>omarharb@hotmail.com</email>\n    </author>\n    <contributor>\n      <name>Mike Borghese</name>\n      <email>mborg031@gmail.com</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <issn>0962-1105</issn>\n    <eissn>1365-2869</eissn>\n    <updated>2014-12-03T03:06:27+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author-short-in-citation\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-in-citation\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-in-citation\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"issued-sort\">\n    <choose>\n      <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <group>\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" prefix=\" \" text-case=\"lowercase\" strip-periods=\"true\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\" delimiter=\", \" and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \"/>\n      <label form=\"short\" prefix=\" (\" text-case=\"lowercase\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n    <text variable=\"publisher-place\" prefix=\", \"/>\n  </macro>\n  <macro name=\"page\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key macro=\"author-in-citation\"/>\n      <key variable=\"issued\" sort=\"descending\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short-in-citation\" suffix=\",\"/>\n        <text macro=\"year-in-citation\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" et-al-min=\"7\" et-al-use-first=\"3\">\n    <sort>\n      <key macro=\"author-in-citation\"/>\n      <key macro=\"issued-sort\" sort=\"ascending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" prefix=\" \" suffix=\" \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"publisher\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\" prefix=\", \"/>\n            </date>\n          </group>\n        </if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" form=\"short\" suffix=\",\"/>\n            <date date-parts=\"year\" form=\"numeric\" variable=\"issued\" suffix=\",\">\n              <date-part name=\"year\"/>\n            </date>\n            <text variable=\"volume\" suffix=\":\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <choose>\n              <if variable=\"container-title\">\n                <text macro=\"editor\" prefix=\"In: \" suffix=\" \"/>\n                <group delimiter=\". \">\n                  <text variable=\"container-title\"/>\n                  <text macro=\"page\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n              </if>\n              <else>\n                <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <text variable=\"event\"/>\n                <text variable=\"event-place\" prefix=\", \"/>\n                <date variable=\"issued\" prefix=\" \">\n                  <date-part name=\"month\" suffix=\" \"/>\n                  <date-part name=\"day\" suffix=\" \"/>\n                </date>\n              </else>\n            </choose>\n            <date variable=\"issued\">\n              <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"event\" suffix=\".\"/>\n            <text variable=\"event-place\" prefix=\", \"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n            </date>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"URL\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"