"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Generic Style Rules for Linguistics</title>\n    <id>http://www.zotero.org/styles/generic-style-rules-for-linguistics</id>\n    <link href=\"http://www.zotero.org/styles/generic-style-rules-for-linguistics\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/unified-style-sheet-for-linguistics\" rel=\"template\"/>\n    <link href=\"https://www.eva.mpg.de/linguistics/past-research-resources/resources/generic-style-rules.html\" rel=\"documentation\"/>\n    <author>\n      <name>Brian Plimley</name>\n      <email>brian.plimley@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"linguistics\"/>\n    <updated>2019-01-06T05:18:01+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"translator\" form=\"verb-short\">trans.</term>\n      <term name=\"edition\" form=\"short\">edn.</term>\n    </terms>\n  </locale>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <group delimiter=\". \">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"editor\">\n                <label form=\"verb-short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <name delimiter=\" &amp; \" delimiter-precedes-et-al=\"never\" name-as-sort-order=\"all\"/>\n              </names>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\">\n                <label form=\"verb-short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <name delimiter=\" &amp; \" delimiter-precedes-et-al=\"never\" name-as-sort-order=\"all\"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"editor\">\n                <name delimiter=\" &amp; \" delimiter-precedes-et-al=\"never\" name-as-sort-order=\"all\"/>\n                <label form=\"short\" suffix=\")\" prefix=\" (\"/>\n              </names>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\">\n                <label form=\"verb-short\" prefix=\" \" suffix=\" \"/>\n                <name delimiter=\" &amp; \" delimiter-precedes-et-al=\"never\" name-as-sort-order=\"all\"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter=\" &amp; \" delimiter-precedes-et-al=\"never\" name-as-sort-order=\"all\" sort-separator=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\").\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name delimiter=\" &amp; \" delimiter-precedes-et-al=\"never\" name-as-sort-order=\"all\" sort-separator=\", \"/>\n      <label form=\"verb-short\" prefix=\" (\" suffix=\").\"/>\n    </names>\n  </macro>\n  <macro name=\"recipient\">\n    <choose>\n      <if type=\"personal_communication\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n          <else>\n            <text term=\"letter\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name delimiter=\" &amp; \" delimiter-precedes-et-al=\"never\" name-as-sort-order=\"all\" sort-separator=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors\">\n    <names variable=\"author\">\n      <name delimiter=\" &amp; \" delimiter-precedes-et-al=\"never\" name-as-sort-order=\"all\" sort-separator=\", \"/>\n      <label form=\"short\" prefix=\", \" suffix=\" \"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"translator\"/>\n      </substitute>\n    </names>\n    <choose>\n      <if variable=\"author editor translator\" match=\"none\">\n        <text macro=\"title\"/>\n      </if>\n    </choose>\n    <text macro=\"recipient\" prefix=\". \"/>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\" &amp; \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <choose>\n      <if variable=\"author editor translator\" match=\"none\">\n        <text macro=\"title\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name delimiter=\" &amp; \" delimiter-precedes-et-al=\"never\" name-as-sort-order=\"all\"/>\n    </names>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\", \">\n      <text variable=\"archive_location\" text-case=\"capitalize-first\"/>\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\") (\">\n      <choose>\n        <if type=\"graphic report\" match=\"any\">\n          <text macro=\"archive\" suffix=\".\"/>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"none\">\n          <text macro=\"archive\" suffix=\".\"/>\n        </else-if>\n      </choose>\n      <text variable=\"DOI\" prefix=\"doi:\"/>\n      <text variable=\"URL\"/>\n      <group delimiter=\" \" suffix=\".\">\n        <text term=\"accessed\" text-case=\"capitalize-first\"/>\n        <date variable=\"accessed\" form=\"text\" date-parts=\"year-month-day\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"personal_communication\" match=\"none\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"book\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"bill graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"fulltitle\">\n    <group delimiter=\" \">\n      <choose>\n        <if variable=\"author editor translator\" match=\"any\">\n          <text macro=\"title\"/>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"book\">\n          <text macro=\"collection-title\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\" \">\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n          <group delimiter=\" \">\n            <number variable=\"number-of-volumes\" form=\"numeric\"/>\n            <text term=\"volume\" form=\"short\" plural=\"true\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-chapter\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"volume\">\n              <group delimiter=\" \">\n                <text term=\"volume\" form=\"short\"/>\n                <text variable=\"volume\"/>\n              </group>\n            </if>\n          </choose>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-article\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group prefix=\", \" delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"edition\"/>\n            <text term=\"edition\"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\" \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-journal\">\n        <group delimiter=\". \" prefix=\" \">\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"point-locators\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n        </if>\n      </choose>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if variable=\"container-title\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <text macro=\"collection-title\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"collection-title\">\n    <choose>\n      <if variable=\"collection-title\">\n        <group prefix=\"(\" suffix=\")\">\n          <text variable=\"collection-title\" text-case=\"title\"/>\n          <text variable=\"collection-number\" prefix=\" \"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"container-title\"/>\n      <else-if type=\"speech\" match=\"none\"/>\n      <else>\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"event\">\n              <choose>\n                <if variable=\"genre\" match=\"none\">\n                  <group delimiter=\" \">\n                    <text term=\"presented at\" text-case=\"capitalize-first\"/>\n                    <text variable=\"event\"/>\n                  </group>\n                </if>\n                <else>\n                  <group delimiter=\" \">\n                    <text variable=\"genre\" text-case=\"capitalize-first\"/>\n                    <text term=\"presented at\"/>\n                    <text variable=\"event\"/>\n                  </group>\n                </else>\n              </choose>\n            </if>\n            <else>\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            </else>\n          </choose>\n          <text variable=\"event-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"description\">\n    <group delimiter=\". \">\n      <text macro=\"interviewer\"/>\n      <text variable=\"medium\" text-case=\"capitalize-first\"/>\n    </group>\n    <choose>\n      <if variable=\"title\" match=\"none\"/>\n      <else-if type=\"thesis speech\" match=\"any\"/>\n      <else>\n        <text variable=\"genre\" text-case=\"capitalize-first\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\", \">\n      <text macro=\"publisher\"/>\n      <choose>\n        <if type=\"manuscript\">\n          <text value=\"ms\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"extra\">\n    <group prefix=\" (\" delimiter=\") (\" suffix=\")\">\n      <choose>\n        <if type=\"thesis\">\n          <text variable=\"genre\" suffix=\".\"/>\n        </if>\n      </choose>\n      <text macro=\"description\" suffix=\".\"/>\n      <text macro=\"event\" suffix=\".\"/>\n      <text macro=\"secondary-contributors\" suffix=\".\"/>\n      <text macro=\"access\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"contributors-short\"/>\n          <text macro=\"date\"/>\n        </group>\n        <text macro=\"point-locators\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"11\" et-al-use-first=\"7\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"contributors\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"contributors\"/>\n        <text macro=\"date\"/>\n        <text macro=\"fulltitle\"/>\n        <group>\n          <group delimiter=\". \">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <text macro=\"container-contributors\"/>\n                <text macro=\"container-title\"/>\n                <text macro=\"locators-chapter\"/>\n              </group>\n              <text macro=\"edition\"/>\n              <text macro=\"locators\"/>\n            </group>\n            <text macro=\"issue\"/>\n          </group>\n          <text macro=\"locators-article\"/>\n        </group>\n      </group>\n      <text macro=\"extra\"/>\n    </layout>\n  </bibliography>\n</style>\n"