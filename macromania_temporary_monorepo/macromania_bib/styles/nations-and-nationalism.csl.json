"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"minimal\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Nations and Nationalism</title>\n    <id>http://www.zotero.org/styles/nations-and-nationalism</id>\n    <link href=\"http://www.zotero.org/styles/nations-and-nationalism\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/acta-palaeontologica-polonica\" rel=\"template\"/>\n    <link href=\"https://onlinelibrary.wiley.com/page/journal/14698129/homepage/forauthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>citationstyler@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <issn>1354-5078</issn>\n    <eissn>1469-8129</eissn>\n    <updated>2019-07-15T07:36:10+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"edition\" form=\"short\">edn.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\".\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text variable=\"title\" text-case=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if type=\"book chapter webpage\" variable=\"container-title volume page\" match=\"none\">\n        <text term=\"forthcoming\"/>\n      </else-if>\n      <else-if type=\"book chapter webpage\" variable=\"volume page\" match=\"none\">\n        <text term=\"in press\"/>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"article-journal webpage post-weblog post chapter paper-conference\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" quotes=\"true\"/>\n      </if>\n      <else-if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <number variable=\"edition\" form=\"ordinal\"/>\n      <label variable=\"edition\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n      <number variable=\"volume\" suffix=\".\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"false\" disambiguate-add-givenname=\"false\" collapse=\"year-suffix\" year-suffix-delimiter=\", \">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n          <text variable=\"year-suffix\" font-style=\"italic\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else-if type=\"book chapter webpage\" variable=\"container-title volume page\" match=\"none\">\n            <text term=\"forthcoming\" text-case=\"capitalize-first\"/>\n          </else-if>\n          <else-if type=\"book chapter webpage\" variable=\"volume page\" match=\"none\">\n            <text term=\"in press\" text-case=\"capitalize-first\"/>\n          </else-if>\n        </choose>\n        <text variable=\"year-suffix\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <text macro=\"title\" font-style=\"normal\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"collection-title\" text-case=\"title\" font-style=\"italic\"/>\n              <text macro=\"volume\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"thesis\" match=\"any\">\n            <text macro=\"title\" suffix=\".\"/>\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <text macro=\"title\"/>\n                  <text term=\"in\" font-style=\"normal\"/>\n                  <text macro=\"editor\"/>\n                </group>\n                <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n                <group delimiter=\" \">\n                  <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n                  <number variable=\"volume\"/>\n                </group>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\" font-style=\"italic\"/>\n              <group delimiter=\". \">\n                <text variable=\"URL\"/>\n                <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                  <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                  <date variable=\"accessed\">\n                    <date-part name=\"day\" suffix=\" \"/>\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <group delimiter=\": \">\n                <group delimiter=\" \">\n                  <text variable=\"container-title\" form=\"long\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n                  <group delimiter=\", \">\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\"/>\n                  </group>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"