"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>New Solutions</title>\n    <id>http://www.zotero.org/styles/new-solutions</id>\n    <link href=\"http://www.zotero.org/styles/new-solutions\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/ieee\" rel=\"template\"/>\n    <link href=\"http://baywood.com/authors/Guidelines/NS_Guidelines.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Charles Parnot</name>\n      <uri>http://twitter.com/cparnot</uri>\n      <email>charles.parnot@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1048-2911</issn>\n    <eissn>1541-3772</eissn>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <!-- Macros -->\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" text-case=\"capitalize-first\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\" form=\"long\"/>\n    </date>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper paper-conference\" match=\"any\">\n        <names variable=\"author\">\n          <name initialize-with=\". \" delimiter=\", \" and=\"text\"/>\n          <label form=\"short\" prefix=\", \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name delimiter=\", \" and=\"text\"/>\n          <label form=\"short\" prefix=\", \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n      <name delimiter=\", \" and=\"text\"/>\n    </names>\n  </macro>\n  <macro name=\"locators\">\n    <group>\n      <text macro=\"edition\" suffix=\" \"/>\n      <number variable=\"volume\" form=\"numeric\"/>\n      <number variable=\"issue\" form=\"numeric\" prefix=\"(\" suffix=\") \"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference song\" match=\"any\">\n        <text variable=\"publisher-place\" suffix=\": \"/>\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <choose>\n          <if variable=\"URL\">\n            <group delimiter=\". \">\n              <text variable=\"URL\" suffix=\" \"/>\n              <group prefix=\"(\" suffix=\")\">\n                <date variable=\"accessed\" prefix=\"accessed: \">\n                  <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n                  <date-part name=\"month\" form=\"short\" suffix=\"-\" strip-periods=\"true\"/>\n                  <date-part name=\"year\" form=\"long\"/>\n                </date>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"book\" match=\"none\">\n        <text variable=\"page\"/>\n      </if>\n    </choose>\n  </macro>\n  <!-- Citation -->\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\"], [\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <!-- Bibliography -->\n  <bibliography et-al-min=\"4\" et-al-use-first=\"1\" entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"article-journal article-magazine article-newspaper paper-conference\" match=\"any\">\n          <text macro=\"title\" suffix=\", \"/>\n          <text variable=\"container-title\" font-style=\"italic\" suffix=\" \"/>\n          <text macro=\"locators\" suffix=\" \"/>\n          <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n          <text macro=\"page\" prefix=\": \"/>\n          <text variable=\"DOI\" prefix=\", doi:\"/>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <group prefix=\" (\" delimiter=\", \" suffix=\").\">\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group delimiter=\", \" suffix=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" prefix=\"in \" font-style=\"italic\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <text macro=\"editor\" suffix=\" \"/>\n          <group prefix=\"(\" delimiter=\", \" suffix=\")\">\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n          <text macro=\"page\" prefix=\", \"/>\n          <text variable=\"DOI\" prefix=\", doi:\"/>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"issued\"/>\n          </group>\n          <text macro=\"access\"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\", \" suffix=\". \">\n            <text macro=\"title\"/>\n          </group>\n          <text macro=\"access\"/>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <group prefix=\"(\" suffix=\")\" delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <text macro=\"title\" suffix=\", \"/>\n          <text variable=\"number\"/>\n          <text macro=\"issued\"/>\n        </else-if>\n        <else-if type=\"broadcast interview manuscript map patent personal_communication speech thesis webpage\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\" prefix=\": \"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text variable=\"page\" prefix=\": \"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"