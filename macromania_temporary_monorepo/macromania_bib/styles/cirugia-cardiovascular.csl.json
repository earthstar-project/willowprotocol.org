"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" page-range-format=\"minimal\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"es-ES\">\n  <info>\n    <title>Cirugia Cardiovascular (Espa√±ol)</title>\n    <title-short>CIRCV</title-short>\n    <id>http://www.zotero.org/styles/cirugia-cardiovascular</id>\n    <link href=\"http://www.zotero.org/styles/cirugia-cardiovascular\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/elsevier-vancouver\" rel=\"template\"/>\n    <link href=\"http://www.elsevier.com/journals/cirugia-cardiovascular/1134-0096/guide-for-authors\" rel=\"documentation\"/>\n    <author>\n      <name>Sankar</name>\n      <email>s.murugesan@elsevier.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1134-0096</issn>\n    <summary>A style for some Elsevier journals, resembles Vancouver style</summary>\n    <updated>2015-05-18T10:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"Doi: \"/>\n      </if>\n      <else-if type=\"webpage post-weblog\" match=\"any\">\n        <choose>\n          <if variable=\"URL\">\n            <group delimiter=\" \">\n              <text term=\"available at\" suffix=\": \" text-case=\"capitalize-first\"/>\n              <text variable=\"URL\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"access-date\">\n    <group delimiter=\" \">\n      <text term=\"accessed\"/>\n      <date variable=\"accessed\" delimiter=\" \">\n        <date-part name=\"day\"/>\n        <date-part name=\"month\"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name suffix=\".\" form=\"long\" initialize=\"true\" initialize-with=\"\" delimiter=\", \" delimiter-precedes-et-al=\"always\" delimiter-precedes-last=\"contextual\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name form=\"long\" initialize=\"true\" initialize-with=\"\" delimiter=\", \" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"long\" prefix=\", \" plural=\"contextual\" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\" range-delimiter=\"&#8211;\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"journal-location\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"volume\" form=\"long\"/>\n        <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\"; \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher_thesis\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\" form=\"long\"/>\n      <text variable=\"publisher-place\" form=\"long\"/>\n    </group>\n  </macro>\n  <macro name=\"edition_book\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition_chapter\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\" sort=\"ascending\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"7\" et-al-use-first=\"6\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\" suffix=\" \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\" form=\"long\"/>\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"volume\" prefix=\"vol. \" form=\"long\"/>\n            <text macro=\"edition_book\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <group delimiter=\". \">\n              <text variable=\"title\" form=\"long\"/>\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <group delimiter=\", \">\n                <text variable=\"container-title\"/>\n                <group delimiter=\" \">\n                  <text term=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n              </group>\n              <text macro=\"edition_chapter\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\" plural=\"never\"/>\n              <text variable=\"page\" form=\"short\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text variable=\"title\" form=\"long\"/>\n              <text variable=\"number\" form=\"long\"/>\n            </group>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\" \">\n            <text variable=\"title\" form=\"long\" suffix=\".\"/>\n            <text variable=\"genre\" form=\"long\" suffix=\",\"/>\n            <text macro=\"publisher_thesis\" suffix=\",\" text-case=\"title\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\". \">\n            <text variable=\"title\" form=\"long\"/>\n            <text variable=\"container-title\" form=\"long\" suffix=\".\" text-case=\"title\"/>\n            <text macro=\"access-date\" prefix=\"[\" suffix=\"]\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\":\">\n            <group delimiter=\". \">\n              <group delimiter=\". \">\n                <text variable=\"title\" form=\"long\"/>\n                <text variable=\"container-title\" form=\"short\" text-case=\"title\" strip-periods=\"true\"/>\n              </group>\n              <group delimiter=\";\">\n                <text macro=\"year-date\"/>\n                <text macro=\"journal-location\"/>\n              </group>\n            </group>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n          <text macro=\"access\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"