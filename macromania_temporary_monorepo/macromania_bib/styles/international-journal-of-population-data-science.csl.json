"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\" default-locale=\"en-US\">\n  <info>\n    <title>International Journal of Population Data Science</title>\n    <title-short>IJPDS</title-short>\n    <id>http://www.zotero.org/styles/international-journal-of-population-data-science</id>\n    <link href=\"http://www.zotero.org/styles/international-journal-of-population-data-science\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/european-journal-of-clinical-microbiology-and-infectious-diseases\" rel=\"template\"/>\n    <link href=\"https://ijpds.org/author-guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <category field=\"biology\"/>\n    <eissn>2399-4908</eissn>\n    <updated>2020-01-17T10:55:48+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\" suffix=\"; \"/>\n    <group suffix=\". \">\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n      <text macro=\"accessed-date\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\". \" prefix=\" \">\n      <group delimiter=\": \">\n        <text term=\"available at\" text-case=\"capitalize-first\"/>\n        <text variable=\"URL\"/>\n      </group>\n      <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n    </group>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n          <text term=\"cited\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"year\"/>\n            <date-part name=\"day\" prefix=\" \"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group suffix=\" \">\n          <text variable=\"container-title\" form=\"short\"/>\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"internet\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\" \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"article-journal article-magazine\" match=\"none\">\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\". \">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\"/>\n          </group>\n          <text macro=\"publisher\" prefix=\" \"/>\n          <text variable=\"page\" prefix=\" p. \" suffix=\".\"/>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group suffix=\";\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n              <date-part name=\"month\" form=\"short\" prefix=\" \"/>\n            </date>\n            <text macro=\"accessed-date\" prefix=\" \"/>\n          </group>\n          <group suffix=\". \">\n            <text variable=\"volume\"/>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"