"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" default-locale=\"en-GB\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\">\n  <info>\n    <title>Medical History</title>\n    <id>http://www.zotero.org/styles/medical-history</id>\n    <link href=\"http://www.zotero.org/styles/medical-history\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/clio-medica\" rel=\"template\"/>\n    <link href=\"http://assets.cambridge.org/MDH/MDH_ifc.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Fiachra Byrne</name>\n      <email>fiachrapbyrne@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <category field=\"humanities\"/>\n    <issn>0025-7273</issn>\n    <eissn>2048-8343</eissn>\n    <summary>This style is for the journal Medical History.</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-GB\">\n    <terms>\n      <term name=\"edition\" form=\"short\">\n        <single>edn</single>\n        <multiple>edns</multiple>\n      </term>\n      <term name=\"editor\" form=\"short\">\n        <single>(ed.)</single>\n        <multiple>(eds)</multiple>\n      </term>\n      <term name=\"editortranslator\" form=\"short\">\n        <single>(ed. and trans.)</single>\n        <multiple>(eds and trans.)</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">(trans.)</term>\n      <term name=\"container-author\" form=\"short\">\n        <single>(author)</single>\n        <multiple>(authors)</multiple>\n      </term>\n      <term name=\"volume\" form=\"short\">\n        <single>Vol.</single>\n        <multiple>vols</multiple>\n      </term>\n      <term name=\"column\" form=\"short\">\n        <single>col.</single>\n        <multiple>cols</multiple>\n      </term>\n      <term name=\"page\" form=\"short\">\n        <single/>\n        <multiple/>\n      </term>\n      <term name=\"chapter\" form=\"short\">\n        <single>ch.</single>\n        <multiple>chs</multiple>\n      </term>\n      <term name=\"book\" form=\"short\">\n        <single>bk</single>\n        <multiple>bks</multiple>\n      </term>\n      <term name=\"figure\" form=\"short\">\n        <single>fig.</single>\n        <multiple>figs</multiple>\n      </term>\n      <term name=\"folio\" form=\"short\">\n        <single>fo.</single>\n        <multiple>ff.</multiple>\n      </term>\n      <term name=\"issue\" form=\"short\">\n        <single>no.</single>\n        <multiple>nos</multiple>\n      </term>\n      <term name=\"paragraph\" form=\"short\">\n        <single>para.</single>\n        <multiple>paras</multiple>\n      </term>\n      <term name=\"part\" form=\"short\">\n        <single>pt.</single>\n        <multiple>pts</multiple>\n      </term>\n      <term name=\"section\" form=\"short\">\n        <single>sec.</single>\n        <multiple>secs</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"primary-contributors\">\n    <group delimiter=\", \">\n      <names variable=\"author\">\n        <name and=\"text\"/>\n        <label form=\"short\" prefix=\" \"/>\n        <substitute>\n          <names variable=\"translator editor\"/>\n          <names variable=\"editor\"/>\n          <names variable=\"translator\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter entry-dictionary entry-encyclopedia paper-conference\" match=\"any\">\n        <text macro=\"editor-translator\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"personal_communication\">\n            <choose>\n              <if variable=\"recipient\" match=\"any\">\n                <names variable=\"recipient\">\n                  <name and=\"text\" prefix=\"Letter to \"/>\n                </names>\n              </if>\n              <else>\n                <text variable=\"title\"/>\n              </else>\n            </choose>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation map motion_picture report song\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"interview manuscript personal_communication\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-case=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter entry-dictionary entry-encyclopedia paper-conference\" match=\"none\">\n        <text macro=\"editor-translator\"/>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"interview\">\n        <choose>\n          <if variable=\"interviewer\" match=\"any\">\n            <names variable=\"interviewer\" prefix=\" interview with \">\n              <name and=\"text\"/>\n            </names>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor-translator\">\n    <group delimiter=\", \">\n      <choose>\n        <if variable=\"author\">\n          <choose>\n            <if variable=\"container-author\">\n              <group>\n                <names variable=\"container-author\">\n                  <name and=\"text\" delimiter=\", \"/>\n                  <label form=\"short\" prefix=\" \"/>\n                </names>\n              </group>\n            </if>\n          </choose>\n          <names variable=\"editor translator\" delimiter=\", \">\n            <name and=\"text\" delimiter=\", \"/>\n            <label form=\"short\" prefix=\" \"/>\n          </names>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"locators\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"article-journal article-magazine\" match=\"any\">\n          <group delimiter=\", \">\n            <number variable=\"volume\"/>\n            <number variable=\"issue\"/>\n          </group>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"article-journal article-magazine\" match=\"none\">\n          <choose>\n            <if variable=\"volume\" match=\"any\">\n              <group>\n                <text term=\"volume\" form=\"short\" suffix=\" \"/>\n                <number variable=\"volume\"/>\n              </group>\n            </if>\n          </choose>\n        </if>\n      </choose>\n      <choose>\n        <if variable=\"number-of-volumes\" match=\"any\">\n          <group>\n            <number variable=\"number-of-volumes\" suffix=\" \"/>\n            <text term=\"volume\" form=\"short\" plural=\"true\"/>\n          </group>\n        </if>\n      </choose>\n      <text macro=\"edition\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" text-case=\"lowercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"place-publisher-issued\">\n    <choose>\n      <if type=\"article-journal\">\n        <group prefix=\" (\" suffix=\")\">\n          <text macro=\"issued\"/>\n        </group>\n      </if>\n      <else-if type=\"speech\">\n        <group prefix=\", \" delimiter=\", \">\n          <text variable=\"event-place\"/>\n          <text macro=\"issued\"/>\n        </group>\n      </else-if>\n      <else-if type=\"article-newspaper article-magazine interview manuscript personal_communication\" match=\"any\">\n        <group prefix=\", \">\n          <text macro=\"issued\"/>\n        </group>\n      </else-if>\n      <else-if type=\"thesis\">\n        <group prefix=\"(\" suffix=\")\">\n          <group delimiter=\", \">\n            <group delimiter=\": \">\n              <text variable=\"genre\" prefix=\"unpublished \"/>\n              <text variable=\"publisher\"/>\n            </group>\n            <text macro=\"issued\"/>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <group prefix=\" (\" suffix=\")\">\n          <group delimiter=\", \">\n            <group delimiter=\": \">\n              <text variable=\"publisher-place\"/>\n              <text variable=\"publisher\"/>\n            </group>\n            <text macro=\"issued\"/>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"article-newspaper article-magazine broadcast manuscript speech webpage\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else-if type=\"personal_communication\">\n            <group prefix=\"dated \" delimiter=\" \">\n              <date variable=\"issued\">\n                <date-part name=\"day\" suffix=\" \"/>\n                <date-part name=\"month\" suffix=\" \"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </else-if>\n          <else-if type=\"interview\">\n            <choose>\n              <if variable=\"interviewer\" match=\"none\">\n                <group prefix=\"interview, \">\n                  <text variable=\"medium\" suffix=\", \"/>\n                  <date variable=\"issued\">\n                    <date-part name=\"day\" suffix=\" \"/>\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </if>\n              <else>\n                <group>\n                  <text variable=\"medium\" suffix=\", \"/>\n                  <date variable=\"issued\">\n                    <date-part name=\"day\" suffix=\" \"/>\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </else>\n            </choose>\n          </else-if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"manuscript report\" match=\"none\">\n        <text term=\"no date\" form=\"short\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <group prefix=\", \">\n      <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <group prefix=\", \">\n          <text variable=\"page\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archival-details-subsequent\">\n    <choose>\n      <if type=\"manuscript personal_communication\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"archive\"/>\n          <text variable=\"archive_location\"/>\n          <text variable=\"archive-place\"/>\n          <text variable=\"call-number\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"archive\"/>\n          <text variable=\"archive_location\"/>\n          <text variable=\"archive-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"archival-details\">\n    <choose>\n      <if type=\"manuscript personal_communication\" match=\"any\">\n        <group prefix=\", \" delimiter=\", \">\n          <text variable=\"archive\"/>\n          <text variable=\"archive_location\"/>\n          <text variable=\"archive-place\"/>\n          <text variable=\"call-number\"/>\n        </group>\n      </if>\n      <else>\n        <group prefix=\", \" delimiter=\", \">\n          <text variable=\"archive\"/>\n          <text variable=\"archive_location\"/>\n          <text variable=\"archive-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"URL-access-date\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"URL\" prefix=\", online: &lt;\" suffix=\"&gt;\"/>\n        <date variable=\"accessed\" prefix=\", accessed \">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"interview\">\n            <text term=\"interview\"/>\n          </if>\n          <else-if type=\"speech\" match=\"any\">\n            <text variable=\"genre\" form=\"short\"/>\n          </else-if>\n          <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            </group>\n          </else-if>\n          <else-if type=\"personal_communication\">\n            <names variable=\"recipient\" prefix=\"Letter to \">\n              <name and=\"text\"/>\n            </names>\n          </else-if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation map motion_picture report song\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" form=\"short\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"personal_communication manuscript\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" form=\"short\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-case=\"title\" form=\"short\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title-short\"/>\n        <text macro=\"issued\"/>\n        <text macro=\"archival-details-subsequent\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"medium\">\n    <choose>\n      <if type=\"interview\" match=\"none\">\n        <text variable=\"medium\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <text variable=\"number\"/>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"speech\">\n        <group prefix=\", \" delimiter=\" \">\n          <text variable=\"genre\" text-case=\"lowercase\" suffix=\" presented at \"/>\n          <text variable=\"event\"/>\n        </group>\n      </if>\n      <else-if type=\"graphic\">\n        <text variable=\"genre\" prefix=\", \" suffix=\" \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <sort>\n      <key macro=\"primary-contributors\"/>\n    </sort>\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" font-style=\"italic\"/>\n            <text macro=\"locator\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\" font-style=\"italic\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"contributors-short\"/>\n            <choose>\n              <if disambiguate=\"true\">\n                <text macro=\"title-short\"/>\n                <text value=\"op. cit.\" font-style=\"italic\"/>\n              </if>\n              <else>\n                <text value=\"op. cit.\" font-style=\"italic\"/>\n              </else>\n            </choose>\n            <group prefix=\"(\" suffix=\")\" delimiter=\" \">\n              <text value=\"note\"/>\n              <text variable=\"first-reference-note-number\"/>\n            </group>\n            <text macro=\"locator\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"primary-contributors\"/>\n            <text macro=\"title\"/>\n            <text macro=\"number\"/>\n            <text macro=\"medium\"/>\n            <text macro=\"secondary-contributors\"/>\n            <text macro=\"container-contributors\" prefix=\"in \"/>\n            <text macro=\"container-title\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <text macro=\"genre\"/>\n          <text macro=\"place-publisher-issued\"/>\n          <text macro=\"locator\"/>\n          <text macro=\"pages\"/>\n          <text macro=\"archival-details\"/>\n          <text macro=\"URL-access-date\"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"