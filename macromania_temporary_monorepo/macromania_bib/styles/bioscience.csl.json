"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>BioScience</title>\n    <id>http://www.zotero.org/styles/bioscience</id>\n    <link href=\"http://www.zotero.org/styles/bioscience\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/bulletin-of-marine-science\" rel=\"template\"/>\n    <link href=\"https://academic.oup.com/bioscience/pages/General_Instructions#MANUSCRIPT%20PREPARATION\" rel=\"documentation\"/>\n    <link href=\"https://static.primary.prod.gcms.the-infra.com/static/site/bioscience/document/2015-bioscience-style-guide.doc?node=c6b9d18daed88f6f4d7d&amp;version=23527:b7fbc6a6c9646efff583\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0006-3568</issn>\n    <eissn>1525-3244</eissn>\n    <updated>2020-06-19T08:49:33+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name delimiter=\", \" name-as-sort-order=\"all\" sort-separator=\" \" form=\"long\" initialize-with=\"\"/>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\" suffix=\". \">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label text-case=\"capitalize-first\" variable=\"page\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <choose>\n        <if type=\"thesis\" match=\"any\">\n          <text variable=\"publisher-place\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <number variable=\"edition\" form=\"ordinal\"/>\n      <label variable=\"edition\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label variable=\"volume\" form=\"short\"/>\n      <number variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" collapse=\"year\" et-al-min=\"3\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"year\"/>\n      <key variable=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\" \">\n        <names variable=\"author\">\n          <name and=\"text\" delimiter=\" \" name-as-sort-order=\"all\" sort-separator=\", \" form=\"short\"/>\n        </names>\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year\" sort=\"ascending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <text macro=\"year\"/>\n      <choose>\n        <if type=\"book report\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n            <choose>\n              <if type=\"report\" match=\"any\">\n                <group delimiter=\" \">\n                  <text value=\"Report no.\"/>\n                  <number variable=\"number\"/>\n                </group>\n              </if>\n            </choose>\n          </group>\n        </if>\n        <else-if type=\"chapter\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text macro=\"pages\"/>\n              <text term=\"in\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <group delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"volume\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"event\" prefix=\"Paper presented at \"/>\n            <group delimiter=\", \">\n              <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\"/>\n            <group delimiter=\"; \" prefix=\"(\" suffix=\")\">\n              <date form=\"text\" variable=\"accessed\"/>\n              <text variable=\"URL\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"long\"/>\n              <group delimiter=\": \">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"