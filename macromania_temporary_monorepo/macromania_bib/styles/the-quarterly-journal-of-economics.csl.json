"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>The Quarterly Journal of Economics</title>\n    <title-short>QJE</title-short>\n    <id>http://www.zotero.org/styles/the-quarterly-journal-of-economics</id>\n    <link href=\"http://www.zotero.org/styles/the-quarterly-journal-of-economics\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-and-proceedings-of-the-royal-society-of-new-south-wales\" rel=\"template\"/>\n    <link href=\"https://academic.oup.com/qje/pages/Instructions_To_Authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"math\"/>\n    <issn>0033-5533</issn>\n    <eissn>1531-4650</eissn>\n    <updated>2021-01-04T11:31:37+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor translator\" delimiter=\", \">\n          <name and=\"text\"/>\n          <label form=\"short\" prefix=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" name-as-sort-order=\"first\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\" quotes=\"true\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n      <choose>\n        <if type=\"book chapter\" match=\"any\">\n          <text macro=\"issued-year\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"volume\"/>\n            <text macro=\"issued-year\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"webpage post post-weblog\">\n        <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n      </else-if>\n      <else-if type=\"article-journal article-magazine article-newspaper book chapter webpage post post-weblog\" match=\"none\">\n        <text macro=\"issued-year\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <date variable=\"accessed\" prefix=\" (\" suffix=\")\">\n          <date-part name=\"month\" form=\"short\" suffix=\" \"/>\n          <date-part name=\"day\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n        </group>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&#8212;&#8212;\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\", \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <group delimiter=\" \">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <choose>\n                <if type=\"chapter paper-conference\" match=\"any\">\n                  <text term=\"in\"/>\n                </if>\n              </choose>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </group>\n            <text macro=\"container-contributors\"/>\n            <text variable=\"collection-title\"/>\n            <text macro=\"secondary-contributors\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <text macro=\"publisher\"/>\n        </group>\n      </group>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"