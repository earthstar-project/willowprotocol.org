"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"text\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Arctic</title>\n    <id>http://www.zotero.org/styles/arctic</id>\n    <link href=\"http://www.zotero.org/styles/arctic\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-structural-geology\" rel=\"template\"/>\n    <link href=\"https://arctic.ucalgary.ca/guide-authors\" rel=\"documentation\"/>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geology\"/>\n    <issn>0004-0843</issn>\n    <eissn>1923-1245</eissn>\n    <updated>2019-09-17T12:37:31+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"container-chapter-journal\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\": \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text variable=\"container-title\" text-case=\"title\"/>\n          <text variable=\"collection-title\" text-case=\"title\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-other\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal\" match=\"none\">\n        <group delimiter=\", \">\n          <text variable=\"container-title\"/>\n          <text variable=\"collection-title\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else-if type=\"webpage post post-weblog\">\n        <group delimiter=\" \">\n          <text value=\"URL\"/>\n          <text variable=\"URL\"/>\n          <group prefix=\"(\" suffix=\").\">\n            <text term=\"accessed\" suffix=\" \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"month\" form=\"numeric\" suffix=\".\"/>\n              <date-part name=\"day\" suffix=\".\"/>\n              <date-part name=\"year\" form=\"short\"/>\n            </date>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\"/>\n          <group delimiter=\" \">\n            <text variable=\"genre\"/>\n            <text variable=\"number\" prefix=\"No. \"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song speech\" match=\"any\">\n        <text variable=\"title\"/>\n        <text macro=\"edition\" prefix=\", \"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <text variable=\"event\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <number variable=\"edition\" form=\"ordinal\"/>\n        </if>\n        <else>\n          <text variable=\"edition\" suffix=\".\"/>\n        </else>\n      </choose>\n      <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\":\" prefix=\" \">\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\". \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"patent\">\n        <text variable=\"number\" prefix=\". \"/>\n      </else-if>\n      <else-if type=\"map\">\n        <group delimiter=\", \" prefix=\", \">\n          <group delimiter=\" \">\n            <text term=\"scale\"/>\n            <text variable=\"scale\"/>\n          </group>\n          <text macro=\"publisher\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key macro=\"issued\" sort=\"ascending\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\":\">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"8\" et-al-use-first=\"7\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"descending\"/>\n    </sort>\n    <layout>\n      <group suffix=\".\" delimiter=\". \">\n        <group delimiter=\". \">\n          <text macro=\"author\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <group>\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"container-chapter-journal\"/>\n            </group>\n            <text macro=\"container-other\"/>\n          </group>\n          <text macro=\"locators\"/>\n        </group>\n      </group>\n      <group display=\"block\">\n        <text macro=\"access\" prefix=\". \"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"