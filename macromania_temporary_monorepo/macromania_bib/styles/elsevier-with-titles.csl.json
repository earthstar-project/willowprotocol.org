"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Elsevier (numeric, with titles)</title>\n    <id>http://www.zotero.org/styles/elsevier-with-titles</id>\n    <link href=\"http://www.zotero.org/styles/elsevier-with-titles\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/elsevier-without-titles\" rel=\"template\"/>\n    <link href=\"http://www.elsevier.com/journals/journal-of-hazardous-materials/0304-3894/guide-for-authors#68001\" rel=\"documentation\"/>\n    <author>\n      <name>Richard Karnesky</name>\n      <email>karnesky+zotero@gmail.com</email>\n      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>\n    </author>\n    <contributor>\n      <name>Rintze Zelle</name>\n      <uri>http://twitter.com/rintzezelle</uri>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"generic-base\"/>\n    <summary>A style for many of Elsevier's journals that includes article titles in the reference list</summary>\n    <updated>2019-10-15T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" text-case=\"capitalize-first\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" suffix=\", \"/>\n    <text variable=\"publisher-place\" suffix=\", \"/>\n    <text macro=\"year-date\"/>\n  </macro>\n  <macro name=\"edition\">\n    <!--TODO: CSL should have low numeric be text (e.g. '3'->'third')-->\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"URL\"/>\n        <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n          <text term=\"accessed\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" suffix=\", \"/>\n          <text term=\"in\" suffix=\": \"/>\n          <text macro=\"editor\" suffix=\", \"/>\n          <text variable=\"container-title\" form=\"short\" text-case=\"title\" suffix=\", \"/>\n          <text macro=\"edition\" suffix=\", \"/>\n          <text macro=\"publisher\"/>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\" prefix=\": \"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text variable=\"title\" suffix=\",\"/>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\"/>\n            <text variable=\"volume\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </else>\n      </choose>\n      <choose>\n        <if variable=\"DOI\">\n          <text variable=\"DOI\" prefix=\". https://doi.org/\"/>\n        </if>\n        <else>\n          <text macro=\"access\" prefix=\". \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"