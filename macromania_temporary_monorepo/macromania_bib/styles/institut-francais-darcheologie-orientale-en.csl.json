"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" initialize-with=\".\" page-range-format=\"expanded\" demote-non-dropping-particle=\"sort-only\">\n  <!-- This style was edited with the Visual CSL Editor (https://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Institut français d'archéologie orientale - Egyptology (English)</title>\n    <title-short>IFAO (EN)</title-short>\n    <id>http://www.zotero.org/styles/institut-francais-darcheologie-orientale-en</id>\n    <link href=\"http://www.zotero.org/styles/institut-francais-darcheologie-orientale-en\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/institut-francais-darcheologie-orientale\" rel=\"template\"/>\n    <link href=\"https://www.ifao.egnet.net/publications/publier/normes-ed/\" rel=\"documentation\"/>\n    <link href=\"https://www.ifao.egnet.net/uploads/publications/normes/IFAO_publications_normes_bibliographiques_pub_egypto_2022_angl.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Nicolas Souchon</name>\n      <email>souchon.nicolas.ns@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <summary>Style sheet for the archeological and egyptological publications of the Institut français d'archéologie orientale</summary>\n    <updated>2022-03-15T14:37:59+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n      <term name=\"open-quote\">“</term>\n      <term name=\"close-quote\">”</term>\n      <term name=\"open-inner-quote\">‘</term>\n      <term name=\"close-inner-quote\">’</term>\n    </terms>\n    <style-options/>\n  </locale>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book manuscript\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" font-style=\"normal\" font-weight=\"normal\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <text variable=\"title-short\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"container-title\">\n    <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"collection-title\">\n    <text variable=\"collection-title\" form=\"short\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" et-al-min=\"5\" et-al-use-first=\"1\" initialize-with=\".\">\n        <name-part name=\"given\"/>\n        <name-part name=\"family\"/>\n      </name>\n      <et-al/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" font-variant=\"normal\" et-al-min=\"3\" et-al-use-first=\"1\" name-as-sort-order=\"all\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <et-al font-style=\"normal\"/>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" et-al-min=\"5\" et-al-use-first=\"1\" initialize-with=\".\">\n        <name-part name=\"given\"/>\n        <name-part name=\"family\"/>\n      </name>\n      <et-al/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <text macro=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" et-al-min=\"5\" et-al-use-first=\"1\" initialize-with=\".\">\n        <name-part name=\"given\" font-variant=\"small-caps\"/>\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <et-al/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" text-case=\"capitalize-first\"/>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if match=\"any\" variable=\"publisher-place\">\n        <text variable=\"publisher-place\" text-case=\"capitalize-first\"/>\n      </if>\n      <else>\n        <text value=\"n.p.\" prefix=\"[\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <text variable=\"archive\" text-case=\"capitalize-first\"/>\n  </macro>\n  <macro name=\"archive-location\">\n    <text variable=\"archive_location\"/>\n  </macro>\n  <macro name=\"collection-number\">\n    <number variable=\"collection-number\"/>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if match=\"any\" is-numeric=\"volume\">\n        <number text-case=\"uppercase\" variable=\"volume\" form=\"roman\"/>\n      </if>\n      <else-if match=\"none\" is-numeric=\"volume\">\n        <text variable=\"volume\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"genre\">\n    <text variable=\"genre\"/>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\"&#160;\">\n      <choose>\n        <if match=\"any\" is-numeric=\"page\">\n          <text term=\"page\" form=\"short\" plural=\"true\"/>\n        </if>\n      </choose>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"issued-year-month-day\">\n    <choose>\n      <if match=\"any\" variable=\"issued\">\n        <date delimiter=\"&#160;\" variable=\"issued\">\n          <date-part name=\"day\" range-delimiter=\"&#8211;\"/>\n          <date-part name=\"month\" range-delimiter=\"&#8211;\"/>\n          <date-part name=\"year\" range-delimiter=\"&#8211;\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\" text-case=\"lowercase\" prefix=\"[\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if match=\"any\" variable=\"issued\">\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\">\n          <date-part name=\"year\" range-delimiter=\"&#8211;\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\" prefix=\"[\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"original-date\">\n    <date delimiter=\"\" variable=\"original-date\">\n      <date-part name=\"year\" range-delimiter=\"&#8211;\"/>\n    </date>\n  </macro>\n  <macro name=\"URL\">\n    <text variable=\"URL\"/>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\"&#160;\" prefix=\"(\" suffix=\")\">\n      <number variable=\"edition\" form=\"ordinal\"/>\n      <label variable=\"edition\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"sort\">\n    <choose>\n      <if match=\"any\" variable=\"author\">\n        <text macro=\"author\"/>\n      </if>\n      <else-if match=\"any\" variable=\"editor\">\n        <text macro=\"editor\"/>\n      </else-if>\n      <else-if type=\"webpage\" match=\"all\" variable=\"container-title\">\n        <text macro=\"container-title\"/>\n      </else-if>\n      <else>\n        <text macro=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <group delimiter=\"&#160;\">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <name form=\"short\" font-variant=\"normal\" et-al-min=\"3\" et-al-use-first=\"1\" name-as-sort-order=\"all\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <et-al font-style=\"normal\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"citation\">\n    <choose>\n      <if match=\"any\" variable=\"title-short\">\n        <text macro=\"title-short\"/>\n      </if>\n      <else-if match=\"any\" variable=\"author editor\">\n        <group delimiter=\"&#160;\">\n          <choose>\n            <if match=\"any\" variable=\"author\">\n              <text macro=\"author-short\"/>\n            </if>\n            <else>\n              <text macro=\"editor-short\"/>\n            </else>\n          </choose>\n          <group>\n            <choose>\n              <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n                <choose>\n                  <if match=\"any\" variable=\"status\">\n                    <text macro=\"status\"/>\n                  </if>\n                  <else>\n                    <text macro=\"issued-year\"/>\n                  </else>\n                </choose>\n              </if>\n              <else>\n                <choose>\n                  <if match=\"any\" variable=\"original-date\">\n                    <group delimiter=\"&#160;\">\n                      <text macro=\"original-date\"/>\n                      <group delimiter=\"&#160;\" prefix=\"(\" suffix=\")\">\n                        <text term=\"edition\" form=\"short\"/>\n                        <text macro=\"issued-year\"/>\n                      </group>\n                    </group>\n                  </if>\n                  <else-if match=\"any\" variable=\"status\">\n                    <group>\n                      <text macro=\"status\" prefix=\"(\" suffix=\")\"/>\n                    </group>\n                  </else-if>\n                  <else>\n                    <group>\n                      <text macro=\"issued-year\"/>\n                    </group>\n                  </else>\n                </choose>\n              </else>\n            </choose>\n            <text macro=\"year-suffix\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\"&#160;\">\n          <text macro=\"container-title\"/>\n          <text macro=\"volume\"/>\n        </group>\n        <group delimiter=\"&#160;\" prefix=\", \">\n          <text term=\"sub-verbo\" form=\"short\" font-style=\"normal\"/>\n          <text macro=\"title\"/>\n        </group>\n      </else-if>\n      <else>\n        <choose>\n          <if type=\"webpage\" match=\"any\">\n            <text macro=\"container-title\"/>\n          </if>\n          <else>\n            <text macro=\"title\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"status\">\n    <text term=\"forthcoming\"/>\n  </macro>\n  <macro name=\"event-place\">\n    <text variable=\"event-place\"/>\n  </macro>\n  <macro name=\"event-date\">\n    <date delimiter=\" \" variable=\"event-date\">\n      <date-part name=\"day\" range-delimiter=\"&#8211;\"/>\n      <date-part name=\"month\" form=\"short\" range-delimiter=\"&#8211;\"/>\n      <date-part name=\"year\" range-delimiter=\"&#8211;\"/>\n    </date>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" et-al-min=\"5\" et-al-use-first=\"1\" initialize-with=\".\">\n        <name-part name=\"given\"/>\n        <name-part name=\"family\"/>\n      </name>\n      <et-al/>\n    </names>\n  </macro>\n  <macro name=\"year-suffix\">\n    <text variable=\"year-suffix\"/>\n  </macro>\n  <macro name=\"note\">\n    <text variable=\"note\"/>\n  </macro>\n  <macro name=\"issued-year-month-day-short\">\n    <choose>\n      <if match=\"any\" variable=\"issued\">\n        <date delimiter=\" \" variable=\"issued\">\n          <date-part name=\"day\" range-delimiter=\"&#8211;\"/>\n          <date-part name=\"month\" form=\"short\" range-delimiter=\"&#8211;\"/>\n          <date-part name=\"year\" range-delimiter=\"&#8211;\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <citation name-delimiter=\", \" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" givenname-disambiguation-rule=\"all-names\">\n    <sort>\n      <key macro=\"sort\"/>\n    </sort>\n    <layout delimiter=\"&#160;; \" suffix=\".\">\n      <text macro=\"citation\"/>\n      <text macro=\"locator\" prefix=\", \"/>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"sort\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <group display=\"block\">\n        <text macro=\"citation\"/>\n      </group>\n      <group display=\"left-margin\" delimiter=\", \" suffix=\".\">\n        <choose>\n          <if type=\"book\" match=\"any\">\n            <group delimiter=\", \">\n              <choose>\n                <if match=\"any\" variable=\"author\">\n                  <text macro=\"author\"/>\n                </if>\n                <else>\n                  <text macro=\"editor\"/>\n                </else>\n              </choose>\n              <group delimiter=\"&#160;\">\n                <text macro=\"title\"/>\n                <text macro=\"volume\"/>\n                <text macro=\"original-date\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <choose>\n                <if match=\"any\" variable=\"genre\">\n                  <group delimiter=\", \">\n                    <text macro=\"genre\"/>\n                    <text macro=\"event-place\"/>\n                    <text macro=\"event-date\"/>\n                  </group>\n                </if>\n              </choose>\n              <text macro=\"note\"/>\n              <text macro=\"translator\"/>\n              <group delimiter=\"&#160;\">\n                <text macro=\"collection-title\"/>\n                <text macro=\"collection-number\"/>\n              </group>\n              <choose>\n                <if match=\"none\" variable=\"publisher-place issued\">\n                  <text value=\"n.p., n.d.\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n                <else>\n                  <group delimiter=\", \">\n                    <text macro=\"publisher-place\"/>\n                    <choose>\n                      <if match=\"any\" variable=\"status\">\n                        <text macro=\"status\"/>\n                      </if>\n                      <else>\n                        <group delimiter=\"&#160;\">\n                          <text macro=\"issued-year\"/>\n                          <text macro=\"edition\"/>\n                        </group>\n                      </else>\n                    </choose>\n                  </group>\n                </else>\n              </choose>\n            </group>\n          </if>\n          <else-if type=\"article-journal article article-magazine article-newspaper\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"author\"/>\n              <text macro=\"title\" quotes=\"false\" suffix=\",\"/>\n              <group delimiter=\"&#160;\">\n                <text macro=\"container-title\"/>\n                <group delimiter=\"/\">\n                  <number variable=\"volume\"/>\n                  <number variable=\"issue\"/>\n                </group>\n              </group>\n              <choose>\n                <if match=\"any\" variable=\"status\">\n                  <text macro=\"status\"/>\n                </if>\n                <else>\n                  <text macro=\"issued-year-month-day\"/>\n                </else>\n              </choose>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"author\"/>\n              <text macro=\"title\"/>\n              <group delimiter=\"&#160;\">\n                <text term=\"in\" font-style=\"normal\"/>\n                <text macro=\"editor\" suffix=\",\"/>\n                <text macro=\"container-title\"/>\n                <text macro=\"volume\"/>\n                <text macro=\"original-date\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <choose>\n                <if match=\"any\" variable=\"genre\">\n                  <group delimiter=\", \">\n                    <text macro=\"genre\"/>\n                    <text macro=\"event-place\"/>\n                    <text macro=\"event-date\"/>\n                  </group>\n                </if>\n              </choose>\n              <text macro=\"note\"/>\n              <group delimiter=\"&#160;\">\n                <text macro=\"collection-title\"/>\n                <text macro=\"collection-number\"/>\n              </group>\n              <choose>\n                <if match=\"none\" variable=\"publisher-place issued\">\n                  <text value=\"n.p., n.d.\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n                <else>\n                  <group delimiter=\", \">\n                    <text macro=\"publisher-place\"/>\n                    <choose>\n                      <if match=\"any\" variable=\"status\">\n                        <text macro=\"status\"/>\n                      </if>\n                      <else>\n                        <group delimiter=\"&#160;\">\n                          <text macro=\"issued-year\"/>\n                          <text macro=\"edition\"/>\n                        </group>\n                      </else>\n                    </choose>\n                  </group>\n                </else>\n              </choose>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"manuscript\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"author\"/>\n              <text macro=\"title\"/>\n              <text macro=\"genre\"/>\n              <text macro=\"issued-year\"/>\n              <text macro=\"archive\"/>\n              <text macro=\"archive-location\"/>\n            </group>\n          </else-if>\n          <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"author\"/>\n              <group delimiter=\"&#160;\">\n                <text macro=\"container-title\"/>\n                <text macro=\"volume\"/>\n              </group>\n              <choose>\n                <if match=\"any\" variable=\"status\">\n                  <text macro=\"status\"/>\n                </if>\n                <else>\n                  <text macro=\"issued-year\"/>\n                </else>\n              </choose>\n              <text macro=\"pages\"/>\n              <group delimiter=\"&#160;\">\n                <text term=\"sub-verbo\" form=\"short\" font-style=\"normal\"/>\n                <text macro=\"title\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"author\"/>\n              <text macro=\"title\"/>\n              <text macro=\"genre\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"issued-year\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"author\"/>\n              <text macro=\"title\"/>\n              <text macro=\"genre\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"publisher-place\"/>\n              <text macro=\"issued-year\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"author\"/>\n              <text macro=\"container-title\"/>\n              <text macro=\"genre\"/>\n              <text macro=\"URL\"/>\n              <choose>\n                <if match=\"any\" variable=\"issued\">\n                  <group delimiter=\"&#160;\">\n                    <text value=\"version\"/>\n                    <text macro=\"issued-year-month-day-short\"/>\n                  </group>\n                </if>\n              </choose>\n            </group>\n          </else-if>\n          <else-if type=\"post-weblog\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"author\"/>\n              <text macro=\"title\"/>\n              <group delimiter=\"&#160;\">\n                <text term=\"in\" font-style=\"normal\"/>\n                <text macro=\"container-title\"/>\n              </group>\n              <group delimiter=\"&#160;\">\n                <text term=\"online\"/>\n                <text macro=\"genre\"/>\n              </group>\n              <text macro=\"URL\"/>\n              <group delimiter=\"&#160;\">\n                <text value=\"published\"/>\n                <text macro=\"issued-year-month-day-short\"/>\n              </group>\n            </group>\n          </else-if>\n        </choose>\n        <choose>\n          <if match=\"any\" variable=\"URL\">\n            <choose>\n              <if type=\"webpage post-weblog\" match=\"none\">\n                <group delimiter=\", \">\n                  <text macro=\"URL\"/>\n                </group>\n              </if>\n            </choose>\n          </if>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"