"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Statistika: Statistics and Economy Journal</title>\n    <id>http://www.zotero.org/styles/statistika-statistics-and-economy-journal</id>\n    <link href=\"http://www.zotero.org/styles/statistika-statistics-and-economy-journal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-urban-and-environmental-engineering\" rel=\"template\"/>\n    <link href=\"https://www.czso.cz/csu/czso/guide_for_authors\" rel=\"documentation\"/>\n    <author>\n      <name>Libor Ansorge</name>\n      <email>libor.ansorge@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <issn>0322-788x</issn>\n    <eissn>1804-8765</eissn>\n    <updated>2016-04-19T17:32:13+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"cited\" form=\"short\">cit</term>\n    </terms>\n  </locale>\n  <!-- MACRO SECTION -->\n  <macro name=\"author\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <names variable=\"author\">\n          <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n          </name>\n          <label form=\"short\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text term=\"anonymous\" form=\"short\" text-case=\"uppercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <names variable=\"author\">\n          <name form=\"short\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text term=\"anonymous\" form=\"short\" text-case=\"uppercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" form=\"short\" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label prefix=\", \" form=\"short\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if is-uncertain-date=\"issued\">\n            <text term=\"circa\" form=\"short\" suffix=\" \"/>\n          </if>\n        </choose>\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n            <text macro=\"edition\"/>\n          </group>\n          <text macro=\"editor-translator\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\". \">\n          <text variable=\"title\" text-case=\"title\"/>\n          <text variable=\"genre\"/>\n        </group>\n      </else>\n    </choose>\n    <choose>\n      <if variable=\"DOI URL\" match=\"any\">\n        <text term=\"online\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <label variable=\"edition\" form=\"short\" prefix=\" \"/>\n        <number variable=\"edition\" form=\"ordinal\"/>\n      </if>\n      <else>\n        <text variable=\"edition\" form=\"long\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"journal-location\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"container-title\" font-style=\"italic\" form=\"short\" text-case=\"title\"/>\n          <!--<group delimiter=\", \">-->\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n          <!--</group>-->\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"chapter-info\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \" suffix=\".\">\n          <group delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text macro=\"editor-translator\"/>\n            <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n            <text variable=\"genre\" prefix=\"genre\" font-style=\"italic\"/>\n            <text variable=\"collection-title\" font-style=\"italic\"/>\n            <text variable=\"event\" font-style=\"italic\"/>\n            <text variable=\"collection-number\" font-style=\"italic\"/>\n            <choose>\n              <if type=\"paper-conference\">\n                <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                  <text variable=\"event\"/>\n                  <text variable=\"event-place\"/>\n                  <date variable=\"event-date\">\n                    <date-part name=\"day\" suffix=\" \" range-delimiter=\"-\"/>\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"year\" range-delimiter=\"/\"/>\n                  </date>\n                </group>\n              </if>\n            </choose>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publication_info\">\n    <group delimiter=\", \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <choose>\n        <if type=\"article-journal article-magazine article-newspaper\" match=\"none\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if variable=\"URL DOI\" match=\"any\">\n        <group prefix=\" [\" suffix=\"]. \">\n          <text term=\"cited\" form=\"short\" suffix=\". \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\".\"/>\n            <date-part name=\"month\" suffix=\".\" form=\"numeric\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"&lt;doi:\" suffix=\"&gt;\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <!-- CITATION SECTION -->\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <!-- BIBLIOGRAPHY SECTION -->\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"title\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <text macro=\"journal-location\"/>\n        <text macro=\"chapter-info\"/>\n        <text macro=\"publication_info\"/>\n        <text macro=\"online\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"