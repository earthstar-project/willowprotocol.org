"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"text\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Food Science and Biotechnology</title>\n    <title-short>FSB</title-short>\n    <id>http://www.zotero.org/styles/food-science-and-biotechnology</id>\n    <link href=\"http://www.zotero.org/styles/food-science-and-biotechnology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/animal\" rel=\"template\"/>\n    <link href=\"http://www.fsnb.or.kr/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1226-7708</issn>\n    <eissn>2092-6456</eissn>\n    <updated>2022-07-08T12:59:27+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\"(\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <label variable=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume-booklike\">\n    <group delimiter=\" \">\n      <label variable=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n      <number variable=\"volume\" form=\"numeric\"/>\n    </group>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" initialize-with=\". \"/>\n      <et-al font-style=\"normal\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\" \">\n      <group delimiter=\", \">\n        <text variable=\"publisher\"/>\n        <text variable=\"publisher-place\"/>\n      </group>\n      <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation delimiter-precedes-last=\"never\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\" year-suffix-delimiter=\"; \">\n    <sort>\n      <key variable=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography sort-separator=\"\" entry-spacing=\"0\" line-spacing=\"2\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text variable=\"title\" prefix=\" \"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"edition\"/>\n              <text macro=\"volume-booklike\"/>\n              <group delimiter=\" \">\n                <text macro=\"publisher\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\" prefix=\" \"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <text macro=\"volume-booklike\"/>\n                <text macro=\"pages\"/>\n              </group>\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text variable=\"container-title\" prefix=\" \"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"editor\"/>\n                <text variable=\"collection-title\" prefix=\" \"/>\n              </group>\n              <text macro=\"publisher\" prefix=\" \"/>\n            </group>\n          </else-if>\n          <else-if type=\"post-weblog webpage post\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"container-title\"/>\n              <group delimiter=\". \">\n                <group delimiter=\": \">\n                  <text term=\"available at\" text-case=\"capitalize-first\"/>\n                  <text variable=\"URL\" font-style=\"italic\"/>\n                </group>\n                <group>\n                  <text term=\"accessed\" text-case=\"capitalize-first\" font-style=\"normal\" suffix=\" \"/>\n                  <date variable=\"accessed\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\": \" prefix=\" \">\n              <group delimiter=\". \">\n                <text variable=\"container-title\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <group delimiter=\" \">\n                <text variable=\"page\"/>\n                <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"