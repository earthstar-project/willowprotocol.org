"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" page-range-format=\"expanded\" default-locale=\"en-GB\">\n  <info>\n    <title>Leeds Beckett University - Harvard</title>\n    <id>http://www.zotero.org/styles/harvard-leeds-beckett-university</id>\n    <link href=\"http://www.zotero.org/styles/harvard-leeds-beckett-university\" rel=\"self\"/>\n    <link href=\"https://library.leedsbeckett.ac.uk/articulate/rise/quote_unquote/index_target.html#/lessons/nPbUYlJ7xTxtNuf93tegoBBjfEyjdJdo\" rel=\"documentation\"/>\n    <author>\n      <name>Joe Joiner</name>\n      <email>work@joejoiner.me</email>\n      <uri>http://twitter.com/joejoinerr</uri>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <updated>2015-03-23T12:34:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"presented at\">presented at</term>\n    </terms>\n  </locale>\n  <macro name=\"sort-term\">\n    <choose>\n      <if type=\"entry-dictionary\">\n        <choose>\n          <if variable=\"author\">\n            <names variable=\"author\">\n              <name initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"never\" and=\"text\"/>\n            </names>\n          </if>\n          <else>\n            <text macro=\"container-title\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"legal_case legislation\" match=\"any\">\n        <text macro=\"title\"/>\n      </else-if>\n      <else-if type=\"bill\">\n        <text macro=\"title\"/>\n        <text macro=\"container-title\" prefix=\". \" suffix=\" Bill\"/>\n      </else-if>\n      <else-if type=\"webpage\">\n        <choose>\n          <if variable=\"author\">\n            <names variable=\"author\">\n              <name initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"never\" and=\"text\"/>\n            </names>\n          </if>\n          <else-if variable=\"container-title\">\n            <text macro=\"container-title\"/>\n          </else-if>\n          <else>\n            <text macro=\"title\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"song\" variable=\"composer\">\n        <names variable=\"composer\">\n          <name initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"never\" and=\"text\"/>\n        </names>\n      </else-if>\n      <else-if type=\"motion_picture\" variable=\"URL\" match=\"all\">\n        <names variable=\"author\">\n          <name initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"never\" and=\"text\"/>\n        </names>\n      </else-if>\n      <else-if type=\"motion_picture\">\n        <text macro=\"title\"/>\n      </else-if>\n      <else-if variable=\"author editor\" match=\"any\">\n        <names variable=\"author\">\n          <name initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"never\" and=\"text\"/>\n          <label form=\"short\" prefix=\" \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n          </substitute>\n        </names>\n      </else-if>\n      <else-if type=\"book\" variable=\"publisher\">\n        <text variable=\"publisher\"/>\n      </else-if>\n      <else>\n        <text macro=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"online\" text-case=\"capitalize-first\"/>\n          <choose>\n            <if type=\"personal_communication\">\n              <text value=\" discussion list\"/>\n            </if>\n            <else-if type=\"graphic\">\n              <text value=\" image\"/>\n            </else-if>\n            <else-if type=\"post-weblog\">\n              <text value=\" blog\"/>\n            </else-if>\n            <else-if type=\"motion_picture\">\n              <text value=\" video\"/>\n            </else-if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text value=\"Available from: \"/>\n        <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <date variable=\"accessed\" delimiter=\" \">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter post-weblog entry-dictionary entry-encyclopedia article-journal article-newspaper article-magazine paper-conference\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <number variable=\"edition\" form=\"ordinal\" suffix=\" \"/>\n        <label variable=\"edition\" form=\"short\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"page-range\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"locator-range\">\n    <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"webpage\">\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"container-title\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book legal_case legislation map musical_score motion_picture patent broadcast report song thesis webpage bill\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if variable=\"volume\">\n        <text term=\"volume\" form=\"short\" suffix=\" \"/>\n        <number variable=\"volume\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume-edition\">\n    <choose>\n      <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"volume\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\". \">\n          <text macro=\"volume\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <choose>\n      <if variable=\"event-date\">\n        <date variable=\"event-date\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" givenname-disambiguation-rule=\"all-names-with-initials\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key macro=\"sort-term\"/>\n      <key macro=\"year\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <names variable=\"composer\">\n          <name form=\"short\" initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" and=\"text\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n          <substitute>\n            <names variable=\"author\"/>\n            <names variable=\"editor\"/>\n            <text variable=\"container-title\"/>\n            <text variable=\"publisher\"/>\n          </substitute>\n        </names>\n        <text macro=\"year\"/>\n        <choose>\n          <if type=\"legal_case\" match=\"none\">\n            <text macro=\"locator-range\"/>\n          </if>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\">\n    <sort>\n      <key macro=\"sort-term\"/>\n      <key macro=\"year\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <!-- Author/Title -->\n      <text macro=\"sort-term\"/>\n      <!-- Year -->\n      <choose>\n        <if type=\"legal_case\">\n          <text macro=\"year\" prefix=\" [\" suffix=\"]\"/>\n        </if>\n        <else-if type=\"legislation\">\n          <number variable=\"chapter-number\" prefix=\" (c. \" suffix=\")\"/>\n        </else-if>\n        <else>\n          <text macro=\"year\" prefix=\" (\" suffix=\")\"/>\n        </else>\n      </choose>\n      <choose>\n        <if type=\"graphic\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group delimiter=\" \">\n              <choose>\n                <if variable=\"author\">\n                  <text macro=\"title\"/>\n                </if>\n              </choose>\n              <text macro=\"online\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"page-range\"/>\n              <choose>\n                <if variable=\"URL\" match=\"none\">\n                  <text value=\"illus\"/>\n                </if>\n              </choose>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </if>\n        <else-if type=\"post-weblog\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <choose>\n              <if variable=\"author\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <group>\n              <group delimiter=\", \">\n                <text macro=\"container-title\"/>\n                <date variable=\"issued\" delimiter=\" \">\n                  <date-part name=\"day\"/>\n                  <date-part name=\"month\"/>\n                </date>\n              </group>\n              <text macro=\"online\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"entry-dictionary\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group delimiter=\" \">\n              <choose>\n                <if variable=\"author\">\n                  <text macro=\"container-title\"/>\n                </if>\n              </choose>\n              <text macro=\"online\"/>\n            </group>\n            <text macro=\"volume-edition\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <choose>\n                <if variable=\"URL\" match=\"none\">\n                  <text macro=\"page-range\"/>\n                </if>\n              </choose>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"personal_communication\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text macro=\"title\"/>\n            <group>\n              <group font-style=\"italic\" delimiter=\" \">\n                <text variable=\"title\" form=\"short\"/>\n                <date variable=\"issued\" delimiter=\" \">\n                  <date-part name=\"day\"/>\n                  <date-part name=\"month\"/>\n                </date>\n              </group>\n              <text macro=\"online\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <choose>\n              <if variable=\"author\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <group delimiter=\", \">\n              <group>\n                <text macro=\"container-title\"/>\n                <text macro=\"online\"/>\n              </group>\n              <group delimiter=\" \">\n                <number variable=\"volume\"/>\n                <number variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                <date variable=\"issued\">\n                  <date-part name=\"month\"/>\n                </date>\n              </group>\n              <text macro=\"page-range\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"legal_case\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group delimiter=\" \">\n              <number variable=\"volume\"/>\n              <text variable=\"container-title\" form=\"short\"/>\n              <text variable=\"page\"/>\n              <text macro=\"online\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"map\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group>\n              <group delimiter=\", \">\n                <choose>\n                  <if variable=\"author\">\n                    <text macro=\"title\"/>\n                  </if>\n                </choose>\n                <choose>\n                  <if variable=\"title-short author\">\n                    <group>\n                      <text value=\"sheet \"/>\n                      <text variable=\"title\" form=\"short\"/>\n                    </group>\n                  </if>\n                </choose>\n                <choose>\n                  <if variable=\"URL\">\n                    <text variable=\"scale\"/>\n                  </if>\n                </choose>\n              </group>\n              <text macro=\"online\"/>\n            </group>\n            <choose>\n              <if variable=\"URL\" match=\"none\">\n                <text variable=\"scale\"/>\n              </if>\n            </choose>\n            <text variable=\"collection-title\" prefix=\"(\" suffix=\")\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <choose>\n              <if variable=\"author\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <group delimiter=\", \">\n              <group>\n                <text macro=\"container-title\"/>\n                <text macro=\"online\"/>\n              </group>\n              <date variable=\"issued\" delimiter=\" \">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n              </date>\n              <text macro=\"page-range\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"motion_picture\" variable=\"URL\" match=\"all\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group delimiter=\", \">\n              <group>\n                <choose>\n                  <if variable=\"author\">\n                    <text macro=\"title\"/>\n                  </if>\n                </choose>\n                <text macro=\"online\"/>\n              </group>\n              <date variable=\"issued\" delimiter=\" \">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n              </date>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"motion_picture\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group>\n              <names variable=\"author\" prefix=\"Directed by \">\n                <name delimiter=\", \" delimiter-precedes-last=\"never\" and=\"text\"/>\n              </names>\n            </group>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"legislation\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text macro=\"online\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group>\n              <number variable=\"number\"/>\n              <text macro=\"online\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <choose>\n              <if variable=\"author\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <number variable=\"number\"/>\n          </group>\n        </else-if>\n        <else-if type=\"broadcast\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <choose>\n              <if variable=\"author\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"publisher\"/>\n            <date variable=\"issued\" delimiter=\" \">\n              <date-part name=\"day\"/>\n              <date-part name=\"month\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"song\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group delimiter=\", \">\n              <choose>\n                <if variable=\"author\">\n                  <text macro=\"title\"/>\n                </if>\n              </choose>\n              <group delimiter=\" \">\n                <names variable=\"author\"/>\n                <choose>\n                  <if variable=\"URL\">\n                    <text value=\"[Digital audio file]\"/>\n                  </if>\n                </choose>\n              </group>\n            </group>\n            <text macro=\"publisher\"/>\n            <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group delimiter=\" \">\n              <choose>\n                <if variable=\"author container-title\" match=\"any\">\n                  <text macro=\"title\"/>\n                </if>\n              </choose>\n              <text macro=\"online\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group>\n              <choose>\n                <if variable=\"author\">\n                  <text macro=\"title\"/>\n                </if>\n                <else-if type=\"book\" variable=\"publisher\">\n                  <text macro=\"title\"/>\n                </else-if>\n              </choose>\n              <choose>\n                <if type=\"entry-encyclopedia\" match=\"none\">\n                  <text macro=\"online\"/>\n                </if>\n              </choose>\n              <choose>\n                <if type=\"entry-encyclopedia\">\n                  <text term=\"in\" prefix=\", \" suffix=\": \"/>\n                  <text macro=\"container-title\"/>\n                  <text macro=\"online\"/>\n                </if>\n                <else-if type=\"chapter paper-conference\" match=\"any\">\n                  <choose>\n                    <if variable=\"container-title\">\n                      <text term=\"in\" text-case=\"capitalize-first\" prefix=\". \" suffix=\": \"/>\n                      <group delimiter=\", \">\n                        <choose>\n                          <if variable=\"container-author\">\n                            <names variable=\"container-author\">\n                              <name initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"never\" and=\"text\"/>\n                            </names>\n                          </if>\n                          <else>\n                            <group>\n                              <names variable=\"editor\">\n                                <name initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"never\" and=\"text\"/>\n                              </names>\n                              <text term=\"editor\" form=\"short\" prefix=\" \"/>\n                            </group>\n                          </else>\n                        </choose>\n                        <text macro=\"container-title\"/>\n                        <choose>\n                          <if type=\"paper-conference\">\n                            <choose>\n                              <if variable=\"event-date\">\n                                <date variable=\"event-date\" font-style=\"italic\">\n                                  <date-part name=\"month\" suffix=\" \"/>\n                                  <date-part name=\"day\" suffix=\", \"/>\n                                  <date-part name=\"year\"/>\n                                </date>\n                              </if>\n                              <else>\n                                <date variable=\"issued\" font-style=\"italic\">\n                                  <date-part name=\"month\" suffix=\" \"/>\n                                  <date-part name=\"day\" suffix=\", \"/>\n                                  <date-part name=\"year\"/>\n                                </date>\n                              </else>\n                            </choose>\n                          </if>\n                        </choose>\n                      </group>\n                    </if>\n                  </choose>\n                </else-if>\n                <else-if type=\"speech\">\n                  <choose>\n                    <if variable=\"title\">\n                      <text term=\"presented at\" text-case=\"capitalize-first\" prefix=\". \" suffix=\": \"/>\n                      <group font-style=\"italic\" delimiter=\", \">\n                        <text variable=\"event\"/>\n                        <date variable=\"issued\">\n                          <date-part name=\"month\" suffix=\" \"/>\n                          <date-part name=\"day\" suffix=\", \"/>\n                          <date-part name=\"year\"/>\n                        </date>\n                        <text variable=\"event-place\"/>\n                      </group>\n                    </if>\n                  </choose>\n                </else-if>\n              </choose>\n              <choose>\n                <if type=\"thesis\">\n                  <text variable=\"genre\" prefix=\" [\" suffix=\"]\"/>\n                </if>\n              </choose>\n              <choose>\n                <if type=\"book\" variable=\"medium\">\n                  <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n                </if>\n              </choose>\n            </group>\n            <choose>\n              <if type=\"report\">\n                <group delimiter=\" \">\n                  <text variable=\"genre\"/>\n                  <number variable=\"number\"/>\n                </group>\n              </if>\n            </choose>\n            <choose>\n              <if type=\"book\">\n                <group delimiter=\". \">\n                  <group delimiter=\", \">\n                    <text variable=\"collection-title\"/>\n                    <number variable=\"volume\"/>\n                  </group>\n                  <text macro=\"edition\"/>\n                </group>\n              </if>\n              <else>\n                <text macro=\"volume-edition\"/>\n              </else>\n            </choose>\n            <choose>\n              <if type=\"speech\" match=\"none\">\n                <group delimiter=\", \">\n                  <text macro=\"publisher\"/>\n                  <text macro=\"page-range\"/>\n                </group>\n              </if>\n            </choose>\n            <text macro=\"access\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"