"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"symbol\" delimiter-precedes-last=\"never\" page-range-format=\"expanded\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\">\n  <info>\n    <title>Interpreting</title>\n    <title-short>INTP</title-short>\n    <id>http://www.zotero.org/styles/interpreting</id>\n    <link href=\"http://www.zotero.org/styles/interpreting\" rel=\"self\"/>\n    <link href=\"https://benjamins.com/catalog/intp/guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Nannan Liu</name>\n      <email>nnl93@connect.hku.hk</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <issn>1384-6647</issn>\n    <eissn>1569-982X</eissn>\n    <updated>2022-10-02T14:03:36+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editortranslator\" form=\"short\">\n        <single>ed. &amp; trans.</single>\n        <multiple>eds. &amp; trans.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor translator\" delimiter=\", \" suffix=\", \">\n          <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n          <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"translator editor\" delimiter=\", \">\n          <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n          <label form=\"short\" text-case=\"title\" prefix=\" (\" suffix=\"), \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name font-variant=\"normal\" and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" (\" suffix=\".)\"/>\n      <et-al term=\"and others\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\">\n            <text variable=\"publisher\"/>\n            <text macro=\"title\"/>\n          </if>\n          <else>\n            <text macro=\"title\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\">\n            <text variable=\"publisher\"/>\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else-if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n            <text variable=\"title\" form=\"short\"/>\n          </else-if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"URL\"/>\n        <group delimiter=\" \" prefix=\" (\" suffix=\").\">\n          <text term=\"accessed\"/>\n          <date form=\"text\" variable=\"accessed\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else-if variable=\"archive\">\n        <choose>\n          <if type=\"article article-journal article-magazine article-newspaper dataset paper-conference report speech thesis\" match=\"any\">\n            <choose>\n              <if variable=\"archive-place\" match=\"none\">\n                <group delimiter=\" \">\n                  <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n                  <text term=\"from\"/>\n                  <text variable=\"archive\" suffix=\".\"/>\n                  <text variable=\"archive_location\" prefix=\"(\" suffix=\")\"/>\n                </group>\n              </if>\n            </choose>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book graphic  motion_picture report song manuscript speech\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else-if type=\"software\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\".\"/>\n        <text variable=\"version\" prefix=\" Version: \"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"false\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <choose>\n        <if variable=\"event\" match=\"none\">\n          <text variable=\"genre\"/>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"article-journal article-magazine software report\" match=\"none\">\n          <group>\n            <text variable=\"publisher-place\" suffix=\": \"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"page\" prefix=\", \"/>\n          </group>\n        </if>\n        <else-if type=\"software\" match=\"any\">\n          <text variable=\"publisher-place\"/>\n        </else-if>\n        <else-if type=\"report\" match=\"any\">\n          <text variable=\"publisher-place\"/>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <choose>\n          <if variable=\"genre\" match=\"none\">\n            <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <text variable=\"event\"/>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              <text term=\"presented at\"/>\n              <text variable=\"event\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"bill legal_case legislation\" match=\"none\">\n        <choose>\n          <if variable=\"issued\">\n            <group prefix=\" (\" suffix=\").\">\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"year-suffix\"/>\n              <choose>\n                <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n                  <date variable=\"issued\">\n                    <date-part prefix=\", \" name=\"month\"/>\n                    <date-part prefix=\" \" name=\"day\"/>\n                  </date>\n                </if>\n              </choose>\n            </group>\n          </if>\n          <else>\n            <group prefix=\" (\" suffix=\")\">\n              <text term=\"no date\" form=\"short\"/>\n              <text variable=\"year-suffix\" prefix=\"-\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued-sort\">\n    <choose>\n      <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date form=\"text\" variable=\"issued\" date-parts=\"year\"/>\n        <text variable=\"year-suffix\"/>\n      </if>\n      <else-if match=\"any\" is-uncertain-date=\"issued\">\n        <text term=\"in press\" prefix=\", \"/>\n        <date form=\"text\" variable=\"issued\" date-parts=\"year\"/>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n        <text variable=\"year-suffix\" prefix=\"-\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number prefix=\" (\" variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" suffix=\")\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" form=\"short\" prefix=\"(\" suffix=\" ed.).\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\", \" prefix=\" \">\n          <group delimiter=\" \">\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <group delimiter=\" \" prefix=\", \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"book graphic motion_picture report song chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \" prefix=\", \">\n          <group>\n            <text term=\"volume\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1-\"/>\n          </group>\n          <group>\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"legal_case\">\n        <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n          <text variable=\"authority\"/>\n          <date variable=\"issued\" form=\"text\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <choose>\n        <if locator=\"chapter\">\n          <label variable=\"locator\" form=\"long\" text-case=\"capitalize-first\"/>\n        </if>\n      </choose>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <group>\n      <choose>\n        <if type=\"chapter paper-conference entry-encyclopedia\" match=\"any\">\n          <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n        </if>\n      </choose>\n      <text macro=\"container-contributors\"/>\n      <text macro=\"secondary-contributors\"/>\n      <text macro=\"container-title\" text-case=\"title\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"bill legal_case legislation book\" match=\"none\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n        <text macro=\"edition\"/>\n      </if>\n      <else>\n        <group delimiter=\" \" prefix=\", \">\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"volume\"/>\n              <text variable=\"container-title\"/>\n              <group delimiter=\" \">\n                <text term=\"section\" form=\"symbol\"/>\n                <text variable=\"section\"/>\n              </group>\n              <text variable=\"page\"/>\n            </if>\n            <else>\n              <choose>\n                <if type=\"legal_case\">\n                  <text variable=\"number\" prefix=\"No. \"/>\n                </if>\n                <else>\n                  <text variable=\"number\" prefix=\"Pub. L. No. \"/>\n                  <group delimiter=\" \">\n                    <text term=\"section\" form=\"symbol\"/>\n                    <text variable=\"section\"/>\n                  </group>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation and=\"symbol\" delimiter-precedes-last=\"never\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-sort\" sort=\"ascending\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n        </group>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author\" names-min=\"3\" names-use-first=\"1\"/>\n      <key variable=\"title\"/>\n      <key macro=\"issued-year\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group suffix=\".\">\n        <group delimiter=\". \">\n          <group delimiter=\" \">\n            <text macro=\"author\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"title\" suffix=\".\"/>\n          </group>\n          <text macro=\"container\"/>\n        </group>\n        <text macro=\"locators\"/>\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"access\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"