"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"sort-only\" default-locale=\"fr-FR\" page-range-format=\"expanded\">\n  <info>\n    <title>Ethnologie française (Français)</title>\n    <id>http://www.zotero.org/styles/ethnologie-francaise</id>\n    <link href=\"http://www.zotero.org/styles/ethnologie-francaise\" rel=\"self\"/>\n    <link href=\"http://www.puf.com/puf_wiki/images/e/e3/Charte_Ethnologie_fran%C3%A7aise.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Viera Rebolledo-Dhuin</name>\n      <email>viera.rebolledo-dhuin@uvsq.fr</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"history\"/>\n    <category field=\"anthropology\"/>\n    <issn>0046-2616</issn>\n    <eissn>2101-0064</eissn>\n    <summary>Author-date style for Ethnologie française - revue des PUR</summary>\n    <updated>2015-11-14T04:18:55+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"in\">in</term>\n      <term name=\"anonymous\">anonyme</term>\n      <term name=\"anonymous\" form=\"short\">anon.</term>\n      <term name=\"accessed\">consulté&#160;le</term>\n      <term name=\"no date\">sans date</term>\n      <term name=\"translator\" form=\"short\">trad.</term>\n      <term name=\"editor\" form=\"short\">dir.</term>\n      <term name=\"no date\" form=\"short\">[s.&#160;d.]</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\", \">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" sort-separator=\" \" delimiter=\", \">\n\t\t</name>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <text variable=\"page\" prefix=\"&#160;:&#160;\"/>\n    </group>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\", \"/>\n      <et-al font-variant=\"normal\" font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"editor-short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <name form=\"short\" delimiter=\", \"/>\n      <et-al font-variant=\"normal\" font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text term=\"accessed\" suffix=\" \"/>\n      <date variable=\"accessed\">\n        <date-part name=\"day\" suffix=\" \"/>\n        <date-part name=\"month\" suffix=\" \"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"webpage\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" suffix=\", \" text-case=\"lowercase\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"loc-cite\">\n    <choose>\n      <if locator=\"page\">\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <group delimiter=\"\">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation and=\"text\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\" [\" suffix=\"]\" delimiter=\"&#160;; \">\n      <text macro=\"author-short\" suffix=\", \"/>\n      <text macro=\"year-date\"/>\n      <group>\n        <text macro=\"loc-cite\" prefix=\"&#160;:&#160;\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"page-first\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group>\n        <text macro=\"author\" suffix=\", \"/>\n        <text macro=\"year-date\" suffix=\", \"/>\n        <choose>\n          <if type=\"thesis\">\n            <group delimiter=\", \" suffix=\".\">\n              <text macro=\"title\" font-style=\"italic\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n              <group>\n                <text variable=\"number-of-volumes\" suffix=\"&#160;vol.\"/>\n                <text variable=\"number-of-pages\" suffix=\"&#160;p.\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"webpage\">\n            <group>\n              <text macro=\"title\" font-style=\"italic\"/>\n              <text variable=\"URL\" prefix=\", \"/>\n              <text prefix=\" (\" macro=\"access\" suffix=\")\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-journal article-magazine article-newspaper broadcast personal_communication thesis entry-dictionary entry-encyclopedia\" match=\"any\">\n            <group>\n              <text macro=\"title\" quotes=\"true\"/>\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n              <text variable=\"volume\" prefix=\", vol.&#160;\"/>\n              <text variable=\"issue\" prefix=\", n°&#160;\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"book graphic\" match=\"any\">\n            <group suffix=\".\">\n              <text macro=\"title\" suffix=\", \"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text macro=\"title\" quotes=\"true\" suffix=\", \"/>\n            <group>\n              <text value=\"in\" font-style=\"italic\" suffix=\" \"/>\n              <text macro=\"editor\" suffix=\", \"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"publisher\" prefix=\", \"/>\n              <text macro=\"pages\" suffix=\".\"/>\n            </group>\n          </else-if>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"