"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" page-range-format=\"minimal\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Revista Brasileira de CiÃªncia do Solo</title>\n    <title-short>RBCS</title-short>\n    <id>http://www.zotero.org/styles/revista-brasileira-de-ciencia-do-solo</id>\n    <link href=\"http://www.zotero.org/styles/revista-brasileira-de-ciencia-do-solo\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/elsevier-vancouver-author-date\" rel=\"template\"/>\n    <link href=\"http://www.scielo.br/revistas/rbcs/iinstruc.htm\" rel=\"documentation\"/>\n    <author>\n      <name>CompTask</name>\n      <email>contato@comptask.com.br</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <issn>0100-0683</issn>\n    <eissn>1806-9657</eissn>\n    <summary>Brazilian Soil Science Journal style based on Vancouver-author-date style</summary>\n    <updated>2020-03-13T15:13:32+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-US\">\n    <terms>\n      <term name=\"month-01\" form=\"short\">Jan</term>\n      <term name=\"month-02\" form=\"short\">Feb</term>\n      <term name=\"month-03\" form=\"short\">Mar</term>\n      <term name=\"month-04\" form=\"short\">Apr</term>\n      <term name=\"month-05\" form=\"short\">May</term>\n      <term name=\"month-06\" form=\"short\">Jun</term>\n      <term name=\"month-07\" form=\"short\">Jul</term>\n      <term name=\"month-08\" form=\"short\">Aug</term>\n      <term name=\"month-09\" form=\"short\">Sep</term>\n      <term name=\"month-10\" form=\"short\">Oct</term>\n      <term name=\"month-11\" form=\"short\">Nov</term>\n      <term name=\"month-12\" form=\"short\">Dec</term>\n      <term name=\"page-range-delimiter\">-</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"webpage\">\n        <names variable=\"author\">\n          <name initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n          <label form=\"long\" prefix=\", \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text variable=\"container-title\"/>\n          </substitute>\n        </names>\n      </if>\n      <else-if type=\"report\">\n        <text variable=\"publisher\" suffix=\" \"/>\n        <names variable=\"author\">\n          <name initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n          <label form=\"long\" prefix=\", \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n          </substitute>\n        </names>\n      </else-if>\n      <else>\n        <names variable=\"author\">\n          <name initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n          <label form=\"long\" prefix=\", \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text variable=\"title\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\" sort-separator=\" \">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n      </name>\n      <label form=\"long\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event-year-month-day\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"accessed-year-month-day\">\n    <choose>\n      <if variable=\"accessed\">\n        <date variable=\"accessed\" delimiter=\" \">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \" text-case=\"title\"/>\n    <text variable=\"publisher\" suffix=\"; \"/>\n    <text macro=\"year-date\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\". \">\n          <number variable=\"edition\" form=\"numeric\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <group delimiter=\" &#8211; \">\n      <text variable=\"event\"/>\n      <text variable=\"container-title\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else-if type=\"webpage post-weblog\" match=\"any\">\n        <choose>\n          <if variable=\"URL\">\n            <text variable=\"URL\" prefix=\"Available from: \" suffix=\".\"/>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key macro=\"year-date\" sort=\"ascending\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\" sort=\"ascending\"/>\n      <key macro=\"year-date\" sort=\"descending\"/>\n    </sort>\n    <layout suffix=\"\">\n      <text macro=\"author\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill graphic legal_case legislation motion_picture song\" match=\"any\">\n          <group suffix=\". \" delimiter=\". \">\n            <text variable=\"title\"/>\n            <text variable=\"volume\" prefix=\"vol. \"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"book\">\n          <group delimiter=\". \">\n            <text variable=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group suffix=\". \" delimiter=\". \">\n            <text variable=\"title\"/>\n            <group>\n              <text variable=\"publisher-place\" suffix=\": \" text-case=\"title\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group suffix=\". \" delimiter=\". \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group>\n                <text term=\"in\" text-case=\"sentence\" suffix=\": \"/>\n                <text macro=\"editor\"/>\n              </group>\n              <group delimiter=\", \">\n                <text variable=\"container-title\"/>\n                <text variable=\"volume\" prefix=\"vol. \"/>\n              </group>\n              <text macro=\"edition\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\" plural=\"never\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <group suffix=\". \" delimiter=\". \">\n            <group>\n              <text variable=\"title\" suffix=\". \"/>\n              <group suffix=\"; \">\n                <text term=\"in\" text-case=\"sentence\" suffix=\": \"/>\n                <text macro=\"event\"/>\n              </group>\n              <text macro=\"event-year-month-day\" suffix=\"; \"/>\n              <text variable=\"publisher-place\" suffix=\". \" text-case=\"title\"/>\n              <text variable=\"publisher\" suffix=\"; \"/>\n              <text macro=\"year-date\"/>\n            </group>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\" plural=\"never\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group suffix=\". \" delimiter=\", \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"number\"/>\n            </group>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group suffix=\". \" delimiter=\". \">\n            <group delimiter=\" \">\n              <text variable=\"title\"/>\n              <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n            </group>\n            <group>\n              <text variable=\"publisher-place\" suffix=\": \"/>\n              <text variable=\"publisher\" suffix=\"; \"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group suffix=\". \">\n            <text variable=\"title\" suffix=\"; \"/>\n            <text macro=\"publisher\" suffix=\" \"/>\n            <text prefix=\"[cited \" suffix=\"]\" macro=\"accessed-year-month-day\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\":\">\n            <group delimiter=\". \">\n              <group delimiter=\". \">\n                <text variable=\"title\" text-case=\"capitalize-first\"/>\n                <text variable=\"container-title\" form=\"short\" text-case=\"title\" strip-periods=\"true\"/>\n              </group>\n              <group delimiter=\";\">\n                <text macro=\"year-date\"/>\n                <text variable=\"volume\"/>\n              </group>\n            </group>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </else>\n      </choose>\n      <choose>\n        <if type=\"paper-conference webpage\" match=\"any\">\n          <text macro=\"access\"/>\n        </if>\n        <else>\n          <text macro=\"access\" prefix=\". \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"