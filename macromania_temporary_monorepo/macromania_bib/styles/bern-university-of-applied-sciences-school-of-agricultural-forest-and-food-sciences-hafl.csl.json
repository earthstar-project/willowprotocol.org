"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Bern University of Applied Sciences - School of Agricultural, Forest and Food Sciences HAFL (author-date)</title>\n    <title-short>BFH-HAFL</title-short>\n    <id>http://www.zotero.org/styles/bern-university-of-applied-sciences-school-of-agricultural-forest-and-food-sciences-hafl</id>\n    <link href=\"http://www.zotero.org/styles/bern-university-of-applied-sciences-school-of-agricultural-forest-and-food-sciences-hafl\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/council-of-science-editors-author-date\" rel=\"template\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <updated>2024-02-15T10:55:41+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"in\">In</term>\n      <term name=\"from\">vom</term>\n      <term name=\"accessed\">abgerufen am</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"in\">dans </term>\n      <term name=\"accessed\">page consultée le</term>\n      <term name=\"no date\">sans date d’édition</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editortranslator\" form=\"long\">\n        <single>editor and translator</single>\n        <multiple>editors and translators</multiple>\n      </term>\n      <term name=\"collection-editor\" form=\"long\">\n        <single>editor</single>\n        <multiple>editors</multiple>\n      </term>\n      <term name=\"accessed\">accessed on</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"es\">\n    <terms>\n      <term name=\"in\">En</term>\n      <term name=\"accessed\">consultado el</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\" delimiter=\"; \" suffix=\".\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\"; \">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\", \" initialize-with=\".\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"review\">\n    <group delimiter=\". \">\n      <text variable=\"reviewed-title\"/>\n      <text variable=\"container-title\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <text term=\"accessed\" text-case=\"capitalize-first\"/>\n        <date variable=\"accessed\">\n          <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n          <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n      <choose>\n        <if match=\"any\" variable=\"DOI\">\n          <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n        </if>\n        <else>\n          <text variable=\"URL\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <group>\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"thesis\" match=\"any\">\n          <text variable=\"genre\" prefix=\". \"/>\n        </if>\n        <else-if type=\"book\" match=\"any\">\n          <text macro=\"edition\" prefix=\" (\" suffix=\")\"/>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n      <choose>\n        <if type=\"book thesis\" match=\"any\">\n          <group delimiter=\" \">\n            <text variable=\"number-of-pages\"/>\n            <label plural=\"never\" variable=\"number-of-pages\" form=\"short\"/>\n          </group>\n        </if>\n      </choose>\n      <text macro=\"pages\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \" plural=\"never\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if match=\"none\" variable=\"issued\">\n        <text term=\"no date\"/>\n      </if>\n      <else>\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\". \">\n          <number variable=\"edition\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <choose>\n      <if type=\"report\">\n        <group prefix=\" \" suffix=\".\" delimiter=\" \">\n          <text variable=\"collection-title\"/>\n          <text variable=\"number\" prefix=\" Report No.: \"/>\n        </group>\n      </if>\n      <else>\n        <group prefix=\" (\" suffix=\").\" delimiter=\" \">\n          <names variable=\"collection-editor\" suffix=\". \">\n            <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n            <label prefix=\", \"/>\n          </names>\n          <text variable=\"collection-title\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <choose>\n          <if type=\"interview\" match=\"any\">\n            <text term=\"interview\"/>\n          </if>\n        </choose>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <group delimiter=\", \">\n          <text macro=\"author\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text macro=\"title\"/>\n        <group suffix=\".\">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation report song thesis\" match=\"any\">\n              <group delimiter=\". \" prefix=\" \" suffix=\".\">\n                <text macro=\"editor\"/>\n                <text macro=\"publisher\"/>\n                <text prefix=\" \" macro=\"collection\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n              <group prefix=\" \" delimiter=\" \">\n                <group delimiter=\" \">\n                  <text term=\"in\" suffix=\":\"/>\n                  <text macro=\"editor\"/>\n                  <text variable=\"container-title\" suffix=\".\"/>\n                </group>\n                <text variable=\"volume\" prefix=\"Vol. \" suffix=\".\"/>\n                <text macro=\"edition\"/>\n                <group suffix=\".\" delimiter=\". \">\n                  <text prefix=\" \" macro=\"collection\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"review review-book\" match=\"any\">\n              <text macro=\"editor\" prefix=\" \" suffix=\".\"/>\n              <group prefix=\" \">\n                <text macro=\"review\" suffix=\".\"/>\n                <group prefix=\" \">\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\" prefix=\":\"/>\n              </group>\n            </else-if>\n            <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n              <group delimiter=\", \">\n                <text variable=\"container-title\"/>\n                <date variable=\"issued\">\n                  <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                  <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                  <date-part name=\"year\"/>\n                </date>\n                <group delimiter=\" \">\n                  <label variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"motion_picture\" match=\"any\">\n              <group delimiter=\". \">\n                <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n                  <text variable=\"medium\"/>\n                  <text variable=\"publisher\"/>\n                </group>\n                <group delimiter=\", \">\n                  <text variable=\"dimensions\"/>\n                  <date variable=\"issued\">\n                    <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                    <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"webpage post post-weblog\" match=\"any\">\n              <date variable=\"issued\">\n                <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                <date-part name=\"year\"/>\n              </date>\n            </else-if>\n            <else-if type=\"interview\" match=\"any\">\n              <group delimiter=\" \">\n                <text term=\"interview\" text-case=\"capitalize-first\"/>\n                <text term=\"from\"/>\n                <date variable=\"issued\">\n                  <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                  <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </else-if>\n            <else>\n              <text macro=\"editor\" suffix=\".\" prefix=\" \"/>\n              <group delimiter=\", \" prefix=\" \">\n                <text variable=\"container-title\" strip-periods=\"true\"/>\n                <group delimiter=\" \">\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\" (\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </group>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"