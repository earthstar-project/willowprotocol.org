"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <!-- Polyglot; journal publishes in English and French -->\n  <info>\n    <title>Social Anthropology/Anthropologie Sociale</title>\n    <id>http://www.zotero.org/styles/social-anthropology</id>\n    <link href=\"http://www.zotero.org/styles/social-anthropology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/ecological-restoration\" rel=\"template\"/>\n    <link href=\"https://onlinelibrary.wiley.com/page/journal/14698676/homepage/forauthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"anthropology\"/>\n    <issn>0964-0282</issn>\n    <eissn>1469-8676</eissn>\n    <updated>2021-06-17T08:37:04+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <style-options punctuation-in-quote=\"false\"/>\n    <terms>\n      <term name=\"open-quote\">‘</term>\n      <term name=\"close-quote\">’</term>\n      <term name=\"open-inner-quote\">“</term>\n      <term name=\"close-inner-quote\">”</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"in\"/>\n          <names variable=\"editor translator\" delimiter=\", \">\n            <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n            <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n          <label prefix=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \"/>\n      <label prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book report\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\"/>\n            <date form=\"numeric\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"none\">\n        <choose>\n          <if type=\"thesis\" match=\"any\">\n            <text variable=\"genre\" suffix=\", \"/>\n            <text variable=\"publisher\" prefix=\": \"/>\n          </if>\n          <else>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\" prefix=\": \"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <group prefix=\" \" suffix=\".\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <choose>\n            <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part prefix=\", \" name=\"month\"/>\n                <date-part prefix=\" \" name=\"day\"/>\n              </date>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text prefix=\" (\" term=\"no date\" suffix=\").\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <number variable=\"edition\" form=\"long-ordinal\" text-case=\"capitalize-first\"/>\n            <text term=\"edition\" form=\"long\" prefix=\" \" suffix=\".\"/>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" prefix=\" \" font-style=\"italic\"/>\n        <choose>\n          <if variable=\"page volume\" match=\"all\">\n            <text variable=\"volume\" prefix=\" \"/>\n            <text variable=\"page\" prefix=\":\"/>\n          </if>\n          <else>\n            <text variable=\"DOI\" prefix=\" DOI \"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"container-contributors\"/>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <text macro=\"secondary-contributors\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <choose>\n        <if match=\"none\" locator=\"page\">\n          <label variable=\"locator\" form=\"short\"/>\n        </if>\n      </choose>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n        </group>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\".\"/>\n      <text macro=\"issued\" suffix=\" \"/>\n      <group delimiter=\", \">\n        <text macro=\"title\"/>\n        <text macro=\"locators\"/>\n      </group>\n      <group delimiter=\". \" prefix=\". \">\n        <text macro=\"edition\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"