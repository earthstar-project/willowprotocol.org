"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Silva Fennica</title>\n    <id>http://www.zotero.org/styles/silva-fennica</id>\n    <link href=\"http://www.zotero.org/styles/silva-fennica\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-soil-science-and-plant-nutrition\" rel=\"template\"/>\n    <link href=\"https://silvafennica.fi/page/authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>2242-4075</issn>\n    <updated>2023-06-07T14:07:04+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"edition\" form=\"short\">edn.</term>\n    </terms>\n  </locale>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text macro=\"edition\"/>\n          </group>\n          <choose>\n            <if type=\"book\" match=\"any\" variable=\"version\">\n              <text variable=\"version\" prefix=\"Version \"/>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n    <text variable=\"original-title\" prefix=\" [\" suffix=\"]\"/>\n  </macro>\n  <macro name=\"year-parenth\">\n    <date prefix=\"(\" suffix=\")\" variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\" cite-group-delimiter=\", \">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"author\" sort=\"ascending\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author\" names-min=\"3\" names-use-first=\"1\"/>\n      <key macro=\"year\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"year-parenth\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \" prefix=\". \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <names variable=\"editor\">\n              <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n              <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n            </names>\n            <group delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text macro=\"edition\"/>\n            </group>\n          </group>\n          <group prefix=\". \" delimiter=\", \">\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"article-journal\">\n          <choose>\n            <if variable=\"page volume\" match=\"any\">\n              <group prefix=\". \" delimiter=\". \">\n                <group delimiter=\" \">\n                  <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n                  <group delimiter=\": \">\n                    <text variable=\"volume\"/>\n                    <text variable=\"page\"/>\n                  </group>\n                </group>\n                <text prefix=\"https://doi.org/\" variable=\"DOI\"/>\n              </group>\n            </if>\n            <else>\n              <group prefix=\". \" delimiter=\". \">\n                <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n                <text prefix=\"https://doi.org/\" variable=\"DOI\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \">\n            <group prefix=\". \" delimiter=\", \">\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n            <group>\n              <choose>\n                <if match=\"any\" variable=\"version\">\n                  <text variable=\"URL\" prefix=\"URL \"/>\n                </if>\n              </choose>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog post\" match=\"any\">\n          <group prefix=\". \" delimiter=\". \">\n            <text variable=\"container-title\" form=\"short\"/>\n            <text variable=\"URL\"/>\n            <date variable=\"accessed\">\n              <date-part prefix=\"Accessed \" name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" strip-periods=\"false\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\". \" delimiter=\", \">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\". \" delimiter=\" \">\n            <text variable=\"container-title\" form=\"short\"/>\n            <group delimiter=\":\">\n              <text variable=\"volume\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"