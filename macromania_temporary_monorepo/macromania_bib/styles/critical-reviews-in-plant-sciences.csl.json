"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>Critical Reviews in Plant Sciences</title>\n    <id>http://www.zotero.org/styles/critical-reviews-in-plant-sciences</id>\n    <link href=\"http://www.zotero.org/styles/critical-reviews-in-plant-sciences\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/chemical-senses\" rel=\"template\"/>\n    <link href=\"http://www.tandfonline.com/action/authorSubmission?show=instructions&amp;journalCode=bpts20\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"botany\"/>\n    <issn>0735-2689</issn>\n    <eissn>1549-7836</eissn>\n    <updated>2017-03-29T14:51:36+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <et-al font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" initialize-with=\".\" and=\"text\" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text variable=\"title\" suffix=\".\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"first\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \" suffix=\",\"/>\n    </names>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"issued\" suffix=\".\"/>\n        <choose>\n          <if type=\"article article-magazine article-newspaper article-journal review\" match=\"any\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n            <group delimiter=\": \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"page\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text variable=\"title\"/>\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <group delimiter=\" \">\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\" text-case=\"title\" suffix=\".\"/>\n              <text macro=\"publisher\" suffix=\",\"/>\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"genre\" suffix=\".\"/>\n            <text variable=\"publisher\"/>\n          </else-if>\n          <else-if type=\"webpage post-weblog\" match=\"any\">\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"URL\"/>\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date form=\"text\" variable=\"accessed\"/>\n            </group>\n          </else-if>\n          <else>\n            <text variable=\"title\"/>\n            <text macro=\"publisher\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"