"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" default-locale=\"en-US\" demote-non-dropping-particle=\"never\">\n  <info>\n    <title>Sociedade Brasileira de Computação</title>\n    <id>http://www.zotero.org/styles/sociedade-brasileira-de-computacao</id>\n    <link href=\"http://www.zotero.org/styles/sociedade-brasileira-de-computacao\" rel=\"self\"/>\n    <link href=\"https://forums.zotero.org/discussion/19203/preliminar-sociedade-brasileira-de-computacao-style/\" rel=\"documentation\"/>\n    <author>\n      <name>Maurício de Paulo</name>\n      <email>mauricio dot dev at gmail dot com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <summary>The Brazilian standard style</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-US\">\n    <terms>\n      <term name=\"month-01\" form=\"short\">jan</term>\n      <term name=\"month-02\" form=\"short\">feb</term>\n      <term name=\"month-03\" form=\"short\">mar</term>\n      <term name=\"month-04\" form=\"short\">apr</term>\n      <term name=\"month-05\" form=\"short\">may</term>\n      <term name=\"month-06\" form=\"short\">jun</term>\n      <term name=\"month-07\" form=\"short\">jul</term>\n      <term name=\"month-08\" form=\"short\">aug</term>\n      <term name=\"month-09\" form=\"short\">sep</term>\n      <term name=\"month-10\" form=\"short\">oct</term>\n      <term name=\"month-11\" form=\"short\">nov</term>\n      <term name=\"month-12\" form=\"short\">dec</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"container-author\" form=\"short\">\n        <single>ed</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"collection-editor\" form=\"short\">\n        <single>ed</single>\n        <multiple>eds</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter\">\n        <text value=\"In: \"/>\n        <names variable=\"container-author\" delimiter=\", \">\n          <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"always\">\n            <name-part name=\"family\" text-case=\"capitalize-first\"/>\n            <name-part name=\"given\" text-case=\"capitalize-first\"/>\n          </name>\n          <et-al/>\n          <label form=\"short\" prefix=\"[\" suffix=\".]. \" text-case=\"capitalize-first\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"collection-editor\"/>\n          </substitute>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter\" match=\"none\">\n        <names variable=\"editor\" delimiter=\", \" prefix=\" [\" suffix=\"]\">\n          <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\" suffix=\".\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"translator\">\n    <text value=\"Tradução \"/>\n    <names variable=\"translator\" delimiter=\", \">\n      <name delimiter=\"; \" sort-separator=\" \" delimiter-precedes-last=\"always\">\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n      </name>\n      <et-al/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\" and=\"text\">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n      </name>\n      <et-al/>\n      <label form=\"short\" prefix=\" [\" suffix=\".]\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\" and \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n      </name>\n      <et-al/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"book\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <text variable=\"URL\"/>\n    <date variable=\"accessed\" prefix=\", [accessed on \" suffix=\"]\">\n      <date-part name=\"month\" form=\"short\" suffix=\" \" text-case=\"capitalize-first\"/>\n      <date-part name=\"day\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <text variable=\"container-title\" suffix=\". \" prefix=\"In \" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <text variable=\"publisher-place\" suffix=\": \"/>\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <text variable=\"publisher\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\" match=\"any\">\n        <group>\n          <choose>\n            <if type=\"book chapter\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part name=\"day\" suffix=\" \"/>\n                <date-part name=\"month\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n              </date>\n            </if>\n          </choose>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <text value=\"[S.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text value=\"[S.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"numeric\" suffix=\".\"/>\n              <text term=\"edition\" form=\"short\" suffix=\".\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\" ed.\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"bill legislation\" match=\"any\">\n        <group prefix=\". \" delimiter=\", \">\n          <date variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part prefix=\" \" name=\"month\" form=\"short\"/>\n            <date-part prefix=\" \" name=\"year\"/>\n          </date>\n          <text variable=\"section\" prefix=\"Sec. \"/>\n          <text variable=\"page\" prefix=\"p. \" suffix=\".\"/>\n        </group>\n      </if>\n      <else-if match=\"any\" type=\"article-journal article-magazine article-newspaper\">\n        <group delimiter=\", \">\n          <group delimiter=\", \">\n            <text variable=\"volume\" prefix=\"v. \"/>\n            <text variable=\"issue\" prefix=\"n. \"/>\n          </group>\n          <text variable=\"page\" prefix=\"p. \"/>\n        </group>\n      </else-if>\n      <else-if match=\"any\" type=\"book chapter\">\n        <group delimiter=\", \">\n          <group>\n            <text variable=\"volume\" prefix=\"v. \"/>\n            <text variable=\"page\" prefix=\"p. \"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"genre\">\n    <text variable=\"genre\"/>\n  </macro>\n  <macro name=\"abstract\">\n    <text variable=\"abstract\"/>\n  </macro>\n  <macro name=\"place\">\n    <choose>\n      <if match=\"any\" variable=\"publisher-place\">\n        <text variable=\"publisher-place\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group>\n      <text variable=\"archive\" prefix=\" \"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\"; \">\n      <group>\n        <text suffix=\" \" macro=\"author-short\"/>\n        <text macro=\"issued-year\"/>\n        <text prefix=\" \" macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\" et-al-min=\"6\" et-al-use-first=\"3\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <choose>\n        <if type=\"bill legislation\" match=\"any\">\n          <group>\n            <text macro=\"author\"/>\n            <text macro=\"issued\" prefix=\" (\" suffix=\"). \"/>\n            <text variable=\"number\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <!-- use field 'history' to publisher -->\n            <text variable=\"references\" suffix=\", \" font-weight=\"bold\"/>\n            <!-- use field 'extra' place -->\n            <text variable=\"note\"/>\n            <text macro=\"locators\" suffix=\". \"/>\n          </group>\n        </if>\n        <else-if type=\"map\">\n          <group>\n            <text macro=\"author\"/>\n            <text macro=\"issued\" prefix=\" (\" suffix=\"). \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text variable=\"note\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"book\">\n          <group>\n            <text macro=\"author\"/>\n            <text macro=\"issued-year\" prefix=\" (\" suffix=\"). \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"translator\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher\" suffix=\". \"/>\n            <text macro=\"locators\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <!-- paper in conference too -->\n          <group>\n            <text macro=\"author\"/>\n            <text macro=\"issued\" prefix=\" (\" suffix=\"). \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"container-contributors\" suffix=\". \"/>\n            <text macro=\"secondary-contributors\" suffix=\". \"/>\n            <text macro=\"container-title\" suffix=\". \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"translator\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <group suffix=\". \">\n              <text macro=\"publisher\" suffix=\". \"/>\n              <text macro=\"locators\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine article-journal\" match=\"any\">\n          <group>\n            <text macro=\"author\"/>\n            <text macro=\"issued\" prefix=\" (\" suffix=\"). \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"container-title\" suffix=\", \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\", \"/>\n            <text macro=\"locators\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group>\n            <text macro=\"author\"/>\n            <text macro=\"issued\" prefix=\" (\" suffix=\"). \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"manuscript\">\n          <!-- There is no ABNT norm for manuscripts -->\n          <group>\n            <text macro=\"author\"/>\n            <text macro=\"issued\" prefix=\" (\" suffix=\"). \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"abstract\" suffix=\". \"/>\n            <text macro=\"place\" suffix=\", \"/>\n            <text macro=\"issued\" suffix=\". \"/>\n            <text macro=\"access\" suffix=\". \"/>\n            <text macro=\"archive\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group>\n            <text macro=\"author\"/>\n            <text macro=\"issued\" prefix=\" (\" suffix=\"). \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"access\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"author\"/>\n          <text macro=\"issued\" prefix=\" (\" suffix=\"). \"/>\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"container-contributors\"/>\n          <text macro=\"secondary-contributors\"/>\n          <text macro=\"container-title\"/>\n          <text variable=\"collection-title\" prefix=\", \" suffix=\".\"/>\n          <text macro=\"locators\"/>\n          <group delimiter=\". \" prefix=\". \" suffix=\". \">\n            <text macro=\"publisher\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"