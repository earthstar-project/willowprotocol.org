"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Anticancer Research</title>\n    <title-short>AR</title-short>\n    <id>http://www.zotero.org/styles/anticancer-research</id>\n    <link href=\"http://www.zotero.org/styles/anticancer-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/avian-diseases\" rel=\"template\"/>\n    <link href=\"http://ar.iiarjournals.org/site/misc/ifora.xhtml\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0250-7005</issn>\n    <eissn>1791-7530</eissn>\n    <updated>2020-05-28T08:23:14+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog post\" match=\"any\">\n        <text variable=\"URL\" prefix=\"Available from: \"/>\n        <date variable=\"accessed\" form=\"text\" prefix=\" [last accessed \" suffix=\"]\"/>\n      </if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"PMID\" prefix=\"PMID: \"/>\n          <text variable=\"DOI\" prefix=\"DOI: \"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" form=\"long\"/>\n      <text macro=\"author\" suffix=\": \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\"/>\n            <text macro=\"edition\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n            <text prefix=\" \" macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n              <group delimiter=\". \">\n                <text variable=\"container-title\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"editor\"/>\n              </group>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text variable=\"title\" suffix=\".\"/>\n          <group delimiter=\": \" prefix=\" \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n              <text variable=\"volume\" font-style=\"italic\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n      <date variable=\"issued\" prefix=\", \">\n        <date-part name=\"year\"/>\n      </date>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"