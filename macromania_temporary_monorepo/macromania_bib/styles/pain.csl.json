"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>PAIN</title>\n    <id>http://www.zotero.org/styles/pain</id>\n    <link href=\"http://www.zotero.org/styles/pain\" rel=\"self\"/>\n    <link href=\"https://edmgr.ovid.com/pain/accounts/Clinical%20Basic%20Science%20Research%20Reports_PAIN.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0304-3959</issn>\n    <eissn>1872-6623</eissn>\n    <summary>Style for the Elsevier Journal PAIN - modified, alphabetical Vancouver</summary>\n    <updated>2021-06-25T19:28:25+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <choose>\n        <if variable=\"URL\">\n          <choose>\n            <if variable=\"DOI\">\n              <text variable=\"DOI\" prefix=\" doi:\"/>\n            </if>\n            <else>\n              <text variable=\"URL\" prefix=\" Available: \" suffix=\".\"/>\n              <group prefix=\" \" suffix=\".\">\n                <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"day\" suffix=\" \"/>\n                  <date-part name=\"month\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </else>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\" sort=\"ascending\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"] \"/>\n      <text macro=\"author\" suffix=\".\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <group>\n              <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n              <text macro=\"editor\" prefix=\" \"/>\n            </group>\n            <text variable=\"container-title\"/>\n            <text variable=\"collection-title\"/>\n          </group>\n          <group suffix=\".\" delimiter=\", \">\n            <text macro=\"publisher\" prefix=\" \"/>\n            <text macro=\"year-date\"/>\n            <text variable=\"volume\" prefix=\"Vol. \"/>\n          </group>\n          <group prefix=\" \">\n            <label variable=\"page\" suffix=\" \" form=\"short\"/>\n            <text variable=\"page\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\" \" prefix=\" \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]. \"/>\n            <text macro=\"publisher\" suffix=\".\"/>\n            <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n            <text macro=\"year-date\" prefix=\" \"/>\n            <text variable=\"volume\" prefix=\";\"/>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"