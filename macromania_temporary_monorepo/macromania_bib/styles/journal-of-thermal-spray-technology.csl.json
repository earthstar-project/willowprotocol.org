"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Thermal Spray Technology</title>\n    <title-short>J Therm Spray Tech</title-short>\n    <id>http://www.zotero.org/styles/journal-of-thermal-spray-technology</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-thermal-spray-technology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/springer-physics-brackets\" rel=\"template\"/>\n    <link href=\"http://www.springer.com/cda/content/document/cda_downloaddocument/JTST+Author+Guidelines.pdf?SGWID=0-0-45-1510268-p173658292\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"physics\"/>\n    <issn>1059-9630</issn>\n    <eissn>1544-1016</eissn>\n    <updated>2014-05-15T12:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize-with=\".\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" initialize-with=\".\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"article-journal chapter\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"doi:\"/>\n          </if>\n          <else>\n            <group delimiter=\". \">\n              <text variable=\"URL\"/>\n              <group delimiter=\" \">\n                <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"day\" form=\"numeric\" suffix=\" \"/>\n                  <date-part name=\"month\" suffix=\" \"/>\n                  <date-part name=\"year\" form=\"long\"/>\n                </date>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\" prefix=\"(\" suffix=\")\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"journal-locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <group>\n          <text variable=\"volume\" font-weight=\"bold\"/>\n          <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <text term=\"page\" form=\"short\" strip-periods=\"true\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" prefix=\"(Ref \" suffix=\")\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"31\" et-al-use-first=\"30\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\", \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <text macro=\"container-title\"/>\n        <text macro=\"editor\"/>\n        <text macro=\"edition\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"year-date\"/>\n        <text macro=\"journal-locators\"/>\n        <text macro=\"pages\"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"