"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"fr-FR\">\n  <info>\n    <title>France (auteurs et al., auteur-date, Français)</title>\n    <id>http://www.zotero.org/styles/french4</id>\n    <link href=\"http://www.zotero.org/styles/french4\" rel=\"self\"/>\n    <link href=\"http://zotero.hypotheses.org/77\" rel=\"documentation\"/>\n    <author>\n      <name>Stéphanie Bouvier</name>\n      <email>stb.bouvier@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Julien Sicot</name>\n      <email>julien.sicot@gmail.com</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <summary>Style Zotero respectant les normes bibliographiques françaises : NF Z44-005  et NF ISO 690-2</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"lowercase\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"lowercase\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if variable=\"publisher-place\">\n        <text variable=\"publisher-place\" suffix=\" : \" prefix=\" \"/>\n      </if>\n      <else>\n        <text value=\"[s.l.]&#160;: \"/>\n      </else>\n    </choose>\n    <choose>\n      <if variable=\"publisher\">\n        <text variable=\"publisher\" suffix=\", \"/>\n      </if>\n      <else>\n        <text value=\"[s.n.], \"/>\n      </else>\n    </choose>\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\" suffix=\". \">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text value=\"[s.d.].\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text value=\"Disponible sur : \" prefix=\" \"/>\n        <group prefix=\"&lt; \" suffix=\" &gt;\">\n          <text value=\"https://doi.org/\"/>\n          <text variable=\"DOI\"/>\n        </group>\n      </if>\n      <else-if variable=\"URL\">\n        <text value=\"Disponible sur : \" prefix=\" \"/>\n        <group prefix=\"&lt; \" suffix=\" &gt;\">\n          <text variable=\"URL\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"(\" suffix=\")\" delimiter=\" \">\n          <text value=\"consulté le\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else-if variable=\"DOI\">\n        <group prefix=\"(\" suffix=\")\" delimiter=\" \">\n          <text value=\"consulté le\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text value=\"[s.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal article-magazine article-newspaper webpage\" match=\"any\">\n        <group suffix=\". \">\n          <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n          <choose>\n            <if variable=\"URL DOI\" match=\"any\">\n              <text value=\"en ligne\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"chapter paper-conference article-journal article-magazine article-newspaper webpage\" match=\"none\">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <choose>\n            <if variable=\"URL DOI\" match=\"any\">\n              <text value=\"en ligne\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </if>\n        <else>\n          <text variable=\"title\" quotes=\"true\" suffix=\".\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <choose>\n      <if variable=\"collection-number\">\n        <text variable=\"collection-title\" prefix=\"(\"/>\n        <text variable=\"collection-number\" prefix=\", \" suffix=\")\"/>\n      </if>\n      <else>\n        <text variable=\"collection-title\" prefix=\"(\" suffix=\"). \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <text variable=\"volume\" prefix=\" Vol. \" suffix=\",\"/>\n    <text variable=\"issue\" prefix=\" n°\" suffix=\",\"/>\n    <text variable=\"page\" prefix=\" p.&#160;\" suffix=\".\"/>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\" (\" suffix=\")\" delimiter=\"&#160;; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"1\" line-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"thesis\">\n          <group delimiter=\" \">\n            <text variable=\"genre\" suffix=\".\"/>\n            <text macro=\"publisher\"/>\n            <text variable=\"number-of-pages\" suffix=\" p.\"/>\n          </group>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"edition\"/>\n          <text macro=\"publisher\"/>\n          <text variable=\"number-of-pages\" suffix=\" p.\"/>\n          <text macro=\"collection\"/>\n        </else-if>\n        <else-if type=\"patent\">\n          <text variable=\"number\" suffix=\". \"/>\n          <date variable=\"issued\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\" suffix=\".\"/>\n          </date>\n        </else-if>\n        <else-if type=\"chapter paper-conference webpage\" match=\"any\">\n          <group prefix=\" \">\n            <text value=\"In\" suffix=\" : \" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"journal-title\"/>\n            <text variable=\"event\" suffix=\". \" font-style=\"italic\"/>\n          </group>\n          <text macro=\"publisher\" prefix=\" \"/>\n          <text variable=\"page\" prefix=\" p. \" suffix=\".\"/>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group suffix=\".\">\n            <date variable=\"issued\" suffix=\".\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n          <group suffix=\". \">\n            <text macro=\"locators\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n      <text macro=\"accessed-date\" prefix=\" \"/>\n      <choose>\n        <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n          <text variable=\"ISBN\" prefix=\"ISBN&#160;: \" suffix=\".\"/>\n        </if>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"