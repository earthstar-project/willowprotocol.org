"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" default-locale=\"en-US\" version=\"1.0\" demote-non-dropping-particle=\"never\">\n  <info>\n    <title>Acta Ichthyologica et Piscatoria</title>\n    <id>http://www.zotero.org/styles/acta-ichthyologica-et-piscatoria</id>\n    <link href=\"http://www.zotero.org/styles/acta-ichthyologica-et-piscatoria\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/wetlands\" rel=\"template\"/>\n    <link href=\"https://aiep.pl/main/instr.php\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <issn>0137-1592</issn>\n    <eissn>1734-1515</eissn>\n    <updated>2019-06-21T11:50:31+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\": \"/>\n    <names variable=\"editor\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\", \">\n          <text variable=\"title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"DOI: \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"5\" et-al-use-first=\"3\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"author\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \">\n        <group delimiter=\". \" suffix=\".\">\n          <text macro=\"author\" font-weight=\"bold\"/>\n          <text macro=\"year\"/>\n          <text macro=\"title\"/>\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\" \">\n                <label text-case=\"capitalize-first\" strip-periods=\"false\" variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n              <text macro=\"editor\"/>\n              <group delimiter=\", \">\n                <text variable=\"container-title\"/>\n                <text macro=\"edition\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </if>\n            <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\", \">\n                <text variable=\"publisher\"/>\n                <text variable=\"publisher-place\"/>\n              </group>\n            </else-if>\n            <else-if type=\"webpage post-weblog post\" match=\"any\">\n              <group delimiter=\". \">\n                <text prefix=\"In: \" variable=\"container-title\" suffix=\". \"/>\n                <text variable=\"URL\"/>\n                <date variable=\"accessed\">\n                  <date-part prefix=\"Accessed \" name=\"day\" suffix=\" \"/>\n                  <date-part name=\"month\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </else-if>\n            <else-if type=\"thesis\">\n              <group delimiter=\", \">\n                <text variable=\"genre\" text-case=\"capitalize-first\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\": \">\n                <group delimiter=\" \">\n                  <text variable=\"container-title\"/>\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"