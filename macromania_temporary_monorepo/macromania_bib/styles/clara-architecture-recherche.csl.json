"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"fr-FR\">\n  <info>\n    <title>CLARA Architecture/Recherche (Français)</title>\n    <id>http://www.zotero.org/styles/clara-architecture-recherche</id>\n    <link href=\"http://www.zotero.org/styles/clara-architecture-recherche\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/iso690-author-date-fr\" rel=\"template\"/>\n    <link href=\"http://clararevue.ulb.be/OJS/index.php/CLARA\" rel=\"documentation\"/>\n    <author>\n      <name>Florencia Fernandez C.</name>\n      <email>clara.archi@ulb.ac.be</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <issn>2259-3671</issn>\n    <summary>Style based on ISO 690:2010(F), V1.1</summary>\n    <updated>2016-03-01T08:16:15+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"no date\">[sans date]</term>\n      <term name=\"in\">dans</term>\n      <term name=\"online\">consultable&#160;en&#160;ligne</term>\n      <term name=\"accessed\">consulté&#160;le</term>\n      <term name=\"retrieved\">disponible</term>\n      <term name=\"from\">à l'adresse</term>\n      <term name=\"translator\" form=\"short\">trad.</term>\n      <term name=\"editor\" form=\"short\">(sous&#160;la&#160;dir.&#160;de)</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"responsability\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <text macro=\"year-date\" prefix=\", \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author-citation\">\n    <names variable=\"author\">\n      <name form=\"short\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"container-responsability\">\n    <choose>\n      <if variable=\"container-author\">\n        <text macro=\"container-author\"/>\n      </if>\n      <else-if variable=\"editor\">\n        <text macro=\"editor\"/>\n      </else-if>\n      <else>\n        <text macro=\"translator\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis map motion_picture song manuscript\" match=\"any\">\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"paper-conference speech chapter article-journal article-magazine article-newspaper entry entry-dictionary entry-encyclopedia post-weblog post webpage broadcast\" match=\"any\">\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <text variable=\"title\" suffix=\". \"/>\n          </if>\n          <else>\n            <text variable=\"title\" prefix=\"« \" suffix=\" », \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n          </else>\n        </choose>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\"&#160;: \"/>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"container-author\">\n            <text macro=\"container-responsability\"/>\n            <choose>\n              <if variable=\"container-title event\" match=\"any\">\n                <text value=\", \"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"event\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"report\">\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <text variable=\"number\" suffix=\": \"/>\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"number\" suffix=\": \"/>\n            <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"patent\">\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <text variable=\"title\"/>\n          </if>\n          <else>\n            <text variable=\"title\" prefix=\"« \" suffix=\" », \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n    <choose>\n      <if variable=\"URL\">\n        <text term=\"online\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <text variable=\"number\"/>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"map\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n          <else>\n            <text value=\"carte\" prefix=\"[\" suffix=\"]\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"genre\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <text variable=\"edition\" form=\"long\"/>\n  </macro>\n  <macro name=\"publisher-group\">\n    <group delimiter=\"&#160;: \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\", \">\n      <text variable=\"volume\" prefix=\"Vol.&#160;\"/>\n      <choose>\n        <if variable=\"volume\">\n          <text variable=\"issue\" prefix=\"n°&#160;\"/>\n          <text variable=\"page\" prefix=\"pp.&#160;\"/>\n        </if>\n        <else-if variable=\"issue\">\n          <text variable=\"issue\" prefix=\"N°&#160;\"/>\n          <text variable=\"page\" prefix=\"pp.&#160;\"/>\n        </else-if>\n        <else>\n          <text variable=\"page\" prefix=\"pp.&#160;\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" prefix=\"&#160;\"/>\n            <date-part name=\"month\" prefix=\"&#160;\"/>\n            <date-part name=\"year\" prefix=\"&#160;\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\", \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"book thesis manuscript\" match=\"any\">\n        <text variable=\"number-of-pages\" suffix=\"&#160;p\"/>\n      </if>\n      <else-if type=\"chapter paper-conference article-newspaper\" match=\"any\">\n        <text variable=\"page\" prefix=\"pp.&#160;\"/>\n      </else-if>\n      <else-if type=\"report patent\" match=\"any\">\n        <text variable=\"page\" suffix=\"&#160;p\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"isbn\">\n    <text variable=\"ISBN\" prefix=\"ISBN&#160;\"/>\n  </macro>\n  <macro name=\"doi\">\n    <text variable=\"DOI\" prefix=\"DOI&#160;\"/>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text term=\"retrieved\" suffix=\" \" text-case=\"capitalize-first\"/>\n          <text term=\"from\" suffix=\"&#160;: \"/>\n          <text variable=\"URL\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\":&#160;\">\n      <text variable=\"archive\"/>\n      <text macro=\"archive_location\"/>\n    </group>\n  </macro>\n  <macro name=\"archive_location\">\n    <choose>\n      <if variable=\"archive_location\">\n        <text variable=\"archive_location\"/>\n      </if>\n      <else>\n        <text variable=\"call-number\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"abstract\">\n    <text variable=\"abstract\"/>\n  </macro>\n  <macro name=\"note\">\n    <text variable=\"note\"/>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\" year-suffix-delimiter=\", \" after-collapse-delimiter=\"; \">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\", \">\n          <text macro=\"author-citation\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" suffix=\".&#160;\" form=\"short\" strip-periods=\"true\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"responsability\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout>\n      <choose>\n        <if type=\"book map\" match=\"any\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"isbn\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </if>\n        <else-if type=\"article-journal article-magazine\" match=\"any\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"issue\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"doi\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter entry entry-dictionary entry-encyclopedia\" match=\"any\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"isbn\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"isbn\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"post-weblog post webpage\" match=\"any\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"broadcast motion_picture song\" match=\"any\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"medium\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"isbn\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"manuscript\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"number\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else>\n          <group>\n            <text macro=\"responsability\" suffix=\". \"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\" suffix=\". \"/>\n              </if>\n            </choose>\n            <text macro=\"medium\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"number\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"isbn\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else>\n      </choose>\n      <group display=\"right-inline\">\n        <text macro=\"archive\"/>\n      </group>\n      <group display=\"right-inline\">\n        <text macro=\"abstract\"/>\n      </group>\n      <group display=\"right-inline\">\n        <text macro=\"note\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"