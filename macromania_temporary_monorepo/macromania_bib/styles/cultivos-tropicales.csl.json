"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"es-ES\">\n  <info>\n    <title>Cultivos Tropicales (Español)</title>\n    <title-short>CulTrop</title-short>\n    <id>http://www.zotero.org/styles/cultivos-tropicales</id>\n    <link href=\"http://www.zotero.org/styles/cultivos-tropicales\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/iso690-author-date-es\" rel=\"template\"/>\n    <link href=\"http://ediciones.inca.edu.cu/index.php/ediciones/pages/view/autores\" rel=\"documentation\"/>\n    <author>\n      <name>Rafael Cervantes Beyra</name>\n      <email>cervantes@unah.edu.cu</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <issn>0258-5936</issn>\n    <eissn>1819-4087</eissn>\n    <summary>Style based on ISO 690:2010(F), V1</summary>\n    <updated>2014-05-30T00:36:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"anonymous\">Anon.</term>\n      <term name=\"no date\">[s.f]</term>\n      <term name=\"in\">en</term>\n      <term name=\"online\">en&#160;linea</term>\n      <term name=\"retrieved\">disponible&#160;</term>\n      <term name=\"from\">en</term>\n      <term name=\"accessed\">consultado:&#160;</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter=\"; \" delimiter-precedes-last=\"never\" suffix=\". \">\n        <name-part name=\"family\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" suffix=\" \"/>\n      <name and=\"text\" name-as-sort-order=\"all\" initialize-with=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <label form=\"short\" suffix=\" \"/>\n      <name and=\"text\" name-as-sort-order=\"all\" initialize-with=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\" prefix=\"entr. \">\n      <label/>\n      <name and=\"text\" name-as-sort-order=\"all\" initialize-with=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"responsability\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <choose>\n          <if variable=\"author\">\n            <text macro=\"author\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <label form=\"short\" suffix=\" \"/>\n      <name and=\"text\" name-as-sort-order=\"all\" initialize-with=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"collection-editor\">\n    <names variable=\"collection-editor\" prefix=\"ed. ser. \">\n      <label/>\n      <name and=\"text\" name-as-sort-order=\"all\" initialize-with=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <text variable=\"event\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book post webpage post-weblog interview report patent thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"article-journal article-magazine article-newspaper chapter entry-encyclopedia entry entry-dictionary paper-conference\" match=\"any\">\n        <text variable=\"title\" suffix=\"’’\" prefix=\"‘‘\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <text variable=\"container-title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n  </macro>\n  <macro name=\"genre\">\n    <text variable=\"genre\"/>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" de \"/>\n          <date-part name=\"month\" suffix=\" de \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group>\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" prefix=\" \"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-place\">\n    <text variable=\"publisher-place\"/>\n  </macro>\n  <macro name=\"issue\">\n    <text variable=\"issue\" prefix=\"no. \"/>\n  </macro>\n  <macro name=\"volume\">\n    <text term=\"volume\" form=\"short\" suffix=\" \"/>\n    <number variable=\"volume\" form=\"numeric\"/>\n  </macro>\n  <macro name=\"number-of-volumes\">\n    <text term=\"volume\" form=\"short\" plural=\"true\" suffix=\" \"/>\n    <text variable=\"number-of-volumes\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" prefix=\"edit. \"/>\n  </macro>\n  <macro name=\"archive\">\n    <text variable=\"archive\"/>\n  </macro>\n  <macro name=\"archive_location\">\n    <text variable=\"archive_location\"/>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text term=\"accessed\" text-case=\"capitalize-first\" prefix=\" [\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" de \"/>\n            <date-part name=\"month\" suffix=\" de \"/>\n            <date-part name=\"year\" suffix=\"]\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"collection-title\" prefix=\"ser. \"/>\n      <text variable=\"collection-number\" prefix=\"no. ser. \"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <text term=\"number-of-pages\" form=\"short\" suffix=\" \"/>\n    <text variable=\"number-of-pages\"/>\n  </macro>\n  <macro name=\"isbn\">\n    <text variable=\"ISBN\" prefix=\"ISBN \"/>\n  </macro>\n  <macro name=\"issn\">\n    <choose>\n      <if type=\"article-magazine\" match=\"any\">\n        <text variable=\"ISSN\" prefix=\"ISBN \"/>\n      </if>\n      <else-if type=\"article-journal article-newspaper\" match=\"any\">\n        <text variable=\"ISSN\" prefix=\"ISSN \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"doi\">\n    <text variable=\"DOI\" prefix=\"DOI \"/>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n          <text term=\"from\" suffix=\": \"/>\n          <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if variable=\"DOI URL accessed\" match=\"any\">\n        <text value=\" [en línea]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"note\">\n    <text variable=\"note\" prefix=\" [\" suffix=\"]\"/>\n  </macro>\n  <macro name=\"jurisdiction\">\n    <text variable=\"jurisdiction\"/>\n  </macro>\n  <macro name=\"original-publisher\">\n    <text variable=\"original-publisher\"/>\n  </macro>\n  <macro name=\"number\">\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <text variable=\"number\"/>\n      </if>\n      <else>\n        <text variable=\"number\" prefix=\"no. \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"call-number\">\n    <text variable=\"call-number\" prefix=\"no. solc. \"/>\n  </macro>\n  <macro name=\"version\">\n    <text variable=\"version\" prefix=\"versión \"/>\n  </macro>\n  <macro name=\"section\">\n    <text variable=\"section\"/>\n  </macro>\n  <macro name=\"scale\">\n    <choose>\n      <if variable=\"scale\">\n        <text variable=\"scale\" prefix=\"[\" suffix=\"]\"/>\n      </if>\n      <else>\n        <!-- sine nomine (s.n.)-->\n        <text value=\"Escala indeterminada\" prefix=\"[\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation>\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <choose>\n        <if type=\"book post webpage post-weblog interview chapter entry-encyclopedia entry entry-dictionary\" match=\"any\">\n          <group suffix=\", \">\n            <text macro=\"responsability\"/>\n            <text macro=\"title\"/>\n            <text macro=\"online\"/>\n          </group>\n          <choose>\n            <if type=\"book post webpage post-weblog interview\" match=\"any\">\n              <group prefix=\"(\" suffix=\"), \" delimiter=\"; \">\n                <text macro=\"translator\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"collection-editor\"/>\n                <text macro=\"interviewer\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter entry-encyclopedia entry entry-dictionary\" match=\"any\">\n              <group delimiter=\"; \" suffix=\", \">\n                <text macro=\"container-author\" prefix=\"en: \"/>\n                <text macro=\"translator\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"collection-editor\"/>\n              </group>\n            </else-if>\n          </choose>\n          <group delimiter=\", \">\n            <text macro=\"container-title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"version\"/>\n            <text macro=\"medium\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"number-of-volumes\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"date\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"number-of-pages\"/>\n            <text macro=\"page\"/>\n            <text macro=\"isbn\"/>\n          </group>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n          <group suffix=\", \">\n            <text macro=\"responsability\"/>\n            <text macro=\"title\"/>\n          </group>\n          <group prefix=\"(\" suffix=\"), \" delimiter=\"; \">\n            <text macro=\"translator\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"container-title\"/>\n            <text macro=\"section\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"issue\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"date\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"page\"/>\n            <text macro=\"issn\"/>\n            <text macro=\"doi\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <group suffix=\", \">\n            <text macro=\"responsability\"/>\n            <text macro=\"title\"/>\n            <text macro=\"online\"/>\n          </group>\n          <group prefix=\"(\" suffix=\"), \" delimiter=\"; \">\n            <text macro=\"translator\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"collection-editor\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"container-title\" prefix=\"En: \"/>\n            <text macro=\"event\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"date\"/>\n            <text macro=\"page\"/>\n            <text macro=\"isbn\"/>\n            <text macro=\"doi\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group suffix=\", \">\n            <text macro=\"responsability\"/>\n            <text macro=\"title\"/>\n            <text macro=\"online\"/>\n            <text variable=\"genre\" prefix=\" [\" suffix=\"]\"/>\n          </group>\n          <group delimiter=\", \">\n            <text variable=\"publisher\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"number-of-pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group suffix=\", \">\n            <text macro=\"responsability\"/>\n            <text macro=\"title\"/>\n            <text macro=\"online\"/>\n          </group>\n          <group prefix=\"(\" suffix=\"), \" delimiter=\"; \">\n            <text macro=\"translator\"/>\n            <text macro=\"collection-editor\"/>\n          </group>\n          <group prefix=\"[\" suffix=\"], \">\n            <text variable=\"collection-title\" suffix=\", \"/>\n            <text variable=\"number\"/>\n          </group>\n          <group delimiter=\", \">\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"legislation legal_case\" match=\"any\">\n          <group suffix=\", \">\n            <text macro=\"responsability\"/>\n            <text macro=\"title\"/>\n            <text macro=\"online\"/>\n          </group>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" prefix=\"cod. \"/>\n            <text variable=\"section\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"date\"/>\n            <text macro=\"number\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group suffix=\", \">\n            <text macro=\"responsability\"/>\n            <text macro=\"title\"/>\n            <text macro=\"online\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"publisher-place\"/>\n            <text macro=\"number\"/>\n            <text macro=\"call-number\"/>\n            <text macro=\"page\"/>\n            <text macro=\"date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"map\">\n          <group suffix=\", \">\n            <text macro=\"responsability\" suffix=\" \"/>\n            <text variable=\"title\" font-style=\"italic\" suffix=\". \"/>\n            <text macro=\"online\"/>\n          </group>\n          <group prefix=\"(\" suffix=\"), \" delimiter=\"; \">\n            <text macro=\"collection-editor\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"collection\"/>\n            <text macro=\"scale\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"date\"/>\n            <text macro=\"isbn\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\", \">\n            <text macro=\"responsability\"/>\n            <text macro=\"title\"/>\n            <text macro=\"online\"/>\n            <text macro=\"medium\"/>\n          </group>\n          <group delimiter=\"; \" prefix=\"(\" suffix=\"), \">\n            <text macro=\"container-author\" prefix=\"en: \"/>\n            <text macro=\"translator\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"collection-editor\"/>\n            <text macro=\"interviewer\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"collection\"/>\n            <text macro=\"container-title\"/>\n            <text macro=\"event\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"number-of-volumes\"/>\n            <text macro=\"issue\"/>\n            <text macro=\"version\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"jurisdiction\"/>\n            <text macro=\"original-publisher\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"date\"/>\n            <text macro=\"number\"/>\n            <text macro=\"call-number\"/>\n            <text macro=\"number-of-pages\"/>\n            <text macro=\"page\"/>\n            <text macro=\"issn\"/>\n            <text macro=\"isbn\"/>\n            <text macro=\"doi\"/>\n          </group>\n        </else>\n      </choose>\n      <group>\n        <group display=\"block\" delimiter=\", \" prefix=\", \">\n          <text macro=\"archive\"/>\n          <text macro=\"archive_location\"/>\n          <text macro=\"accessed\"/>\n          <text macro=\"url\"/>\n          <text macro=\"note\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"