"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Pakistan Journal of Agricultural Sciences</title>\n    <title-short>PakJAS</title-short>\n    <id>http://www.zotero.org/styles/pakistan-journal-of-agricultural-sciences</id>\n    <link href=\"http://www.zotero.org/styles/pakistan-journal-of-agricultural-sciences\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/the-american-midland-naturalist\" rel=\"template\"/>\n    <link href=\"https://www.pakjas.com.pk/InstructionsToAuthors.php\" rel=\"documentation\"/>\n    <link href=\"https://www.pakjas.com.pk/Author%20instructions.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <issn>0552-9034</issn>\n    <eissn>2076-0906</eissn>\n    <updated>2021-03-23T21:11:01+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\".\"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" font-variant=\"normal\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"first\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\" \">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n          </group>\n          <text macro=\"editor-translator\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"year-suffix\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"capitalize-first\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n          <group delimiter=\":\">\n            <text variable=\"volume\" font-weight=\"normal\"/>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </group>\n        <choose>\n          <if variable=\"page volume\" match=\"none\">\n            <text variable=\"DOI\" prefix=\", doi: \"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\" font-style=\"normal\" suffix=\":\"/>\n              <text macro=\"editor-translator\"/>\n            </group>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </group>\n          <text macro=\"publisher\"/>\n          <group>\n            <label plural=\"always\" variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"webpage post-weblog post\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"container-title\"/>\n          <group delimiter=\" \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"book report\" match=\"any\">\n        <text macro=\"publisher\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\" strip-periods=\"false\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute-rule=\"partial-each\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n        <text macro=\"title\"/>\n        <text macro=\"locators\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"