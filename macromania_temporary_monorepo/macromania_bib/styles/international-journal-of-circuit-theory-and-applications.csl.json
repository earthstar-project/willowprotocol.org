"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>International Journal of Circuit Theory and Applications</title>\n    <id>http://www.zotero.org/styles/international-journal-of-circuit-theory-and-applications</id>\n    <link href=\"http://www.zotero.org/styles/international-journal-of-circuit-theory-and-applications\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/nature-publishing-group-vancouver\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1002/%28ISSN%291097-007X/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <issn>0098-9886</issn>\n    <eissn>1097-007X</eissn>\n    <updated>2017-06-22T11:10:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\" delimiter-precedes-et-al=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <et-al term=\"et-al\" font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\" delimiter-precedes-et-al=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <et-al term=\"et-al\" font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <group delimiter=\": \">\n        <text variable=\"publisher\"/>\n        <text variable=\"publisher-place\"/>\n      </group>\n      <group>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\" doi:\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <text variable=\"URL\"/>\n            <group prefix=\" (\" suffix=\").\" delimiter=\" \">\n              <text term=\"accessed\"/>\n              <date variable=\"accessed\">\n                <date-part name=\"day\" prefix=\" \"/>\n                <date-part name=\"month\" prefix=\" \" form=\"short\" strip-periods=\"true\"/>\n                <date-part name=\"year\" prefix=\" \"/>\n              </date>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" strip-periods=\"false\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"baseline\" delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\" \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\". \">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </group>\n          <group prefix=\" \" delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <text macro=\"journal-title\"/>\n          <group delimiter=\"; \" suffix=\".\">\n            <date variable=\"issued\" prefix=\" \">\n              <date-part name=\"year\"/>\n            </date>\n            <group delimiter=\":\">\n              <group>\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n          <text variable=\"DOI\" prefix=\" DOI: \"/>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group suffix=\".\">\n            <date variable=\"issued\" prefix=\" \">\n              <date-part name=\"year\"/>\n            </date>\n            <group prefix=\"; \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"page\" prefix=\": \"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"