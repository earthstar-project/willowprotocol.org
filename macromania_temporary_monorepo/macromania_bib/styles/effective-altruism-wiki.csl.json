"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"symbol\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" et-al-min=\"4\" et-al-use-first=\"1\" initialize=\"true\" page-range-format=\"expanded\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Effective Altruism Wiki</title>\n    <title-short>EA Wiki</title-short>\n    <id>http://www.zotero.org/styles/effective-altruism-wiki</id>\n    <link href=\"http://www.zotero.org/styles/effective-altruism-wiki\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/apa-6th-edition\" rel=\"template\"/>\n    <link href=\"https://forum.effectivealtruism.org/tag/style-guide\" rel=\"documentation\"/>\n    <author>\n      <name>Pablo Stafforini</name>\n      <email>pablo@stafforini.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <category field=\"science\"/>\n    <updated>2022-01-03T14:11:14+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editortranslator\" form=\"short\">\n        <single>ed. &amp; trans.</single>\n        <multiple>eds. &amp; trans.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"interviewer\" form=\"short\">interviewer</term>\n      <term name=\"circa\" form=\"short\">ca.</term>\n      <term name=\"collection-editor\" form=\"short\">series ed.</term>\n      <term name=\"et-al\"> *et al.*</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors-booklike\">\n    <choose>\n      <if variable=\"container-title\">\n        <names variable=\"editor translator\" delimiter=\", &amp; \">\n          <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n          <substitute>\n            <names variable=\"editorial-director\"/>\n            <names variable=\"collection-editor\"/>\n            <names variable=\"container-author\"/>\n          </substitute>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"book chapter entry entry-dictionary entry-encyclopedia graphic map personal_communication report speech paper-conference\" match=\"any\">\n        <text macro=\"container-contributors-booklike\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors-booklike\">\n    <group delimiter=\", \">\n      <choose>\n        <if variable=\"title\">\n          <names variable=\"interviewer\">\n            <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n            <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n          </names>\n        </if>\n      </choose>\n      <choose>\n        <if variable=\"container-title\" match=\"none\">\n          <group delimiter=\", \" suffix=\",\">\n            <names variable=\"container-author\">\n              <label form=\"verb\" text-case=\"title\" suffix=\" \"/>\n              <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n            </names>\n            <names variable=\"editor translator\" delimiter=\", \">\n              <label form=\"verb\" text-case=\"lowercase\" suffix=\" \"/>\n              <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n            </names>\n          </group>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"book chapter entry entry-dictionary entry-encyclopedia graphic map report paper-conference\" match=\"any\">\n        <text macro=\"secondary-contributors-booklike\"/>\n      </if>\n      <else-if type=\"personal_communication\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <choose>\n              <if variable=\"genre\" match=\"any\">\n                <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              </if>\n              <else>\n                <text term=\"letter\" text-case=\"capitalize-first\"/>\n              </else>\n            </choose>\n            <names variable=\"recipient\" delimiter=\", \">\n              <label form=\"verb\" suffix=\" \"/>\n              <name and=\"symbol\" delimiter=\", \"/>\n            </names>\n          </group>\n          <text variable=\"medium\" text-case=\"capitalize-first\"/>\n          <choose>\n            <if variable=\"container-title\" match=\"none\">\n              <names variable=\"editor translator\" delimiter=\", \">\n                <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n                <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n      <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"title\">\n              <names variable=\"interviewer\" delimiter=\", \">\n                <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n                <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n              </names>\n            </if>\n          </choose>\n          <names variable=\"translator\" delimiter=\", \">\n            <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n            <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n          </names>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"title\">\n              <names variable=\"interviewer\">\n                <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n                <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n              </names>\n            </if>\n          </choose>\n          <names variable=\"editor translator\" delimiter=\", \">\n            <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n            <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n          </names>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\", \">\n      <name and=\"symbol\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" et-al-min=\"4\" et-al-use-first=\"1\" initialize=\"true\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"illustrator\"/>\n        <names variable=\"composer\"/>\n        <names variable=\"director\">\n          <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n          <label form=\"long\" prefix=\" (\" suffix=\")\" text-case=\"title\"/>\n        </names>\n        <choose>\n          <if variable=\"container-title\">\n            <choose>\n              <if type=\"book entry entry-dictionary entry-encyclopedia\">\n                <text macro=\"title\"/>\n              </if>\n              <else>\n                <names variable=\"translator\"/>\n              </else>\n            </choose>\n            <names variable=\"translator\">\n              <name and=\"symbol\" delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n              <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"title\"/>\n            </names>\n          </if>\n        </choose>\n        <names variable=\"editor\" delimiter=\", \">\n          <name and=\"symbol\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" et-al-min=\"4\" et-al-use-first=\"1\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n          <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n        </names>\n        <names variable=\"editorial-director\">\n          <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"title\"/>\n        </names>\n        <names variable=\"collection-editor\">\n          <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"title\"/>\n        </names>\n        <choose>\n          <if type=\"report\">\n            <text variable=\"publisher\"/>\n          </if>\n        </choose>\n        <group delimiter=\" \">\n          <text macro=\"title\"/>\n          <text macro=\"description\"/>\n          <text macro=\"format\"/>\n        </group>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"personal_communication\">\n        <choose>\n          <if variable=\"archive DOI ISBN publisher URL\" match=\"none\">\n            <group delimiter=\", \">\n              <names variable=\"author\">\n                <name and=\"symbol\" delimiter=\", \" initialize-with=\". \"/>\n                <substitute>\n                  <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n                </substitute>\n              </names>\n              <text value=\"personal communication\"/>\n            </group>\n          </if>\n          <else>\n            <names variable=\"author\" delimiter=\", \">\n              <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\". \"/>\n              <substitute>\n                <names variable=\"editor\"/>\n                <names variable=\"translator\"/>\n                <choose>\n                  <if variable=\"container-title\">\n                    <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n                  </if>\n                  <else>\n                    <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n                  </else>\n                </choose>\n                <text macro=\"format-short\" prefix=\"[\" suffix=\"]\"/>\n              </substitute>\n            </names>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <names variable=\"author\" delimiter=\", \">\n          <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\". \"/>\n          <substitute>\n            <names variable=\"illustrator\"/>\n            <names variable=\"composer\"/>\n            <names variable=\"director\"/>\n            <choose>\n              <if variable=\"container-title\">\n                <choose>\n                  <if type=\"book entry entry-dictionary entry-encyclopedia\">\n                    <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n                  </if>\n                  <else>\n                    <names variable=\"translator\"/>\n                  </else>\n                </choose>\n              </if>\n            </choose>\n            <names variable=\"editor\"/>\n            <names variable=\"editorial-director\"/>\n            <names variable=\"translator\"/>\n            <choose>\n              <if type=\"report\" variable=\"publisher\" match=\"all\">\n                <text variable=\"publisher\"/>\n              </if>\n              <else-if type=\"legal_case\">\n                <text variable=\"title\" font-style=\"italic\"/>\n              </else-if>\n              <else-if type=\"bill legislation\" match=\"any\">\n                <text variable=\"title\" form=\"short\"/>\n              </else-if>\n              <else-if variable=\"reviewed-author\" type=\"review review-book\" match=\"any\">\n                <text macro=\"format-short\" prefix=\"[\" suffix=\"]\"/>\n              </else-if>\n              <else-if type=\"post post-weblog webpage broadcast\" variable=\"container-title\" match=\"any\">\n                <text variable=\"title\" form=\"short\" quotes=\"false\"/>\n              </else-if>\n              <else>\n                <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n              </else>\n            </choose>\n            <text macro=\"format-short\" prefix=\"[\" suffix=\"]\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"archive\">\n        <choose>\n          <if type=\"article article-journal article-magazine article-newspaper dataset paper-conference report speech thesis\" match=\"any\">\n            <choose>\n              <if variable=\"archive-place\" match=\"none\">\n                <group delimiter=\" \">\n                  <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n                  <text term=\"from\"/>\n                  <text variable=\"archive\" suffix=\".\"/>\n                  <text variable=\"archive_location\" prefix=\"(\" suffix=\")\"/>\n                </group>\n              </if>\n              <else>\n                <text macro=\"publisher\" suffix=\".\"/>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <text macro=\"publisher\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text macro=\"publisher\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\">\n        <choose>\n          <if variable=\"version\" type=\"book\" match=\"all\">\n            <text variable=\"title\"/>\n          </if>\n          <else-if variable=\"reviewed-author reviewed-title\" type=\"review review-book\" match=\"any\">\n            <choose>\n              <if variable=\"reviewed-title\">\n                <choose>\n                  <if type=\"post post-weblog webpage broadcast\" variable=\"container-title\" match=\"any\">\n                    <text variable=\"title\"/>\n                  </if>\n                  <else>\n                    <text variable=\"title\" font-style=\"italic\"/>\n                  </else>\n                </choose>\n              </if>\n            </choose>\n          </else-if>\n          <else-if type=\"manuscript chapter\" match=\"any\">\n            <choose>\n              <if variable=\"note\" match=\"any\">\n                <choose>\n                  <if match=\"any\" variable=\"DOI\">\n                    <text variable=\"title\" text-case=\"title\" font-style=\"normal\" prefix=\"*[\" suffix=\"]\"/>\n                    <text variable=\"DOI\" prefix=\"(http://doi.org/\" suffix=\")*,\"/>\n                  </if>\n                  <else-if match=\"any\" variable=\"ISBN\">\n                    <text variable=\"title\" text-case=\"title\" font-style=\"normal\" prefix=\"*[\" suffix=\"]\"/>\n                    <text variable=\"ISBN\" prefix=\"(https://en.wikipedia.org/wiki/Special:BookSources/\" suffix=\")*,\"/>\n                  </else-if>\n                  <else-if match=\"any\" variable=\"URL\">\n                    <text variable=\"title\" text-case=\"title\" font-style=\"normal\" prefix=\"*[\" suffix=\"]\"/>\n                    <text variable=\"URL\" prefix=\"(\" suffix=\")*,\"/>\n                  </else-if>\n                  <else>\n                    <text variable=\"title\" text-case=\"title\" font-style=\"normal\" prefix=\"*\" suffix=\"*,\"/>\n                  </else>\n                </choose>\n              </if>\n              <else>\n                <choose>\n                  <if match=\"any\" variable=\"DOI\">\n                    <text variable=\"title\" prefix=\"[\" suffix=\"]\"/>\n                    <text variable=\"DOI\" prefix=\"(http://doi.org/\" suffix=\"),\"/>\n                  </if>\n                  <else-if match=\"any\" variable=\"URL\">\n                    <text variable=\"title\" prefix=\"[\" suffix=\"]\"/>\n                    <text variable=\"URL\" prefix=\"(\" suffix=\"),\"/>\n                  </else-if>\n                  <else-if match=\"any\" variable=\"ISBN\">\n                    <text variable=\"title\" prefix=\"[\" suffix=\"]\"/>\n                    <text variable=\"ISBN\" prefix=\"(https://en.wikipedia.org/wiki/Special:BookSources/\" suffix=\"),\"/>\n                  </else-if>\n                  <else>\n                    <text variable=\"title\" prefix=\"'\" suffix=\"',\"/>\n                  </else>\n                </choose>\n              </else>\n            </choose>\n          </else-if>\n          <else-if type=\"post post-weblog webpage broadcast report article-journal article-magazine article-newspaper paper-conference speech\" match=\"any\">\n            <choose>\n              <if match=\"any\" variable=\"DOI\">\n                <text variable=\"title\" prefix=\"[\" suffix=\"]\"/>\n                <text variable=\"DOI\" prefix=\"(http://doi.org/\" suffix=\"),\"/>\n              </if>\n              <else-if match=\"any\" variable=\"URL\">\n                <text variable=\"title\" prefix=\"[\" suffix=\"]\"/>\n                <text variable=\"URL\" prefix=\"(\" suffix=\"),\"/>\n              </else-if>\n              <else-if match=\"any\" variable=\"ISBN\">\n                <text variable=\"title\" prefix=\"[\" suffix=\"]\"/>\n                <text variable=\"ISBN\" prefix=\"(https://en.wikipedia.org/wiki/Special:BookSources/\" suffix=\"),\"/>\n              </else-if>\n              <else>\n                <text variable=\"title\" prefix=\"'\" suffix=\"',\"/>\n              </else>\n            </choose>\n          </else-if>\n          <else>\n            <choose>\n              <if match=\"any\" variable=\"DOI\">\n                <text variable=\"title\" text-case=\"title\" font-style=\"normal\" prefix=\"*[\" suffix=\"]\"/>\n                <text variable=\"DOI\" prefix=\"(http://doi.org/\" suffix=\")*,\"/>\n              </if>\n              <else-if match=\"any\" variable=\"ISBN\">\n                <text variable=\"title\" text-case=\"title\" font-style=\"normal\" prefix=\"*[\" suffix=\"]\"/>\n                <text variable=\"ISBN\" prefix=\"(https://en.wikipedia.org/wiki/Special:BookSources/\" suffix=\")*,\"/>\n              </else-if>\n              <else-if match=\"any\" variable=\"URL\">\n                <text variable=\"title\" text-case=\"title\" font-style=\"normal\" prefix=\"*[\" suffix=\"]\"/>\n                <text variable=\"URL\" prefix=\"(\" suffix=\")*,\"/>\n              </else-if>\n              <else>\n                <text variable=\"title\" text-case=\"title\" font-style=\"normal\" prefix=\"*\" suffix=\"*,\"/>\n              </else>\n            </choose>\n          </else>\n        </choose>\n      </if>\n      <else-if variable=\"interviewer\" type=\"interview\" match=\"any\">\n        <names variable=\"interviewer\">\n          <label form=\"verb-short\" suffix=\" \" text-case=\"capitalize-first\"/>\n          <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n        </names>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"description\">\n    <group>\n      <choose>\n        <if type=\"book report\" match=\"any\">\n          <choose>\n            <if variable=\"container-title\">\n              <text macro=\"secondary-contributors\"/>\n            </if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"description-report\"/>\n                <text macro=\"secondary-contributors\"/>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <group delimiter=\", \">\n              <text variable=\"genre\" suffix=\",\"/>\n            </group>\n            <text macro=\"locators\"/>\n            <text macro=\"secondary-contributors\"/>\n          </group>\n        </else-if>\n        <else-if type=\"book interview manuscript motion_picture musical_score pamphlet post post-weblog speech webpage broadcast\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"locators\"/>\n            <text macro=\"secondary-contributors\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article dataset figure\" match=\"any\">\n          <choose>\n            <if variable=\"container-title\">\n              <text macro=\"secondary-contributors\"/>\n            </if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"locators\"/>\n                <text macro=\"secondary-contributors\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"bill legislation legal_case patent treaty personal_communication\" match=\"none\">\n          <text macro=\"secondary-contributors\"/>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"format\">\n    <group>\n      <choose>\n        <if variable=\"reviewed-author reviewed-title\" type=\"review review-book\" match=\"any\">\n          <group delimiter=\", \">\n            <names variable=\"reviewed-author\"/>\n          </group>\n        </if>\n        <else>\n          <text macro=\"format-short\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"format-short\">\n    <choose>\n      <if type=\"speech thesis\" match=\"any\">\n        <text variable=\"medium\" text-case=\"capitalize-first\"/>\n      </if>\n      <else-if type=\"book report\" match=\"any\">\n        <choose>\n          <if variable=\"container-title\" match=\"none\">\n            <text macro=\"format-report\"/>\n          </if>\n        </choose>\n      </else-if>\n      <else-if type=\"manuscript pamphlet\" match=\"any\">\n        <text variable=\"medium\" text-case=\"capitalize-first\"/>\n      </else-if>\n      <else-if type=\"personal_communication\">\n        <text macro=\"secondary-contributors\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"description-report\">\n    <choose>\n      <if variable=\"issue\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \" suffix=\",\">\n            <text variable=\"issue\"/>\n          </group>\n          <text macro=\"locators\"/>\n        </group>\n      </if>\n      <else>\n        <text macro=\"locators\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"format-report\">\n    <choose>\n      <if variable=\"number\">\n        <text variable=\"medium\" text-case=\"lowercase\" suffix=\".\"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          <text variable=\"medium\" text-case=\"capitalize-first\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\". \">\n      <group delimiter=\", \">\n        <choose>\n          <if type=\"manuscript\">\n            <text variable=\"genre\"/>\n          </if>\n        </choose>\n        <group delimiter=\" \">\n          <text variable=\"archive\"/>\n          <text variable=\"archive_location\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </group>\n      <group delimiter=\", \">\n        <text variable=\"archive-place\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"manuscript pamphlet\" match=\"any\">\n        <choose>\n          <if variable=\"archive archive_location archive-place\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n              <text macro=\"archive\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"thesis\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <text variable=\"publisher\"/>\n          </group>\n          <text macro=\"archive\"/>\n        </group>\n      </else-if>\n      <else-if type=\"patent\">\n        <group delimiter=\". \">\n          <group delimiter=\": \">\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n          </group>\n          <text macro=\"archive\"/>\n        </group>\n      </else-if>\n      <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text macro=\"archive\"/>\n      </else-if>\n      <else-if type=\"post post-weblog webpage broadcast speech\" match=\"none\">\n        <group delimiter=\" \">\n          <choose>\n            <if match=\"any\" variable=\"page\">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\" suffix=\",\"/>\n              </group>\n              <group>\n                <label suffix=\" \" variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </else>\n          </choose>\n          <text macro=\"archive\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <group>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <text variable=\"year-suffix\"/>\n          <choose>\n            <if type=\"song\">\n              <date variable=\"issued\" delimiter=\" \">\n                <date-part prefix=\", \" name=\"month\"/>\n              </date>\n            </if>\n            <else-if type=\"article article-magazine article-newspaper broadcast interview pamphlet personal_communication post post-weblog speech treaty webpage\" match=\"any\"/>\n            <else-if type=\"paper-conference\">\n              <choose>\n                <if variable=\"container-title\" match=\"none\">\n                  <date variable=\"issued\">\n                    <date-part prefix=\", \" name=\"month\"/>\n                    <date-part prefix=\" \" name=\"day\"/>\n                  </date>\n                </if>\n              </choose>\n            </else-if>\n          </choose>\n        </group>\n      </if>\n      <else-if variable=\"status\">\n        <group>\n          <text variable=\"status\" text-case=\"lowercase\"/>\n          <text variable=\"year-suffix\" prefix=\"-\"/>\n        </group>\n      </else-if>\n      <else>\n        <group>\n          <text term=\"no date\" form=\"short\"/>\n          <text variable=\"year-suffix\" prefix=\"-\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-sort\">\n    <choose>\n      <if type=\"article article-magazine article-newspaper broadcast interview pamphlet personal_communication post post-weblog speech treaty webpage\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <group>\n      <choose>\n        <if type=\"personal_communication\">\n          <choose>\n            <if variable=\"archive DOI ISBN publisher URL\" match=\"none\">\n              <date variable=\"issued\" form=\"text\"/>\n            </if>\n            <else>\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n            </else>\n          </choose>\n        </if>\n        <else>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </else>\n      </choose>\n      <text variable=\"year-suffix\"/>\n    </group>\n  </macro>\n  <macro name=\"issued-citation\">\n    <choose>\n      <if variable=\"issued\">\n        <group delimiter=\"/\">\n          <choose>\n            <if is-uncertain-date=\"original-date\">\n              <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n                <text term=\"circa\" form=\"short\"/>\n                <date variable=\"original-date\">\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </if>\n            <else>\n              <date variable=\"original-date\">\n                <date-part name=\"year\"/>\n              </date>\n            </else>\n          </choose>\n          <choose>\n            <if is-uncertain-date=\"issued\">\n              <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n                <text term=\"circa\" form=\"short\"/>\n                <text macro=\"issued-year\"/>\n              </group>\n            </if>\n            <else>\n              <text macro=\"issued-year\"/>\n            </else>\n          </choose>\n        </group>\n      </if>\n      <else-if variable=\"status\">\n        <text variable=\"status\" text-case=\"lowercase\"/>\n        <text variable=\"year-suffix\" prefix=\"-\"/>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n        <text variable=\"year-suffix\" prefix=\"-\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"original-date\">\n    <choose>\n      <if type=\"article article-magazine article-newspaper broadcast interview pamphlet personal_communication post post-weblog speech treaty webpage\" match=\"any\">\n        <date variable=\"original-date\" form=\"text\"/>\n      </if>\n      <else>\n        <date variable=\"original-date\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"original-published\">\n    <choose>\n      <if type=\"broadcast\">\n        <text value=\"Original work broadcast\"/>\n      </if>\n      <else>\n        <text value=\"Original work published\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \" suffix=\",\">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\" \" suffix=\",\">\n          <text variable=\"edition\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal figure review review-book\" match=\"any\">\n        <choose>\n          <if variable=\"volume page\" match=\"none\">\n            <text variable=\"issue\" font-style=\"normal\" suffix=\".\"/>\n          </if>\n          <else>\n            <group delimiter=\", \" suffix=\".\">\n              <group delimiter=\", \">\n                <text variable=\"volume\" font-style=\"normal\" prefix=\"vol. \"/>\n              </group>\n              <group>\n                <label suffix=\" \" variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"article-magazine\">\n        <choose>\n          <if variable=\"volume\" match=\"any\">\n            <group delimiter=\", \" suffix=\".\">\n              <group delimiter=\", \">\n                <text variable=\"volume\" font-style=\"normal\" prefix=\"vol. \"/>\n              </group>\n              <group>\n                <label suffix=\" \" variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\", \" suffix=\".\">\n              <date delimiter=\" \" variable=\"issued\">\n                <date-part name=\"month\"/>\n                <date-part name=\"day\"/>\n              </date>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"article-newspaper\">\n        <group delimiter=\", \" suffix=\".\">\n          <date delimiter=\" \" variable=\"issued\">\n            <date-part name=\"month\"/>\n            <date-part name=\"day\"/>\n          </date>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"speech\">\n        <group delimiter=\", \" suffix=\".\">\n          <group>\n            <text variable=\"publisher-place\"/>\n          </group>\n          <group>\n            <date delimiter=\" \" variable=\"issued\">\n              <date-part name=\"month\"/>\n              <date-part name=\"day\"/>\n            </date>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"bill broadcast interview legal_case legislation patent post post-weblog speech treaty webpage\" match=\"none\">\n        <text macro=\"locators-booklike\"/>\n      </else-if>\n      <else-if type=\"post post-weblog webpage broadcast report\" match=\"any\">\n        <choose>\n          <if variable=\"number\" match=\"any\">\n            <group delimiter=\", \" suffix=\".\">\n              <group>\n                <text variable=\"container-title\" text-case=\"title\" font-style=\"normal\" prefix=\"*\" suffix=\"*\"/>\n              </group>\n              <group>\n                <date delimiter=\" \" variable=\"issued\">\n                  <date-part name=\"month\" suffix=\" \"/>\n                  <date-part name=\"day\" suffix=\" \"/>\n                </date>\n                <text variable=\"number\" font-style=\"normal\" prefix=\"(updated \" suffix=\")\"/>\n              </group>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\", \" suffix=\".\">\n              <group>\n                <text variable=\"container-title\" text-case=\"title\" font-style=\"normal\" prefix=\"*\" suffix=\"*\"/>\n              </group>\n              <group>\n                <date delimiter=\" \" variable=\"issued\">\n                  <date-part name=\"month\"/>\n                  <date-part name=\"day\" suffix=\".\"/>\n                </date>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locators-booklike\">\n    <group delimiter=\", \">\n      <text macro=\"edition\"/>\n      <group delimiter=\" \">\n        <text term=\"version\" text-case=\"capitalize-first\"/>\n        <text variable=\"version\"/>\n      </group>\n      <choose>\n        <if variable=\"volume\" match=\"any\">\n          <choose>\n            <if is-numeric=\"volume\" match=\"none\"/>\n            <else-if variable=\"collection-title\">\n              <choose>\n                <if variable=\"editor translator\" match=\"none\">\n                  <choose>\n                    <if variable=\"collection-number\">\n                      <group>\n                        <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                        <number variable=\"volume\" form=\"numeric\" suffix=\",\"/>\n                      </group>\n                    </if>\n                  </choose>\n                </if>\n              </choose>\n            </else-if>\n            <else>\n              <group>\n                <text term=\"volume\" form=\"short\" text-case=\"lowercase\" suffix=\" \"/>\n                <number variable=\"volume\" form=\"numeric\" suffix=\",\"/>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <else>\n          <group>\n            <text term=\"volume\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1&#8211;\"/>\n          </group>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <choose>\n        <if locator=\"chapter\">\n          <label variable=\"locator\" text-case=\"capitalize-first\"/>\n        </if>\n        <else>\n          <label variable=\"locator\" form=\"short\"/>\n        </else>\n      </choose>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper review review-book\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"container-title\"/>\n          <text macro=\"locators\"/>\n        </group>\n        <choose>\n          <if variable=\"issued\">\n            <choose>\n              <if variable=\"page issue\" match=\"none\">\n                <text variable=\"status\" text-case=\"capitalize-first\" prefix=\". \"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"article dataset figure\" match=\"any\">\n        <choose>\n          <if variable=\"container-title\">\n            <group delimiter=\", \">\n              <text macro=\"container-title\"/>\n              <text macro=\"locators\"/>\n            </group>\n            <choose>\n              <if variable=\"issued\">\n                <choose>\n                  <if variable=\"page issue\" match=\"none\">\n                    <text variable=\"status\" text-case=\"capitalize-first\" prefix=\". \"/>\n                  </if>\n                </choose>\n              </if>\n            </choose>\n          </if>\n        </choose>\n      </else-if>\n      <else-if type=\"book\" variable=\"container-title\" match=\"all\">\n        <group delimiter=\" \">\n          <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <group delimiter=\", \">\n            <text macro=\"container-contributors\"/>\n            <group delimiter=\" \">\n              <text macro=\"container-title\"/>\n              <text macro=\"description-report\" prefix=\"(\" suffix=\")\"/>\n              <text macro=\"format-report\" prefix=\"[\" suffix=\"]\"/>\n            </group>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"chapter entry entry-dictionary entry-encyclopedia graphic map speech paper-conference\" match=\"any\">\n        <text macro=\"container-booklike\"/>\n      </else-if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\">\n          <date-part name=\"month\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"container-booklike\">\n    <choose>\n      <if variable=\"container-title collection-title\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"in\" text-case=\"lowercase\"/>\n          <group delimiter=\" \">\n            <text macro=\"container-contributors\"/>\n            <choose>\n              <if variable=\"container-author editor translator\" match=\"none\">\n                <group delimiter=\". \">\n                  <group delimiter=\": \">\n                    <text variable=\"collection-title\" font-style=\"italic\" text-case=\"title\"/>\n                    <choose>\n                      <if variable=\"collection-title\">\n                        <group delimiter=\" \">\n                          <text term=\"volume\" form=\"short\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n                          <number variable=\"collection-number\" font-style=\"italic\" form=\"numeric\"/>\n                          <choose>\n                            <if variable=\"collection-number\" match=\"none\">\n                              <number variable=\"volume\" font-style=\"italic\" form=\"numeric\"/>\n                            </if>\n                          </choose>\n                        </group>\n                      </if>\n                    </choose>\n                  </group>\n                  <group delimiter=\": \">\n                    <text macro=\"container-title\"/>\n                    <choose>\n                      <if variable=\"collection-title\" is-numeric=\"volume\" match=\"none\">\n                        <group delimiter=\" \">\n                          <text term=\"volume\" form=\"short\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n                          <text variable=\"volume\" font-style=\"italic\"/>\n                        </group>\n                      </if>\n                    </choose>\n                  </group>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\": \">\n                  <text macro=\"container-title\"/>\n                  <choose>\n                    <if is-numeric=\"volume\" match=\"none\">\n                      <group delimiter=\" \">\n                        <text term=\"volume\" form=\"short\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n                        <text variable=\"volume\" font-style=\"italic\"/>\n                      </group>\n                    </if>\n                  </choose>\n                </group>\n              </else>\n            </choose>\n          </group>\n          <group delimiter=\", \" prefix=\", \">\n            <text macro=\"locators\"/>\n            <names variable=\"container-author\">\n              <label form=\"verb-short\" suffix=\" \" text-case=\"title\"/>\n              <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n            </names>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article article-journal dataset\" match=\"any\">\n        <choose>\n          <if variable=\"volume issue page\" match=\"none\">\n            <text variable=\"container-title\" font-style=\"normal\" text-case=\"title\" prefix=\"*\" suffix=\"*.\"/>\n          </if>\n          <else>\n            <text variable=\"container-title\" font-style=\"normal\" text-case=\"title\" prefix=\"*\" suffix=\"*\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n        <choose>\n          <if variable=\"volume issue page issued\" match=\"none\">\n            <text variable=\"container-title\" font-style=\"normal\" text-case=\"title\" prefix=\"*\" suffix=\"*.\"/>\n          </if>\n          <else>\n            <text variable=\"container-title\" font-style=\"normal\" text-case=\"title\" prefix=\"*\" suffix=\"*\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"bill legal_case legislation post post-weblog webpage\" match=\"none\">\n        <text variable=\"container-title\" text-case=\"title\" strip-periods=\"false\" font-style=\"normal\" prefix=\"*\" suffix=\"*,\"/>\n      </else-if>\n      <else-if type=\"post post-weblog webpage broadcast\" match=\"any\">\n        <text variable=\"publisher\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"6\" et-al-use-first=\"1\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\" givenname-disambiguation-rule=\"primary-name\">\n    <sort>\n      <key macro=\"author\" names-min=\"8\" names-use-first=\"6\"/>\n      <key macro=\"issued-sort\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued-citation\"/>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography and=\"symbol\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" et-al-min=\"4\" et-al-use-first=\"1\" initialize=\"false\" entry-spacing=\"0\" line-spacing=\"2\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-sort\" sort=\"ascending\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <group>\n        <group delimiter=\" \">\n          <text macro=\"author\"/>\n          <choose>\n            <if is-uncertain-date=\"issued\">\n              <group prefix=\" [\" suffix=\"]\" delimiter=\" \">\n                <text term=\"circa\" form=\"short\"/>\n                <text macro=\"issued\"/>\n              </group>\n            </if>\n            <else>\n              <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n            </else>\n          </choose>\n          <group delimiter=\" \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text macro=\"format\"/>\n              <text macro=\"description\"/>\n            </group>\n          </group>\n          <text macro=\"container\"/>\n        </group>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n      <choose>\n        <if is-uncertain-date=\"original-date\">\n          <group prefix=\" [\" suffix=\"]\" delimiter=\" \">\n            <text macro=\"original-published\"/>\n            <text term=\"circa\" form=\"short\"/>\n            <text macro=\"original-date\"/>\n          </group>\n        </if>\n        <else-if variable=\"original-date\">\n          <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n            <text macro=\"original-published\"/>\n            <text macro=\"original-date\"/>\n          </group>\n        </else-if>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"