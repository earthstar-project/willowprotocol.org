"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>American Journal of Respiratory and Critical Care Medicine</title>\n    <title-short>AJRCCM</title-short>\n    <id>http://www.zotero.org/styles/american-journal-of-respiratory-and-critical-care-medicine</id>\n    <link href=\"http://www.zotero.org/styles/american-journal-of-respiratory-and-critical-care-medicine\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/nature\" rel=\"template\"/>\n    <link href=\"http://www.atsjournals.org/page/AJRCCM/instructions_for_contributors\" rel=\"documentation\"/>\n    <author>\n      <name>Charles Parnot</name>\n      <uri>http://twitter.com/cparnot</uri>\n      <email>charles.parnot@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1073-449X</issn>\n    <eissn>1535-4970</eissn>\n    <updated>2022-12-19T14:23:40+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" delimiter=\", \" initialize-with=\"\" name-as-sort-order=\"all\"/>\n      <label prefix=\", \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <group delimiter=\" \">\n      <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n      <names variable=\"editor translator\">\n        <name sort-separator=\" \" delimiter=\", \" initialize-with=\"\" name-as-sort-order=\"all\"/>\n        <label prefix=\", \"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"volume\"/>\n      <else-if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </else-if>\n      <else-if variable=\"URL\">\n        <text term=\"at\"/>\n        <text variable=\"URL\" prefix=\" &lt;\" suffix=\"&gt;\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issuance\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song chapter paper-conference\" match=\"any\">\n        <group prefix=\" \" suffix=\". \">\n          <text variable=\"publisher-place\" suffix=\": \"/>\n          <text variable=\"publisher\" suffix=\"; \"/>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <date prefix=\" \" suffix=\";\" variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\" plural=\"never\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"volume\" suffix=\":\"/>\n        <text variable=\"page\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\" entry-spacing=\"0\" line-spacing=\"2\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <group delimiter=\". \" suffix=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <text macro=\"editor\"/>\n      </group>\n      <group delimiter=\", \" suffix=\" \">\n        <text variable=\"container-title\" font-style=\"italic\" form=\"short\" strip-periods=\"true\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"none\">\n            <text macro=\"edition\"/>\n          </if>\n        </choose>\n      </group>\n      <text macro=\"issuance\"/>\n      <text macro=\"pages\" suffix=\".\"/>\n      <text macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"