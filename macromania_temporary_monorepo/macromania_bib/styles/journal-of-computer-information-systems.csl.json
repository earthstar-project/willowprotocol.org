"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Journal of Computer Information Systems</title>\n    <title-short>JCIS</title-short>\n    <id>http://www.zotero.org/styles/journal-of-computer-information-systems</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-computer-information-systems\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/springer-vancouver\" rel=\"template\"/>\n    <link href=\"http://www.tandfonline.com/action/journalInformation?journalCode=ucis20\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <issn>0887-4417</issn>\n    <eissn>2380-2057</eissn>\n    <updated>2017-06-22T14:13:51+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\", \" suffix=\". \">\n          <text variable=\"title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\" suffix=\". \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <group delimiter=\" \">\n        <text macro=\"author\" suffix=\".\"/>\n        <text macro=\"year\" suffix=\".\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if variable=\"page\">\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\" \" suffix=\". \">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n                <names variable=\"editor\">\n                  <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n                  <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n                </names>\n                <group delimiter=\", \">\n                  <text variable=\"container-title\" form=\"short\"/>\n                  <text macro=\"edition\"/>\n                </group>\n              </group>\n              <text variable=\"publisher\" suffix=\", \"/>\n              <text variable=\"publisher-place\" suffix=\", \"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n                <text variable=\"page\"/>\n              </group>\n            </if>\n            <else>\n              <group>\n                <text variable=\"container-title\" suffix=\" \" form=\"short\" strip-periods=\"true\"/>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                <text variable=\"page\" prefix=\":\"/>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <else-if variable=\"DOI\">\n          <group delimiter=\". \">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n            <text prefix=\"doi: \" variable=\"DOI\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\": \">\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group>\n            <text variable=\"container-title\" form=\"short\" suffix=\". \"/>\n            <group delimiter=\": \" suffix=\", \">\n              <text term=\"available at\" text-case=\"capitalize-first\"/>\n              <text variable=\"URL\"/>\n            </group>\n            <date form=\"numeric\" variable=\"accessed\" prefix=\"[\" suffix=\"]\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group>\n            <text variable=\"container-title\" form=\"short\" suffix=\" \"/>\n            <text variable=\"volume\" suffix=\":\"/>\n            <text variable=\"page\" suffix=\".\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"