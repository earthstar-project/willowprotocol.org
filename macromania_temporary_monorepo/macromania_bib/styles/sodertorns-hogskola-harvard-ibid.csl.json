"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Södertörns högskola - Harvard (with Ibid.)</title>\n    <title-short>SH - Harvard (Ibid.)</title-short>\n    <id>http://www.zotero.org/styles/sodertorns-hogskola-harvard-ibid</id>\n    <link href=\"http://www.zotero.org/styles/sodertorns-hogskola-harvard-ibid\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/sodertorns-hogskola-harvard\" rel=\"template\"/>\n    <link href=\"https://www.sh.se/p3/ext/content.nsf/aget?openagent&amp;key=guider_till_olika_format_1354269274669\" rel=\"documentation\"/>\n    <link href=\"https://www.sh.se/p3/ext/res.nsf/vRes/bibliotek_referenser_harvard_2_1_2_pdf/$File/Referenser%20Harvard%202_1_2.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Kurt Berndt</name>\n      <email>kurt.berndt@sh.se</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Södertörn Högskola Harvard (ibid)</summary>\n    <updated>2018-03-31T04:01:14+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"sv\">\n    <terms>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"page\" form=\"short\">\n        <single>s.</single>\n        <multiple>ss.</multiple>\n      </term>\n      <term name=\"number-of-pages\" form=\"short\">\n        <single>s.</single>\n        <multiple>ss.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"motion_picture broadcast\" match=\"none\">\n        <names variable=\"author\">\n          <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n          <label form=\"short\" prefix=\" \"/>\n          <et-al font-style=\"italic\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"anon\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n    <choose>\n      <if type=\"article-newspaper entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if variable=\"author editor\" match=\"none\">\n            <text variable=\"container-title\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"motion_picture broadcast\" match=\"none\">\n        <names variable=\"author\">\n          <name form=\"short\" and=\"symbol\" delimiter-precedes-last=\"never\" delimiter=\", \" initialize-with=\". \" name-as-sort-order=\"all\" sort-separator=\", \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"anon\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n    <choose>\n      <if type=\"article-newspaper entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if variable=\"author editor\" match=\"none\">\n            <text variable=\"container-title\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"anon\">\n    <choose>\n      <if type=\"motion_picture broadcast article-newspaper entry-dictionary entry-encyclopedia\" match=\"none\">\n        <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <choose>\n          <if type=\"webpage post-weblog\" match=\"any\">\n            <group>\n              <text variable=\"URL\"/>\n            </group>\n            <group prefix=\" [\" suffix=\"]\" delimiter=\" \">\n              <date variable=\"accessed\" delimiter=\"-\">\n                <date-part name=\"year\"/>\n                <date-part name=\"month\" form=\"numeric-leading-zeros\" strip-periods=\"true\"/>\n                <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n              </date>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <group>\n            <text variable=\"genre\"/>\n            <text variable=\"number\" prefix=\" No. \"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation map report song speech webpage\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"title\" font-style=\"italic\"/>\n            </group>\n            <text variable=\"scale\"/>\n          </group>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n      <else-if type=\"motion_picture broadcast\" match=\"any\"/>\n      <else-if type=\"patent\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\". \"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" font-style=\"normal\" suffix=\": \"/>\n        <group delimiter=\" \">\n          <names variable=\"editor translator\" delimiter=\", \">\n            <name and=\"symbol\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n            <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n          </names>\n          <group delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation map motion_picture report song\" match=\"any\">\n        <text variable=\"container-title\"/>\n      </else-if>\n      <else-if type=\"broadcast\">\n        <choose>\n          <if variable=\"container-title\">\n            <group delimiter=\", \">\n              <text variable=\"number\" prefix=\"Episode \"/>\n              <text variable=\"title\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n      <else-if type=\"webpage\">\n        <text variable=\"container-title\"/>\n      </else-if>\n      <else>\n        <group delimiter=\" \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"report-details\">\n    <choose>\n      <if type=\"report\">\n        <group>\n          <text variable=\"genre\"/>\n          <text variable=\"number\" prefix=\" No. \"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\". \">\n          <group delimiter=\": \">\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n          </group>\n          <text macro=\"report-details\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <text variable=\"event\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if variable=\"accessed\">\n        <choose>\n          <if type=\"webpage\">\n            <date variable=\"accessed\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <group delimiter=\" \">\n            <number variable=\"edition\" form=\"ordinal\"/>\n            <text term=\"edition\" form=\"short\"/>\n          </group>\n        </if>\n        <else>\n          <text variable=\"edition\" suffix=\".\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group prefix=\", \" delimiter=\", \">\n          <group delimiter=\"\">\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <group>\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\" prefix=\" \"/>\n          </group>\n          <group>\n            <text variable=\"DOI\" prefix=\"doi:\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <date variable=\"issued\" prefix=\", \" form=\"text\"/>\n        <group delimiter=\" \" prefix=\", \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill  book graphic legal_case legislation map motion_picture report song thesis\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </else-if>\n      <else-if type=\"broadcast\">\n        <group delimiter=\". \" prefix=\", \">\n          <text variable=\"publisher\"/>\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n          <group>\n            <label suffix=\" \" variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"patent\" match=\"any\">\n        <group>\n          <text variable=\"authority\" suffix=\": \"/>\n          <text variable=\"number\" suffix=\", \"/>\n          <date form=\"numeric\" variable=\"issued\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation and=\"symbol\" et-al-min=\"6\" et-al-use-first=\"1\" et-al-subsequent-min=\"4\" et-al-subsequent-use-first=\"1\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" givenname-disambiguation-rule=\"primary-name\" collapse=\"year\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <text term=\"ibid\" suffix=\" \"/>\n          <label suffix=\" \" variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" form=\"short\"/>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text macro=\"author-short\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group delimiter=\" \">\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\" line-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <group delimiter=\". \" prefix=\" \">\n        <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n        <text macro=\"title\"/>\n        <text macro=\"container\"/>\n        <text variable=\"medium\"/>\n      </group>\n      <text macro=\"locators\"/>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"