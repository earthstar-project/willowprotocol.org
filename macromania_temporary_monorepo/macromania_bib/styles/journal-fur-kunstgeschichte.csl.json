"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" default-locale=\"de-DE\" version=\"1.0\">\n  <info>\n    <title>Journal f端r Kunstgeschichte</title>\n    <id>http://www.zotero.org/styles/journal-fur-kunstgeschichte</id>\n    <link href=\"http://www.zotero.org/styles/journal-fur-kunstgeschichte\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/infoclio-de\" rel=\"template\"/>\n    <link href=\"https://github.com/citation-style-language/styles/pull/2043\" rel=\"documentation\"/>\n    <author>\n      <name>Anna Simon</name>\n      <email>simonanna@gmx.net</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <category field=\"humanities\"/>\n    <issn>1432-9506</issn>\n    <summary>Please edit the field \"short title\" and enter \"author/editor\" + \"year\" or \"city\" + \"year\".</summary>\n    <updated>2016-01-26T19:32:01+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"editor\" form=\"short\">Hrsg.</term>\n      <term name=\"interviewer\" form=\"verb\">Interview gef端hrt von</term>\n      <term name=\"accessed\">Stand</term>\n      <term name=\"letter\">Schreiben</term>\n      <term name=\"number-of-volumes\" form=\"short\">Bd.</term>\n      <term name=\"no date\" form=\"short\">o. D.</term>\n      <term name=\"note\">s. Anm.</term>\n      <term name=\"translator\" form=\"verb-short\">端bers. von</term>\n      <term name=\"editortranslator\" form=\"verb-short\">hrsg. und 端bers. von</term>\n    </terms>\n  </locale>\n  <citation>\n    <layout suffix=\".\" delimiter=\"&#160;; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\"/>\n            <text macro=\"locator\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <text macro=\"subsequent-reference\"/>\n        </else-if>\n        <else>\n          <text macro=\"complete-reference\"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"identifier-for-subsequent\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"title\" form=\"short\" font-weight=\"bold\" display=\"block\"/>\n      <text macro=\"complete-reference\"/>\n    </layout>\n  </bibliography>\n  <macro name=\"complete-reference\">\n    <choose>\n      <if match=\"any\" variable=\"container-title\">\n        <choose>\n          <if type=\"article-journal\">\n            <group delimiter=\". \">\n              <group delimiter=\" \">\n                <group delimiter=\", \">\n                  <text macro=\"creator\"/>\n                  <group delimiter=\", \">\n                    <text macro=\"title\" quotes=\"true\"/>\n                    <group delimiter=\": \">\n                      <text macro=\"in\"/>\n                      <text macro=\"container-creator\"/>\n                      <group delimiter=\", \">\n                        <group delimiter=\" \">\n                          <text macro=\"container-information\"/>\n                          <text macro=\"journal-volume\"/>\n                        </group>\n                        <text macro=\"volumes\"/>\n                      </group>\n                    </group>\n                    <text macro=\"type-description\"/>\n                  </group>\n                </group>\n                <text macro=\"alt-publisher\"/>\n                <group delimiter=\" \">\n                  <text macro=\"place\"/>\n                  <text macro=\"date\" prefix=\"(\" suffix=\")\"/>\n                  <date variable=\"original-date\" form=\"text\" prefix=\"[\" suffix=\"]\"/>\n                  <text macro=\"book-series\"/>\n                </group>\n                <text macro=\"artwork-description\"/>\n                <text macro=\"archive-location\"/>\n                <text macro=\"pages\"/>\n                <text macro=\"url-web-documents-only\"/>\n              </group>\n              <text macro=\"url-non-web-documents\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <group delimiter=\", \">\n                  <text macro=\"creator\"/>\n                  <group delimiter=\", \">\n                    <text macro=\"title\" quotes=\"true\"/>\n                    <group delimiter=\" \">\n                      <text macro=\"in\" suffix=\":\"/>\n                      <text macro=\"container-creator\" suffix=\",\"/>\n                      <group delimiter=\", \">\n                        <group delimiter=\" \">\n                          <text macro=\"container-information\"/>\n                          <text macro=\"journal-volume\"/>\n                        </group>\n                        <text macro=\"volumes\"/>\n                      </group>\n                    </group>\n                    <text macro=\"type-description\"/>\n                  </group>\n                </group>\n                <text macro=\"alt-publisher\"/>\n                <group delimiter=\" \">\n                  <text macro=\"place\"/>\n                  <text macro=\"date\"/>\n                  <date variable=\"original-date\" form=\"text\" prefix=\"[\" suffix=\"]\"/>\n                  <text macro=\"book-series\"/>\n                </group>\n                <text macro=\"artwork-description\"/>\n                <text macro=\"archive-location\"/>\n                <text macro=\"pages\"/>\n                <text macro=\"url-web-documents-only\"/>\n              </group>\n              <text macro=\"url-non-web-documents\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n      <else-if match=\"any\" variable=\"translator\">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <group delimiter=\", \">\n              <text macro=\"creator\"/>\n              <group delimiter=\" \">\n                <text macro=\"title\" font-style=\"italic\" suffix=\",\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"book-series\" suffix=\",\"/>\n                <group delimiter=\": \">\n                  <text macro=\"in\"/>\n                  <text macro=\"container-creator\"/>\n                  <group delimiter=\", \">\n                    <group delimiter=\" \">\n                      <text macro=\"container-information\"/>\n                      <text macro=\"journal-volume\"/>\n                    </group>\n                    <text macro=\"volumes\"/>\n                  </group>\n                </group>\n                <text macro=\"type-description\"/>\n              </group>\n            </group>\n            <text macro=\"alt-publisher\"/>\n            <group delimiter=\" \">\n              <text macro=\"place\"/>\n              <text macro=\"date\"/>\n              <date variable=\"original-date\" form=\"text\" prefix=\"[\" suffix=\"]\"/>\n            </group>\n            <text macro=\"artwork-description\"/>\n            <text macro=\"archive-location\"/>\n            <text macro=\"pages\"/>\n            <text macro=\"url-web-documents-only\"/>\n          </group>\n          <text macro=\"url-non-web-documents\"/>\n        </group>\n      </else-if>\n      <else-if type=\"thesis\">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <group delimiter=\", \">\n              <text macro=\"creator\"/>\n              <group delimiter=\" \">\n                <text macro=\"title\" font-style=\"italic\"/>\n                <text macro=\"book-series\" suffix=\",\"/>\n                <group delimiter=\": \">\n                  <text macro=\"in\"/>\n                  <text macro=\"container-creator\"/>\n                  <group delimiter=\", \">\n                    <group delimiter=\" \">\n                      <text macro=\"container-information\"/>\n                      <text macro=\"journal-volume\"/>\n                    </group>\n                    <text macro=\"volumes\"/>\n                  </group>\n                </group>\n                <text macro=\"type-description\"/>\n              </group>\n            </group>\n            <text macro=\"alt-publisher\" prefix=\"Diss., \"/>\n            <group delimiter=\" \">\n              <text macro=\"place\"/>\n              <text macro=\"date\"/>\n              <date variable=\"original-date\" form=\"text\" prefix=\"[\" suffix=\"]\"/>\n            </group>\n            <text macro=\"artwork-description\"/>\n            <text macro=\"archive-location\"/>\n            <text macro=\"pages\"/>\n            <text macro=\"url-web-documents-only\"/>\n          </group>\n          <text macro=\"url-non-web-documents\"/>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <group delimiter=\", \">\n              <text macro=\"creator\"/>\n              <group delimiter=\" \">\n                <text macro=\"title\" font-style=\"italic\"/>\n                <text macro=\"book-series\" suffix=\",\"/>\n                <group delimiter=\": \">\n                  <text macro=\"in\"/>\n                  <text macro=\"container-creator\"/>\n                  <group delimiter=\", \">\n                    <group delimiter=\" \">\n                      <text macro=\"container-information\"/>\n                      <text macro=\"journal-volume\"/>\n                    </group>\n                    <text macro=\"volumes\"/>\n                  </group>\n                </group>\n                <text macro=\"type-description\"/>\n              </group>\n            </group>\n            <text macro=\"alt-publisher\"/>\n            <group delimiter=\" \">\n              <text macro=\"place\"/>\n              <text macro=\"date\"/>\n              <date variable=\"original-date\" form=\"text\" prefix=\"[\" suffix=\"]\"/>\n            </group>\n            <text macro=\"artwork-description\"/>\n            <text macro=\"archive-location\"/>\n            <text macro=\"pages\"/>\n            <text macro=\"url-web-documents-only\"/>\n          </group>\n          <text macro=\"url-non-web-documents\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"subsequent-reference\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <text variable=\"title\" form=\"short\"/>\n        <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n          <text term=\"note\"/>\n          <text variable=\"first-reference-note-number\"/>\n        </group>\n      </group>\n      <text macro=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"creator\">\n    <names variable=\"author\">\n      <name and=\"text\" sort-separator=\", \" et-al-min=\"4\" et-al-use-first=\"3\" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\"&#160;(\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"composer\"/>\n        <names variable=\"director\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"identifier-for-subsequent\">\n    <choose>\n      <if variable=\"title title-short\" match=\"any\">\n        <text variable=\"title\" form=\"short\"/>\n      </if>\n      <else-if type=\"personal_communication\">\n        <group delimiter=\" \">\n          <text term=\"letter\"/>\n          <names variable=\"recipient\">\n            <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n            <name et-al-min=\"2\" et-al-use-first=\"1\"/>\n          </names>\n        </group>\n      </else-if>\n      <else-if type=\"interview\">\n        <names variable=\"interviewer\" delimiter=\", \">\n          <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n          <name et-al-min=\"2\" et-al-use-first=\"1\"/>\n        </names>\n      </else-if>\n      <else-if type=\"report song broadcast motion_picture                      webpage post post-weblog\" match=\"any\">\n        <!-- these types have either collection-title or container-title -->\n        <text variable=\"collection-title\"/>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"in\">\n    <choose>\n      <if type=\"chapter paper-conference                 entry-encyclopedia entry-dictionary                 article-magazine article-newspaper article-journal\" match=\"any\">\n        <text term=\"in\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-creator\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor\">\n          <name and=\"text\" sort-separator=\", \" et-al-min=\"4\" et-al-use-first=\"3\" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" prefix=\"&#160;(\" suffix=\")\"/>\n          <substitute>\n            <names variable=\"container-author\"/>\n            <names variable=\"collection-editor\"/>\n          </substitute>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-information\">\n    <choose>\n      <if type=\"chapter paper-conference                 entry-encyclopedia entry-dictionary                 article-newspaper article-magazine article-journal\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"report song broadcast motion_picture                      webpage post post-weblog\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <!-- these types have either collection-title or container-title -->\n          <text variable=\"collection-title\"/>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"journal-volume\">\n    <choose>\n      <if type=\"article-journal\">\n        <group>\n          <number variable=\"volume\"/>\n          <number variable=\"issue\" prefix=\"/\"/>\n        </group>\n      </if>\n      <else-if type=\"report song broadcast\" match=\"any\">\n        <number variable=\"number\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"volumes\">\n    <choose>\n      <if type=\"book chapter                 entry-encyclopedia entry-dictionary                 song motion_picture\" match=\"any\">\n        <group delimiter=\" / \">\n          <group delimiter=\"&#160;\">\n            <label variable=\"volume\" form=\"short\"/>\n            <number variable=\"volume\"/>\n          </group>\n          <group delimiter=\"&#160;\">\n            <number variable=\"number-of-volumes\"/>\n            <choose>\n              <if variable=\"volume\" match=\"none\">\n                <label variable=\"number-of-volumes\" form=\"short\"/>\n              </if>\n            </choose>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"type-description\">\n    <choose>\n      <if type=\"manuscript thesis speech\" match=\"any\">\n        <text variable=\"genre\"/>\n      </if>\n      <else-if type=\"personal_communication\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text term=\"letter\"/>\n            <names variable=\"recipient\">\n              <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n              <name and=\"text\" delimiter-precedes-last=\"never\"/>\n            </names>\n          </group>\n          <text variable=\"genre\"/>\n        </group>\n      </else-if>\n      <else-if type=\"interview\">\n        <names variable=\"interviewer\" delimiter=\", \">\n          <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n          <name and=\"text\" delimiter-precedes-last=\"never\"/>\n        </names>\n      </else-if>\n      <else-if type=\"motion_picture song broadcast\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"medium\"/>\n          <!-- refers to duration -->\n          <text variable=\"dimensions\"/>\n        </group>\n      </else-if>\n      <!-- Computer Program -->\n      <else-if type=\"book\">\n        <choose>\n          <if variable=\"version medium\" match=\"any\">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"version\"/>\n                <text variable=\"version\"/>\n              </group>\n              <text variable=\"medium\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"alt-publisher\">\n    <choose>\n      <if type=\"book chapter                 paper-conference                 entry-dictionary entry-encyclopedia\" match=\"none\">\n        <!-- university for theses,\n             institution for reports,\n             label for songs,\n             distributor for films,\n             studio for video recordings,\n             network for broadcasts -->\n        <text variable=\"publisher\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"place\">\n    <choose>\n      <if type=\"speech\">\n        <group delimiter=\", \">\n          <text variable=\"event\"/>\n          <text variable=\"event-place\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"publisher-place\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"book chapter paper-conference thesis\" match=\"any\">\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n            <choose>\n              <if is-numeric=\"edition\">\n                <number vertical-align=\"sup\" variable=\"edition\"/>\n              </if>\n            </choose>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"article-journal article-newspaper article-magazine                      graphic entry-encyclopedia entry-dictionary                      report speech interview                      manuscript personal_communication\" match=\"any\">\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\" form=\"numeric\" date-parts=\"year-month-day\"/>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <date variable=\"issued\" form=\"numeric\" date-parts=\"year-month-day\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if variable=\"locator\">\n        <text macro=\"locator\"/>\n      </if>\n      <else>\n        <group delimiter=\"&#160;\">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <group delimiter=\"&#160;\">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"book-series\">\n    <choose>\n      <if type=\"book chapter paper-conference                 entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group prefix=\"(\" suffix=\")\">\n          <text variable=\"collection-title\"/>\n          <choose>\n            <if is-numeric=\"collection-number\">\n              <number variable=\"collection-number\" prefix=\", \"/>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"artwork-description\">\n    <choose>\n      <if type=\"graphic\">\n        <group delimiter=\", \">\n          <text variable=\"medium\"/>\n          <text variable=\"dimensions\"/>\n          <text variable=\"genre\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive-location\">\n    <choose>\n      <if variable=\"archive\">\n        <group delimiter=\", \">\n          <text variable=\"archive\"/>\n          <text variable=\"archive_location\"/>\n          <text variable=\"call-number\" prefix=\"Signatur: \"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"url-web-documents-only\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <text macro=\"url\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"url-non-web-documents\">\n    <choose>\n      <if variable=\"DOI\">\n        <text term=\"online\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <group delimiter=\", \">\n          <text variable=\"source\"/>\n          <group delimiter=\": \">\n            <text value=\"DOI\"/>\n            <text variable=\"DOI\"/>\n          </group>\n        </group>\n      </if>\n      <else-if variable=\"URL\">\n        <choose>\n          <if type=\"webpage post post-weblog\" match=\"none\">\n            <group delimiter=\" \">\n              <text term=\"online\" text-case=\"capitalize-first\" suffix=\": \"/>\n              <group delimiter=\", \">\n                <text variable=\"source\"/>\n                <text macro=\"url\"/>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"url\">\n    <group delimiter=\", \">\n      <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n      <group delimiter=\": \">\n        <text term=\"accessed\"/>\n        <date variable=\"accessed\" form=\"numeric\" date-parts=\"year-month-day\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\" delimiter=\". \">\n      <label form=\"verb-short\"/>\n      <name delimiter=\"/\" delimiter-precedes-last=\"always\" prefix=\" \"/>\n    </names>\n  </macro>\n</style>\n"