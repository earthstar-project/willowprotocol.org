"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"fr-FR\">\n  <info>\n    <title>Revue archéologique de l’Est (Français)</title>\n    <title-short>RAE</title-short>\n    <id>http://www.zotero.org/styles/revue-archeologique-de-lest</id>\n    <link href=\"http://www.zotero.org/styles/revue-archeologique-de-lest\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/documents-d-archeologie-francaise\" rel=\"template\"/>\n    <link href=\"https://journals.openedition.org/rae/1161#tocfrom2n1\" rel=\"documentation\"/>\n    <author>\n      <name>Francois Capron</name>\n      <email>f.capron37@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"history\"/>\n    <category field=\"anthropology\"/>\n    <issn>1266-7706</issn>\n    <eissn>1760-7264</eissn>\n    <updated>2020-06-01T04:36:07+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"collection-editor\" form=\"short\">dir.</term>\n      <term name=\"no date\">[sans date]</term>\n      <term name=\"in\">in</term>\n      <term name=\"online\">en&#160;ligne</term>\n      <term name=\"accessed\">lien valide au</term>\n      <term name=\"from\">URL</term>\n      <term name=\"and others\">et alii</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" initialize-with=\". \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" initialize-with=\". \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\" \" form=\"short\"/>\n    </names>\n  </macro>\n  <macro name=\"collection-editor\">\n    <group>\n      <names variable=\"collection-editor\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" initialize-with=\". \">\n          <name-part name=\"family\" font-variant=\"small-caps\"/>\n          <name-part name=\"given\"/>\n        </name>\n      </names>\n      <text term=\"collection-editor\" form=\"short\" font-style=\"italic\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name>\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"responsability\">\n    <choose>\n      <if variable=\"version\">\n        <group delimiter=\" \">\n          <text macro=\"author\"/>\n          <text term=\"editor\" form=\"short\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n      <else-if variable=\"author editor translator collection-editor\" match=\"any\">\n        <choose>\n          <if variable=\"author\">\n            <text macro=\"author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n          <else-if variable=\"collection-editor\">\n            <text macro=\"collection-editor\"/>\n          </else-if>\n          <else>\n            <text macro=\"translator\"/>\n          </else>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"author-citation\">\n    <choose>\n      <if variable=\"author editor translator collection-editor\" match=\"any\">\n        <names variable=\"author\">\n          <name form=\"short\" font-variant=\"small-caps\"/>\n          <et-al font-variant=\"normal\" term=\"and others\" font-style=\"italic\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <names variable=\"collection-editor\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text variable=\"title\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"container-responsability\">\n    <choose>\n      <if variable=\"container-author editor translator collection-editor\" match=\"any\">\n        <choose>\n          <if variable=\"container-author\">\n            <text macro=\"container-author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n          <else-if variable=\"collection-editor\">\n            <text macro=\"collection-editor\"/>\n          </else-if>\n          <else>\n            <text macro=\"translator\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis map motion_picture song manuscript\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"chapter paper-conference broadcast entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n          <choose>\n            <if variable=\"container-author editor translator collection-editor\" match=\"any\">\n              <group delimiter=\" \">\n                <text term=\"in\"/>\n                <text macro=\"container-responsability\" suffix=\",\"/>\n              </group>\n            </if>\n            <else>\n              <text term=\"in\"/>\n            </else>\n          </choose>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else-if type=\"report\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text variable=\"number\" prefix=\", \"/>\n      </else-if>\n      <else-if type=\"article-journal speech article-magazine article-newspaper post-weblog post\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" quotes=\"true\"/>\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n    <choose>\n      <if variable=\"URL\">\n        <text term=\"online\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"map\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n          <else>\n            <text value=\"carte\" prefix=\"[\" suffix=\"]\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"genre\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if variable=\"publisher-place\">\n        <text variable=\"publisher-place\"/>\n      </if>\n      <else>\n        <text value=\"s.l.\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"broadcast motion_picture song report\" match=\"any\">\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\"/>\n          </if>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\"/>\n          </if>\n          <else>\n            <text value=\"s.n.\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publi_place\">\n    <text macro=\"publisher-place\"/>\n    <text macro=\"publisher\" prefix=\", \"/>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\", \">\n      <text variable=\"volume\"/>\n      <text variable=\"issue\"/>\n      <group delimiter=\"&#160;\">\n        <label variable=\"page\" form=\"short\" text-case=\"lowercase\"/>\n        <text variable=\"page\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"[\" suffix=\"]\">\n          <text term=\"accessed\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" prefix=\"&#160;\"/>\n            <date-part name=\"month\" prefix=\"&#160;\"/>\n            <date-part name=\"year\" prefix=\"&#160;\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <text variable=\"collection-title\" font-style=\"italic\" prefix=\" (\"/>\n    <text variable=\"collection-number\" prefix=\",&#160;\" suffix=\")\"/>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"book thesis manuscript\" match=\"any\">\n        <text variable=\"number-of-pages\" suffix=\"&#160;p.\"/>\n      </if>\n      <else-if type=\"chapter paper-conference article-newspaper entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\"&#160;\">\n          <label variable=\"page\" form=\"short\" text-case=\"lowercase\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"report patent\" match=\"any\">\n        <text variable=\"page\" suffix=\"&#160;p\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text term=\"from\" suffix=\"&#160;: \"/>\n          <text variable=\"URL\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"URLaccess\">\n    <group delimiter=\" \">\n      <text macro=\"url\"/>\n      <text macro=\"accessed\"/>\n    </group>\n  </macro>\n  <macro name=\"archive\">\n    <choose>\n      <if type=\"map\" match=\"any\">\n        <text variable=\"archive\"/>\n      </if>\n      <else-if type=\"report\">\n        <text variable=\"archive\" prefix=\" (\" suffix=\")\"/>\n      </else-if>\n      <else>\n        <text variable=\"archive\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" year-suffix-delimiter=\", \" after-collapse-delimiter=\"&#160;; \" name-delimiter=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\" name-as-sort-order=\"all\" sort-separator=\" \">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"&#160;; \">\n      <group delimiter=\",&#160;\">\n        <text macro=\"author-citation\"/>\n        <text macro=\"year-date\"/>\n        <text variable=\"locator\" prefix=\"p.\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"responsability\" suffix=\", \"/>\n      <text macro=\"year-date\" suffix=\", \"/>\n      <choose>\n        <if type=\"book\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"volume\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n            <text variable=\"note\"/>\n          </group>\n          <choose>\n            <if variable=\"version\" match=\"none\">\n              <text macro=\"collection\"/>\n            </if>\n          </choose>\n          <text macro=\"archive\"/>\n          <text macro=\"URLaccess\"/>\n        </if>\n        <else-if type=\"article-journal article-magazine\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"issue\"/>\n            <text macro=\"URLaccess\"/>\n            <text macro=\"archive\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference chapter entry entry-dictionary entry-encyclopedia\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"event\"/>\n            <text variable=\"note\"/>\n            <group delimiter=\" \">\n              <label variable=\"volume\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text macro=\"publi_place\"/>\n            <text macro=\"page\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"archive\"/>\n            <text macro=\"URLaccess\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"URLaccess\"/>\n            <text macro=\"archive\"/>\n            <text macro=\"page\"/>\n            <text variable=\"note\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"publi_place\"/>\n            <text variable=\"note\"/>\n            <text macro=\"URLaccess\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"volume\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n            <text variable=\"note\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"