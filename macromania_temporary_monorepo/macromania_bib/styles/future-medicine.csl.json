"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Future Medicine journals</title>\n    <id>http://www.zotero.org/styles/future-medicine</id>\n    <link href=\"http://www.zotero.org/styles/future-medicine\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-applied-pharmaceutical-research\" rel=\"template\"/>\n    <link href=\"https://www.futuremedicine.com/authorguide/preparingyourarticle\" rel=\"documentation\"/>\n    <link href=\"https://www.futuremedicine.com/pb-assets/Future-Medicine-Author-Guidelines-1585216135307.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Bien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <category field=\"biology\"/>\n    <updated>2020-06-03T12:27:46+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"presented at\">presented at</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-et-al=\"never\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\" (\" suffix=\")\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <number suffix=\" \" variable=\"edition\" form=\"ordinal\"/>\n    <label plural=\"never\" text-case=\"capitalize-first\" variable=\"edition\"/>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" quotes=\"false\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"webpage\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <date date-parts=\"year\" form=\"numeric\" variable=\"issued\" prefix=\"(\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" plural=\"never\" text-case=\"capitalize-first\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"volume\"/>\n        <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n      </if>\n      <else-if type=\"book chapter\" match=\"any\">\n        <group font-style=\"italic\" delimiter=\", \" prefix=\"(\" suffix=\")\">\n          <group delimiter=\" \">\n            <text term=\"volume\" text-case=\"capitalize-first\"/>\n            <text variable=\"volume\"/>\n          </group>\n          <group>\n            <text term=\"chapter\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <text variable=\"chapter-number\"/>\n          </group>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"baseline\" delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" second-field-align=\"flush\" entry-spacing=\"0\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\"/>\n      <text macro=\"author\" suffix=\". \"/>\n      <choose>\n        <if type=\"article-journal\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text macro=\"container-title\"/>\n              <group delimiter=\", \">\n                <text macro=\"locators\"/>\n                <text macro=\"page\"/>\n              </group>\n              <text macro=\"issued\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"book\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"locators\"/>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <group delimiter=\" \">\n                <text macro=\"container-title\"/>\n                <text macro=\"locators\"/>\n              </group>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"editor\"/>\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <text macro=\"page\"/>\n                <text macro=\"issued\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"presented at\" text-case=\"capitalize-first\"/>\n              <text macro=\"container-title\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\" \">\n            <text variable=\"number\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\". \">\n            <group delimiter=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <text variable=\"URL\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"