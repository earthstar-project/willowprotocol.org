"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\">\n  <!-- This style was edited with the Visual CSL Editor (https://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Journal of Human Rights</title>\n    <id>http://www.zotero.org/styles/journal-of-human-rights</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-human-rights\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/climate-change-economics\" rel=\"template\"/>\n    <link href=\"https://jhr.uconn.edu/submissions/\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <issn>1475-4835</issn>\n    <eissn>1475-4843</eissn>\n    <updated>2021-01-23T02:49:44+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"available at\">available</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </if>\n      <else>\n        <group delimiter=\". \">\n          <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </group>\n        <date form=\"text\" variable=\"accessed\" prefix=\" [\" suffix=\"].\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publication-details\">\n    <group delimiter=\", \">\n      <text macro=\"volume-book\"/>\n      <text macro=\"edition\"/>\n      <group delimiter=\": \" prefix=\"(\" suffix=\")\">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"volume-book\">\n    <choose>\n      <if match=\"any\" is-numeric=\"volume\">\n        <group delimiter=\" \">\n          <text term=\"volume\" form=\"short\"/>\n          <number variable=\"volume\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"volume\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key macro=\"issued\" sort=\"ascending\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"issued\" prefix=\"(\" suffix=\"). \"/>\n      </group>\n      <group suffix=\".\">\n        <choose>\n          <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <group>\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"title\"/>\n              <group delimiter=\" \">\n                <text variable=\"collection-title\"/>\n                <text variable=\"genre\"/>\n                <text variable=\"number\" prefix=\"No. \"/>\n              </group>\n              <text macro=\"publication-details\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"title\"/>\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                  <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n                </group>\n                <names variable=\"editor translator\" delimiter=\", \">\n                  <name delimiter-precedes-last=\"always\"/>\n                  <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n                </names>\n              </group>\n              <text macro=\"publication-details\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"genre\"/>\n              <text macro=\"publication-details\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text macro=\"publication-details\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"