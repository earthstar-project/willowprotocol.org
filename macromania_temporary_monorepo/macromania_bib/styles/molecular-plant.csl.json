"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Molecular Plant</title>\n    <id>http://www.zotero.org/styles/molecular-plant</id>\n    <link href=\"http://www.zotero.org/styles/molecular-plant\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/the-company-of-biologists\" rel=\"template\"/>\n    <link href=\"http://www.cell.com/molecular-plant/authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1674-2052</issn>\n    <eissn>1752-9867</eissn>\n    <updated>2017-07-07T12:09:49+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\",\">\n      <label form=\"short\" plural=\"never\" prefix=\"(\" suffix=\" \"/>\n      <name name-as-sort-order=\"all\" sort-separator=\", \" and=\"text\" initialize-with=\". \" delimiter=\", \" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"authorcount\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n    </names>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author-short\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"authorcount\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author-short\" names-min=\"3\" names-use-first=\"1\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"11\" et-al-use-first=\"10\">\n    <sort>\n      <key macro=\"author-short\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"authorcount\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author-short\" names-min=\"3\" names-use-first=\"1\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" font-weight=\"bold\"/>\n      <date variable=\"issued\" prefix=\" (\" suffix=\").\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text macro=\"title\" font-style=\"italic\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" suffix=\", \" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\" suffix=\".\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"container-title\"/>\n            <group delimiter=\" \">\n              <text variable=\"URL\"/>\n              <group delimiter=\" \">\n                <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                <date form=\"text\" variable=\"accessed\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <group delimiter=\". \">\n              <text macro=\"title\" prefix=\" \"/>\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            </group>\n            <choose>\n              <if match=\"any\" variable=\"volume\">\n                <group delimiter=\":\">\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\" \">\n                  <text value=\"Advance Access published\"/>\n                  <group delimiter=\", \">\n                    <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n                    <text variable=\"DOI\" prefix=\"doi:\"/>\n                  </group>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"