"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" page-range-format=\"minimal\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Entomological Review</title>\n    <id>http://www.zotero.org/styles/entomological-review</id>\n    <link href=\"http://www.zotero.org/styles/entomological-review\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/revista-brasileira-de-ciencia-do-solo\" rel=\"template\"/>\n    <link href=\"https://www.pleiades.online/en/journal/enteng/authors-instructions/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <issn>0013-8738</issn>\n    <eissn>1555-6689</eissn>\n    <updated>2022-07-05T12:57:10+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event-year-month-day\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"accessed-year-month-day\">\n    <group delimiter=\" \">\n      <text term=\"accessed\" text-case=\"capitalize-first\"/>\n      <date form=\"text\" variable=\"accessed\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\" text-case=\"title\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <group delimiter=\" &#8211; \">\n      <text variable=\"event\"/>\n      <text variable=\"container-title\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else-if type=\"webpage post-weblog post\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"URL\"/>\n          <text macro=\"accessed-year-month-day\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label variable=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key macro=\"year-date\" sort=\"ascending\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"20\" et-al-use-first=\"3\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\" sort=\"ascending\"/>\n      <key macro=\"year-date\" sort=\"descending\"/>\n    </sort>\n    <layout suffix=\"\">\n      <group delimiter=\", \">\n        <text macro=\"author\"/>\n        <text macro=\"title\" text-case=\"capitalize-first\"/>\n        <choose>\n          <if type=\"bill graphic legal_case legislation motion_picture song book report\" match=\"any\">\n            <group delimiter=\", \" suffix=\". \">\n              <text macro=\"volume\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter\">\n            <group delimiter=\", \" suffix=\". \">\n              <group delimiter=\" \">\n                <text term=\"in\" font-style=\"italic\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n              </group>\n              <text macro=\"volume\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\" plural=\"never\"/>\n                <text variable=\"page-first\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\">\n            <group suffix=\". \" delimiter=\". \">\n              <group>\n                <group suffix=\"; \">\n                  <text term=\"in\" text-case=\"sentence\" suffix=\": \"/>\n                  <text macro=\"event\"/>\n                </group>\n                <text macro=\"event-year-month-day\" suffix=\"; \"/>\n                <text variable=\"publisher-place\" suffix=\". \" text-case=\"title\"/>\n                <text variable=\"publisher\" suffix=\"; \"/>\n                <text macro=\"year-date\"/>\n              </group>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\" plural=\"never\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"patent\">\n            <group delimiter=\", \">\n              <text variable=\"number\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <text variable=\"container-title\"/>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" form=\"short\" text-case=\"title\" strip-periods=\"false\" font-style=\"italic\"/>\n              <text macro=\"year-date\"/>\n              <text macro=\"volume\"/>\n              <group delimiter=\" \">\n                <label plural=\"never\" variable=\"page\" form=\"short\"/>\n                <text variable=\"page-first\" form=\"short\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"