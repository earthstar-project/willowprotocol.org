"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Trames</title>\n    <id>http://www.zotero.org/styles/trames</id>\n    <link href=\"http://www.zotero.org/styles/trames\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/estonian-journal-of-earth-sciences\" rel=\"template\"/>\n    <link href=\"https://kirj.ee/trames-style-sheet/?v=04c19fa1e772\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <issn>1406-0922</issn>\n    <eissn>1736-7514</eissn>\n    <updated>2023-03-29T09:18:43+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available online at</term>\n      <term name=\"open-quote\">“</term>\n      <term name=\"close-quote\">”</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"first\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if match=\"any\" variable=\"title-short\">\n            <text variable=\"title-short\"/>\n          </if>\n          <else>\n            <text variable=\"title\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"DOI: https://doi.org/\"/>\n      </if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <text variable=\"URL\"/>\n      </else-if>\n      <else-if variable=\"URL\">\n        <group delimiter=\". \" suffix=\".\">\n          <group delimiter=\" \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n              <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis report patent webpage post post-weblog\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n    <text variable=\"original-title\" prefix=\". [\" suffix=\".]\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <choose>\n      <if match=\"none\" locator=\"page\">\n        <label suffix=\" \" variable=\"locator\" form=\"short\"/>\n      </if>\n    </choose>\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"collection-booklike\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"collection-title\" text-case=\"title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"volume-booklike\">\n    <group delimiter=\" \">\n      <label variable=\"volume\" text-case=\"capitalize-first\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" year-suffix-delimiter=\",\">\n    <sort>\n      <key variable=\"issued\" sort=\"ascending\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture song thesis\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <choose>\n                <if type=\"thesis\" match=\"any\">\n                  <text variable=\"genre\"/>\n                </if>\n              </choose>\n              <text macro=\"volume-booklike\"/>\n              <text macro=\"collection-booklike\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text term=\"in\" suffix=\" \" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n                <group>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <text macro=\"volume-booklike\" prefix=\". \"/>\n                  <text variable=\"page\" prefix=\", \"/>\n                </group>\n              </group>\n              <text macro=\"edition\"/>\n              <text macro=\"collection-booklike\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <text macro=\"title\" suffix=\".\"/>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text variable=\"genre\"/>\n                <text variable=\"number\"/>\n              </group>\n              <text variable=\"container-title\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"patent\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <text term=\"patent\" text-case=\"capitalize-first\"/>\n                <text variable=\"number\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <group delimiter=\" \">\n                <text variable=\"container-title\" form=\"long\" font-style=\"italic\"/>\n                <group delimiter=\", \">\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\"/>\n                  <text variable=\"collection-title\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"