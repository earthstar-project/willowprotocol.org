"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Diatom Research</title>\n    <id>http://www.zotero.org/styles/diatom-research</id>\n    <link href=\"http://www.zotero.org/styles/diatom-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/african-zoology\" rel=\"template\"/>\n    <link href=\"http://www.tandf.co.uk/journals/authors/style/reference/ref_TDIA.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Christof Pearce</name>\n      <email>christofpearce@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"geology\"/>\n    <issn>0269-249X</issn>\n    <eissn>2159-8347</eissn>\n    <updated>2013-12-16T18:17:20+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter\" match=\"any\">\n        <names variable=\"editor\" delimiter=\", \" prefix=\"(Ed. by \" suffix=\")\">\n          <name and=\"symbol\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\" sort-separator=\" \" font-variant=\"small-caps\">\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"genre\" suffix=\", \"/>\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <group prefix=\" \" suffix=\".\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <choose>\n            <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part prefix=\", \" name=\"month\"/>\n                <date-part prefix=\" \" name=\"day\"/>\n              </date>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text prefix=\" (\" term=\"no date\" suffix=\").\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"chapter\" match=\"all\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" prefix=\". \" font-style=\"italic\"/>\n        <text variable=\"volume\" prefix=\" \"/>\n        <text variable=\"page\" prefix=\": \"/>\n      </if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group prefix=\". In: \">\n          <text variable=\"container-title\" font-style=\"italic\" suffix=\" \"/>\n          <text macro=\"container-contributors\" suffix=\", \"/>\n          <text macro=\"secondary-contributors\"/>\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\" prefix=\" \"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued-year\"/>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <text macro=\"issued\" suffix=\" \"/>\n      <text macro=\"edition\"/>\n      <text macro=\"locators\" font-style=\"normal\"/>\n      <group delimiter=\". \" prefix=\". \">\n        <text macro=\"publisher\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"