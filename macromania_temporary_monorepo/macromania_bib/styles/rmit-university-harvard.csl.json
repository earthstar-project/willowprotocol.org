"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>RMIT University - Harvard</title>\n    <id>http://www.zotero.org/styles/rmit-university-harvard</id>\n    <link href=\"http://www.zotero.org/styles/rmit-university-harvard\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-deakin-university\" rel=\"template\"/>\n    <link href=\"https://www.lib.rmit.edu.au/easy-cite/\" rel=\"documentation\"/>\n    <author>\n      <name>Katie Finch</name>\n      <email>library.systems@rmit.edu.au</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <updated>2020-06-24T03:04:07+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">\n        <single>trans.</single>\n        <multiple>trans</multiple>\n      </term>\n      <term name=\"edition\" form=\"short\">edn</term>\n      <term name=\"volume\" form=\"short\">vol.</term>\n      <term name=\"chapter\" form=\"short\">ch.</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"symbol\" initialize-with=\"\" delimiter=\", \"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"noauthor_title\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <names variable=\"author\">\n          <name/>\n          <substitute>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name/>\n          <substitute>\n            <choose>\n              <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n                <text variable=\"title\" strip-periods=\"true\" font-style=\"italic\"/>\n              </if>\n              <else>\n                <text variable=\"title\" quotes=\"true\"/>\n              </else>\n            </choose>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"noauthor_title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"noauthor_title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\" type=\"article-newspaper webpage speech report\">\n        <text value=\"viewed\"/>\n        <group prefix=\" \" delimiter=\", \">\n          <date variable=\"accessed\" delimiter=\" \">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\" suffix=\",\"/>\n          </date>\n          <text variable=\"URL\" prefix=\" &lt;\" suffix=\"&gt;\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"newspaper-webpage\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group prefix=\" \" delimiter=\", \">\n          <date variable=\"issued\" prefix=\"\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\"\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"presentation\">\n    <choose>\n      <if type=\"speech\">\n        <group delimiter=\", \">\n          <!-- suffix=\", \"> -->\n          <text variable=\"genre\"/>\n          <!-- <text term=\"presented at\"/> -->\n          <text variable=\"event\"/>\n          <text variable=\"event-place\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"report-number\">\n    <choose>\n      <if type=\"report\">\n        <text variable=\"number\" suffix=\",\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" strip-periods=\"false\" font-style=\"italic\" suffix=\",\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <label suffix=\" \" variable=\"page\" form=\"short\"/>\n    <text variable=\"page\"/>\n    <choose>\n      <if match=\"none\" variable=\"page\">\n        <text variable=\"page-first\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" strip-periods=\"false\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <date variable=\"issued\" prefix=\" \" suffix=\",\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"report-number\"/>\n            <text macro=\"edition\" suffix=\",\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text prefix=\" \" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group delimiter=\" \" prefix=\", \">\n            <text term=\"in\"/>\n            <group delimiter=\", \">\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"collection-title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\",\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group prefix=\", \" delimiter=\", \">\n              <text variable=\"volume\" prefix=\"vol. \"/>\n              <text variable=\"issue\" prefix=\"no. \"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\", \" macro=\"presentation\"/>\n      <text prefix=\", \" macro=\"newspaper-webpage\"/>\n      <text prefix=\", \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"