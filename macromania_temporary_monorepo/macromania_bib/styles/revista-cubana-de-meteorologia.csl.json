"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\">\n  <info>\n    <title>Revista Cubana de Meteorologia</title>\n    <title-short>Rev. Cub. Met.</title-short>\n    <id>http://www.zotero.org/styles/revista-cubana-de-meteorologia</id>\n    <link href=\"http://www.zotero.org/styles/revista-cubana-de-meteorologia\" rel=\"self\"/>\n    <link href=\"http://rcm.insmet.cu/index.php/rcm/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Rafael Cervantes Beyra</name>\n      <email>cervantes@unah.edu.cu</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"engineering\"/>\n    <issn>0864-151x</issn>\n    <updated>2015-06-16T20:22:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"anonymous\">Anon.</term>\n      <term name=\"no date\">n.d.</term>\n      <term name=\"retrieved\">Available</term>\n      <term name=\"accessed\">Consulted</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <substitute>\n        <names variable=\"editor\">\n          <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\").\"/>\n        </names>\n        <names variable=\"collection-editor\" suffix=\" (ed. ser.).\">\n          <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"never\"/>\n          <label/>\n        </names>\n        <names variable=\"translator\">\n          <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\").\"/>\n        </names>\n        <names variable=\"container-author\">\n          <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" prefix=\" \"/>\n        </names>\n        <names variable=\"interviewer\" suffix=\" (interv.).\">\n          <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"never\"/>\n          <label/>\n        </names>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if variable=\"author editor translator collection-editor interviewer\" match=\"any\">\n        <names variable=\"author\">\n          <name form=\"short\" and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n          <et-al font-style=\"italic\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <names variable=\"collection-editor\"/>\n            <names variable=\"interviewer\"/>\n          </substitute>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\" suffix=\" (interv.)\">\n      <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label/>\n    </names>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"collection-editor\">\n    <names variable=\"collection-editor\" suffix=\" (ed. ser.)\">\n      <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\" prefix=\" \">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-newspaper paper-conference post-weblog\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <text variable=\"event\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book post webpage interview report patent thesis\" match=\"any\">\n        <text variable=\"title\" prefix=\". \" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"article-journal article-magazine article-newspaper chapter entry-encyclopedia entry entry-dictionary paper-conference post-weblog\" match=\"any\">\n        <text variable=\"title\" suffix=\"”\" prefix=\". “\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" prefix=\". \" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <text variable=\"container-title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"responsability\">\n    <choose>\n      <if variable=\"author editor translator collection-editor interviewer\" match=\"any\">\n        <group delimiter=\"; \" suffix=\".\">\n          <text macro=\"author\"/>\n          <text macro=\"editor\"/>\n          <text macro=\"translator\"/>\n          <text macro=\"collection-editor\"/>\n          <text macro=\"interviewer\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n  </macro>\n  <macro name=\"genre\">\n    <text variable=\"genre\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group>\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" prefix=\" \"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n        <text term=\"edition\" form=\"short\" prefix=\" \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if type=\"book paper-conference chapter entry-encyclopedia entry-dictionary entry post-weblog map patent legislation report thesis\">\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n          <text variable=\"authority\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if type=\"article-journal article-magazine\">\n        <group>\n          <text variable=\"volume\"/>\n          <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          <text variable=\"page\" prefix=\": \"/>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <label variable=\"page\" form=\"short\" suffix=\" \"/>\n        <text variable=\"page\"/>\n      </else-if>\n      <else>\n        <text term=\"volume\" form=\"short\" suffix=\" \"/>\n        <number variable=\"volume\" form=\"numeric\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"number-of-volumes\">\n    <text term=\"volume\" form=\"short\" plural=\"true\" suffix=\". \"/>\n    <text variable=\"number-of-volumes\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\" [\" suffix=\"]\" delimiter=\":&#160;\">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"day\" suffix=\", \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"collection-title\" prefix=\"ser. \"/>\n      <text variable=\"collection-number\" prefix=\"no. ser. \"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper post-weblog\" match=\"any\">\n        <text variable=\"page\" prefix=\": \"/>\n      </if>\n      <else-if type=\"report\" match=\"any\">\n        <text variable=\"page\" suffix=\" p.\"/>\n      </else-if>\n      <else>\n        <label variable=\"page\" form=\"short\" suffix=\" \"/>\n        <text variable=\"page\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <text variable=\"number-of-pages\"/>\n    <text term=\"number-of-pages\" form=\"short\" prefix=\" \"/>\n  </macro>\n  <macro name=\"isbn\">\n    <text variable=\"ISBN\" prefix=\"ISBN: \"/>\n  </macro>\n  <macro name=\"issn\">\n    <text variable=\"ISSN\" prefix=\"ISSN: \"/>\n  </macro>\n  <macro name=\"doi\">\n    <text variable=\"DOI\" prefix=\"DOI: \"/>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n          <text variable=\"URL\" prefix=\": &lt;\" suffix=\"&gt;\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <text variable=\"number\" prefix=\"no. \"/>\n  </macro>\n  <macro name=\"authority\">\n    <text variable=\"authority\"/>\n  </macro>\n  <macro name=\"version\">\n    <text variable=\"version\" prefix=\"version \"/>\n  </macro>\n  <macro name=\"scale\">\n    <choose>\n      <if variable=\"scale\">\n        <text variable=\"scale\" prefix=\"[\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography line-spacing=\"1\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"book interview manuscript map patent legislation legal_case report thesis\" match=\"any\">\n          <group suffix=\". \">\n            <text macro=\"author\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"container-author\"/>\n            <text macro=\"translator\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"collection-editor\"/>\n            <text variable=\"section\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"scale\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"version\"/>\n            <text macro=\"medium\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"volume\"/>\n            <text variable=\"container-title\" prefix=\"cod. \"/>\n            <text macro=\"number-of-volumes\"/>\n            <text macro=\"number\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"number-of-pages\"/>\n            <text macro=\"page\"/>\n            <text macro=\"isbn\"/>\n            <text macro=\"url\"/>\n            <text macro=\"accessed\"/>\n          </group>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper post-weblog\">\n          <group suffix=\". \">\n            <text macro=\"author\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"container-title\"/>\n            <text macro=\"date\"/>\n            <text variable=\"section\" prefix=\"sect. \"/>\n            <text macro=\"edition\"/>\n            <text macro=\"volume\"/>\n            <text variable=\"publisher-place\"/>\n            <text macro=\"issn\"/>\n            <text macro=\"doi\"/>\n            <text macro=\"url\"/>\n            <text macro=\"accessed\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference chapter entry-encyclopedia entry-dictionary entry\">\n          <group suffix=\". \">\n            <text macro=\"author\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n          </group>\n          <group delimiter=\", \" prefix=\"In: \">\n            <text macro=\"container-author\"/>\n            <text macro=\"translator\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"collection-editor\"/>\n            <text macro=\"container-title\"/>\n            <text macro=\"event\"/>\n            <text macro=\"date\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"number-of-volumes\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"page\"/>\n            <text macro=\"isbn\"/>\n            <text macro=\"doi\"/>\n            <text macro=\"url\"/>\n            <text macro=\"accessed\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\". \">\n            <text macro=\"responsability\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n          </group>\n          <group delimiter=\", \">\n            <text variable=\"section\"/>\n            <text macro=\"event\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"scale\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"version\"/>\n            <text macro=\"medium\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"volume\"/>\n            <text variable=\"container-title\"/>\n            <text macro=\"date\"/>\n            <text macro=\"number-of-volumes\"/>\n            <text macro=\"number\"/>\n            <text macro=\"authority\"/>\n            <text variable=\"publisher-place\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"number-of-pages\"/>\n            <text macro=\"page\"/>\n            <text macro=\"isbn\"/>\n            <text macro=\"url\"/>\n            <text macro=\"accessed\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"