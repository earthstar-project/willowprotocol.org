"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Financial and Quantitative Analysis</title>\n    <title-short>JFQA</title-short>\n    <id>http://www.zotero.org/styles/journal-of-financial-and-quantitative-analysis</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-financial-and-quantitative-analysis\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/geophysics\" rel=\"template\"/>\n    <link href=\"https://jfqa.org/submissions/style-guide-for-accepted-and-conditionally-accepted-papers/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <issn>0022-1090</issn>\n    <eissn>1756-6916</eissn>\n    <updated>2021-02-12T13:25:22+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n      <label form=\"long\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\". \" and=\"text\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if variable=\"edition\">\n        <text variable=\"container-title\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"cite-locator\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if is-numeric=\"volume\">\n        <group delimiter=\" \">\n          <text value=\"Vol. \"/>\n          <number variable=\"volume\" suffix=\".\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"volume\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"false\" disambiguate-add-givenname=\"false\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n        </group>\n        <text macro=\"cite-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text variable=\"title\" text-case=\"title\" quotes=\"true\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group suffix=\":\">\n              <group delimiter=\", \">\n                <text macro=\"edition\"/>\n              </group>\n              <text macro=\"editor\" prefix=\" (\" suffix=\")\"/>\n            </group>\n            <text macro=\"publisher\" prefix=\" \"/>\n          </if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n              <text variable=\"number-of-pages\"/>\n            </group>\n            <text term=\"page\" form=\"short\"/>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"container-title\" text-case=\"title\"/>\n              </group>\n              <text variable=\"collection-title\" prefix=\". \"/>\n              <text macro=\"edition\"/>\n              <text macro=\"volume\"/>\n              <group delimiter=\" \">\n                <group delimiter=\". \">\n                  <text macro=\"editor\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n                <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <text variable=\"URL\" prefix=\"Available at \"/>\n              <date variable=\"accessed\" prefix=\"Accessed \">\n                <date-part name=\"month\" suffix=\" \"/>\n                <date-part name=\"day\" suffix=\", \"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n            <text prefix=\" \" macro=\"publisher\"/>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n              <group delimiter=\" \">\n                <text variable=\"volume\"/>\n                <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"