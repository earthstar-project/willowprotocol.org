"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"minimal\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\">\n  <info>\n    <title>Intellect Newgen Books</title>\n    <id>http://www.zotero.org/styles/intellect-newgen-books</id>\n    <link href=\"http://www.zotero.org/styles/intellect-newgen-books\" rel=\"self\"/>\n    <link href=\"https://www.intellectbooks.com/journal-editors-and-contributors#citation-style-language\" rel=\"documentation\"/>\n    <link href=\"https://www.intellectbooks.com/asset/1255/house-style-guide-5th-ed-2021.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>amyrolla</name>\n      <uri>https://github.com/amyrolla</uri>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <summary>Style for Intellect Newgen Books based on the 5th edition of the style guide.</summary>\n    <updated>2022-11-17T11:22:54+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <label variable=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <choose>\n      <if variable=\"issued original-date\" match=\"any\">\n        <group delimiter=\" \">\n          <date form=\"numeric\" variable=\"original-date\" prefix=\"[\" suffix=\"]\"/>\n          <date date-parts=\"year\" form=\"numeric\" variable=\"issued\"/>\n        </group>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anonymous\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-sort\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter-precedes-last=\"never\" et-al-min=\"2\" et-al-use-first=\"1\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anonymous\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </if>\n      <else-if type=\"interview\" match=\"any\"/>\n      <else>\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </else>\n    </choose>\n    <text macro=\"translator-editor\" prefix=\" (\" suffix=\")\"/>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n          <group delimiter=\":\">\n            <text variable=\"volume\" font-weight=\"normal\"/>\n            <text variable=\"issue\"/>\n          </group>\n          <group>\n            <choose>\n              <if type=\"post post-weblog\" match=\"any\">\n                <group>\n                  <date delimiter=\"\" variable=\"issued\">\n                    <date-part name=\"day\" suffix=\" \"/>\n                    <date-part name=\"month\"/>\n                  </date>\n                </group>\n              </if>\n              <else-if type=\"song\" match=\"any\">\n                <text value=\"CD\"/>\n              </else-if>\n            </choose>\n          </group>\n          <choose>\n            <if type=\"report song\" match=\"any\">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n          <date variable=\"issued\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\"/>\n          </date>\n          <text macro=\"pages\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference entry-dictionary\" match=\"any\">\n        <group delimiter=\", \">\n          <choose>\n            <if type=\"chapter entry-dictionary entry-encyclopedia\" match=\"any\">\n              <choose>\n                <if variable=\"editor translator\" match=\"any\">\n                  <group delimiter=\", \">\n                    <group delimiter=\" \">\n                      <text term=\"in\"/>\n                      <names variable=\"editor\" delimiter=\", \">\n                        <name and=\"text\" initialize-with=\". \"/>\n                        <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n                        <substitute>\n                          <names variable=\"translator\"/>\n                        </substitute>\n                      </names>\n                      <choose>\n                        <if variable=\"original-date\">\n                          <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n                            <date-part name=\"year\"/>\n                          </date>\n                        </if>\n                      </choose>\n                    </group>\n                    <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n                  </group>\n                </if>\n                <else>\n                  <group delimiter=\" \">\n                    <choose>\n                      <if variable=\"original-date\">\n                        <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n                          <date-part name=\"year\"/>\n                        </date>\n                      </if>\n                    </choose>\n                  </group>\n                  <group>\n                    <choose>\n                      <if type=\"entry-encyclopedia\" match=\"any\">\n                        <group>\n                          <text value=\"15th\" suffix=\" \"/>\n                          <label variable=\"edition\" form=\"short\"/>\n                        </group>\n                      </if>\n                      <else-if type=\"entry-dictionary\" match=\"any\">\n                        <group>\n                          <text value=\"16th\" suffix=\" \"/>\n                          <label variable=\"edition\" form=\"short\"/>\n                        </group>\n                      </else-if>\n                    </choose>\n                  </group>\n                </else>\n              </choose>\n            </if>\n            <else>\n              <choose>\n                <if variable=\"original-date\">\n                  <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n                    <date-part name=\"year\"/>\n                  </date>\n                </if>\n              </choose>\n            </else>\n          </choose>\n          <text macro=\"edition\"/>\n          <text variable=\"medium\"/>\n          <group delimiter=\" \">\n            <choose>\n              <if variable=\"event\">\n                <choose>\n                  <if type=\"paper-conference\" match=\"none\">\n                    <group delimiter=\" \">\n                      <text variable=\"genre\"/>\n                      <text term=\"presented at\"/>\n                      <text variable=\"event\"/>\n                    </group>\n                  </if>\n                </choose>\n              </if>\n              <else>\n                <text variable=\"genre\"/>\n                <choose>\n                  <if variable=\"number\">\n                    <text term=\"number\" form=\"short\" suffix=\" \"/>\n                    <text variable=\"number\"/>\n                  </if>\n                </choose>\n              </else>\n            </choose>\n          </group>\n          <text macro=\"volume\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"pages\"/>\n        </group>\n      </else-if>\n      <else-if type=\"interview\" match=\"any\">\n        <group delimiter=\", \">\n          <names variable=\"interviewer\">\n            <label form=\"verb\"/>\n            <name prefix=\" \"/>\n          </names>\n          <text variable=\"container-title\"/>\n          <text variable=\"medium\"/>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"container-title\"/>\n          <text macro=\"edition\"/>\n          <text macro=\"volume\"/>\n          <text macro=\"publisher\"/>\n          <text variable=\"collection-title\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"interview\" match=\"any\">\n        <date delimiter=\" \" variable=\"issued\">\n          <date-part name=\"day\"/>\n          <date-part name=\"month\"/>\n        </date>\n      </if>\n      <else-if type=\"graphic pamphlet\" match=\"any\">\n        <date delimiter=\"\" variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </else-if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <group delimiter=\"&#8211;\">\n          <date delimiter=\"\" variable=\"issued\">\n            <date-part name=\"day\"/>\n          </date>\n          <date delimiter=\"\" variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\" prefix=\" \"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"personal_communication\" match=\"any\">\n        <date delimiter=\"\" variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <group delimiter=\". \">\n          <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n      <else-if variable=\"URL\">\n        <group delimiter=\". \">\n          <text variable=\"URL\"/>\n          <group>\n            <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"anonymous\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <choose>\n        <if match=\"any\" variable=\"publisher-place\">\n          <text variable=\"publisher-place\"/>\n        </if>\n        <else>\n          <text term=\"no-place\" form=\"short\"/>\n        </else>\n      </choose>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"translator-editor\">\n    <names variable=\"editor translator\" delimiter=\"; \">\n      <label form=\"short\"/>\n      <name prefix=\" \" and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author-sort\"/>\n      <key macro=\"year\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-sort\"/>\n          <text macro=\"year\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"author-sort\"/>\n      <key macro=\"year\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author\"/>\n          <text macro=\"year\" prefix=\"(\" suffix=\")\"/>\n        </group>\n        <text macro=\"title\"/>\n        <text macro=\"locators\"/>\n        <text macro=\"issued\"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"