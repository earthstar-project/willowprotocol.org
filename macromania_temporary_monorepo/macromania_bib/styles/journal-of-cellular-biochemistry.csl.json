"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Cellular Biochemistry</title>\n    <title-short>JCB</title-short>\n    <id>http://www.zotero.org/styles/journal-of-cellular-biochemistry</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-cellular-biochemistry\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/developmental-neurobiology\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1002/(ISSN)1097-4644\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"medicine\"/>\n    <issn>0730-2312</issn>\n    <eissn>1097-4644</eissn>\n    <updated>2018-03-05T16:24:28+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" initialize-with=\"\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\":\" suffix=\".\">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"volume page\">\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" et-al-min=\"3\" et-al-use-first=\"1\" collapse=\"year\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"issued\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\"[\" suffix=\"]\">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\" suffix=\".\"/>\n        <text macro=\"issued\" suffix=\".\"/>\n        <choose>\n          <if type=\"book\">\n            <text variable=\"title\" suffix=\".\" text-case=\"title\"/>\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <text variable=\"number-of-pages\"/>\n              <label variable=\"page\" form=\"short\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"container-title\" text-case=\"title\"/>\n              <text variable=\"edition\" suffix=\"e.\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <group delimiter=\" \">\n              <label plural=\"never\" strip-periods=\"true\" variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"event\" prefix=\"In: \" suffix=\",\"/>\n            <text variable=\"event-place\"/>\n          </else-if>\n          <else-if type=\"article-journal\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n            </group>\n            <group delimiter=\":\" prefix=\" \">\n              <group>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post-weblog\" match=\"any\">\n            <text variable=\"container-title\" suffix=\".\"/>\n            <text variable=\"URL\" suffix=\".\"/>\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </else-if>\n          <else>\n            <text variable=\"container-title\"/>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"