"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" default-locale=\"fr-FR\" page-range-format=\"expanded\">\n  <info>\n    <title>Zilsel (Français)</title>\n    <title-short>Zilsel</title-short>\n    <id>http://www.zotero.org/styles/zilsel</id>\n    <link href=\"http://www.zotero.org/styles/zilsel\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/revue-francaise-de-sociologie\" rel=\"template\"/>\n    <link href=\"https://www.cairn.info/revue-zilsel.htm\" rel=\"documentation\"/>\n    <author>\n      <name>Sébastien Plutniak</name>\n      <email>sebastien.plutniak@posteo.net</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"social_science\"/>\n    <issn>2551-8313</issn>\n    <eissn>2553-6133</eissn>\n    <summary>Style for the Zilsel journal.</summary>\n    <updated>2019-07-30T18:30:00+01:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>(dir.)</single>\n        <multiple>(dir.)</multiple>\n      </term>\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>p.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>(ed.)</single>\n        <multiple>(eds.)</multiple>\n      </term>\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>p.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <!-- Macros definition -->\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\" \">\n      <name and=\"text\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\" initialize=\"false\"/>\n      <label form=\"short\" prefix=\" \" suffix=\" \" text-case=\"lowercase\"/>\n    </names>\n  </macro>\n  <macro name=\"author-last\">\n    <names variable=\"author\">\n      <name and=\"text\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\" initialize=\"false\"/>\n      <label form=\"short\" prefix=\" \" text-case=\"lowercase\"/>\n      <substitute>\n        <names variable=\"editor\">\n          <name and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\" initialize=\"false\"/>\n          <label form=\"short\" prefix=\" \" suffix=\"\" text-case=\"lowercase\"/>\n        </names>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date-short\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <group>\n      <choose>\n        <if variable=\"locator\"/>\n        <else>\n          <label variable=\"page\" suffix=\".&#160;\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\" suffix=\".&#160;\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <!-- End of macros definition -->\n  <!-- Format of in-line references -->\n  <citation>\n    <sort>\n      <key macro=\"author-last\"/>\n      <key macro=\"year-date-short\"/>\n    </sort>\n    <layout>\n      <text macro=\"author-last\" suffix=\", \"/>\n      <choose>\n        <if type=\"book\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\" text-case=\"capitalize-first\" quotes=\"false\"/>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date-short\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </if>\n        <else-if type=\"article article-newspaper article-magazine\" match=\"any\">\n          <group delimiter=\" \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"container-title-short\" font-style=\"italic\" prefix=\" \"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"page\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"false\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"number\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n            <text macro=\"year-date-short\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <choose>\n              <if variable=\"genre\">\n                <text variable=\"genre\"/>\n              </if>\n              <else>\n                <text value=\"PhD. thesis\"/>\n              </else>\n            </choose>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" suffix=\", \" quotes=\"true\"/>\n          <group delimiter=\" \">\n            <text value=\"in\" prefix=\"\" suffix=\" \" font-style=\"italic\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"editor\" suffix=\"\"/>\n            <text variable=\"container-title\" suffix=\"\" font-style=\"italic\"/>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date-short\"/>\n            <text macro=\"page\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else-if>\n        <else>\n          <text variable=\"title\" font-style=\"italic\" suffix=\", \" quotes=\"true\"/>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"volume\" prefix=\"vol.&#160;\"/>\n            <text variable=\"issue\" prefix=\"no&#160;\"/>\n            <text macro=\"year-date-short\"/>\n            <text macro=\"page\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <!-- Zilsel don't use final references list -->\n  <bibliography>\n    <sort>\n      <key macro=\"author-last\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <text macro=\"author-last\" suffix=\", \"/>\n      <choose>\n        <if type=\"book\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\" text-case=\"capitalize-first\" quotes=\"false\"/>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date-short\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </if>\n        <else-if type=\"article article-newspaper article-magazine\" match=\"any\">\n          <group delimiter=\" \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"container-title-short\" font-style=\"italic\" prefix=\" \"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"page\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"false\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"number\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n            <text macro=\"year-date-short\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <choose>\n              <if variable=\"genre\">\n                <text variable=\"genre\"/>\n              </if>\n              <else>\n                <text value=\"PhD. thesis\"/>\n              </else>\n            </choose>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" suffix=\", \" quotes=\"true\"/>\n          <group delimiter=\" \">\n            <text value=\"in\" prefix=\"\" suffix=\" \" font-style=\"italic\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"editor\" suffix=\"\"/>\n            <text variable=\"container-title\" suffix=\"\" font-style=\"italic\"/>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date-short\"/>\n            <text macro=\"page\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else-if>\n        <else>\n          <text variable=\"title\" font-style=\"italic\" suffix=\", \" quotes=\"true\"/>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"volume\" prefix=\"vol.&#160;\"/>\n            <text variable=\"issue\" prefix=\"no&#160;\"/>\n            <text macro=\"year-date-short\"/>\n            <text macro=\"page\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"