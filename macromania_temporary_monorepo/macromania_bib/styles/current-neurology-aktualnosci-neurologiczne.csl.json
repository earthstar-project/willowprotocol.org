"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Current Neurology - Aktualno≈õci Neurologiczne</title>\n    <id>http://www.zotero.org/styles/current-neurology-aktualnosci-neurologiczne</id>\n    <link href=\"http://www.zotero.org/styles/current-neurology-aktualnosci-neurologiczne\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/health-sports-and-rehabilitation-medicine\" rel=\"template\"/>\n    <link href=\"http://neurologia.com.pl/index.php/instructions-for-authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <issn>2451-0696</issn>\n    <updated>2022-01-18T11:24:26+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-et-al=\"never\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\" \">\n      <group delimiter=\", \">\n        <text variable=\"publisher-place\" text-case=\"title\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <choose>\n          <if match=\"none\" variable=\"page volume\">\n            <text variable=\"DOI\" prefix=\"DOI: \"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"webpage post-weblog\" match=\"any\">\n        <choose>\n          <if variable=\"URL\">\n            <text variable=\"URL\"/>\n            <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n              <text term=\"accessed\"/>\n              <date variable=\"accessed\" form=\"text\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\" sort=\"descending\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\" sort=\"descending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\": \">\n        <text macro=\"author\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text macro=\"volume\"/>\n              <group delimiter=\", \">\n                <text macro=\"edition\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group delimiter=\": \">\n                <group>\n                  <text term=\"in\" text-case=\"sentence\" suffix=\": \"/>\n                  <text macro=\"editor\"/>\n                </group>\n                <text variable=\"container-title\" form=\"short\"/>\n              </group>\n              <text macro=\"edition\"/>\n              <text macro=\"volume\" prefix=\"vol. \"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"patent\">\n            <group delimiter=\", \">\n              <group delimiter=\". \">\n                <text variable=\"title\"/>\n                <text variable=\"number\"/>\n              </group>\n              <text macro=\"year-date\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"genre\"/>\n              <group delimiter=\", \">\n                <text variable=\"publisher\"/>\n                <text macro=\"year-date\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\": \">\n              <group delimiter=\" \">\n                <group delimiter=\". \">\n                  <text variable=\"title\"/>\n                  <text variable=\"container-title\" form=\"short\" text-case=\"title\" strip-periods=\"true\"/>\n                </group>\n                <group delimiter=\"; \">\n                  <text macro=\"year-date\"/>\n                  <text variable=\"volume\"/>\n                </group>\n              </group>\n              <text variable=\"page\" form=\"short\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"