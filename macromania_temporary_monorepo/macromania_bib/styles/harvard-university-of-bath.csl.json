"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>University of Bath - Harvard</title>\n    <id>http://www.zotero.org/styles/harvard-university-of-bath</id>\n    <link href=\"http://www.zotero.org/styles/harvard-university-of-bath\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-bournemouth-university\" rel=\"template\"/>\n    <link href=\"https://library.bath.ac.uk/referencing/harvard-bath\" rel=\"documentation\"/>\n    <link href=\"https://github.com/alex-ball/bathbib/\" rel=\"documentation\"/>\n    <author>\n      <name>Alex Ball</name>\n      <email>ab318@bath.ac.uk</email>\n    </author>\n    <contributor>\n      <name>Rob Fuller</name>\n      <email>rob.fuller1@gmail.com</email>\n    </contributor>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Adaptation of Harvard referencing style used at the University of Bath.</summary>\n    <updated>2024-02-01T09:39:05+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n      <term name=\"chapter\" form=\"short\">\n        <single>c.</single>\n        <multiple>cc.</multiple>\n      </term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"number\" form=\"long\">number</term>\n      <term name=\"number\" form=\"short\">no.</term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"version\" form=\"short\">v.</term>\n    </terms>\n  </locale>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL DOI accessed\" match=\"any\">\n        <text term=\"available at\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <text variable=\"URL\"/>\n          </else-if>\n          <else>\n            <text variable=\"archive\"/>\n          </else>\n        </choose>\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <choose>\n      <if variable=\"archive\">\n        <choose>\n          <if type=\"graphic manuscript map pamphlet\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <text variable=\"archive_collection\"/>\n                <text variable=\"archive_location\"/>\n              </group>\n              <group delimiter=\", \">\n                <text variable=\"archive\"/>\n                <text variable=\"archive-place\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"broadcast motion_picture song\" match=\"any\"/>\n          <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n            <choose>\n              <if variable=\"container-title\" match=\"none\">\n                <group delimiter=\" \">\n                  <text variable=\"archive\" font-style=\"italic\"/>\n                  <text macro=\"online\"/>\n                </group>\n              </if>\n            </choose>\n          </else-if>\n          <else>\n            <group delimiter=\" \">\n              <text variable=\"archive\" font-style=\"italic\"/>\n              <choose>\n                <if type=\"article bill report standard webpage\" match=\"any\">\n                  <text macro=\"online\"/>\n                </if>\n              </choose>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <substitute>\n        <choose>\n          <if type=\"entry\" variable=\"title container-title\">\n            <text variable=\"container-title\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <names variable=\"editor\">\n              <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n              <label form=\"short\" prefix=\", \" text-case=\"lowercase\"/>\n            </names>\n          </else-if>\n          <else>\n            <text macro=\"title-label\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n      <substitute>\n        <choose>\n          <if type=\"entry\"/>\n          <else>\n            <names variable=\"editor\"/>\n          </else>\n        </choose>\n        <text macro=\"title-short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"broadcast\">\n    <group delimiter=\". \">\n      <group delimiter=\" \">\n        <group delimiter=\", \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <text variable=\"number\" font-style=\"italic\"/>\n        </group>\n        <text macro=\"online\"/>\n      </group>\n      <text variable=\"genre\"/>\n      <group delimiter=\", \">\n        <text variable=\"publisher\"/>\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"case-law\">\n    <choose>\n      <if variable=\"number\">\n        <group suffix=\".\" delimiter=\" \">\n          <text macro=\"author\"/>\n          <text variable=\"number\" prefix=\"(\" suffix=\")\"/>\n          <text macro=\"year-date\" prefix=\"[\" suffix=\"]\"/>\n          <text macro=\"title\"/>\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group>\n                <text variable=\"collection-title\"/>\n                <group delimiter=\"/\">\n                  <text variable=\"volume\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </if>\n            <else>\n              <text variable=\"collection-title\"/>\n              <group delimiter=\"&#8211;\">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <group suffix=\".\" delimiter=\". \">\n          <text macro=\"author\"/>\n          <choose>\n            <if variable=\"volume\">\n              <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            </if>\n            <else>\n              <text macro=\"year-date\" prefix=\"[\" suffix=\"]\"/>\n            </else>\n          </choose>\n          <text macro=\"title\"/>\n          <choose>\n            <if variable=\"container-title\">\n              <group delimiter=\" \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <group>\n                  <text variable=\"collection-title\"/>\n                  <group delimiter=\"/\">\n                    <text variable=\"volume\"/>\n                    <text variable=\"page\"/>\n                  </group>\n                </group>\n              </group>\n            </if>\n            <else>\n              <text variable=\"volume\"/>\n              <group delimiter=\" \">\n                <text variable=\"collection-title\"/>\n                <text variable=\"page\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if variable=\"container-title\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <choose>\n              <if type=\"bill book chapter musical_score paper-conference report speech standard webpage\" match=\"any\">\n                <choose>\n                  <if variable=\"editor\">\n                    <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n                    <names variable=\"editor\">\n                      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n                      <label form=\"short\" prefix=\", \" suffix=\".\" text-case=\"lowercase\"/>\n                    </names>\n                  </if>\n                </choose>\n              </if>\n            </choose>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <choose>\n              <if type=\"entry\" variable=\"title\"/>\n              <else>\n                <text macro=\"online\"/>\n              </else>\n            </choose>\n          </group>\n          <choose>\n            <if type=\"paper-conference\">\n              <text variable=\"event\"/>\n              <date variable=\"event-date\" form=\"text\"/>\n              <text variable=\"event-place\"/>\n            </if>\n          </choose>\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"credits\">\n    <group delimiter=\". \">\n      <choose>\n        <if type=\"bill book chapter musical_score paper-conference report speech standard webpage\" match=\"any\">\n          <choose>\n            <if variable=\"container-title\" match=\"none\">\n              <names variable=\"editor\">\n                <label form=\"verb-short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n              </names>\n            </if>\n          </choose>\n        </if>\n        <else>\n          <names variable=\"editor\">\n            <label form=\"verb-short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n          </names>\n        </else>\n      </choose>\n      <names variable=\"composer\">\n        <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <name and=\"text\" delimiter-precedes-last=\"never\" initialize=\"false\" initialize-with=\".\"/>\n      </names>\n      <names variable=\"director\">\n        <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <name and=\"text\" delimiter-precedes-last=\"never\" initialize=\"false\" initialize-with=\".\"/>\n      </names>\n      <names variable=\"interviewer\">\n        <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <name and=\"text\" delimiter-precedes-last=\"never\" initialize=\"false\" initialize-with=\".\"/>\n      </names>\n      <names variable=\"illustrator\">\n        <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <name and=\"text\" delimiter-precedes-last=\"never\" initialize=\"false\" initialize-with=\".\"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"journal\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n        <choose>\n          <if type=\"article\">\n            <choose>\n              <if variable=\"archive\" match=\"none\">\n                <text macro=\"online\"/>\n              </if>\n            </choose>\n          </if>\n          <else>\n            <text macro=\"online\"/>\n          </else>\n        </choose>\n      </group>\n      <group>\n        <text variable=\"volume\"/>\n        <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n      </group>\n      <choose>\n        <if type=\"article-magazine article-newspaper\" match=\"any\">\n          <date variable=\"issued\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\"/>\n          </date>\n        </if>\n      </choose>\n      <group>\n        <label variable=\"page\" form=\"short\"/>\n        <text variable=\"page\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if variable=\"URL DOI accessed\" match=\"any\">\n        <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publication\">\n    <choose>\n      <if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n        <choose>\n          <if variable=\"container-title\">\n            <text macro=\"journal\"/>\n          </if>\n          <else>\n            <text macro=\"status\"/>\n            <text macro=\"publisher\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"broadcast\">\n        <text macro=\"broadcast\"/>\n      </else-if>\n      <else>\n        <group delimiter=\". \">\n          <text macro=\"container\"/>\n          <choose>\n            <if type=\"motion_picture song\" match=\"any\">\n              <choose>\n                <if variable=\"medium\">\n                  <text variable=\"medium\"/>\n                </if>\n                <else>\n                  <text variable=\"genre\"/>\n                </else>\n              </choose>\n            </if>\n          </choose>\n          <text macro=\"credits\"/>\n          <choose>\n            <if type=\"entry\" match=\"none\">\n              <text macro=\"edition\"/>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"container-title\" match=\"none\">\n              <group>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n            </if>\n          </choose>\n          <choose>\n            <if type=\"report bill\" match=\"any\">\n              <text macro=\"series-genre\" prefix=\"(\" suffix=\")\"/>\n            </if>\n            <else-if type=\"thesis patent\" match=\"any\">\n              <text macro=\"series-genre\"/>\n            </else-if>\n            <else>\n              <text macro=\"series\"/>\n            </else>\n          </choose>\n          <text macro=\"status\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <group>\n        <label variable=\"page\" form=\"short\"/>\n        <text variable=\"page\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <choose>\n      <if variable=\"collection-title\">\n        <text variable=\"collection-title\"/>\n        <text variable=\"number\" prefix=\", \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"series-genre\">\n    <choose>\n      <if variable=\"collection-title genre number\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"collection-title\"/>\n          <group delimiter=\" \">\n            <text variable=\"genre\"/>\n            <text variable=\"number\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"status\">\n    <text variable=\"status\"/>\n  </macro>\n  <macro name=\"statute-law\">\n    <group delimiter=\" \">\n      <text macro=\"author\"/>\n      <choose>\n        <if variable=\"container-title\">\n          <text macro=\"year-date\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n        <else>\n          <text macro=\"year-date\" font-style=\"italic\"/>\n        </else>\n      </choose>\n      <text macro=\"title\"/>\n      <choose>\n        <if type=\"regulation\">\n          <text macro=\"online\"/>\n        </if>\n      </choose>\n    </group>\n    <choose>\n      <if variable=\"container-title\">\n        <group prefix=\" \" delimiter=\" \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <group>\n            <text variable=\"collection-title\"/>\n            <group delimiter=\"/\">\n              <number variable=\"volume\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </group>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"collection-title\">\n            <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n              <text variable=\"collection-title\"/>\n              <group>\n                <text term=\"chapter\" form=\"short\"/>\n                <number variable=\"chapter-number\"/>\n              </group>\n            </group>\n          </if>\n          <else>\n            <group prefix=\", \">\n              <text term=\"chapter\" form=\"short\"/>\n              <number variable=\"chapter-number\"/>\n            </group>\n          </else>\n        </choose>\n        <choose>\n          <if variable=\"number\">\n            <group prefix=\", \" delimiter=\", \">\n              <group>\n                <text term=\"number\" form=\"short\" text-case=\"capitalize-first\"/>\n                <number variable=\"number\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else>\n            <text prefix=\". \" macro=\"publisher\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"entry\">\n          <text variable=\"title\" font-style=\"italic\"/>\n        </if>\n        <else>\n          <choose>\n            <if type=\"bill book dataset graphic legal_case map motion_picture musical_score patent report software song speech standard webpage thesis\" match=\"any\">\n              <text variable=\"title\" font-style=\"italic\"/>\n            </if>\n            <else>\n              <choose>\n                <if variable=\"container-title\">\n                  <text variable=\"title\"/>\n                </if>\n                <else>\n                  <text variable=\"title\" font-style=\"italic\"/>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </else>\n      </choose>\n      <text variable=\"original-title\" prefix=\"[\" suffix=\"]\"/>\n      <group prefix=\"(\" suffix=\")\">\n        <text term=\"version\" form=\"short\"/>\n        <text variable=\"version\"/>\n      </group>\n      <choose>\n        <if type=\"bill broadcast graphic motion_picture patent report song standard thesis\" match=\"any\">\n          <choose>\n            <if type=\"graphic song\" match=\"any\">\n              <choose>\n                <if variable=\"medium\">\n                  <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n                <else-if variable=\"URL DOI accessed\" match=\"any\">\n                  <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                </else-if>\n              </choose>\n            </if>\n            <else-if type=\"motion_picture\" match=\"any\">\n              <choose>\n                <if variable=\"URL DOI accessed\" match=\"any\">\n                  <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n              </choose>\n            </else-if>\n            <else-if variable=\"archive\" match=\"none\">\n              <choose>\n                <if variable=\"URL DOI accessed\" match=\"any\">\n                  <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n              </choose>\n            </else-if>\n          </choose>\n        </if>\n        <else-if type=\"regulation\"/>\n        <else-if type=\"entry\" variable=\"title\">\n          <choose>\n            <if variable=\"genre\">\n              <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n            </if>\n            <else>\n              <choose>\n                <if variable=\"URL DOI accessed\" match=\"any\">\n                  <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n              </choose>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n          <choose>\n            <if variable=\"genre\">\n              <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n            </if>\n            <else>\n              <choose>\n                <if variable=\"container-title archive\" match=\"none\">\n                  <choose>\n                    <if variable=\"URL DOI accessed\" match=\"any\">\n                      <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                    </if>\n                  </choose>\n                </if>\n              </choose>\n            </else>\n          </choose>\n        </else-if>\n        <else-if variable=\"container-title\" match=\"none\">\n          <choose>\n            <if variable=\"genre\">\n              <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n            </if>\n            <else>\n              <choose>\n                <if variable=\"URL DOI accessed\" match=\"any\">\n                  <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n              </choose>\n            </else>\n          </choose>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"title-block\">\n    <choose>\n      <if type=\"entry\">\n        <group delimiter=\". \">\n          <text macro=\"edition\"/>\n          <text macro=\"title\"/>\n        </group>\n      </if>\n      <else-if type=\"graphic map\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"title\"/>\n          <text variable=\"scale\"/>\n        </group>\n      </else-if>\n      <else-if type=\"book motion_picture musical_score song speech\" match=\"any\">\n        <group delimiter=\" \">\n          <text macro=\"title\"/>\n          <text macro=\"translator\"/>\n        </group>\n      </else-if>\n      <else-if type=\"post\">\n        <choose>\n          <if variable=\"container-title\" match=\"none\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <date variable=\"issued\">\n                <date-part name=\"day\" suffix=\" \"/>\n                <date-part name=\"month\"/>\n              </date>\n            </group>\n          </if>\n          <else>\n            <text macro=\"title\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text macro=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-label\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"book broadcast\" match=\"any\">\n          <text variable=\"title\"/>\n        </if>\n        <else>\n          <choose>\n            <if type=\"bill dataset graphic legal_case map motion_picture musical_score patent report software song speech standard webpage thesis\" match=\"any\">\n              <text variable=\"title\" font-style=\"italic\"/>\n            </if>\n            <else>\n              <choose>\n                <if variable=\"container-title\">\n                  <text variable=\"title\"/>\n                </if>\n                <else>\n                  <text variable=\"title\" font-style=\"italic\"/>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </else>\n      </choose>\n      <text variable=\"original-title\" prefix=\"[\" suffix=\"]\"/>\n      <group prefix=\"(\" suffix=\")\">\n        <text term=\"version\" form=\"short\"/>\n        <text variable=\"version\"/>\n      </group>\n      <choose>\n        <if type=\"bill broadcast graphic motion_picture patent report song standard thesis\" match=\"any\">\n          <choose>\n            <if type=\"graphic song\" match=\"any\">\n              <choose>\n                <if variable=\"medium\">\n                  <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n                <else-if variable=\"URL DOI accessed\" match=\"any\">\n                  <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                </else-if>\n              </choose>\n            </if>\n            <else-if type=\"motion_picture\" match=\"any\">\n              <choose>\n                <if variable=\"URL DOI accessed\" match=\"any\">\n                  <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n              </choose>\n            </else-if>\n            <else-if variable=\"archive\" match=\"none\">\n              <choose>\n                <if variable=\"URL DOI accessed\" match=\"any\">\n                  <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n              </choose>\n            </else-if>\n          </choose>\n        </if>\n        <else-if type=\"regulation\"/>\n        <else-if variable=\"container-title\" match=\"none\">\n          <choose>\n            <if variable=\"genre\">\n              <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n            </if>\n            <else-if variable=\"container-title\" match=\"none\">\n              <choose>\n                <if variable=\"URL DOI accessed\" match=\"any\">\n                  <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                </if>\n              </choose>\n            </else-if>\n          </choose>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"entry\" variable=\"title container-title\">\n        <text variable=\"container-title\" form=\"short\"/>\n      </if>\n      <else-if type=\"bill dataset entry graphic legal_case map motion_picture musical_score patent report software song speech standard webpage thesis\" match=\"any\">\n        <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"legislation pamphlet post regulation\" match=\"any\">\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text variable=\"title\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\" prefix=\"(\" suffix=\")\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"post\">\n            <choose>\n              <if variable=\"container-title\">\n                <date variable=\"issued\">\n                  <date-part name=\"day\" suffix=\" \"/>\n                  <date-part name=\"month\" suffix=\" \"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </if>\n              <else>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year\"/>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"1\" hanging-indent=\"false\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \">\n        <choose>\n          <if type=\"legislation regulation\" match=\"any\">\n            <text macro=\"statute-law\" suffix=\".\"/>\n          </if>\n          <else-if type=\"legal_case\">\n            <text macro=\"case-law\" suffix=\".\"/>\n          </else-if>\n          <else>\n            <group delimiter=\". \" suffix=\".\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"year-date\"/>\n              </group>\n              <text macro=\"title-block\"/>\n              <text macro=\"publication\"/>\n              <text macro=\"archive\"/>\n            </group>\n          </else>\n        </choose>\n        <group delimiter=\". \" suffix=\".\">\n          <text variable=\"annote\"/>\n          <text macro=\"access\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"