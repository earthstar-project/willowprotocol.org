"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Vienna Legal</title>\n    <id>http://www.zotero.org/styles/vienna-legal</id>\n    <link href=\"http://www.zotero.org/styles/vienna-legal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/inter-ro\" rel=\"template\"/>\n    <link href=\"http://www.wu.ac.at/taxlaw/en\" rel=\"documentation\"/>\n    <author>\n      <name>Pedro Herrera</name>\n      <email>pedro.m.herrera@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Richard Karnesky</name>\n      <email>karnesky+zotero@gmail.com</email>\n      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>\n    </contributor>\n    <category citation-format=\"note\"/>\n    <category field=\"law\"/>\n    <summary>Prepared according the authors' guidelines of the Institute of Austrian and International Tax Law at the Vienna University of Economic and Business regarding their \"ECJ pending cases series\"</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <label form=\"verb\"/>\n      <name and=\"text\" delimiter=\", \" prefix=\" \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\"/\" delimiter-precedes-last=\"always\" sort-separator=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-bibliography\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" form=\"short\" delimiter=\"/\" delimiter-precedes-last=\"always\" sort-separator=\",.\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text term=\"accessed\" suffix=\" \"/>\n      <date variable=\"accessed\" suffix=\", \">\n        <date-part name=\"day\" suffix=\" \"/>\n        <date-part name=\"month\" suffix=\" \" text-case=\"lowercase\"/>\n        <date-part name=\"year\"/>\n      </date>\n      <group>\n        <text term=\"at\" suffix=\" \"/>\n        <text variable=\"URL\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" text-case=\"capitalize-first\" suffix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" text-case=\"capitalize-first\" font-style=\"italic\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"title\" text-case=\"capitalize-first\" prefix=\" \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"6\" et-al-use-first=\"1\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if position=\"first\">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\" \">\n                <text macro=\"author\" suffix=\",\"/>\n                <group delimiter=\" \">\n                  <text macro=\"title\"/>\n                  <!-- Here will be the original-date, when zotero will have an ui field for it.\n                                        <date variable=\"original-date\" prefix=\"(\" suffix=\")\">\n                                        <date-part name=\"year\"/>\n                                        </date>\n                                    -->\n                </group>\n                <group delimiter=\" \">\n                  <text term=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n                <text variable=\"edition\"/>\n                <!-- text macro=\"publisher\"/-->\n                <date variable=\"issued\">\n                  <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n                </date>\n                <group>\n                  <label variable=\"locator\" form=\"short\"/>\n                  <text variable=\"locator\" prefix=\" \"/>\n                </group>\n                <text macro=\"access\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\" \">\n                <text macro=\"author\" suffix=\",\"/>\n                <text macro=\"title\" suffix=\",\"/>\n                <group delimiter=\" \">\n                  <text term=\"in\"/>\n                  <names variable=\"editor translator\" delimiter=\", \">\n                    <name sort-separator=\",\"/>\n                    <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n                  </names>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </group>\n                <group delimiter=\" \">\n                  <text term=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n                <text variable=\"edition\"/>\n                <!--text macro=\"publisher\"/-->\n                <group>\n                  <date variable=\"issued\">\n                    <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n                  </date>\n                  <text variable=\"collection-title\" prefix=\" (\" suffix=\")\"/>\n                </group>\n                <!--group>\n                  <label variable=\"locator\" include-period=\"true\" form=\"short\"/>\n                  <text variable=\"locator\" prefix=\" \"/>\n                </group-->\n                <text macro=\"access\"/>\n              </group>\n            </else-if>\n            <else-if type=\"article-journal article-magazine article-newspaper broadcast interview manuscript map patent personal_communication song speech thesis webpage\" match=\"any\">\n              <group delimiter=\" \">\n                <text macro=\"author\" suffix=\", \"/>\n                <text macro=\"title\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <group delimiter=\" \">\n                  <text term=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n                <text variable=\"issue\"/>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n                <text macro=\"editor-translator\"/>\n                <text variable=\"edition\"/>\n                <group delimiter=\" \">\n                  <text macro=\"publisher\"/>\n                  <text variable=\"collection-title\" prefix=\" (\" suffix=\")\"/>\n                </group>\n                <group>\n                  <label variable=\"locator\" form=\"short\"/>\n                  <text variable=\"locator\" prefix=\" \"/>\n                </group>\n                <text macro=\"access\"/>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title\"/>\n                <text macro=\"editor-translator\"/>\n                <group delimiter=\", \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <group delimiter=\" \">\n                    <text term=\"volume\" form=\"short\"/>\n                    <text variable=\"volume\"/>\n                  </group>\n                  <text variable=\"issue\"/>\n                </group>\n                <group>\n                  <label variable=\"locator\" form=\"short\"/>\n                  <text variable=\"locator\" prefix=\" \"/>\n                </group>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <else-if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" form=\"long\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n            <group>\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\" prefix=\" \"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\" form=\"long\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title-short\"/>\n                <group delimiter=\" \">\n                  <text term=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n                <group>\n                  <label variable=\"locator\" form=\"short\"/>\n                  <text variable=\"locator\" prefix=\" \"/>\n                </group>\n              </group>\n            </if>\n            <else-if type=\"article-journal article-magazine article-newspaper broadcast interview manuscript map patent personal_communication song speech thesis webpage\" match=\"any\">\n              <group delimiter=\" \">\n                <text macro=\"author\" suffix=\", \"/>\n                <!--text macro=\"title\"/-->\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <group delimiter=\" \">\n                  <text term=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n                <text variable=\"issue\"/>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n                <text macro=\"editor-translator\"/>\n                <text variable=\"edition\"/>\n                <group delimiter=\" \">\n                  <text macro=\"publisher\"/>\n                  <text variable=\"collection-title\" prefix=\" (\" suffix=\")\"/>\n                </group>\n                <group>\n                  <label variable=\"locator\" form=\"short\"/>\n                  <text variable=\"locator\" prefix=\" \"/>\n                </group>\n                <text macro=\"access\"/>\n              </group>\n            </else-if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\" \">\n                <text macro=\"author\" suffix=\",\"/>\n                <text macro=\"title\" suffix=\",\"/>\n                <group delimiter=\" \">\n                  <text term=\"in\"/>\n                  <names variable=\"editor translator\" delimiter=\", \">\n                    <name sort-separator=\",\"/>\n                    <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n                  </names>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </group>\n                <group delimiter=\" \">\n                  <text term=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n                <text variable=\"edition\"/>\n                <!--text macro=\"publisher\"/-->\n                <!--group>\n                  <date variable=\"issued\">\n                    <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n                  </date>\n                  <text variable=\"collection-title\" prefix=\" (\" suffix=\")\"/>\n                </group-->\n                <!--group>\n                  <label variable=\"locator\" include-period=\"true\" form=\"short\"/>\n                  <text variable=\"locator\" prefix=\" \"/>\n                </group-->\n                <text macro=\"access\"/>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title-short\"/>\n                <group>\n                  <label variable=\"locator\" form=\"short\"/>\n                  <text variable=\"locator\" prefix=\" \"/>\n                </group>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"7\" et-al-use-first=\"6\">\n    <sort>\n      <key macro=\"author-bibliography\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author-bibliography\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <group delimiter=\" \">\n              <text macro=\"title\"/>\n              <!-- Here will be the original-date, when zotero will have an ui field for it.\n                                        <date variable=\"original-date\" prefix=\"(\" suffix=\")\">\n                                        <date-part name=\"year\"/>\n                                        </date>\n                                    -->\n            </group>\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"edition\"/>\n            <!-- text macro=\"publisher\"/-->\n            <date variable=\"issued\">\n              <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n            </date>\n            <group>\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\" prefix=\" \"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\",\"/>\n            <group delimiter=\" \">\n              <text term=\"in\"/>\n              <names variable=\"editor translator\" delimiter=\", \">\n                <name sort-separator=\",\"/>\n                <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n              </names>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </group>\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"edition\"/>\n            <!--text macro=\"publisher\"/-->\n            <group>\n              <date variable=\"issued\">\n                <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n              </date>\n              <text variable=\"collection-title\" prefix=\" (\" suffix=\")\"/>\n            </group>\n            <!--group>\n                  <label variable=\"locator\" include-period=\"true\" form=\"short\"/>\n                  <text variable=\"locator\" prefix=\" \"/>\n                </group-->\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article-newspaper broadcast interview manuscript map patent personal_communication song speech thesis webpage\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"issue\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n            <text macro=\"editor-translator\"/>\n            <text variable=\"edition\"/>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <text variable=\"collection-title\" prefix=\" (\" suffix=\")\"/>\n            </group>\n            <group>\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\" prefix=\" \"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"editor-translator\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group delimiter=\" \">\n                <text term=\"volume\" form=\"short\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <text variable=\"issue\"/>\n            </group>\n            <group>\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\" prefix=\" \"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\", \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"