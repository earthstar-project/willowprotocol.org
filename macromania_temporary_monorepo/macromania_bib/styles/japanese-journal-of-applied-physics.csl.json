"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Japanese Journal of Applied Physics</title>\n    <title-short>JJAP</title-short>\n    <id>http://www.zotero.org/styles/japanese-journal-of-applied-physics</id>\n    <link href=\"http://www.zotero.org/styles/japanese-journal-of-applied-physics\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/royal-society-of-chemistry\" rel=\"template\"/>\n    <link href=\"https://iopscience.iop.org/journal/1347-4065/page/Author_Guidelines\" rel=\"documentation\"/>\n    <link href=\"https://cms.iopscience.org/fca83bc8-15ad-11e9-9ac9-e38b18a16377/Instructions%20for%20Preparation%20of%20Manuscript.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"physics\"/>\n    <issn>0021-4922</issn>\n    <eissn>1347-4065</eissn>\n    <updated>2019-11-29T14:02:24+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"presented at\">presented in part at the</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"patent\">\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"authority\">\n              <text variable=\"authority\"/>\n            </if>\n            <else>\n              <text variable=\"publisher\"/>\n            </else>\n          </choose>\n          <text variable=\"number\"/>\n        </group>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name initialize-with=\". \" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" suffix=\" \"/>\n      <name initialize-with=\". \" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n      <text macro=\"issued\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page-first\"/>\n  </macro>\n  <macro name=\"book-container\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <text term=\"in\"/>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </group>\n      <text macro=\"editor\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"speech\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"month\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <label variable=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\" suffix=\")\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\")\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"thesis\">\n          <group delimiter=\" \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"title\"/>\n            <text variable=\"number\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"volume\" prefix=\"Vol. \"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"book-container\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"volume\" prefix=\"Vol. \"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" form=\"short\" font-style=\"normal\"/>\n            <choose>\n              <if variable=\"page\">\n                <group delimiter=\" \">\n                  <group delimiter=\", \">\n                    <group delimiter=\" \">\n                      <text variable=\"volume\" font-weight=\"bold\"/>\n                      <text variable=\"issue\" prefix=\"[\" suffix=\"]\"/>\n                    </group>\n                    <text variable=\"page-first\"/>\n                  </group>\n                  <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n                </group>\n              </if>\n              <else>\n                <text variable=\"DOI\" prefix=\"[ DOI:\" suffix=\"]\"/>\n              </else>\n            </choose>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" form=\"short\"/>\n            <text variable=\"URL\"/>\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <text term=\"accessed\"/>\n              <date form=\"text\" variable=\"accessed\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"speech\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text term=\"presented at\"/>\n              <text variable=\"event\"/>\n            </group>\n            <text variable=\"event-place\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <text macro=\"issued\"/>\n            <text variable=\"volume\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"