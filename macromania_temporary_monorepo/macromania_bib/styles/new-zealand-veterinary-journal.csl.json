"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>New Zealand Veterinary Journal</title>\n    <id>http://www.zotero.org/styles/new-zealand-veterinary-journal</id>\n    <link href=\"http://www.zotero.org/styles/new-zealand-veterinary-journal\" rel=\"self\"/>\n    <link href=\"http://www.vetjournal.org.nz/sites/default/files/domain-19/NZVJ%20Instructions%20for%20Authors.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>sumostorm</name>\n      <email>sumostorm@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <category field=\"zoology\"/>\n    <issn>0048-0169</issn>\n    <eissn>1176-0710</eissn>\n    <summary>New Zealand Veterinary Journal Style</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"edition\" form=\"short\">Edtn.</term>\n      <term name=\"no date\" form=\"long\">date unknown</term>\n    </terms>\n  </locale>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author-bib\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" font-weight=\"bold\" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-cit\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\" \" initialize-with=\"\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\" \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" suffix=\")\" prefix=\" (\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" suffix=\", \"/>\n    <text variable=\"publisher-place\" suffix=\", \"/>\n    <text macro=\"date\"/>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <group>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <text variable=\"year-suffix\"/>\n        </group>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\"/>\n      <!--don't use if there is a DOI-->\n      <else>\n        <choose>\n          <if variable=\"URL\">\n            <group delimiter=\" \" prefix=\" \">\n              <group>\n                <text variable=\"URL\"/>\n              </group>\n              <group prefix=\"(accessed \" suffix=\"). \" delimiter=\" \">\n                <date variable=\"accessed\">\n                  <date-part name=\"day\"/>\n                  <date-part name=\"month\" prefix=\" \" suffix=\" \" form=\"long\" strip-periods=\"true\"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <choose>\n                <if variable=\"URL\">\n                  <text variable=\"title\"/>\n                </if>\n                <else>\n                  <text variable=\"title\" font-style=\"italic\"/>\n                </else>\n              </choose>\n            </if>\n            <else>\n              <text variable=\"title\"/>\n            </else>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <!--   formatting citation      -->\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"date\"/>\n      <key macro=\"author-bib\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <group delimiter=\" \">\n          <text macro=\"author-cit\"/>\n          <text macro=\"date\"/>\n        </group>\n        <!-- <text variable=\"locator\" prefix=\"p.\"/> -->\n      </group>\n    </layout>\n  </citation>\n  <!--   formatting bibliography      -->\n  <bibliography>\n    <sort>\n      <key macro=\"author-bib\"/>\n      <key macro=\"date\"/>\n    </sort>\n    <layout>\n      <text macro=\"author-bib\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"edition\" suffix=\".\"/>\n            <text macro=\"access\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <group>\n              <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\" suffix=\". \"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </group>\n          </group>\n          <text macro=\"edition\" prefix=\". \"/>\n          <text variable=\"page\" prefix=\". Pp \" suffix=\". \"/>\n          <text variable=\"event\"/>\n          <text macro=\"access\" suffix=\" \"/>\n          <group delimiter=\", \" suffix=\", \">\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <choose>\n            <if variable=\"issue volume\" match=\"any\">\n              <text variable=\"container-title\" suffix=\" \" form=\"short\" font-style=\"italic\"/>\n              <text variable=\"volume\"/>\n              <text variable=\"page\" prefix=\", \"/>\n              <text macro=\"date\" prefix=\", \"/>\n            </if>\n            <else>\n              <choose>\n                <if variable=\"DOI\">\n                  <text variable=\"container-title\" suffix=\" \" form=\"short\" font-style=\"italic\"/>\n                  <group prefix=\"(\" suffix=\").\">\n                    <date variable=\"issued\">\n                      <date-part name=\"month\" prefix=\" \" suffix=\" \"/>\n                      <date-part name=\"day\" suffix=\", \"/>\n                      <date-part name=\"year\"/>\n                    </date>\n                  </group>\n                  <text variable=\"DOI\" prefix=\" doi: \"/>\n                </if>\n                <else>\n                  <text variable=\"container-title\" suffix=\". \" form=\"short\" font-style=\"italic\"/>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </else-if>\n        <else>\n          <text variable=\"container-title\" suffix=\". \" form=\"short\" font-style=\"italic\"/>\n          <text variable=\"volume\"/>\n          <text macro=\"access\"/>\n          <text variable=\"publisher\" suffix=\". \"/>\n          <text variable=\"page\" prefix=\": \" suffix=\", \"/>\n          <text macro=\"date\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"