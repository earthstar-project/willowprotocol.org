cture paper-conference report song\" match=\"none\">\n          <date variable=\"issued\">\n            <date-part prefix=\", \" name=\"month\"/>\n            <date-part prefix=\" \" name=\"day\"/>\n          </date>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal\" match=\"none\">\n        <text variable=\"container-title\" form=\"short\" prefix=\" \"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" form=\"short\" prefix=\" \" strip-periods=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" prefix=\" \" suffix=\" \"/>\n      </if>\n    </choose>\n    <names variable=\"editor\" delimiter=\", \" prefix=\" \">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"book chapter paper-conference thesis\" match=\"any\">\n        <group delimiter=\", \" prefix=\" \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <choose>\n          <if match=\"all\" variable=\"page volume\">\n            <group prefix=\" \" delimiter=\":\">\n              <group>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"DOI\" prefix=\"DOI: \"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\" \" prefix=\", \">\n          <label strip-periods=\"true\" variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n    </sort>\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"issued\"/>\n      <text variable=\"title\" prefix=\" \" suffix=\".\"/>\n      <text macro=\"container-contributors\"/>\n      <text macro=\"container-title\"/>\n      <text macro=\"publisher\"/>\n      <text macro=\"locators\"/>\n    </layout>\n  </bibliography>\n</style>\n"