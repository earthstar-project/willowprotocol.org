"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" initialize-with=\". \" name-as-sort-order=\"first\" default-locale=\"en-GB\">\n  <info>\n    <title>International Journal of Lexicography</title>\n    <title-short>IJL</title-short>\n    <id>http://www.zotero.org/styles/international-journal-of-lexicography</id>\n    <link href=\"http://www.zotero.org/styles/international-journal-of-lexicography\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/unified-style-sheet-for-linguistics\" rel=\"template\"/>\n    <link href=\"http://www.oxfordjournals.org/our_journals/lexico/for_authors/index.html\" rel=\"documentation\"/>\n    <author>\n      <name>Robert Lew</name>\n    </author>\n    <contributor>\n      <name>Mark Dingemanse</name>\n      <email>mark.dingemanse@mpi.nl</email>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <contributor>\n      <name>Frank Bennett</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"linguistics\"/>\n    <issn>0950-3846</issn>\n    <eissn>1477-4577</eissn>\n    <summary>International Journal of Lexicography Style, created by modifying the Unified Style Sheet for Linguistics</summary>\n    <updated>2013-04-27T22:07:21+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"translator\" form=\"verb-short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <group delimiter=\". \">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"editor\">\n                <label form=\"verb-short\" prefix=\" (\" text-case=\"capitalize-first\" suffix=\") \"/>\n                <name and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n              </names>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\">\n                <label form=\"verb-short\" prefix=\" (\" text-case=\"capitalize-first\" suffix=\") \"/>\n                <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n              </names>\n            </if>\n            <else>\n              <names variable=\"editor\">\n                <label form=\"short\" suffix=\")\" prefix=\"(\"/>\n                <name and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n              </names>\n            </else>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"editor\">\n                <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n                <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n              </names>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\">\n                <label form=\"verb-short\" prefix=\" \" suffix=\" \"/>\n                <name and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\").\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name name-as-sort-order=\"first\" and=\"symbol\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"verb-short\" prefix=\" (\" suffix=\").\"/>\n    </names>\n  </macro>\n  <macro name=\"recipient\">\n    <choose>\n      <if type=\"personal_communication\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n          <else>\n            <text term=\"letter\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"contributors\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n      <label form=\"short\" prefix=\", \" suffix=\" \"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient\"/>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter-precedes-last=\"never\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\". \">\n      <text variable=\"archive_location\" text-case=\"capitalize-first\"/>\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\". \">\n      <choose>\n        <if type=\"graphic report\" match=\"any\">\n          <text macro=\"archive\"/>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"none\">\n          <text macro=\"archive\"/>\n        </else-if>\n      </choose>\n      <text variable=\"DOI\" prefix=\"doi:\"/>\n      <text variable=\"URL\"/>\n    </group>\n    <group prefix=\" (\" suffix=\")\">\n      <date variable=\"accessed\">\n        <date-part name=\"day\" suffix=\" \"/>\n        <date-part name=\"month\" suffix=\", \"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"personal_communication\" match=\"none\">\n            <text variable=\"genre\" text-case=\"title\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"article article-journal article-magazine article-newspaper chapter paper-conference\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" quotes=\"true\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" strip-periods=\"false\" quotes=\"false\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"volume\" prefix=\" \"/>\n        <text variable=\"issue\" prefix=\".\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group prefix=\". \" delimiter=\". \">\n          <group>\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n          <group>\n            <number variable=\"number-of-volumes\" form=\"numeric\"/>\n            <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locators-chapter\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"volume\" prefix=\"vol. \"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-article\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group prefix=\", \" delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"edition\"/>\n            <text term=\"edition\"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\" \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"page\" prefix=\": \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"point-locators\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n        </if>\n      </choose>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <text variable=\"container-title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"collection-title\">\n    <group prefix=\"(\" suffix=\")\">\n      <text variable=\"collection-title\" text-case=\"title\"/>\n      <text variable=\"collection-number\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"event\">\n    <group>\n      <text term=\"presented at\" prefix=\"Paper \" suffix=\" \"/>\n      <text variable=\"event\"/>\n    </group>\n  </macro>\n  <macro name=\"description\">\n    <group delimiter=\". \">\n      <text macro=\"interviewer\"/>\n      <text variable=\"medium\" text-case=\"capitalize-first\"/>\n    </group>\n    <choose>\n      <if variable=\"title\" match=\"none\"/>\n      <else-if type=\"thesis\"/>\n      <else>\n        <text variable=\"genre\" text-case=\"capitalize-first\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue\">\n    <choose>\n      <if type=\"speech\">\n        <group delimiter=\", \">\n          <text macro=\"event\"/>\n          <text variable=\"event-place\"/>\n        </group>\n      </if>\n      <else-if type=\"thesis\">\n        <group delimiter=\" \">\n          <text macro=\"publisher\"/>\n          <text variable=\"genre\"/>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <text macro=\"publisher\"/>\n          <choose>\n            <if type=\"manuscript\">\n              <text value=\"ms\"/>\n            </if>\n          </choose>\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <text variable=\"page\"/>\n            </if>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" et-al-use-last=\"true\">\n    <layout delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\":\">\n        <group delimiter=\" \">\n          <text macro=\"contributors-short\"/>\n          <text macro=\"date\"/>\n        </group>\n        <text macro=\"point-locators\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"7\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"contributors\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"contributors\" font-weight=\"bold\"/>\n        <text macro=\"date\" suffix=\".\" font-weight=\"bold\"/>\n        <text macro=\"title\"/>\n        <text macro=\"description\"/>\n        <text macro=\"secondary-contributors\"/>\n        <group>\n          <group delimiter=\". \">\n            <group>\n              <group delimiter=\". \">\n                <group delimiter=\", \">\n                  <text macro=\"container-contributors\"/>\n                  <text macro=\"container-title\"/>\n                  <text macro=\"locators-chapter\"/>\n                </group>\n                <text macro=\"edition\"/>\n              </group>\n              <text macro=\"locators\"/>\n            </group>\n            <text macro=\"collection-title\"/>\n            <text macro=\"issue\"/>\n          </group>\n          <text macro=\"locators-article\"/>\n        </group>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"