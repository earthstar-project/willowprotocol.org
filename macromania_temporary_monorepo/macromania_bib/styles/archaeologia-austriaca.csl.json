"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" page-range-format=\"expanded\" demote-non-dropping-particle=\"never\">\n  <!-- Polyglot; journal publishes in English and German -->\n  <info>\n    <title>Archaeologia Austriaca</title>\n    <title-short>ArchA</title-short>\n    <id>http://www.zotero.org/styles/archaeologia-austriaca</id>\n    <link href=\"http://www.zotero.org/styles/archaeologia-austriaca\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/collection-du-centre-jean-berard\" rel=\"template\"/>\n    <link href=\"https://www.orea.oeaw.ac.at/en/publications/journals/archaeologia-austriaca/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>citationstyler@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <issn>0003-8008</issn>\n    <eissn>1816-2959</eissn>\n    <updated>2019-08-05T08:19:29+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"accessed\">letzter Zugriff</term>\n      <term name=\"et-al\">et al.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\", \">\n      <name sort-separator=\", \" initialize-with=\". \" delimiter=\", \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name font-variant=\"small-caps\" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <text macro=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\" delimiter=\", \">\n      <name sort-separator=\", \" initialize-with=\".\" delimiter=\", \"/>\n      <et-al font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" font-variant=\"small-caps\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title-short\" font-style=\"italic\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\" \" prefix=\" (\" suffix=\")\">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if locator=\"paragraph\">\n        <text variable=\"locator\" prefix=\", ยง \"/>\n      </if>\n      <else>\n        <text variable=\"locator\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"lowercase\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"volume page\">\n            <text variable=\"DOI\" prefix=\"doi: \"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"3\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <layout delimiter=\" &#8211; \">\n      <group delimiter=\", \">\n        <group>\n          <text macro=\"author-short\" suffix=\" \"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text macro=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"page-first\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\", \">\n        <choose>\n          <if variable=\"author\">\n            <names variable=\"author\">\n              <name et-al-min=\"4\" et-al-use-first=\"1\" initialize-with=\". \">\n                <name-part name=\"family\" font-variant=\"small-caps\"/>\n              </name>\n              <et-al font-style=\"italic\"/>\n            </names>\n          </if>\n          <else-if variable=\"editor\">\n            <names variable=\"editor\">\n              <name form=\"short\" delimiter=\", \" et-al-min=\"4\" et-al-use-first=\"1\"/>\n              <substitute>\n                <text macro=\"container-author\"/>\n              </substitute>\n            </names>\n          </else-if>\n          <else>\n            <text variable=\"title-short\"/>\n          </else>\n        </choose>\n        <group>\n          <choose>\n            <if type=\"thesis\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text variable=\"genre\"/>\n                <text variable=\"publisher\"/>\n                <text macro=\"year-date\"/>\n              </group>\n            </if>\n            <else-if type=\"webpage post post-weblog\" match=\"any\">\n              <group delimiter=\" \">\n                <group suffix=\",\" delimiter=\", \">\n                  <choose>\n                    <if variable=\"container-title\" match=\"none\">\n                      <text macro=\"title\" font-style=\"italic\"/>\n                    </if>\n                    <else>\n                      <text macro=\"title\"/>\n                      <text variable=\"container-title\" font-style=\"italic\"/>\n                    </else>\n                  </choose>\n                </group>\n                <text macro=\"year-date\"/>\n                <group delimiter=\" \">\n                  <text variable=\"URL\"/>\n                  <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                    <text term=\"accessed\"/>\n                    <date form=\"numeric\" variable=\"accessed\"/>\n                  </group>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"article-journal article-magazine article-newspaper broadcast personal_communication thesis\" match=\"any\">\n              <group delimiter=\". \">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <group delimiter=\" \">\n                    <text variable=\"container-title\"/>\n                    <text variable=\"volume\"/>\n                  </group>\n                  <text macro=\"year-date\"/>\n                  <text macro=\"pages\"/>\n                </group>\n                <text macro=\"access\"/>\n              </group>\n            </else-if>\n            <else-if type=\"book graphic\" match=\"any\">\n              <group>\n                <text macro=\"title\"/>\n                <text macro=\"edition\" prefix=\", \"/>\n                <text macro=\"publisher\" prefix=\", \"/>\n                <text macro=\"year-date\" prefix=\" \"/>\n                <text macro=\"collection\"/>\n              </group>\n            </else-if>\n            <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <choose>\n                  <if variable=\"title-short\" match=\"any\">\n                    <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n                  </if>\n                  <else>\n                    <text variable=\"container-title\" font-style=\"italic\"/>\n                  </else>\n                </choose>\n                <group delimiter=\" \">\n                  <label variable=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n                <text macro=\"year-date\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </else-if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\". \">\n                <text macro=\"title\"/>\n                <group delimiter=\", \">\n                  <group delimiter=\": \">\n                    <text term=\"in\" text-case=\"capitalize-first\"/>\n                    <text macro=\"editor\"/>\n                  </group>\n                  <text variable=\"container-title\"/>\n                </group>\n                <text macro=\"collection\" prefix=\" \"/>\n                <group delimiter=\", \">\n                  <group delimiter=\" \">\n                    <text macro=\"publisher\"/>\n                    <text macro=\"year-date\"/>\n                  </group>\n                  <text macro=\"pages\"/>\n                </group>\n              </group>\n            </else-if>\n          </choose>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"