"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>International Journal of Polymer Analysis and Characterization</title>\n    <title-short>IJPAC</title-short>\n    <id>http://www.zotero.org/styles/international-journal-of-polymer-analysis-and-characterization</id>\n    <link href=\"http://www.zotero.org/styles/international-journal-of-polymer-analysis-and-characterization\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/nature\" rel=\"template\"/>\n    <link href=\"https://www.tandfonline.com/action/authorSubmission?journalCode=gpac20&amp;page=instructions#refs\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <issn>1023-666X</issn>\n    <eissn>1563-5341</eissn>\n    <updated>2019-07-04T14:42:52+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" strip-periods=\"false\" suffix=\" \"/>\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"volume\"/>\n      <else-if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </else-if>\n      <else-if variable=\"URL\">\n        <group delimiter=\". \">\n          <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n            <text term=\"cited\"/>\n            <date variable=\"accessed\" delimiter=\" \">\n              <date-part name=\"day\"/>\n              <date-part name=\"month\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"in\" text-case=\"capitalize-first\"/>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issuance\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\" prefix=\" \">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\" \" prefix=\", \">\n          <label variable=\"page\" strip-periods=\"true\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"page\" prefix=\":\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"volume\" prefix=\" \"/>\n      </if>\n      <else>\n        <group delimiter=\" \" prefix=\", \" suffix=\".\">\n          <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n          <text variable=\"volume\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n        <text macro=\"title\"/>\n        <group delimiter=\", \">\n          <text macro=\"container-title\"/>\n          <text macro=\"editor\"/>\n        </group>\n      </group>\n      <text macro=\"volume\"/>\n      <group delimiter=\". \">\n        <text macro=\"pages\"/>\n        <text macro=\"issuance\" prefix=\" \"/>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"