"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Newcastle University - Harvard</title>\n    <id>http://www.zotero.org/styles/harvard-newcastle-university</id>\n    <link href=\"http://www.zotero.org/styles/harvard-newcastle-university\" rel=\"self\"/>\n    <link href=\"http://libguides.ncl.ac.uk/referencing\" rel=\"documentation\"/>\n    <link href=\"https://libguides.ncl.ac.uk/ld.php?content_id=33783920\" rel=\"documentation\"/>\n    <author>\n      <name>McKenzie Lloyd-Smith</name>\n      <uri>http://www.mendeley.com/profiles/mckenzie-lloyd-smith/</uri>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>The Harvard author-date style - adapted for Newcastle University</summary>\n    <updated>2021-12-07T09:47:27+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"symbol\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\".)\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" initialize-with=\". \" sort-separator=\",\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <text term=\"online\" prefix=\"[\" suffix=\"].\"/>\n        <text value=\" Available from: \"/>\n        <text variable=\"URL\"/>\n        <group prefix=\" (\" delimiter=\" \" suffix=\")\">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book webpage graphic\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\" suffix=\".\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if type=\"article-journal article-magazine article article-newspaper book report review-book\" match=\"none\">\n        <text value=\"[Online]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-newspaper paper-conference broadcast\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" prefix=\" \"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group>\n      <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\". \"/>\n      <number variable=\"volume\" form=\"numeric\"/>\n    </group>\n    <group>\n      <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\", \"/>\n      <text term=\"volume\" form=\"short\" prefix=\" \" suffix=\".\" plural=\"true\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\" text-case=\"capitalize-first\"/>\n        <text macro=\"year-date\"/>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"20\" et-al-use-first=\"19\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n      <choose>\n        <if type=\"thesis\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text macro=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"genre\" suffix=\" thesis\"/>\n            <text macro=\"online\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"webpage\">\n          <group prefix=\" \" delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"online\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-newspaper broadcast personal_communication thesis webpage\" match=\"any\">\n          <group suffix=\",\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <choose>\n            <if variable=\"author\" match=\"any\">\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\",\"/>\n            </if>\n          </choose>\n          <text macro=\"online\" prefix=\" \"/>\n          <group prefix=\" \" suffix=\".\">\n            <group>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\"), \"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"book graphic\" match=\"any\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text variable=\"note\"/>\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <text variable=\"collection-number\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"online\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\", \"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text term=\"in\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text macro=\"edition\" prefix=\" \"/>\n            <text macro=\"online\" suffix=\".\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"publisher\" suffix=\".\"/>\n            <text macro=\"pages\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <text variable=\"container-title\" prefix=\" \"/>\n          <text macro=\"access\" prefix=\" \" suffix=\". \"/>\n          <group prefix=\" \" suffix=\".\">\n            <group prefix=\" \">\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\" (\" suffix=\")\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group prefix=\" \">\n              <label variable=\"locator\" suffix=\".\" form=\"short\" strip-periods=\"true\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"