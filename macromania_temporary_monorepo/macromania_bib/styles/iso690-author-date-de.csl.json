"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"de-DE\">\n  <info>\n    <title>ISO-690 (author-date, Deutsch)</title>\n    <id>http://www.zotero.org/styles/iso690-author-date-de</id>\n    <link href=\"http://www.zotero.org/styles/iso690-author-date-de\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/iso690-author-date-en\" rel=\"template\"/>\n    <link href=\"http://www.iso.org/iso/catalogue_detail.htm?csnumber=43320\" rel=\"documentation\"/>\n    <author>\n      <name>Jan Drees</name>\n      <email>jdrees@mail.uni-paderborn.de</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Style based on DIN ISO 690:2013-10</summary>\n    <updated>2019-10-10T11:30:00+01:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"no date\">[kein Datum]</term>\n      <term name=\"accessed\">Zugriff am:</term>\n      <term name=\"retrieved\">Verf√ºgbar</term>\n      <term name=\"from\">unter</term>\n      <term name=\"editor\" form=\"short\">Hrsg.</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"responsibility\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <text macro=\"year-date\" prefix=\", \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author-citation\">\n    <names variable=\"author\">\n      <name form=\"short\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"container-responsibility\">\n    <choose>\n      <if variable=\"container-author\">\n        <text macro=\"container-author\"/>\n      </if>\n      <else-if variable=\"editor\">\n        <text macro=\"editor\"/>\n      </else-if>\n      <else>\n        <text macro=\"translator\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis map motion_picture song manuscript\" match=\"any\">\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"paper-conference speech chapter article-journal article-magazine article-newspaper entry entry-dictionary entry-encyclopedia post-weblog post webpage broadcast\" match=\"any\">\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <text variable=\"title\" suffix=\". \"/>\n          </if>\n          <else>\n            <text variable=\"title\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n          </else>\n        </choose>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n          </if>\n        </choose>\n        <group delimiter=\", \">\n          <text macro=\"container-responsibility\"/>\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </if>\n            <else>\n              <text variable=\"event\" font-style=\"italic\"/>\n            </else>\n          </choose>\n        </group>\n      </else-if>\n      <else-if type=\"report\">\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <text variable=\"number\" suffix=\": \"/>\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"number\" suffix=\": \"/>\n            <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"patent\">\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <text variable=\"title\"/>\n          </if>\n          <else>\n            <text variable=\"title\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n    <choose>\n      <if variable=\"URL\">\n        <text term=\"online\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <text variable=\"number\"/>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"map\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n          <else>\n            <text value=\"map\" prefix=\"[\" suffix=\"]\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"genre\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <text variable=\"edition\" form=\"long\"/>\n  </macro>\n  <macro name=\"publisher-group\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\", \">\n      <group delimiter=\"&#160;\">\n        <text term=\"volume\" form=\"short\"/>\n        <text variable=\"volume\"/>\n      </group>\n      <choose>\n        <if variable=\"volume\">\n          <group delimiter=\"&#160;\">\n            <text term=\"issue\" form=\"short\"/>\n            <text variable=\"issue\"/>\n          </group>\n          <group delimiter=\"&#160;\">\n            <text term=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </if>\n        <else-if variable=\"issue\">\n          <group delimiter=\"&#160;\">\n            <text term=\"issue\" form=\"short\"/>\n            <text variable=\"issue\"/>\n          </group>\n          <group delimiter=\"&#160;\">\n            <text term=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\"&#160;\">\n            <text term=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" prefix=\"&#160;\"/>\n            <date-part name=\"month\" prefix=\"&#160;\"/>\n            <date-part name=\"year\" prefix=\"&#160;\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\", \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"book thesis manuscript\" match=\"any\">\n        <text variable=\"number-of-pages\" suffix=\"&#160;p\"/>\n      </if>\n      <else-if type=\"chapter paper-conference article-newspaper\" match=\"any\">\n        <group delimiter=\"&#160;\">\n          <text term=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"report patent\" match=\"any\">\n        <group delimiter=\"&#160;\">\n          <text variable=\"page\"/>\n          <text term=\"page\" form=\"short\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"isbn\">\n    <text variable=\"ISBN\" prefix=\"ISBN&#160;\"/>\n  </macro>\n  <macro name=\"doi\">\n    <text variable=\"DOI\" prefix=\"DOI&#160;\"/>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text term=\"retrieved\" suffix=\" \" text-case=\"capitalize-first\"/>\n          <text term=\"from\" suffix=\": \"/>\n          <text variable=\"URL\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\":&#160;\">\n      <text variable=\"archive\"/>\n      <text macro=\"archive_location\"/>\n    </group>\n  </macro>\n  <macro name=\"archive_location\">\n    <choose>\n      <if variable=\"archive_location\">\n        <text variable=\"archive_location\"/>\n      </if>\n      <else>\n        <text variable=\"call-number\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\" year-suffix-delimiter=\", \" after-collapse-delimiter=\"; \">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-citation\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" suffix=\"&#160;\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"responsibility\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout>\n      <choose>\n        <if type=\"book map\" match=\"any\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"genre\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher-group\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"isbn\"/>\n            <text macro=\"url\"/>\n          </group>\n        </if>\n        <else-if type=\"article-journal article-magazine\" match=\"any\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"edition\"/>\n            <text macro=\"date\"/>\n            <text macro=\"issue\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"doi\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"edition\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher-group\"/>\n              <text macro=\"date\"/>\n            </group>\n            <text macro=\"page\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter entry entry-dictionary entry-encyclopedia\" match=\"any\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher-group\"/>\n            <text macro=\"page\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"isbn\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"genre\"/>\n            <text macro=\"publisher-group\"/>\n            <text macro=\"date\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"page\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"genre\"/>\n            <text macro=\"publisher-group\"/>\n            <text macro=\"date\"/>\n            <text macro=\"page\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"isbn\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"genre\"/>\n            <text macro=\"publisher-group\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"post-weblog post webpage\" match=\"any\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"date\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"broadcast motion_picture song\" match=\"any\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"medium\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher-group\"/>\n              <text macro=\"date\"/>\n            </group>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"isbn\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"genre\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher-group\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"manuscript\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"genre\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher-group\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"number\"/>\n            <text macro=\"date\"/>\n            <text macro=\"publisher-group\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else>\n          <group display=\"block\" delimiter=\". \">\n            <text macro=\"responsibility\"/>\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n            </choose>\n            <text macro=\"medium\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"date\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher-group\"/>\n            <text macro=\"number\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"page\"/>\n            <text macro=\"isbn\"/>\n            <text macro=\"url\"/>\n          </group>\n        </else>\n      </choose>\n      <group display=\"right-inline\">\n        <text macro=\"archive\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"