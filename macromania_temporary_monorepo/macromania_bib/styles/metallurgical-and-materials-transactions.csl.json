"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"chicago\" default-locale=\"en-US\">\n  <info>\n    <title>Metallurgical and Materials Transactions</title>\n    <id>http://www.zotero.org/styles/metallurgical-and-materials-transactions</id>\n    <link href=\"http://www.zotero.org/styles/metallurgical-and-materials-transactions\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-physics-society\" rel=\"template\"/>\n    <link href=\"http://www.tms.org/pubs/journals/MT/MTauthorguide.aspx\" rel=\"documentation\"/>\n    <author>\n      <name>Richard Karnesky</name>\n      <email>karnesky+zotero@gmail.com</email>\n      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <!--<category term=\"materials science\"/>-->\n    <summary>Style used by Met. Trans. A and B.</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"month-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"month\" form=\"short\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\" text-case=\"title\"/>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\".&#160;\" strip-periods=\"true\"/>\n    <text variable=\"page\" form=\"short\"/>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" prefix=\"&#160;[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\": \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text term=\"in\" suffix=\" \"/>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"number\"/>\n            <text macro=\"month-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n            <text macro=\"year-date\"/>\n            <text variable=\"volume\" prefix=\"vol.&#160;\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"