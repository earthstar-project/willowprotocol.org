"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" and=\"symbol\" initialize=\"false\" demote-non-dropping-particle=\"never\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Uppsala universitet - Historia</title>\n    <title-short>UU.Hist</title-short>\n    <id>http://www.zotero.org/styles/uppsala-universitet-historia</id>\n    <link href=\"http://www.zotero.org/styles/uppsala-universitet-historia\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/chicago-fullnote-bibliography\" rel=\"template\"/>\n    <link href=\"http://hist.uu.se/Student/Masterutbildning/Skrivauppsats/Formaliainstruktioner/tabid/5600/language/en-US/Default.aspx\" rel=\"documentation\"/>\n    <author>\n      <name>Torsten Feurstein</name>\n      <email>torstenfeurstein@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <summary>Style for Studia series (footnotes, no Ibid)</summary>\n    <updated>2016-10-13T20:38:45+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editortranslator\" form=\"short\">\n        <single>ed. &amp; trans.</single>\n        <multiple>eds. &amp; trans.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor container-author\" delimiter=\", \" suffix=\", \">\n          <name and=\"symbol\" initialize-with=\". \"/>\n          <label form=\"short\" prefix=\" (\" suffix=\".)\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"article-journal chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" font-weight=\"normal\" delimiter=\", \" prefix=\" \">\n          <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"verb-short\" plural=\"never\" strip-periods=\"true\" prefix=\" (\" suffix=\".)\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\" \">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\".)\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\">\n            <text variable=\"publisher\"/>\n            <text macro=\"title\"/>\n          </if>\n          <else>\n            <text macro=\"title\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\">\n            <group delimiter=\" \">\n              <text variable=\"title\" form=\"short\" font-style=\"normal\"/>\n            </group>\n          </if>\n          <else-if type=\"bill book graphic legislation motion_picture song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </else-if>\n          <else-if match=\"any\" type=\"broadcast\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" quotes=\"true\" font-style=\"italic\"/>\n              <text variable=\"publisher\"/>\n              <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\" form=\"short\"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </else-if>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"thesis\">\n        <choose>\n          <if variable=\"archive\" match=\"any\">\n            <group>\n              <text term=\"retrieved\" text-case=\"capitalize-first\" suffix=\" \"/>\n              <text term=\"from\" suffix=\" \"/>\n              <text variable=\"archive\" suffix=\".\"/>\n              <text variable=\"archive_location\" prefix=\" (\" suffix=\")\"/>\n            </group>\n          </if>\n          <else>\n            <group>\n              <text term=\"retrieved\" text-case=\"capitalize-first\" suffix=\" \"/>\n              <text term=\"from\" suffix=\" \"/>\n              <text variable=\"URL\"/>\n            </group>\n            <choose>\n              <if type=\"article\" match=\"none\"/>\n            </choose>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if type=\"webpage\" match=\"any\" variable=\"URL\">\n            <group delimiter=\" \">\n              <text variable=\"URL\" prefix=\"[\" suffix=\"]\"/>\n              <group prefix=\"(\" suffix=\")\">\n                <text term=\"accessed\" suffix=\": \"/>\n                <date form=\"numeric\" variable=\"accessed\"/>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"book graphic  motion_picture report manuscript speech webpage song\" match=\"any\">\n        <choose>\n          <if variable=\"version\">\n            <group delimiter=\" \">\n              <text variable=\"title\"/>\n              <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                <text term=\"version\" text-case=\"capitalize-first\"/>\n                <text variable=\"version\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"song graphic\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </else-if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"broadcast motion_picture article\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group delimiter=\" \">\n              <text term=\"part\" form=\"short\"/>\n              <number variable=\"number\"/>\n            </group>\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"publisher\"/>\n          </group>\n          <date delimiter=\" \" variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\" form=\"short\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"entry-encyclopedia\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\" font-style=\"italic\"/>\n        <text variable=\"collection-title\"/>\n        <text macro=\"access\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" prefix=\"&quot;\" suffix=\"&quot;\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n      <else-if type=\"thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"event\" type=\"speech\" match=\"none\">\n              <text variable=\"genre\"/>\n            </if>\n          </choose>\n          <choose>\n            <if type=\"article-journal article-magazine\" match=\"none\">\n              <group delimiter=\": \">\n                <choose>\n                  <if variable=\"publisher-place\">\n                    <text variable=\"publisher-place\" prefix=\" \"/>\n                  </if>\n                  <else>\n                    <text variable=\"event-place\"/>\n                  </else>\n                </choose>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"container-title\" match=\"none\">\n        <choose>\n          <if variable=\"event\">\n            <choose>\n              <if variable=\"genre\" match=\"none\">\n                <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <text variable=\"event\"/>\n              </if>\n              <else>\n                <group delimiter=\" \">\n                  <text variable=\"genre\" text-case=\"capitalize-first\"/>\n                  <text term=\"presented at\"/>\n                  <text variable=\"event\"/>\n                </group>\n              </else>\n            </choose>\n          </if>\n          <else-if type=\"speech\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"bill legal_case legislation\" match=\"none\">\n        <choose>\n          <if variable=\"issued\">\n            <group prefix=\" \">\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"year-suffix\"/>\n              <choose>\n                <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n                  <date variable=\"issued\">\n                    <date-part prefix=\", \" name=\"month\"/>\n                    <date-part prefix=\" \" name=\"day\"/>\n                  </date>\n                </if>\n              </choose>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued-sort\">\n    <choose>\n      <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if match=\"none\" variable=\"archive\" type=\"manuscript\">\n            <group>\n              <date variable=\"issued\" prefix=\" \">\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"year-suffix\"/>\n            </group>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"song\" match=\"any\">\n        <group>\n          <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n        </group>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\" prefix=\" (\" suffix=\")\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <choose>\n          <if type=\"book article\" match=\"any\"/>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"edition\" form=\"short\" text-case=\"lowercase\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group prefix=\", \" delimiter=\", \">\n          <group>\n            <text variable=\"volume\" font-style=\"italic\"/>\n            <text variable=\"issue\" font-style=\"italic\" prefix=\" (\" suffix=\")\"/>\n          </group>\n          <group>\n            <label prefix=\" \" suffix=\" \" variable=\"locator\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\"/>\n      <else-if type=\"book graphic motion_picture report song chapter paper-conference\" match=\"any\">\n        <group delimiter=\"., \" prefix=\"  \">\n          <text macro=\"edition\"/>\n          <group>\n            <text term=\"volume\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1-\"/>\n          </group>\n          <group>\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"legal_case\">\n        <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n          <text variable=\"authority\"/>\n          <date variable=\"issued\" form=\"text\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group prefix=\", \">\n      <choose>\n        <if locator=\"chapter\">\n          <label text-case=\"lowercase\" suffix=\".\" variable=\"locator\" form=\"short\"/>\n        </if>\n        <else>\n          <label suffix=\".\" variable=\"locator\" form=\"short\"/>\n        </else>\n      </choose>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <group>\n      <choose>\n        <if type=\"chapter paper-conference entry-encyclopedia\" match=\"any\">\n          <text term=\"in\" text-case=\"lowercase\" suffix=\" \"/>\n        </if>\n      </choose>\n      <text macro=\"container-contributors\"/>\n      <text macro=\"secondary-contributors\"/>\n      <text macro=\"container-title\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"bill legal_case legislation\" match=\"none\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"motion_picture article\" match=\"any\"/>\n    </choose>\n  </macro>\n  <macro name=\"legal-cites\">\n    <choose>\n      <if type=\"bill legal_case legislation\" match=\"any\">\n        <group delimiter=\" \" prefix=\", \">\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"volume\"/>\n              <text variable=\"container-title\"/>\n              <group delimiter=\" \">\n                <text term=\"section\" form=\"symbol\"/>\n                <text variable=\"section\"/>\n              </group>\n              <text variable=\"page\"/>\n            </if>\n            <else>\n              <choose>\n                <if type=\"legal_case\">\n                  <text variable=\"number\" prefix=\"No. \"/>\n                </if>\n                <else>\n                  <text variable=\"number\" prefix=\"Pub. L. No. \"/>\n                  <group delimiter=\" \">\n                    <text term=\"section\" form=\"symbol\"/>\n                    <text variable=\"section\"/>\n                  </group>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"sort-key\">\n    <choose>\n      <if variable=\"archive archive_location\" match=\"all\">\n        <text value=\"2\"/>\n      </if>\n      <else-if type=\"manuscript\" match=\"any\">\n        <text value=\"2\"/>\n      </else-if>\n      <else>\n        <text value=\"1\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"6\" et-al-use-first=\"1\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\" givenname-disambiguation-rule=\"primary-name\">\n    <sort>\n      <key macro=\"sort-key\"/>\n      <key macro=\"author\"/>\n      <key macro=\"issued-sort\"/>\n    </sort>\n    <layout delimiter=\"; \" suffix=\".\">\n      <group delimiter=\"; \">\n        <choose>\n          <if match=\"all\" variable=\"archive archive_location\">\n            <group>\n              <group delimiter=\", \" suffix=\", \">\n                <text variable=\"archive\"/>\n                <text variable=\"archive_location\"/>\n              </group>\n              <choose>\n                <if type=\"article-newspaper article-magazine article-journal\" match=\"any\">\n                  <group>\n                    <text variable=\"title\" prefix=\"&quot;\" suffix=\"&quot; \"/>\n                    <text variable=\"container-title\" font-style=\"italic\" suffix=\" \"/>\n                    <date delimiter=\" \" variable=\"issued\">\n                      <date-part name=\"day\"/>\n                      <date-part name=\"month\" text-case=\"lowercase\"/>\n                      <date-part name=\"year\"/>\n                    </date>\n                  </group>\n                </if>\n                <else>\n                  <group delimiter=\", \">\n                    <text variable=\"title\" font-style=\"normal\"/>\n                    <date delimiter=\" \" variable=\"issued\">\n                      <date-part name=\"day\"/>\n                      <date-part name=\"month\" form=\"short\" text-case=\"lowercase\"/>\n                      <date-part name=\"year\"/>\n                    </date>\n                  </group>\n                </else>\n              </choose>\n            </group>\n          </if>\n          <else-if type=\"manuscript\" match=\"any\">\n            <group>\n              <text variable=\"title\"/>\n              <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\" form=\"short\"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </else-if>\n        </choose>\n      </group>\n      <group>\n        <text macro=\"author-short\"/>\n        <text macro=\"issued-year\"/>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"8\" et-al-use-first=\"6\" et-al-use-last=\"true\" entry-spacing=\"0\" line-spacing=\"2\">\n    <sort>\n      <key macro=\"sort-key\"/>\n      <key macro=\"author\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <group>\n        <choose>\n          <if match=\"all\" variable=\"archive archive_location\">\n            <group delimiter=\" \">\n              <group delimiter=\", \" suffix=\", \">\n                <text variable=\"archive\"/>\n                <text variable=\"archive_location\"/>\n              </group>\n              <choose>\n                <if type=\"article-newspaper article-magazine article-journal\" match=\"any\">\n                  <group delimiter=\" \">\n                    <text variable=\"title\" prefix=\"&quot;\" suffix=\"&quot;\"/>\n                    <text variable=\"container-title\" font-style=\"normal\" text-decoration=\"none\"/>\n                    <date delimiter=\" \" variable=\"issued\">\n                      <date-part name=\"day\"/>\n                      <date-part name=\"month\"/>\n                      <date-part name=\"year\"/>\n                    </date>\n                  </group>\n                </if>\n                <else-if match=\"any\" variable=\"issued\">\n                  <group delimiter=\", \">\n                    <text variable=\"title\" font-style=\"normal\"/>\n                    <date delimiter=\" \" variable=\"issued\">\n                      <date-part name=\"day\"/>\n                      <date-part name=\"month\" form=\"short\" text-case=\"lowercase\"/>\n                      <date-part name=\"year\"/>\n                    </date>\n                  </group>\n                </else-if>\n                <else>\n                  <text variable=\"title\"/>\n                  <text term=\"no date\" form=\"short\" prefix=\"(\" suffix=\")\"/>\n                </else>\n              </choose>\n            </group>\n          </if>\n          <else-if type=\"manuscript\" match=\"any\">\n            <group>\n              <text variable=\"title\"/>\n            </group>\n          </else-if>\n          <else>\n            <group suffix=\".\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"issued\"/>\n                <text macro=\"title\" prefix=\" \"/>\n                <text macro=\"container\"/>\n              </group>\n              <text macro=\"legal-cites\"/>\n              <text macro=\"locators\"/>\n              <group delimiter=\", \" prefix=\", \">\n                <text macro=\"event\"/>\n                <text macro=\"publisher\"/>\n              </group>\n              <group>\n                <text macro=\"access\" prefix=\" \"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"