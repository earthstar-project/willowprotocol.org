"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Oikos</title>\n    <id>http://www.zotero.org/styles/oikos</id>\n    <link href=\"http://www.zotero.org/styles/oikos\" rel=\"self\"/>\n    <link href=\"http://www.oikos.ekol.lu.se/\" rel=\"documentation\"/>\n    <author>\n      <name>Johan Asplund</name>\n      <email>asplundj@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Josefa Bleu</name>\n      <email>josefa.bleu@gmail.com</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0030-1299</issn>\n    <eissn>1600-0706</eissn>\n    <updated>2023-10-12T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\"\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" suffix=\"n.\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\"\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"book\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"in press\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"page-chapter\">\n    <choose>\n      <if variable=\"page\">\n        <label variable=\"page\" form=\"short\" prefix=\"p\" suffix=\" \" plural=\"never\"/>\n        <text variable=\"page\"/>\n      </if>\n      <else>\n        <text term=\"in press\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"year-suffix\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n        <group>\n          <label variable=\"locator\" suffix=\".\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-short\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <date variable=\"issued\" prefix=\" \" suffix=\".\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"book\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <text prefix=\" - \" suffix=\".\" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\". - \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <names variable=\"editor translator\">\n              <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n              <label form=\"short\" prefix=\" (\" suffix=\"),\" strip-periods=\"true\"/>\n            </names>\n            <text variable=\"container-title\" prefix=\" \" suffix=\".\"/>\n            <text macro=\"edition\" prefix=\" \"/>\n            <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n            <group suffix=\".\">\n              <text macro=\"publisher\" prefix=\" \" suffix=\",\"/>\n              <text macro=\"page-chapter\" prefix=\" \"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article article-journal review\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <text variable=\"container-title\" form=\"short\" prefix=\" - \"/>\n          <choose>\n            <if variable=\"page\">\n              <text variable=\"volume\" prefix=\" \"/>\n              <text variable=\"page\" prefix=\": \" suffix=\".\"/>\n            </if>\n            <else>\n              <text term=\"in press\" prefix=\" \" suffix=\".\"/>\n            </else>\n          </choose>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <text variable=\"container-title\" form=\"short\" prefix=\" - \"/>\n          <text variable=\"volume\" prefix=\" \"/>\n          <text variable=\"page\" prefix=\": \" suffix=\".\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"