"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Media, Culture &amp; Society</title>\n    <id>http://www.zotero.org/styles/media-culture-and-society</id>\n    <link href=\"http://www.zotero.org/styles/media-culture-and-society\" rel=\"self\"/>\n    <link href=\"http://www.sagepub.com/journals/Journal200958/manuscriptSubmission\" rel=\"documentation\"/>\n    <author>\n      <name>David Nicolas Hopmann</name>\n      <email>dnh@gmx.net</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"communications\"/>\n    <issn>0163-4437</issn>\n    <eissn>1460-3675</eissn>\n    <summary>The MCS style.</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\"\" and=\"text\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\"\" and=\"text\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\", \" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"URL\"/>\n        <group prefix=\" (\" suffix=\")\">\n          <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"day\" suffix=\", \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"thesis bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"day-month\">\n    <date variable=\"issued\">\n      <date-part name=\"month\"/>\n      <date-part name=\"day\" prefix=\" \"/>\n    </date>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"false\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\" suffix=\",\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"6\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n      </group>\n      <choose>\n        <if type=\"article-newspaper article-magazine\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n            <text macro=\"title\" prefix=\"'\" suffix=\"',\"/>\n          </group>\n          <group prefix=\" \" delimiter=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"day-month\"/>\n            <text variable=\"edition\"/>\n            <text variable=\"page\"/>\n          </group>\n        </if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n            <text macro=\"title\" suffix=\",\"/>\n          </group>\n          <group prefix=\" \" delimiter=\" \">\n            <text variable=\"container-title\" suffix=\",\" font-style=\"italic\"/>\n            <group delimiter=\": \">\n              <text variable=\"volume\"/>\n              <text variable=\"page\" suffix=\".\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"title\" prefix=\" \" suffix=\",\"/>\n          <group prefix=\" \" delimiter=\", \">\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" suffix=\".\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" suffix=\".\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n            <text macro=\"title\" suffix=\",\"/>\n            <group delimiter=\", \" suffix=\",\">\n              <group delimiter=\" \">\n                <text macro=\"editor\" prefix=\"in \"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n              </group>\n              <text variable=\"volume\" prefix=\"vol. \"/>\n              <text variable=\"collection-title\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\" \" prefix=\" \" delimiter=\" \">\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            <text macro=\"title\" suffix=\",\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <group prefix=\" \" delimiter=\" \">\n            <text variable=\"container-title\" suffix=\",\" font-style=\"italic\"/>\n            <group delimiter=\": \">\n              <text variable=\"volume\"/>\n              <text variable=\"page\" suffix=\".\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"