"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>The American Midland Naturalist</title>\n    <id>http://www.zotero.org/styles/the-american-midland-naturalist</id>\n    <link href=\"http://www.zotero.org/styles/the-american-midland-naturalist\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/the-biological-bulletin\" rel=\"template\"/>\n    <link href=\"https://www3.nd.edu/~ammidnat/editorial.html\" rel=\"documentation\"/>\n    <link href=\"http://www3.nd.edu/~ammidnat/authors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <issn>0003-0031</issn>\n    <eissn>1938-4238</eissn>\n    <updated>2017-06-01T15:24:16+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" font-variant=\"small-caps\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\" \">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text macro=\"edition\"/>\n          </group>\n          <text macro=\"editor-translator\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"year-suffix\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n          <group delimiter=\":\">\n            <text variable=\"volume\" font-weight=\"bold\"/>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </group>\n        <choose>\n          <if variable=\"page volume\" match=\"none\">\n            <text variable=\"DOI\" prefix=\", doi: \"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"chapter-info\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\" \">\n            <label plural=\"never\" variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\":\"/>\n            <text macro=\"editor-translator\" suffix=\".\"/>\n            <text variable=\"container-title\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\" strip-periods=\"false\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"book-access\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"number-of-pages\"/>\n          <label variable=\"page\" form=\"short\" plural=\"never\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" subsequent-author-substitute=\"---\" subsequent-author-substitute-rule=\"partial-each\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n        <text macro=\"title\"/>\n        <text macro=\"access\"/>\n        <text macro=\"chapter-info\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"book-access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"