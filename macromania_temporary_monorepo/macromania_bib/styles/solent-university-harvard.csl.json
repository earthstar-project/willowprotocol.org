"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Solent University - Harvard</title>\n    <id>http://www.zotero.org/styles/solent-university-harvard</id>\n    <link href=\"http://www.zotero.org/styles/solent-university-harvard\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/taylor-and-francis-harvard-x\" rel=\"template\"/>\n    <link href=\"https://portal.solent.ac.uk/library/help/factsheets/resources/referencing-harvard.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Solent University Harvard style based on based on BRITISH STANDARDS INSTITUTION, 2010. BS ISO 690:2010\n    Information and documentation &#8211; Guidelines for bibliographic\n    references and citations to information resources.</summary>\n    <updated>2018-11-15T09:54:44+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"motion_picture broadcast\" match=\"none\">\n        <names variable=\"author\">\n          <name name-as-sort-order=\"first\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" and=\"text\" delimiter-precedes-last=\"never\">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n          </name>\n          <label form=\"short\" prefix=\", \"/>\n          <et-al font-style=\"italic\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"anon-bib\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"motion_picture broadcast\" match=\"none\">\n        <names variable=\"author\">\n          <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n          <et-al font-style=\"italic\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"anon\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\"/>\n  </macro>\n  <macro name=\"anon-bib\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"uppercase\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"graphic report webpage post post-weblog motion_picture broadcast\">\n        <choose>\n          <if variable=\"URL\">\n            <group delimiter=\". \">\n              <group prefix=\" [\" suffix=\"]\" delimiter=\" \">\n                <date variable=\"accessed\" delimiter=\" \" prefix=\"viewed \">\n                  <date-part name=\"day\"/>\n                  <date-part name=\"month\" form=\"long\" strip-periods=\"true\"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n              <group>\n                <text value=\"Available from: \"/>\n                <text variable=\"URL\"/>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\" text-case=\"capitalize-first\"/>\n          <group>\n            <text variable=\"genre\"/>\n            <text variable=\"number\" prefix=\" No. \"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"graphic\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\" \">\n            <text variable=\"title\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n            <choose>\n              <if variable=\"URL\">\n                <text value=\"digital image\" prefix=\"[\" suffix=\"]\"/>\n              </if>\n            </choose>\n          </group>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill book legal_case legislation song report speech webpage\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\" \">\n            <text variable=\"title\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n          </group>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n      <else-if type=\"motion_picture broadcast\" match=\"any\"/>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"report-details\">\n    <choose>\n      <if type=\"report\">\n        <group>\n          <text variable=\"genre\"/>\n          <text variable=\"number\" prefix=\" No. \"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <group delimiter=\". \">\n          <names variable=\"editor translator\" delimiter=\", \">\n            <name sort-separator=\", \" initialize-with=\".\" and=\"text\" delimiter=\", \">\n              <name-part name=\"family\" text-case=\"uppercase\"/>\n            </name>\n            <label form=\"short\" prefix=\", \"/>\n          </names>\n          <text variable=\"container-title\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"container-title\"/>\n      </else-if>\n      <else-if type=\"broadcast webpage\"/>\n      <else-if type=\"post post-weblog\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <group delimiter=\" \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\" \">\n          <text variable=\"container-title\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <group delimiter=\": \">\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n          </group>\n          <text macro=\"report-details\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <text variable=\"event\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if variable=\"accessed\">\n        <choose>\n          <if type=\"webpage\">\n            <date variable=\"accessed\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-bib\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if variable=\"accessed\">\n        <choose>\n          <if type=\"webpage\">\n            <date variable=\"accessed\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n    <group prefix=\" \">\n      <choose>\n        <if type=\"motion_picture\">\n          <choose>\n            <if variable=\"medium\">\n              <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n            </if>\n            <else>\n              <text value=\"film\" prefix=\"[\" suffix=\"]\"/>\n            </else>\n          </choose>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"broadcast\">\n          <choose>\n            <if variable=\"medium\">\n              <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n            </if>\n            <else>\n              <text value=\"broadcast\" prefix=\"[\" suffix=\"]\"/>\n            </else>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <group delimiter=\" \">\n            <number variable=\"edition\" form=\"ordinal\"/>\n            <text term=\"edition\" form=\"short\"/>\n          </group>\n        </if>\n        <else>\n          <text variable=\"edition\" suffix=\".\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article article-journal article-magazine\" match=\"any\">\n        <group prefix=\", \" delimiter=\", \">\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <date variable=\"issued\" delimiter=\" \" prefix=\", \">\n          <date-part name=\"day\"/>\n          <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n        </date>\n        <group delimiter=\" \" prefix=\", \">\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill  book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </else-if>\n      <else-if type=\"broadcast\">\n        <group delimiter=\". \" prefix=\". \">\n          <text variable=\"publisher\"/>\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\" form=\"long\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <choose>\n            <if variable=\"container-title\" match=\"none\">\n              <text macro=\"event\"/>\n            </if>\n          </choose>\n          <text macro=\"publisher\"/>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"director\">\n    <choose>\n      <if type=\"motion_picture\">\n        <names variable=\"author\">\n          <name prefix=\"Directed by \" delimiter=\", \" and=\"text\" delimiter-precedes-last=\"never\">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n          </name>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"blog-post-date\">\n    <choose>\n      <if type=\"post-weblog\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" line-spacing=\"1\" et-al-min=\"4\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\" suffix=\",\"/>\n      <group delimiter=\". \" prefix=\" \">\n        <text macro=\"issued-bib\"/>\n        <text macro=\"title\"/>\n        <text macro=\"director\"/>\n        <text macro=\"container\"/>\n        <text macro=\"blog-post-date\"/>\n      </group>\n      <text macro=\"locators\"/>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"