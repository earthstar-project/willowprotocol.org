"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\" page-range-format=\"minimal\">\n  <info>\n    <title>Medicine &amp; Science in Sports &amp; Exercise</title>\n    <title-short>MSSE</title-short>\n    <id>http://www.zotero.org/styles/medicine-and-science-in-sports-and-exercise</id>\n    <link href=\"http://www.zotero.org/styles/medicine-and-science-in-sports-and-exercise\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/vancouver\" rel=\"template\"/>\n    <link href=\"http://edmgr.ovid.com/msse/accounts/ifauth.htm\" rel=\"documentation\"/>\n    <author>\n      <name>Charles Parnot</name>\n      <email>charles.parnot@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0195-9131</issn>\n    <eissn>1530-0315</eissn>\n    <updated>2020-08-23T15:13:28+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"no date\" form=\"long\">date unknown</term>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \" suffix=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\" suffix=\"; \"/>\n    <text macro=\"date\"/>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"article-journal\">\n            <group delimiter=\" \">\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <choose>\n                <if match=\"none\" variable=\"page\">\n                  <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n                    <text term=\"cited\"/>\n                    <date variable=\"accessed\">\n                      <date-part name=\"year\"/>\n                      <date-part name=\"month\" prefix=\" \" form=\"short\" strip-periods=\"true\"/>\n                      <date-part name=\"day\" prefix=\" \"/>\n                    </date>\n                  </group>\n                </if>\n              </choose>\n            </group>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" prefix=\"[\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"page\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text term=\"available at\" text-case=\"capitalize-first\"/>\n                <text variable=\"URL\"/>\n              </group>\n              <text variable=\"DOI\" prefix=\"doi:\"/>\n            </group>\n          </if>\n        </choose>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n            <text term=\"cited\"/>\n            <date variable=\"accessed\" suffix=\" \">\n              <date-part name=\"year\"/>\n              <date-part name=\"month\" prefix=\" \" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"day\" prefix=\" \"/>\n            </date>\n          </group>\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" et-al-min=\"7\" et-al-use-first=\"3\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\" suffix=\".\"/>\n            <group delimiter=\" \">\n              <text variable=\"number-of-pages\"/>\n              <label plural=\"never\" variable=\"page\" form=\"short\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"publisher\" prefix=\" \"/>\n            <group delimiter=\" \">\n              <label plural=\"never\" variable=\"page\" form=\"short\"/>\n              <text variable=\"page\" suffix=\".\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"title\" suffix=\". \"/>\n          <group delimiter=\" \" suffix=\". \">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n            <choose>\n              <if type=\"article-journal\" match=\"any\">\n                <choose>\n                  <if variable=\"page\" match=\"none\">\n                    <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n                  </if>\n                </choose>\n              </if>\n            </choose>\n          </group>\n          <text macro=\"date\" suffix=\";\"/>\n          <text variable=\"volume\"/>\n          <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          <text variable=\"page\" prefix=\":\" suffix=\".\"/>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"