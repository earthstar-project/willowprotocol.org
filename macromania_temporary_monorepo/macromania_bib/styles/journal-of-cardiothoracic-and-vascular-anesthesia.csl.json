"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Cardiothoracic and Vascular Anesthesia</title>\n    <id>http://www.zotero.org/styles/journal-of-cardiothoracic-and-vascular-anesthesia</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-cardiothoracic-and-vascular-anesthesia\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/vancouver\" rel=\"template\"/>\n    <link href=\"http://www.jcardioanesthesia.com/authorinfo\" rel=\"documentation\"/>\n    <author>\n      <name>Matt Levin</name>\n      <email>mlevin@mlevin.net</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1053-0770</issn>\n    <eissn>1532-8422</eissn>\n    <summary>Style for the Journal for Cardiothoracic and Vascular Anesthesia, published by Elsevier. Based on Vancouver.</summary>\n    <updated>2013-09-05T20:11:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <date form=\"text\" delimiter=\" \">\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"et-al\">et al</term>\n      <term name=\"page\" form=\"short\">\n        <single>p</single>\n        <multiple>pp</multiple>\n      </term>\n      <term name=\"edition\" form=\"short\">ed</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\" delimiter-precedes-et-al=\"always\"/>\n      <et-al term=\"et-al\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <group delimiter=\": \">\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <text term=\"in\"/>\n        </if>\n      </choose>\n      <names variable=\"editor\" suffix=\" (ed):\">\n        <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" prefix=\", \"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" suffix=\",\">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\": \">\n          <group delimiter=\" \">\n            <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n            <text term=\"from\"/>\n          </group>\n          <text variable=\"URL\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n          <text term=\"cited\" text-case=\"lowercase\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal article-magazine chapter paper-conference article-newspaper\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\" form=\"short\"/>\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </group>\n        <text macro=\"edition\" prefix=\" \"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper book\" match=\"any\">\n        <text variable=\"title\" suffix=\". \"/>\n        <choose>\n          <if variable=\"URL\">\n            <text term=\"internet\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n        <text macro=\"edition\" prefix=\". \"/>\n      </if>\n      <else-if type=\"chapter\">\n        <text variable=\"title\" suffix=\", \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \" prefix=\"(\" suffix=\").\">\n          <text term=\"edition\" form=\"short\"/>\n          <number variable=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper book chapter\" match=\"any\">\n        <group delimiter=\" \">\n          <date variable=\"issued\" form=\"text\"/>\n          <text macro=\"accessed-date\"/>\n        </group>\n      </if>\n      <else-if type=\"report\">\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else>\n        <group suffix=\".\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <text macro=\"accessed-date\" prefix=\" \"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"page\" prefix=\":\" suffix=\", \"/>\n      </if>\n      <else-if type=\"book chapter\" match=\"any\">\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else>\n        <group prefix=\" \" delimiter=\" \">\n          <label variable=\"page\" plural=\"never\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"journal-location\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"volume\"/>\n        <!--<text variable=\"issue\" prefix=\"(\" suffix=\")\"/>-->\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"date-pages\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text macro=\"pages\"/>\n        <text macro=\"date\"/>\n      </if>\n      <else-if type=\"book chapter\" match=\"any\">\n        <group prefix=\" \" suffix=\" \">\n          <text macro=\"date\" suffix=\", \"/>\n          <text macro=\"pages\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\": \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n      </group>\n      <group delimiter=\" \">\n        <text macro=\"editor\"/>\n        <text macro=\"container-title\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"journal-location\"/>\n      </group>\n      <text macro=\"date-pages\"/>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"