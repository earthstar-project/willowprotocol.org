"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"chicago\" default-locale=\"en-US\">\n  <info>\n    <title>Law &amp; Society Review</title>\n    <id>http://www.zotero.org/styles/law-and-society-review</id>\n    <link href=\"http://www.zotero.org/styles/law-and-society-review\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-marketing-association\" rel=\"template\"/>\n    <link href=\"http://www.lawandsociety.org/review/review_stylesheet.html\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <category field=\"law\"/>\n    <issn>0023-9216</issn>\n    <eissn>1540-5893</eissn>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" suffix=\" \"/>\n        <names variable=\"editor translator\" delimiter=\", \">\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"first\" and=\"symbol\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\" delimiter-precedes-et-al=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"legal_case\" match=\"none\">\n        <names variable=\"author\">\n          <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\". \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <choose>\n              <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n                <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n              </if>\n              <else>\n                <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n              </else>\n            </choose>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\".\" text-case=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" suffix=\",\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"none\">\n        <text variable=\"genre\" suffix=\", \"/>\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <group prefix=\" (\" suffix=\")\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <text prefix=\" (\" term=\"no date\" suffix=\")\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if type=\"legal_case\" match=\"none\">\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \" suffix=\".\">\n          <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n          <text variable=\"volume\" prefix=\"vol. \"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <text variable=\"volume\"/>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators-journal\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"page\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-newspaper\">\n    <choose>\n      <if type=\"article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <choose>\n              <if is-numeric=\"section\">\n                <text value=\"sec.\"/>\n              </if>\n            </choose>\n            <text variable=\"section\"/>\n          </group>\n          <group delimiter=\" \">\n            <choose>\n              <if is-numeric=\"page\">\n                <label variable=\"page\" form=\"short\"/>\n              </if>\n            </choose>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"date-newspaper\">\n    <choose>\n      <if type=\"article-magazine article-newspaper\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"month\" form=\"short\"/>\n          <date-part name=\"day\" form=\"numeric\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\" delimiter=\" \">\n              <date-part name=\"day\"/>\n              <date-part name=\"month\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"type-order\">\n    <choose>\n      <if type=\"legislation\">\n        <text value=\"3\"/>\n      </if>\n      <else-if type=\"legal_case\">\n        <text value=\"2\"/>\n      </else-if>\n      <else>\n        <text value=\"1\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\":\">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\" et-al-min=\"3\" et-al-use-first=\"1\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\">\n    <sort>\n      <key macro=\"type-order\"/>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"legal_case\">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <group suffix=\",\" prefix=\" \">\n            <text macro=\"secondary-contributors\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <group prefix=\" \" delimiter=\" \">\n            <text variable=\"container-title\"/>\n            <text variable=\"volume\"/>\n          </group>\n          <text variable=\"page\" prefix=\", \"/>\n          <group prefix=\" (\" suffix=\").\" delimiter=\" \">\n            <text variable=\"authority\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </if>\n        <else>\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <group>\n                <text macro=\"author\"/>\n                <text macro=\"issued\"/>\n              </group>\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <text macro=\"container-contributors\"/>\n                <text macro=\"container-title\"/>\n              </group>\n              <text variable=\"collection-title\"/>\n              <text macro=\"secondary-contributors\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"locators-journal\"/>\n            </group>\n            <text macro=\"date-newspaper\"/>\n            <text macro=\"locators-newspaper\"/>\n          </group>\n          <text macro=\"access\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"