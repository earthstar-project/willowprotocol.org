"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Acta Chiropterologica</title>\n    <id>http://www.zotero.org/styles/acta-chiropterologica</id>\n    <link href=\"http://www.zotero.org/styles/acta-chiropterologica\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-vertebrate-paleontology\" rel=\"template\"/>\n    <link href=\"http://system.miiz.waw.pl/ac/journal/for_authors/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>citationstyler@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1508-1109</issn>\n    <eissn>1733-5329</eissn>\n    <updated>2019-07-12T13:11:31+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\", \" initialize-with=\". \" and=\"text\" name-as-sort-order=\"first\" delimiter=\", \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" initialize-with=\". \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n      <text macro=\"number-of-pages\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <text variable=\"container-title\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-pages\"/>\n      <label plural=\"never\" variable=\"number-of-pages\" form=\"short\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"false\" disambiguate-add-givenname=\"false\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group>\n              <group delimiter=\", \">\n                <text variable=\"title\" text-case=\"title\"/>\n                <text macro=\"edition\"/>\n              </group>\n              <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text macro=\"publisher\" prefix=\" \"/>\n          </if>\n          <else-if type=\"thesis\" match=\"any\">\n            <text variable=\"title\" suffix=\".\"/>\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n              <text variable=\"number-of-pages\"/>\n            </group>\n            <text term=\"page\" form=\"short\" prefix=\"p\"/>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <label text-case=\"capitalize-first\" variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\" form=\"short\"/>\n                </group>\n                <group delimiter=\" \">\n                  <text term=\"in\"/>\n                  <text macro=\"container-title\" text-case=\"title\"/>\n                  <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n                </group>\n              </group>\n              <group delimiter=\", \">\n                <text variable=\"collection-title\" prefix=\". \"/>\n                <text macro=\"edition\"/>\n              </group>\n              <group delimiter=\". \">\n                <text variable=\"volume\" prefix=\"vol. \" suffix=\".\"/>\n                <group delimiter=\", \">\n                  <text macro=\"publisher\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\" form=\"short\"/>\n              <text variable=\"URL\" prefix=\"Available at \"/>\n              <date variable=\"accessed\" prefix=\"Accessed \">\n                <date-part name=\"month\" suffix=\" \"/>\n                <date-part name=\"day\" suffix=\", \"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n            <text prefix=\" \" macro=\"publisher\"/>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n            </group>\n            <group delimiter=\". \">\n              <text variable=\"container-title\" form=\"long\" text-case=\"title\"/>\n              <group delimiter=\": \">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"