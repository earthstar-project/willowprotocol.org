"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"de-DE\">\n  <info>\n    <title>Pädagogische Hochschule Vorarlberg (Deutsch)</title>\n    <title-short>PH Vorarlberg</title-short>\n    <id>http://www.zotero.org/styles/padagogische-hochschule-vorarlberg</id>\n    <link href=\"http://www.zotero.org/styles/padagogische-hochschule-vorarlberg\" rel=\"self\"/>\n    <link href=\"http://www.ph-vorarlberg.ac.at/fileadmin/user_upload/RED_inst/RED_bac/Richtlinien_wissArb__April_2010_korrNa_end.pdf\" rel=\"documentation\"/>\n    <!--  useful things: non breaking space: &#160;   -->\n    <author>\n      <name>Karl Peböck</name>\n      <email>karl.peboeck@ph-vorarlberg.ac.at</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Citation Style of the Paedagogische Hochschule Vorarlberg, mostly german</summary>\n    <updated>2014-07-20T12:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\"; \">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\"/\" delimiter-precedes-last=\"always\" form=\"long\" suffix=\":&#160;\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"anon\"/>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\" delimiter=\"; \">\n      <name form=\"short\" delimiter=\"/\" initialize-with=\". \" delimiter-precedes-last=\"always\" sort-separator=\",\" suffix=\"&#160;\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"anon\"/>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\"; \">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\"; \" delimiter-precedes-last=\"always\" form=\"long\" suffix=\"\" prefix=\".&#160;\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\").&#160;\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"none\">\n        <text term=\"anonymous\" form=\"short\" suffix=\"&#160;\" strip-periods=\"true\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"normal\" suffix=\".&#160;\"/>\n      </if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"title\" font-style=\"normal\" suffix=\". \"/>\n      </else-if>\n      <else-if type=\"webpage\">\n        <text variable=\"title\" font-style=\"normal\" suffix=\".&#160;\"/>\n      </else-if>\n      <else-if type=\"article-journal article-newspaper article-magazine\" match=\"any\">\n        <text variable=\"title\" font-style=\"normal\" suffix=\". \"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"normal\" prefix=\"&#160;\" suffix=\"&#160;\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"internet\">\n    <choose>\n      <if type=\"webpage\">\n        <text value=\", Internet\"/>\n      </if>\n      <else>\n        <text value=\"\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection-title\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"collection-title\" prefix=\"&#160;(=&#160;\" suffix=\"),&#160;\"/>\n      </if>\n      <else-if type=\"article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song webpage\" match=\"any\">\n        <text variable=\"collection-title\" prefix=\"&#160;(=&#160;\" suffix=\")\"/>\n      </else-if>\n      <else>\n        <text value=\"o.&#160;J.\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <choose>\n          <if variable=\"URL\">\n            <text value=\" (In: \"/>\n            <text variable=\"URL\" suffix=\")\"/>\n            <group prefix=\"&#160;(\" delimiter=\" \" suffix=\")\">\n              <text value=\"Zugriff:\"/>\n              <date variable=\"accessed\">\n                <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                <date-part name=\"month\" suffix=\".\" form=\"numeric-leading-zeros\"/>\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n            </group>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"article-journal article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if variable=\"URL\">\n            <text value=\".&#160;(In: \"/>\n            <text variable=\"URL\" suffix=\")\"/>\n            <group prefix=\" (\" delimiter=\" \" suffix=\")\">\n              <text value=\"Zugriff:\"/>\n              <date variable=\"accessed\">\n                <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                <date-part name=\"month\" suffix=\".\" form=\"numeric-leading-zeros\"/>\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <choose>\n        <if variable=\"publisher-place\">\n          <text variable=\"publisher-place\" suffix=\"\"/>\n        </if>\n        <else>\n          <text value=\"o. O.\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"published-date\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" form=\"numeric\" suffix=\".&#160;\"/>\n          <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if type=\"article-journal article-newspaper webpage\" match=\"any\">\n        <text value=\"\"/>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text value=\"o.&#160;J.\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date-two\">\n    <choose>\n      <if type=\"article-journal article-newspaper\" match=\"any\">\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text value=\"o.&#160;J.\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text value=\"\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date-three\">\n    <choose>\n      <if type=\"webpage\">\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\">\n              <date-part name=\"year\" prefix=\"Stand: \" suffix=\".\"/>\n            </date>\n          </if>\n          <else>\n            <text value=\"o.&#160;J.\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text value=\"\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date-four\">\n    <choose>\n      <if type=\"webpage\">\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text value=\"o.&#160;J.\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text value=\"\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"volume\" prefix=\"\" suffix=\"&#160;\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\" prefix=\"(\" suffix=\"),&#160;\"/>\n        </date>\n        <text variable=\"issue\" prefix=\"\" suffix=\"\"/>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <text variable=\"volume\" prefix=\",&#160;\" suffix=\"&#160;\"/>\n        <!-- <date variable=\"issued\">\n\t     <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n\t     </date> -->\n        <text variable=\"issue\" prefix=\",&#160;\" suffix=\"\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal article-newspaper\" match=\"any\">\n        <label variable=\"page\" form=\"short\" prefix=\",&#160;\" suffix=\".&#160;\" strip-periods=\"true\"/>\n        <text variable=\"page\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"numeric\" suffix=\". Aufl.&#160;\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".&#160;\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-prefix\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal article-magazine article-newspaper\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\":&#160;\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"normal\" suffix=\".&#160;\"/>\n      </if>\n      <else-if type=\"article-journal article-newspaper article-magazine\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"normal\" suffix=\"&#160;\"/>\n      </else-if>\n      <else-if type=\"webpage\">\n        <text variable=\"container-title\" font-style=\"normal\" suffix=\".&#160;\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" font-style=\"normal\" suffix=\"&#160;\"/>\n      </else>\n    </choose>\n  </macro>\n  <!-- Hier beginnt die Formatierung für das Kurzzitat im Text - this is where the citation starts -->\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <text macro=\"author-short\"/>\n      <text macro=\"year-date\"/>\n      <text macro=\"year-date-two\"/>\n      <text macro=\"year-date-four\"/>\n      <text variable=\"locator\" prefix=\",&#160;S.&#160;\"/>\n      <text macro=\"internet\"/>\n    </layout>\n  </citation>\n  <!-- Hier beginnt die Formatierung für das Literaturverzeichnis -  this is where the bibliography starts -->\n  <bibliography hanging-indent=\"true\" et-al-min=\"3\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\"\">\n      <text macro=\"author\"/>\n      <text macro=\"title\"/>\n      <text macro=\"container-prefix\"/>\n      <!-- text macro=\"editor\"/  -->\n      <text macro=\"container-title\"/>\n      <text macro=\"collection-title\" suffix=\",&#160;\"/>\n      <text macro=\"edition\"/>\n      <text macro=\"locator\"/>\n      <text macro=\"publisher\"/>\n      <text macro=\"editor\"/>\n      <text macro=\"published-date\"/>\n      <text macro=\"year-date\" prefix=\" \" suffix=\"\"/>\n      <text macro=\"year-date-three\"/>\n      <text macro=\"pages\"/>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"