"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-GB\">\n  <info>\n    <title>The Cancer Journal</title>\n    <id>http://www.zotero.org/styles/the-cancer-journal</id>\n    <link href=\"http://www.zotero.org/styles/the-cancer-journal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/baghdad-science-journal\" rel=\"template\"/>\n    <link href=\"http://edmgr.ovid.com/ppo/accounts/ifauth.htm\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1528-9117</issn>\n    <eissn>1540-336X</eissn>\n    <updated>2019-06-17T15:16:15+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <date form=\"text\" delimiter=\" \">\n      <date-part name=\"day\" form=\"ordinal\"/>\n      <date-part name=\"month\"/>\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"presented at\">presented at</term>\n      <term name=\"available at\">available from</term>\n      <term name=\"section\" form=\"short\">sect.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <group delimiter=\": \">\n      <choose>\n        <if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n          <text term=\"in\" text-case=\"capitalize-first\"/>\n        </if>\n      </choose>\n      <names variable=\"editor\">\n        <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" prefix=\", \"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \" suffix=\",\">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" type=\"webpage post post-weblog\">\n        <group>\n          <text macro=\"accessed-date\"/>\n        </group>\n        <text term=\"available at\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <text variable=\"URL\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL DOI\" match=\"any\"/>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal article-magazine chapter paper-conference article-newspaper review review-book entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <text macro=\"edition\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\". \">\n            <text variable=\"container-title\"/>\n            <group delimiter=\" \">\n              <text term=\"section\" form=\"short\" text-case=\"capitalize-first\"/>\n              <text variable=\"section\"/>\n            </group>\n          </group>\n          <text variable=\"number\"/>\n        </group>\n      </else-if>\n      <else-if type=\"speech\">\n        <group delimiter=\": \" suffix=\";\">\n          <group delimiter=\" \">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            <text term=\"presented at\"/>\n          </group>\n          <text variable=\"event\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"article-journal article-magazine chapter paper-conference article-newspaper review review-book entry-dictionary entry-encyclopedia\" match=\"none\">\n        <text variable=\"title\" font-style=\"normal\" suffix=\".\"/>\n        <text macro=\"edition\" prefix=\". \"/>\n      </if>\n      <else>\n        <text variable=\"title\" suffix=\".\"/>\n      </else>\n    </choose>\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"genre\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper review review-book\" match=\"any\">\n        <group suffix=\";\" delimiter=\" \">\n          <date variable=\"issued\" form=\"text\" date-parts=\"year\"/>\n        </group>\n      </if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <group delimiter=\", \">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"report\">\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n        </date>\n        <text macro=\"accessed-date\" prefix=\" \"/>\n      </else-if>\n      <else-if type=\"patent\">\n        <group suffix=\".\">\n          <group delimiter=\", \">\n            <text variable=\"number\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n          <text macro=\"accessed-date\" prefix=\" \"/>\n        </group>\n      </else-if>\n      <else-if type=\"speech\">\n        <group delimiter=\"; \">\n          <group delimiter=\" \">\n            <date variable=\"issued\" delimiter=\" \">\n              <date-part name=\"year\"/>\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"day\"/>\n            </date>\n          </group>\n          <text variable=\"event-place\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage\" match=\"none\">\n        <group>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper review review-book chapter paper-conference\" match=\"any\">\n        <text variable=\"page\" prefix=\":\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-location\">\n    <choose>\n      <if type=\"article-journal article-magazine review review-book\" match=\"any\">\n        <text variable=\"volume\"/>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\". \" suffix=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n      </group>\n      <group delimiter=\" \">\n        <text macro=\"editor\" suffix=\".\"/>\n        <text macro=\"container-title\"/>\n        <text macro=\"publisher\"/>\n        <group>\n          <text macro=\"date\"/>\n          <text macro=\"journal-location\"/>\n          <text macro=\"pages\"/>\n        </group>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"