"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\">\n  <info>\n    <title>University of Westminster - Harvard</title>\n    <id>http://www.zotero.org/styles/harvard-university-of-westminster</id>\n    <link href=\"http://www.zotero.org/styles/harvard-university-of-westminster\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/the-open-university-harvard\" rel=\"template\"/>\n    <link href=\"https://libguides.westminster.ac.uk/referencing/examples\" rel=\"documentation\"/>\n    <link href=\"https://www.westminster.ac.uk/sites/default/public-files/general-documents/Referencing%20Your%20Work%20booklet_06.1.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Cristiano Diamanti</name>\n      <uri>http://www.mendeley.com/profiles/cristiano-diamanti1/</uri>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Harvard - University of Westminster</summary>\n    <updated>2020-09-26T16:50:41+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-GB\">\n    <terms>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\" \">\n      <name delimiter=\" \" and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\",\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" et-al-use-first=\"1\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL DOI\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"available at\" text-case=\"capitalize-first\"/>\n          <choose>\n            <if match=\"any\" variable=\"DOI\">\n              <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n            </if>\n            <else>\n              <text variable=\"URL\"/>\n            </else>\n          </choose>\n          <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book legal_case motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else-if type=\"graphic\" match=\"any\">\n        <text variable=\"title\"/>\n        <text value=\"image\" prefix=\" [\" suffix=\"]\"/>\n      </else-if>\n      <else-if type=\"thesis\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"title\"/>\n          <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"published-date\">\n    <choose>\n      <if type=\"article-newspaper article-magazine\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" form=\"long\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"director\">\n    <group delimiter=\" \">\n      <text value=\"Directed by\"/>\n      <names variable=\"director\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" strip-periods=\"true\" form=\"short\" plural=\"never\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"4\" et-al-use-first=\"3\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author\" names-min=\"3\" names-use-first=\"1\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\" prefix=\" \"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"collection-title\"/>\n              <choose>\n                <if type=\"paper-conference\" match=\"any\">\n                  <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n                </if>\n              </choose>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"report book thesis\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"published-date\"/>\n            </group>\n          </else-if>\n          <else-if type=\"legislation\" match=\"any\">\n            <text variable=\"page\"/>\n          </else-if>\n          <else-if type=\"legal_case bill\" match=\"any\">\n            <text variable=\"page\" prefix=\" \"/>\n          </else-if>\n          <else-if type=\"motion_picture\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\" \">\n                <text macro=\"director\"/>\n                <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"broadcast\" match=\"any\">\n            <group>\n              <text variable=\"title\"/>\n              <date form=\"text\" variable=\"issued\" prefix=\" (\" suffix=\").\">\n                <date-part name=\"year\"/>\n              </date>\n              <choose>\n                <if variable=\"collection-title\" match=\"any\">\n                  <text variable=\"collection-title\" font-style=\"italic\" prefix=\" \"/>\n                  <text value=\"television programme\" prefix=\" [\" suffix=\"]\"/>\n                  <text macro=\"publisher\"/>\n                  <text variable=\"page\" prefix=\", \"/>\n                </if>\n                <else>\n                  <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\" \"/>\n                  <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n                  <text macro=\"publisher\" suffix=\", \"/>\n                </else>\n              </choose>\n              <group>\n                <text variable=\"dimensions\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </else-if>\n          <else-if type=\"manuscript\" match=\"any\">\n            <group delimiter=\": \">\n              <group delimiter=\". \">\n                <group delimiter=\", \">\n                  <text variable=\"archive_location\"/>\n                  <group delimiter=\" \">\n                    <label text-case=\"capitalize-first\" variable=\"number\" form=\"short\"/>\n                    <text variable=\"call-number\"/>\n                  </group>\n                </group>\n                <text variable=\"archive-place\"/>\n              </group>\n              <text variable=\"archive\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group delimiter=\" \">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\" prefix=\" \"/>\n            </group>\n          </else>\n        </choose>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"