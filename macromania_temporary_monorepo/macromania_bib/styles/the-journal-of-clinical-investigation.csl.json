"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>The Journal of Clinical Investigation</title>\n    <title-short>JCI</title-short>\n    <id>http://www.zotero.org/styles/the-journal-of-clinical-investigation</id>\n    <link href=\"http://www.zotero.org/styles/the-journal-of-clinical-investigation\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-clinical-oncology\" rel=\"template\"/>\n    <link href=\"https://www.jci.org/kiosks/publish/examples\" rel=\"documentation\"/>\n    <author>\n      <name>Francesco Caiazza</name>\n      <email>francesco.caiazza@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0021-9738</issn>\n    <eissn>1558-8238</eissn>\n    <updated>2023-09-13T13:56:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group>\n      <text variable=\"publisher-place\" suffix=\": \"/>\n      <text variable=\"publisher\" suffix=\"; \"/>\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <group delimiter=\". \">\n              <text variable=\"URL\"/>\n              <group delimiter=\" \">\n                <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"month\"/>\n                  <date-part name=\"day\" prefix=\" \"/>\n                  <date-part name=\"year\" prefix=\", \"/>\n                </date>\n              </group>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"article\">\n          <text term=\"preprint\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n        <else-if match=\"any\" type=\"webpage post post-weblog\">\n          <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal\">\n        <choose>\n          <if variable=\"volume page\" match=\"none\">\n            <choose>\n              <if variable=\"DOI URL\" match=\"any\">\n                <group prefix=\"[published online ahead of print: \" suffix=\"]\">\n                  <date variable=\"issued\" delimiter=\" \">\n                    <date-part name=\"month\" form=\"long\" text-case=\"capitalize-first\"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </if>\n              <else>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography delimiter-precedes-et-al=\"always\" et-al-min=\"4\" et-al-use-first=\"1\" line-spacing=\"2\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"title\" font-style=\"italic\"/>\n          <text macro=\"publisher\" prefix=\". \"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group delimiter=\":\">\n              <text macro=\"publisher\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\";\" suffix=\".\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n              <text macro=\"date\"/>\n            </group>\n            <group delimiter=\":\">\n              <group>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n          <text macro=\"access\" prefix=\" \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"