"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Entomologia Experimentalis et Applicata</title>\n    <id>http://www.zotero.org/styles/entomologia-experimentalis-et-applicata</id>\n    <link href=\"http://www.zotero.org/styles/entomologia-experimentalis-et-applicata\" rel=\"self\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/%28ISSN%291570-7458/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Adam Zeilinger</name>\n      <email>arzeilinger@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0013-8703</issn>\n    <eissn>1570-7458</eissn>\n    <updated>2012-09-09T21:58:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed. by</term>\n      <term name=\"editor\" form=\"short\">ed. by </term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <name and=\"symbol\" initialize-with=\"\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" suffix=\"n.\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\"\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation collapse=\"year-suffix\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\". \">\n            <text variable=\"container-title\" prefix=\" \" suffix=\". \"/>\n            <names variable=\"editor translator\" prefix=\"(\" suffix=\")\">\n              <label form=\"short\" strip-periods=\"true\"/>\n              <name and=\"symbol\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n            </names>\n            <text macro=\"edition\" prefix=\" \"/>\n            <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n            <group suffix=\".\">\n              <text macro=\"publisher\" prefix=\" \" suffix=\",\"/>\n              <group prefix=\" \">\n                <label variable=\"page\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\"/>\n            <group prefix=\" \">\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"