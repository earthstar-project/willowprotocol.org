"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"display-and-sort\" page-range-format=\"expanded\" default-locale=\"de-DE\">\n  <info>\n    <title>Steinbeis-Hochschule - School of Management &amp; Innovation (Deutsch)</title>\n    <id>http://www.zotero.org/styles/steinbeis-hochschule-school-of-management-and-innovation</id>\n    <link href=\"http://www.zotero.org/styles/steinbeis-hochschule-school-of-management-and-innovation\" rel=\"self\"/>\n    <link href=\"https://shb-smi-community.academyfive.net/files/Startseite/Info_Point/Wissenschaftliches_Arbeiten/Guidelineszumwissenschaftlichenarbeiten_v1.3.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"social_science\"/>\n    <summary>Format for the Steinbeis Hochschule (SMI). Zitierformat nach der Pr√§sentation \"Wissenschaftliches Arbeiten\"</summary>\n    <updated>2020-04-26T18:22:50+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"volume\" form=\"short\">Jg.</term>\n      <term name=\"issue\">Heft</term>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"ibid\">ebenda</term>\n      <term name=\"accessed\">Stand</term>\n      <term name=\"anonymous\" form=\"short\">o.V.</term>\n    </terms>\n  </locale>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\"/ \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <choose>\n          <if type=\"post-weblog webpage\" match=\"none\">\n            <text macro=\"anon\"/>\n          </if>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-long\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" delimiter=\"/ \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n  </macro>\n  <macro name=\"anon\">\n    <group>\n      <text value=\"o.\" text-case=\"lowercase\"/>\n      <text value=\"V.\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text macro=\"edition\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"container-journal\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\": \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text variable=\"container-title\"/>\n          </group>\n          <group delimiter=\" \">\n            <number variable=\"volume\" form=\"ordinal\"/>\n            <text term=\"volume\" form=\"short\"/>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"issue\"/>\n            <text variable=\"issue\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\": \">\n          <choose>\n            <if type=\"post-weblog webpage\" match=\"none\">\n              <text term=\"online\"/>\n            </if>\n          </choose>\n          <text variable=\"URL\"/>\n        </group>\n        <group delimiter=\" \" prefix=\" (\" suffix=\")\">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\" form=\"numeric\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-chapter\">\n    <choose>\n      <if type=\"paper-conference chapter\" match=\"any\">\n        <group delimiter=\": \">\n          <text term=\"in\"/>\n          <names variable=\"editor\">\n            <name initialize-with=\". \" delimiter=\"/ \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\"/>\n            <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n          </names>\n          <text variable=\"container-title\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"type-sort\">\n    <choose>\n      <if type=\"post-weblog webpage\" match=\"any\">\n        <text value=\"2\"/>\n      </if>\n      <else>\n        <text value=\"1\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\" match=\"any\">\n          <group delimiter=\", \">\n            <text term=\"ibid\"/>\n            <text macro=\"locators\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <choose>\n              <if type=\"post-weblog webpage\" match=\"none\">\n                <group delimiter=\" \">\n                  <text macro=\"author-short\"/>\n                  <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\": \">\n                  <choose>\n                    <if variable=\"author editor\" match=\"any\">\n                      <text macro=\"author-short\"/>\n                      <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n                    </if>\n                    <else>\n                      <text macro=\"access\"/>\n                    </else>\n                  </choose>\n                </group>\n              </else>\n            </choose>\n            <text macro=\"locators\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"11\" et-al-use-first=\"7\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"type-sort\"/>\n      <key macro=\"author-long\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"post-weblog webpage\" match=\"none\">\n          <group delimiter=\": \">\n            <group delimiter=\" \">\n              <text macro=\"author-long\"/>\n              <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group>\n              <group delimiter=\", \">\n                <text variable=\"title\"/>\n                <text macro=\"container-chapter\"/>\n              </group>\n              <text macro=\"container-journal\" prefix=\". \"/>\n            </group>\n          </group>\n          <text macro=\"publisher\" prefix=\". \"/>\n          <text macro=\"pages\" prefix=\", \"/>\n          <text macro=\"access\" prefix=\", \"/>\n        </if>\n        <else>\n          <!--Internetquellen-->\n          <group delimiter=\": \">\n            <group delimiter=\" \">\n              <text macro=\"author-long\"/>\n              <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group delimiter=\". \">\n              <text variable=\"container-title\"/>\n              <text macro=\"access\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"