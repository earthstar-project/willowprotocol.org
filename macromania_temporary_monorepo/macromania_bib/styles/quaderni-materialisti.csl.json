"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" default-locale=\"it-IT\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Quaderni Materialisti (Italiano)</title>\n    <id>http://www.zotero.org/styles/quaderni-materialisti</id>\n    <link href=\"http://www.zotero.org/styles/quaderni-materialisti\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/universita-di-bologna-lettere\" rel=\"template\"/>\n    <link href=\"http://www.quadernimaterialisti.unimib.it/?p=107\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"humanities\"/>\n    <category field=\"linguistics\"/>\n    <category field=\"philosophy\"/>\n    <issn>1972-3792</issn>\n    <updated>2018-02-15T10:42:23+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"it-IT\">\n    <terms>\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>pp.</multiple>\n      </term>\n      <term name=\"translator\" form=\"verb-short\">tr. it. di</term>\n      <term name=\"no date\">s.d.</term>\n      <term name=\"ibid\" form=\"short\">ivi</term>\n      <term name=\"ibid\" form=\"long\">ibidem</term>\n      <term name=\"cited\" form=\"short\">cit.</term>\n      <term name=\"cited\" form=\"long\">op.cit.</term>\n      <term name=\"issue\" form=\"short\">fasc.</term>\n      <term name=\"season-01\">primavera</term>\n      <term name=\"season-02\">estate</term>\n      <term name=\"season-03\">autunno</term>\n      <term name=\"season-04\">inverno</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <group delimiter=\", \">\n      <choose>\n        <if variable=\"author\">\n          <names variable=\"editor\" delimiter=\", \">\n            <name initialize-with=\". \"/>\n            <label form=\"verb\" text-case=\"lowercase\" prefix=\" (\" suffix=\") \" strip-periods=\"true\"/>\n          </names>\n          <choose>\n            <if variable=\"container-author\">\n              <group>\n                <text term=\"in\" text-case=\"lowercase\" suffix=\" \"/>\n                <names variable=\"container-author\">\n                  <name initialize-with=\". \"/>\n                </names>\n              </group>\n            </if>\n          </choose>\n        </if>\n      </choose>\n      <choose>\n        <if variable=\"author editor\" match=\"any\">\n          <choose>\n            <if match=\"none\" variable=\"original-title\">\n              <names variable=\"translator\" delimiter=\", \">\n                <label form=\"verb-short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\" \"/>\n                <name prefix=\" \"/>\n              </names>\n            </if>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <text macro=\"editor-translator\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text macro=\"editor-translator\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor-note\">\n    <names variable=\"editor\">\n      <name initialize-with=\". \"/>\n      <label form=\"verb\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"translator-note\">\n    <names variable=\"translator\">\n      <name sort-separator=\", \" delimiter=\", \"/>\n      <label form=\"verb-short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"recipient-note\">\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"lowercase\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-note\">\n    <names variable=\"author\">\n      <name initialize-with=\". \"/>\n      <substitute>\n        <text macro=\"editor-note\"/>\n        <text macro=\"translator-note\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient-note\"/>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n      <label form=\"verb\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"verb-short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"recipient\">\n    <choose>\n      <if type=\"personal_communication\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n          <else>\n            <text term=\"letter\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n    <text macro=\"recipient-note\" prefix=\" \"/>\n  </macro>\n  <macro name=\"contributors\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient\" prefix=\". \"/>\n  </macro>\n  <macro name=\"recipient-short\">\n    <names variable=\"recipient\">\n      <label form=\"verb\" prefix=\" \" text-case=\"lowercase\" suffix=\" \"/>\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"long\" initialize-with=\".\" delimiter=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient-short\"/>\n  </macro>\n  <macro name=\"contributors-sort\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"interviewer-note\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"lowercase\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title-note\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <text variable=\"genre\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"original-note\"/>\n          <text variable=\"title\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"false\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"personal_communication\" match=\"none\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"original\"/>\n          <text variable=\"title\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if disambiguate=\"true\">\n        <choose>\n          <if variable=\"title\" match=\"none\">\n            <choose>\n              <if type=\"interview\">\n                <text term=\"interview\" text-case=\"lowercase\"/>\n              </if>\n              <else-if type=\"manuscript speech\" match=\"any\">\n                <text variable=\"genre\" form=\"short\"/>\n              </else-if>\n              <else-if type=\"personal_communication\">\n                <text macro=\"issued\"/>\n              </else-if>\n            </choose>\n          </if>\n          <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\"/>\n          </else-if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n        <text term=\"cited\" form=\"short\"/>\n      </if>\n      <else>\n        <text term=\"cited\" form=\"long\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"description-note\">\n    <group delimiter=\", \">\n      <text macro=\"interviewer-note\"/>\n      <text variable=\"medium\"/>\n      <choose>\n        <if variable=\"title\" match=\"none\"/>\n        <else-if type=\"thesis speech\" match=\"any\"/>\n        <else>\n          <text variable=\"genre\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"description\">\n    <group delimiter=\", \">\n      <group delimiter=\". \">\n        <text macro=\"interviewer\"/>\n        <text variable=\"medium\" text-case=\"capitalize-first\"/>\n      </group>\n      <choose>\n        <if variable=\"title\" match=\"none\"/>\n        <else-if type=\"thesis speech\" match=\"any\"/>\n        <else>\n          <text variable=\"genre\" text-case=\"capitalize-first\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"legal_case\" match=\"none\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection-title\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <group delimiter=\" \" prefix=\", \">\n          <text term=\"volume\" form=\"short\"/>\n          <text variable=\"volume\"/>\n        </group>\n        <group prefix=\", \">\n          <text term=\"issue\" form=\"short\" suffix=\" \"/>\n          <number variable=\"issue\"/>\n        </group>\n      </if>\n      <else-if type=\"legal_case\">\n        <text variable=\"volume\" prefix=\", \"/>\n        <text variable=\"container-title\" prefix=\" \"/>\n        <text variable=\"page\" prefix=\" \"/>\n        <text variable=\"locator\" prefix=\", \"/>\n      </else-if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group prefix=\", \" delimiter=\", \">\n          <group>\n            <text term=\"volume\" form=\"short\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"roman\"/>\n          </group>\n          <choose>\n            <if variable=\"locator\" match=\"none\">\n              <group>\n                <number variable=\"number-of-volumes\" form=\"numeric\"/>\n                <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locators-newspaper\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"edition\"/>\n            <text term=\"edition\"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\" \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <group>\n      <text term=\"presented at\" suffix=\" \"/>\n      <text variable=\"event\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"graphic report\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else-if type=\"legal_case\">\n            <text variable=\"authority\" suffix=\" \"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else-if type=\"article-journal\">\n            <date variable=\"issued\">\n              <date-part name=\"month\" text-case=\"lowercase\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"month\" text-case=\"lowercase\" suffix=\" \"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"point-locators-subsequent\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <choose>\n                <if variable=\"volume\">\n                  <group>\n                    <text term=\"volume\" form=\"short\" text-case=\"lowercase\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <label variable=\"locator\" form=\"short\" prefix=\", \" suffix=\" \"/>\n                  </group>\n                </if>\n                <else>\n                  <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n                </else>\n              </choose>\n            </if>\n          </choose>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <number variable=\"volume\" form=\"numeric\" suffix=\":\"/>\n        </else-if>\n      </choose>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"point-locators\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <text macro=\"pages\"/>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"locator\" prefix=\": \"/>\n      </else-if>\n      <else-if type=\"legal_case\"/>\n      <else>\n        <group prefix=\", \">\n          <choose>\n            <if locator=\"page\" match=\"none\">\n              <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n            </if>\n          </choose>\n          <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" prefix=\", \" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"locators-chapter\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <choose>\n          <if variable=\"page\">\n            <text variable=\"volume\" suffix=\":\"/>\n            <text variable=\"page\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-journal\">\n    <choose>\n      <if type=\"article-journal\">\n        <label variable=\"page\" form=\"short\" prefix=\", \" suffix=\" \"/>\n        <text variable=\"page\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive-note\">\n    <group delimiter=\", \">\n      <text variable=\"archive_location\"/>\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\". \">\n      <text variable=\"archive_location\" text-case=\"capitalize-first\"/>\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"issue\">\n    <choose>\n      <if type=\"article-journal legal_case\" match=\"any\">\n        <text macro=\"issued\" prefix=\", \"/>\n      </if>\n      <else-if variable=\"publisher-place publisher\" match=\"any\">\n        <group prefix=\", \" delimiter=\", \">\n          <group delimiter=\", \">\n            <choose>\n              <if variable=\"title\" match=\"none\"/>\n              <else-if type=\"thesis speech\" match=\"any\">\n                <text variable=\"genre\"/>\n              </else-if>\n            </choose>\n            <text macro=\"event\"/>\n          </group>\n          <text macro=\"publisher\"/>\n          <text macro=\"issued\"/>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"issued\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access-note\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"graphic report\" match=\"any\">\n          <text macro=\"archive-note\"/>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"none\">\n          <text macro=\"archive-note\"/>\n        </else-if>\n      </choose>\n      <choose>\n        <if type=\"legal_case\" match=\"none\">\n          <text variable=\"URL\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\". \">\n      <choose>\n        <if type=\"graphic report\" match=\"any\">\n          <text macro=\"archive\"/>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"none\">\n          <text macro=\"archive\"/>\n        </else-if>\n      </choose>\n      <choose>\n        <if type=\"legal_case\" match=\"none\">\n          <text variable=\"URL\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"original-note\">\n    <group delimiter=\", \">\n      <text variable=\"original-title\" font-style=\"italic\"/>\n      <text variable=\"original-publisher-place\"/>\n      <text variable=\"original-publisher\"/>\n      <date form=\"text\" variable=\"original-date\"/>\n      <names variable=\"translator\">\n        <label form=\"verb-short\" suffix=\" \"/>\n        <name/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"original\">\n    <group delimiter=\", \">\n      <text variable=\"original-title\"/>\n      <text variable=\"original-publisher-place\"/>\n      <text variable=\"original-publisher\"/>\n      <date form=\"text\" variable=\"original-date\"/>\n      <names variable=\"translator\">\n        <label form=\"verb-short\" suffix=\" \"/>\n        <name initialize-with=\". \"/>\n      </names>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\">\n    <layout delimiter=\"; \" suffix=\".\">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" form=\"long\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n            <text macro=\"point-locators-subsequent\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\" form=\"short\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"contributors-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"point-locators-subsequent\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"contributors-note\"/>\n            <text macro=\"title-note\"/>\n            <text macro=\"description-note\"/>\n            <text macro=\"secondary-contributors\"/>\n            <text macro=\"container-contributors\"/>\n            <text macro=\"container-title\"/>\n          </group>\n          <text macro=\"locators\"/>\n          <text macro=\"collection-title\" prefix=\", \"/>\n          <text macro=\"issue\"/>\n          <text macro=\"locators-newspaper\" prefix=\", \"/>\n          <text macro=\"access-note\" prefix=\", \"/>\n          <text macro=\"point-locators\"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"11\" et-al-use-first=\"7\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"contributors-sort\"/>\n      <key variable=\"title\"/>\n      <key variable=\"genre\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\", \">\n        <text macro=\"contributors\"/>\n        <text macro=\"title\"/>\n        <text macro=\"description\"/>\n        <text macro=\"secondary-contributors\"/>\n        <group delimiter=\", \">\n          <text macro=\"container-contributors\"/>\n          <text macro=\"container-title\"/>\n          <text macro=\"locators-chapter\"/>\n        </group>\n      </group>\n      <text macro=\"locators\"/>\n      <text macro=\"collection-title\" prefix=\". \"/>\n      <text macro=\"issue\"/>\n      <text macro=\"locators-newspaper\" prefix=\", \"/>\n      <text macro=\"locators-journal\"/>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"