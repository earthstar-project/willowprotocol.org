"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>American Journal of Enology and Viticulture</title>\n    <title-short>AJEV</title-short>\n    <id>http://www.zotero.org/styles/american-journal-of-enology-and-viticulture</id>\n    <link href=\"http://www.zotero.org/styles/american-journal-of-enology-and-viticulture\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/friedrich-schiller-universitat-jena-medizinische-fakultat\" rel=\"template\"/>\n    <link href=\"http://www.ajevonline.org/content/authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <category field=\"botany\"/>\n    <issn>0002-9254</issn>\n    <updated>2017-12-08T19:33:34+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">as found on the</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\"\" delimiter=\", \"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" et-al-min=\"11\" et-al-use-first=\"10\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\"\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"version\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n        <text variable=\"title\" font-style=\"normal\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"available at\"/>\n          <text variable=\"publisher\"/>\n          <text value=\"website\"/>\n          <text variable=\"URL\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\" line-spacing=\"2\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author-short\"/>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"editor-translator\" prefix=\" \"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\" prefix=\" \"/>\n              <group>\n                <text variable=\"genre\" prefix=\" \"/>\n                <text variable=\"number\" prefix=\" No. \"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n                <text variable=\"publisher\"/>\n                <text variable=\"publisher-place\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n                <text variable=\"container-title\"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"editor-translator\"/>\n                <text macro=\"pages\" prefix=\" \" suffix=\".\"/>\n              </group>\n              <text macro=\"publisher\" prefix=\" \" suffix=\".\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"editor-translator\" prefix=\" \"/>\n              <group prefix=\" \" suffix=\".\">\n                <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n                <group prefix=\"[\" suffix=\"]\">\n                  <text variable=\"genre\"/>\n                </group>\n                <group prefix=\" \">\n                  <text variable=\"volume\"/>\n                </group>\n                <text variable=\"page\" prefix=\":\"/>\n              </group>\n              <text macro=\"access\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"