"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"de-DE\">\n  <info>\n    <title>Jahrbuch für evangelikale Theologie (Deutsch)</title>\n    <title-short>JETh</title-short>\n    <id>http://www.zotero.org/styles/jahrbuch-fur-evangelikale-theologie</id>\n    <link href=\"http://www.zotero.org/styles/jahrbuch-fur-evangelikale-theologie\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/kritische-ausgabe\" rel=\"template\"/>\n    <link href=\"http://www.afet.de/jahrbuch/Richtlinien.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Dirk Kellner</name>\n      <email>dirk.kellner@arcor.de</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"humanities\"/>\n    <category field=\"theology\"/>\n    <issn>0933-3835</issn>\n    <updated>2012-12-22T18:37:55+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">Hrsg. v.</term>\n      <term name=\"translator\" form=\"verb-short\">Übers. v.</term>\n      <term name=\"editortranslator\" form=\"verb-short\">Hrsg. &amp; übers. von</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\": \">\n      <name delimiter=\"/\" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\"/>\n      <substitute>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\"/\" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\"/>\n      <substitute>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if variable=\"container-title\">\n        <choose>\n          <if type=\"book thesis chapter\" match=\"any\">\n            <text term=\"in\" text-case=\"lowercase\" suffix=\": \"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\",\"/>\n          </if>\n          <else>\n            <text term=\"in\" text-case=\"lowercase\" suffix=\": \"/>\n            <text variable=\"container-title\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <label form=\"verb-short\"/>\n      <name delimiter=\"/\" delimiter-precedes-last=\"always\" prefix=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if match=\"any\" is-numeric=\"edition\">\n        <number vertical-align=\"sup\" variable=\"edition\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volumes\">\n    <choose>\n      <if match=\"any\" is-numeric=\"number-of-volumes\">\n        <number vertical-align=\"baseline\" suffix=\" Bände,\" variable=\"number-of-volumes\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume-edition\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\"/>\n      <else>\n        <text variable=\"volume\" prefix=\" \"/>\n        <text variable=\"issue\" prefix=\"/\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\",\"/>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-journal\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text macro=\"edition\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"point-locators-subsequent\">\n    <text variable=\"locator\" prefix=\" \"/>\n  </macro>\n  <macro name=\"point-locators\">\n    <choose>\n      <if type=\"book\" match=\"all\" variable=\"volume\">\n        <text variable=\"volume\" prefix=\" \" suffix=\",\"/>\n      </if>\n    </choose>\n    <choose>\n      <if variable=\"page\">\n        <text variable=\"locator\" prefix=\" \"/>\n      </if>\n      <else>\n        <text variable=\"locator\" prefix=\" \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\" prefix=\" \"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\". \">\n          <text variable=\"URL\" prefix=\"Internet: \"/>\n          <date delimiter=\".\" variable=\"accessed\" prefix=\"Zuletzt geprüft am: \">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\" form=\"numeric\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"3\" disambiguate-add-names=\"true\">\n    <layout delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\"/>\n            <text macro=\"point-locators-subsequent\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group>\n            <choose>\n              <if type=\"book thesis\" match=\"any\">\n                <group>\n                  <text macro=\"author-short\"/>\n                  <text variable=\"title\" form=\"short\" font-style=\"italic\" text-decoration=\"none\" prefix=\", \"/>\n                  <choose>\n                    <if match=\"any\" variable=\"volume\">\n                      <text variable=\"volume\" prefix=\" \"/>\n                    </if>\n                  </choose>\n                  <text macro=\"point-locators-subsequent\" prefix=\", \"/>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\", \">\n                  <text macro=\"author-short\"/>\n                  <text variable=\"title\" form=\"short\" font-style=\"normal\" prefix=\" \"/>\n                  <text macro=\"point-locators-subsequent\"/>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </else-if>\n        <else>\n          <group>\n            <group suffix=\", \">\n              <choose>\n                <if type=\"book thesis\" match=\"any\">\n                  <text macro=\"author\"/>\n                  <text variable=\"title\" font-style=\"italic\"/>\n                  <text variable=\"collection-title\" prefix=\", \"/>\n                  <text variable=\"collection-number\" prefix=\" \"/>\n                </if>\n                <else>\n                  <text macro=\"author\"/>\n                  <text variable=\"title\"/>\n                </else>\n              </choose>\n            </group>\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text macro=\"container-title\"/>\n                <text macro=\"editor\" suffix=\",\"/>\n                <text macro=\"volume-edition\" suffix=\",\"/>\n                <text macro=\"volumes\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"issued\"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"access\"/>\n                <group delimiter=\", \">\n                  <text macro=\"pages\"/>\n                  <text macro=\"point-locators\"/>\n                </group>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"7\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;: \" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group suffix=\", \">\n        <text macro=\"author\"/>\n        <group>\n          <choose>\n            <if type=\"book thesis\" match=\"any\">\n              <text variable=\"title\" font-style=\"italic\"/>\n            </if>\n            <else>\n              <text variable=\"title\"/>\n            </else>\n          </choose>\n        </group>\n      </group>\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"container-title\"/>\n          <text macro=\"editor\" suffix=\",\"/>\n          <text macro=\"volume-edition\" suffix=\", \"/>\n          <text macro=\"volumes\"/>\n          <text variable=\"collection-title\" suffix=\" \"/>\n          <text variable=\"collection-number\" suffix=\",\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <group delimiter=\". \">\n          <text macro=\"access\"/>\n          <text macro=\"pages\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"