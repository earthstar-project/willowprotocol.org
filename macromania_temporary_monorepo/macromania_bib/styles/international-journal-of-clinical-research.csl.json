"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"minimal\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>International Journal of Clinical Research</title>\n    <title-short>IJCR</title-short>\n    <id>http://www.zotero.org/styles/international-journal-of-clinical-research</id>\n    <link href=\"http://www.zotero.org/styles/international-journal-of-clinical-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/elsevier-vancouver\" rel=\"template\"/>\n    <link href=\"https://www.ijcrcentral.com/index.php/IJCR/information/authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>2675-2611</issn>\n    <updated>2021-08-06T07:59:58+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"long\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" prefix=\" \"/>\n          <date-part name=\"day\" prefix=\" \"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \" text-case=\"title\"/>\n    <text variable=\"publisher\" suffix=\"; \"/>\n    <text macro=\"year-date\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\">\n        <group delimiter=\". \">\n          <text variable=\"PMID\" prefix=\"PMID: \"/>\n          <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n        </group>\n      </if>\n      <else-if type=\"webpage post-weblog\" match=\"any\">\n        <choose>\n          <if variable=\"URL\">\n            <text variable=\"URL\"/>\n            <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n              <text term=\"accessed\"/>\n              <date variable=\"accessed\" form=\"text\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\"/>\n            <text variable=\"volume\" prefix=\"vol. \"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group>\n                <text term=\"in\" text-case=\"sentence\" suffix=\": \"/>\n                <text macro=\"editor\"/>\n              </group>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" form=\"short\"/>\n                <text variable=\"volume\" prefix=\"vol. \"/>\n              </group>\n              <text macro=\"edition\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\" plural=\"never\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"number\"/>\n            </group>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\". \">\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <group delimiter=\", \">\n              <text variable=\"publisher\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\":\">\n            <group delimiter=\" \">\n              <group delimiter=\". \">\n                <text variable=\"title\"/>\n                <text variable=\"container-title\" form=\"short\" text-case=\"title\" strip-periods=\"true\" suffix=\".\"/>\n              </group>\n              <group delimiter=\";\">\n                <text macro=\"year-date\"/>\n                <group>\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\"/>\n                </group>\n              </group>\n            </group>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"