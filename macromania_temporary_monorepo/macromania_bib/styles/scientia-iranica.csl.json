"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" demote-non-dropping-particle=\"sort-only\" version=\"1.0\" class=\"in-text\" default-locale=\"en-US\">\n  <info>\n    <title>Scientia Iranica</title>\n    <id>http://www.zotero.org/styles/scientia-iranica</id>\n    <link href=\"http://www.zotero.org/styles/scientia-iranica\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/the-optical-society\" rel=\"template\"/>\n    <link href=\"http://scientiairanica.sharif.edu/journal/authors.note\" rel=\"documentation\"/>\n    <author>\n      <name>Sayyed Mohsen Vazirizade</name>\n      <email>smvazirizade@email.arizona.edu</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <category field=\"engineering\"/>\n    <issn>1026-3098</issn>\n    <eissn>2345-3605</eissn>\n    <summary>Common style use by Scientia Iranica</summary>\n    <updated>2019-01-17T02:53:15+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <style-options punctuation-in-quote=\"false\"/>\n    <terms>\n      <term name=\"edition\" form=\"short\">edn.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter=\", \" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label prefix=\", \" form=\"short\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <label prefix=\", \" form=\"short\" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text form=\"short\" term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"day-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n          <date-part name=\"day\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text form=\"short\" term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <text variable=\"volume\" font-weight=\"bold\"/>\n    <number prefix=\"(\" suffix=\")\" variable=\"issue\"/>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text form=\"short\" term=\"edition\" text-case=\"capitalize-first\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"series\">\n    <group delimiter=\" \">\n      <text variable=\"collection-title\" text-case=\"title\"/>\n      <text variable=\"collection-number\" prefix=\"No. \"/>\n    </group>\n  </macro>\n  <macro name=\"personal-communication\">\n    <group prefix=\"personal communication (\" suffix=\")\">\n      <text macro=\"day-date\"/>\n    </group>\n  </macro>\n  <macro name=\"patent-authority-number\">\n    <choose>\n      <if variable=\"authority\">\n        <group>\n          <text variable=\"authority\" suffix=\" patent \"/>\n          <text variable=\"number\"/>\n        </group>\n      </if>\n      <else>\n        <group>\n          <text variable=\"number\" prefix=\"U.S. patent \"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if match=\"any\" type=\"bill book graphic legal_case legislation motion_picture report song\">\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"series\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </if>\n        <else-if type=\"paper-conference\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"series\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"pages\"/>\n          </group>\n          <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n        </else-if>\n        <else-if type=\"speech\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"event\" prefix=\" presented at \" form=\"long\" text-case=\"title\"/>\n            <text variable=\"event-place\" text-case=\"title\"/>\n          </group>\n          <text prefix=\" (\" suffix=\")\" macro=\"day-date\"/>\n        </else-if>\n        <else-if match=\"any\" type=\"chapter\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <group delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text variable=\"container-title\" form=\"long\" text-case=\"title\" font-style=\"italic\"/>\n            </group>\n            <text macro=\"editor\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"series\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"pages\"/>\n          </group>\n          <text prefix=\" (\" suffix=\")\" macro=\"day-date\"/>\n        </else-if>\n        <else-if type=\"patent\">\n          <text variable=\"title\" quotes=\"true\" suffix=\", \"/>\n          <group delimiter=\" \">\n            <text macro=\"patent-authority-number\"/>\n            <text prefix=\" (\" suffix=\")\" macro=\"day-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text variable=\"title\" quotes=\"true\" suffix=\", \"/>\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n          </group>\n          <text prefix=\" (\" suffix=\")\" macro=\"year-date\"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <text variable=\"title\" quotes=\"true\" suffix=\", \"/>\n          <text variable=\"URL\"/>\n        </else-if>\n        <else-if type=\"personal_communication\">\n          <text macro=\"personal-communication\"/>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <text variable=\"title\" quotes=\"true\" suffix=\", \"/>\n          <text variable=\"container-title\" form=\"long\"/>\n          <text prefix=\" (\" suffix=\")\" macro=\"day-date\"/>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"number\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"pages\"/>\n          </group>\n          <text prefix=\" (\" suffix=\")\" macro=\"year-date\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"