"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\" default-locale=\"en-US\">\n  <info>\n    <title>Institute of Mathematical Statistics journals</title>\n    <title-short>IMS</title-short>\n    <id>http://www.zotero.org/styles/institute-of-mathematical-statistics</id>\n    <link href=\"http://www.zotero.org/styles/institute-of-mathematical-statistics\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/institute-of-physics-numeric\" rel=\"template\"/>\n    <link href=\"http://www.imstat.org/aop/manprep.htm\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"physics\"/>\n    <category field=\"math\"/>\n    <summary>Style for IMS journals.</summary>\n    <updated>2017-07-17T12:52:02+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"et-al\">et al</term>\n      <term name=\"editor\" form=\"short\">ed</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" font-variant=\"small-caps\" suffix=\".\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" strip-periods=\"true\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" et-al-min=\"99\" et-al-use-first=\"98\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <group delimiter=\". \" prefix=\" \">\n        <text macro=\"author\"/>\n        <date variable=\"issued\" prefix=\"(\" suffix=\").\">\n          <date-part name=\"year\"/>\n        </date>\n        <text macro=\"title\" suffix=\".\"/>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <group prefix=\" \" delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\" prefix=\" \"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"collection-title\"/>\n              <text variable=\"event\"/>\n              <group delimiter=\", \">\n                <text macro=\"volume\"/>\n                <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text macro=\"pages\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \" prefix=\" \">\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"container-title\"/>\n            <group delimiter=\" \">\n              <text term=\"available at\" text-case=\"capitalize-first\"/>\n              <text variable=\"URL\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            <text variable=\"volume\" font-weight=\"bold\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"