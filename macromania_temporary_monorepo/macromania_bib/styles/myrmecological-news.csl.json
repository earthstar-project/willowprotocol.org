"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Myrmecological News</title>\n    <id>http://www.zotero.org/styles/myrmecological-news</id>\n    <link href=\"http://www.zotero.org/styles/myrmecological-news\" rel=\"self\"/>\n    <link href=\"http://myrmecologicalnews.org/cms/index.php?option=com_content&amp;view=article&amp;id=31&amp;Itemid=40\" rel=\"documentation\"/>\n    <link href=\"https://github.com/citation-style-language/styles/pull/498\" rel=\"documentation\"/>\n    <author>\n      <name>Wolfgang Arthofer</name>\n      <email>wolfgang.arthofer@uibk.ac.at</email>\n    </author>\n    <contributor>\n      <name>Florian M. Steiner</name>\n    </contributor>\n    <contributor>\n      <name>Timothée Brütsch</name>\n      <email>timbrutsch@gmail.com</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1994-4136</issn>\n    <eissn>1997-3500</eissn>\n    <updated>2013-05-02T09:16:37+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"page-range-delimiter\">-</term>\n      <term name=\"et-al\">&amp; al.</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\".\" font-variant=\"small-caps\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" \" suffix=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" font-variant=\"normal\" and=\"symbol\" delimiter-precedes-last=\"never\" et-al-min=\"3\" et-al-use-first=\"1\" initialize-with=\". \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\" font-variant=\"small-caps\"/>\n      </name>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\" suffix=\". \"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n      <text variable=\"number-of-pages\" suffix=\" pp.\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" suffix=\" \"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\" \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" suffix=\".\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \">\n        <text macro=\"author\" font-variant=\"small-caps\"/>\n        <date variable=\"issued\" suffix=\":\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n      <choose>\n        <if type=\"book\">\n          <group prefix=\" \" delimiter=\" \" suffix=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\" suffix=\". \"/>\n          </group>\n          <text prefix=\" &#8211; \" suffix=\".\" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter\">\n          <text macro=\"title\" prefix=\" \" suffix=\". \"/>\n          <group delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <text macro=\"editor\" suffix=\" (Eds.): \" text-case=\"capitalize-all\"/>\n            <text variable=\"container-title\" suffix=\". &#8211;\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text variable=\"event\" suffix=\".\"/>\n            <group suffix=\".\" delimiter=\", \">\n              <text macro=\"publisher\" prefix=\" \"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\" \" suffix=\" \" delimiter=\", \">\n            <text macro=\"title\" suffix=\" &#8211; PhD thesis\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\". &#8211;\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\". \">\n            <text variable=\"container-title\"/>\n            <group prefix=\" \">\n              <text variable=\"volume\"/>\n            </group>\n            <group prefix=\": \">\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"