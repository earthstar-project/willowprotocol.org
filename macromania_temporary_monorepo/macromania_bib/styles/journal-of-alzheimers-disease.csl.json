"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Alzheimer's Disease</title>\n    <id>http://www.zotero.org/styles/journal-of-alzheimers-disease</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-alzheimers-disease\" rel=\"self\"/>\n    <link href=\"https://www.j-alz.com/prep\" rel=\"documentation\"/>\n    <author>\n      <name>Rintze Zelle</name>\n      <uri>http://twitter.com/rintzezelle</uri>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1387-2877</issn>\n    <eissn>1875-8908</eissn>\n    <updated>2020-09-06T12:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\"\" delimiter=\", \" name-as-sort-order=\"all\" sort-separator=\" \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\"\" delimiter=\", \" name-as-sort-order=\"all\" sort-separator=\" \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n    <text variable=\"publisher-place\" prefix=\", \"/>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group>\n          <label variable=\"page\" form=\"short\" suffix=\" \"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"page\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"] \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"author\"/>\n          <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n            <date-part name=\"year\"/>\n          </date>\n          <group delimiter=\", \" prefix=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"author\"/>\n          <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n            <date-part name=\"year\"/>\n          </date>\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <text term=\"in\" text-case=\"capitalize-first\" prefix=\" \"/>\n          <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n          <text macro=\"editor\" prefix=\", \"/>\n          <text macro=\"publisher\" prefix=\" \"/>\n          <text macro=\"page\" prefix=\", \"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\", \">\n            <text variable=\"container-title\"/>\n            <text macro=\"title\"/>\n            <date variable=\"issued\" prefix=\"Last updated \">\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n            <date variable=\"issued\" prefix=\"Accessed on \">\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text macro=\"author\"/>\n            <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n              <date-part name=\"year\"/>\n            </date>\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\" strip-periods=\"true\"/>\n            <text variable=\"volume\" font-weight=\"bold\" suffix=\",\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"