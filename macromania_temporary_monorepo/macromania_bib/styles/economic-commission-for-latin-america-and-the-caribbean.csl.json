"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Economic Commission for Latin America and the Caribbean</title>\n    <title-short>ECLAC</title-short>\n    <id>http://www.zotero.org/styles/economic-commission-for-latin-america-and-the-caribbean</id>\n    <link href=\"http://www.zotero.org/styles/economic-commission-for-latin-america-and-the-caribbean\" rel=\"self\"/>\n    <link href=\"http://www.eclac.org\" rel=\"documentation\"/>\n    <author>\n      <name>Marcelo LaFleur</name>\n      <email>marcelo.lafleur@cepal.org</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"political_science\"/>\n    <summary>Bibliography style for use in ECLAC/CEPAL publications, English version.</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"no date\" form=\"short\">n/d</term>\n      <term name=\"accessed\">date of reference</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <label form=\"short\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"none\">\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <names variable=\"author\">\n              <name name-as-sort-order=\"all\" and=\"symbol\" initialize-with=\". \" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n              <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n              <substitute>\n                <names variable=\"editor\"/>\n                <names variable=\"translator\"/>\n              </substitute>\n            </names>\n          </if>\n          <else-if variable=\"publisher\" match=\"any\">\n            <choose>\n              <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n                <text variable=\"publisher\" font-style=\"italic\"/>\n              </if>\n              <else>\n                <text variable=\"publisher\"/>\n              </else>\n            </choose>\n          </else-if>\n          <else>\n            <choose>\n              <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n              </if>\n              <else>\n                <text variable=\"container-title\"/>\n              </else>\n            </choose>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <names variable=\"author\">\n              <name name-as-sort-order=\"all\" and=\"symbol\" initialize-with=\". \" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n              <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n              <substitute>\n                <names variable=\"editor\"/>\n                <names variable=\"translator\"/>\n              </substitute>\n            </names>\n          </if>\n          <else-if variable=\"collection-title\">\n            <text variable=\"collection-title\" font-style=\"italic\"/>\n          </else-if>\n          <else-if variable=\"publisher\" match=\"any\">\n            <text variable=\"publisher\"/>\n          </else-if>\n          <else>\n            <choose>\n              <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n              </if>\n            </choose>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" text-case=\"lowercase\" prefix=\" [\" suffix=\"]\"/>\n          </if>\n          <else>\n            <text value=\"online\" prefix=\" [\" suffix=\"]\"/>\n          </else>\n        </choose>\n        <text variable=\"URL\" prefix=\" &lt;\" suffix=\"&gt;\" font-style=\"normal\"/>\n        <group prefix=\" \" delimiter=\" \">\n          <group prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\" suffix=\": \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <date variable=\"issued\" prefix=\" \">\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"day\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report\">\n        <text variable=\"title\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"onusig\">\n    <choose>\n      <if variable=\"note\">\n        <text variable=\"note\" prefix=\" (\" suffix=\")\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"onuventa\">\n    <choose>\n      <if variable=\"note\">\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"call-number\" prefix=\"United Nations publication, sales No. \"/>\n          </if>\n          <else>\n            <choose>\n              <if variable=\"call-number\">\n                <text variable=\"call-number\" prefix=\"United Nations publication, sales No. \"/>\n              </if>\n              <else-if variable=\"number\">\n                <text variable=\"number\" prefix=\"United Nations publication, sales No. \"/>\n              </else-if>\n              <else-if variable=\"volume\">\n                <text variable=\"volume\" prefix=\"United Nations publication, sales No. \"/>\n              </else-if>\n            </choose>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"event\"/>\n      <text variable=\"publisher-place\"/>\n      <choose>\n        <if variable=\"author editor translator\" match=\"any\">\n          <text variable=\"publisher\"/>\n        </if>\n      </choose>\n      <choose>\n        <if variable=\"publisher publisher-place\" match=\"none\">\n          <text variable=\"container-title\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if type=\"webpage\">\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else-if variable=\"accessed\">\n            <date variable=\"accessed\">\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"volume\" prefix=\" vol. \"/>\n          <text variable=\"issue\" prefix=\" No. \"/>\n        </group>\n      </if>\n      <else-if type=\"report\">\n        <text variable=\"number\" prefix=\" No. \"/>\n      </else-if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if variable=\"collection-title\" match=\"none\">\n            <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n              <text macro=\"edition\"/>\n              <group>\n                <text term=\"volume\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1-\"/>\n              </group>\n              <group>\n                <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <number variable=\"volume\" form=\"numeric\"/>\n              </group>\n              <group>\n                <label variable=\"page\" form=\"short\" suffix=\" \"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\", \">\n              <text variable=\"volume\" prefix=\" vol. \"/>\n              <text variable=\"collection-number\" prefix=\" No. \"/>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <choose>\n          <if variable=\"author editor translator\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"author-short\"/>\n              <text macro=\"year-date\"/>\n            </group>\n            <text macro=\"citation-locator\"/>\n          </if>\n          <else-if variable=\"publisher\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"publisher\" font-style=\"italic\"/>\n              <text macro=\"year-date\"/>\n            </group>\n            <text macro=\"citation-locator\"/>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"year-date\"/>\n            </group>\n            <text macro=\"citation-locator\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"4\" et-al-use-first=\"1\" subsequent-author-substitute=\"---\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\" sort=\"descending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\" \"/>\n      <text macro=\"year-date\" prefix=\"(\" suffix=\"),\"/>\n      <choose>\n        <if type=\"report\">\n          <text macro=\"title\" prefix=\" \" quotes=\"true\"/>\n          <group prefix=\", \" delimiter=\", \">\n            <text variable=\"collection-title\" font-style=\"italic\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <text macro=\"onusig\"/>\n          <group prefix=\", \" delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text variable=\"page\" prefix=\"pg. \"/>\n            <date variable=\"issued\">\n              <date-part name=\"month\"/>\n            </date>\n          </group>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <choose>\n            <if variable=\"collection-title\" match=\"none\">\n              <group>\n                <text macro=\"title\" prefix=\" \"/>\n                <text macro=\"onusig\"/>\n              </group>\n              <group prefix=\", \" delimiter=\", \">\n                <text macro=\"edition\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"publisher\"/>\n                <date variable=\"issued\">\n                  <date-part name=\"month\"/>\n                </date>\n              </group>\n            </if>\n            <else>\n              <text macro=\"title\" prefix=\" \" suffix=\", \"/>\n              <text variable=\"collection-title\" suffix=\", \"/>\n              <text macro=\"locators\"/>\n              <text macro=\"onusig\"/>\n              <text macro=\"publisher\" prefix=\", \"/>\n              <date variable=\"issued\" prefix=\", \">\n                <date-part name=\"month\"/>\n                <date-part name=\"day\" prefix=\" \"/>\n              </date>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group>\n            <text macro=\"title\" prefix=\" \" quotes=\"true\"/>\n            <text macro=\"onusig\"/>\n          </group>\n          <group prefix=\", \" delimiter=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"collection-title\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"page\" prefix=\"pg. \"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n          <group>\n            <text macro=\"title\" prefix=\" \" quotes=\"true\"/>\n            <choose>\n              <if variable=\"container-title\" match=\"none\">\n                <text macro=\"onusig\"/>\n              </if>\n            </choose>\n          </group>\n          <group prefix=\", \" delimiter=\", \">\n            <choose>\n              <if variable=\"author editor translator\" match=\"any\">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n              </if>\n            </choose>\n            <text variable=\"collection-title\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <choose>\n            <if variable=\"container-title\">\n              <text macro=\"onusig\"/>\n            </if>\n          </choose>\n          <group prefix=\", \" delimiter=\", \">\n            <text macro=\"editor\"/>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <text macro=\"title\" prefix=\" \" quotes=\"true\"/>\n          <choose>\n            <if variable=\"genre\">\n              <text variable=\"event\" prefix=\", document prepared for the project \"/>\n            </if>\n            <else>\n              <text variable=\"event\" prefix=\", document presented in \"/>\n            </else>\n          </choose>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\" prefix=\" \" quotes=\"true\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\",\"/>\n      <text macro=\"onuventa\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"