"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" page-range-format=\"chicago\" default-locale=\"en-US\">\n  <info>\n    <title>The Review of Financial Studies</title>\n    <id>http://www.zotero.org/styles/the-review-of-financial-studies</id>\n    <link href=\"http://www.zotero.org/styles/the-review-of-financial-studies\" rel=\"self\"/>\n    <link href=\"http://www.oxfordjournals.org/our_journals/revfin/for_authors/ms_preparation.html\" rel=\"documentation\"/>\n    <author>\n      <name>Ted Moorman</name>\n    </author>\n    <contributor>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <issn>0893-9454</issn>\n    <eissn>1465-7368</eissn>\n    <summary>RFS style based on Chicago - the instructions say to follow Chicago's latest edition, but that's not what the journal does.</summary>\n    <updated>2012-10-25T21:15:26+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"translator\" form=\"verb-short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter\" match=\"none\">\n        <group delimiter=\". \">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"editor\">\n                <label form=\"verb-short\" text-case=\"capitalize-first\" suffix=\". \"/>\n                <name and=\"text\" delimiter=\", \"/>\n              </names>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\">\n                <label form=\"verb-short\" text-case=\"capitalize-first\" suffix=\". \"/>\n                <name and=\"text\" delimiter=\", \"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter\">\n        <group prefix=\" \" delimiter=\", \">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"editor\">\n                <name delimiter=\", \" prefix=\"In \" initialize-with=\". \" name-as-sort-order=\"first\" sort-separator=\", \" and=\"text\"/>\n                <label form=\"short\" prefix=\" (\" suffix=\"),\"/>\n              </names>\n              <choose>\n                <if variable=\"container-author\">\n                  <group prefix=\" by \">\n                    <names variable=\"container-author\">\n                      <label form=\"verb-short\" prefix=\" \" suffix=\" \"/>\n                      <name and=\"text\" delimiter=\", \"/>\n                    </names>\n                  </group>\n                </if>\n              </choose>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\">\n                <label form=\"verb-short\" prefix=\" \" suffix=\". \"/>\n                <name and=\"text\" delimiter=\", \"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"anon\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"none\">\n        <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" suffix=\".\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\". \" name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name initialize-with=\". \" name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"verb-short\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"recipient\">\n    <choose>\n      <if type=\"personal_communication\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n          <else>\n            <text term=\"letter\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" name-as-sort-order=\"first\" sort-separator=\", \" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"verb-short\" prefix=\", \" suffix=\".\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"anon\"/>\n    <text macro=\"recipient\"/>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"anon\"/>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"personal_communication\" match=\"none\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"report\">\n        <text variable=\"title\" text-case=\"title\" suffix=\".\"/>\n      </else-if>\n      <else-if type=\"book\">\n        <text variable=\"title\" text-case=\"title\" suffix=\". \" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-case=\"title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\" suffix=\".\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"volume\" prefix=\" \"/>\n      </if>\n      <else-if type=\"legal_case\">\n        <text variable=\"volume\" prefix=\", \"/>\n        <text variable=\"container-title\" prefix=\" \"/>\n        <text variable=\"page\" prefix=\" \"/>\n      </else-if>\n      <else-if type=\"book\">\n        <group prefix=\". \" delimiter=\". \">\n          <group>\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\". \"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n          <group>\n            <number variable=\"number-of-volumes\" form=\"numeric\"/>\n            <text term=\"volume\" form=\"short\" prefix=\" \" suffix=\".\" plural=\"true\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"chapter\">\n        <choose>\n          <if variable=\"page\" match=\"none\">\n            <group prefix=\". \">\n              <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\". \"/>\n              <number variable=\"volume\" form=\"numeric\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locators-article\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group prefix=\", \" delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"edition\"/>\n            <text term=\"edition\"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\". \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"page\" prefix=\":\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"point-locators\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <choose>\n            <if type=\"book\">\n              <choose>\n                <if variable=\"volume\">\n                  <group>\n                    <text term=\"volume\" form=\"short\" suffix=\". \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <label variable=\"locator\" form=\"short\" prefix=\", \" suffix=\" \"/>\n                  </group>\n                </if>\n                <else>\n                  <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n                </else>\n              </choose>\n            </if>\n          </choose>\n        </if>\n        <else-if type=\"book\">\n          <number variable=\"volume\" form=\"numeric\" suffix=\",\"/>\n        </else-if>\n      </choose>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter\">\n        <text variable=\"container-title\" suffix=\".\" font-style=\"italic\"/>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"legal_case chapter\" match=\"none\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"article-journal\" match=\"none\">\n        <choose>\n          <if variable=\"publisher\" match=\"none\">\n            <choose>\n              <if type=\"personal_communication\" match=\"none\">\n                <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              </if>\n            </choose>\n          </if>\n          <else>\n            <text variable=\"publisher-place\" suffix=\": \"/>\n            <text variable=\"publisher\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"collection-title\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"description\">\n    <choose>\n      <if type=\"interview\">\n        <group delimiter=\". \">\n          <text macro=\"interviewer\"/>\n          <text variable=\"medium\" text-case=\"capitalize-first\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"medium\" text-case=\"capitalize-first\" prefix=\". \"/>\n      </else>\n    </choose>\n    <choose>\n      <if variable=\"title\" match=\"none\"/>\n      <else-if type=\"thesis\"/>\n      <else>\n        <text variable=\"genre\" form=\"long\" text-case=\"capitalize-first\" prefix=\" \" suffix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" givenname-disambiguation-rule=\"primary-name\">\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"contributors-short\"/>\n          <text macro=\"date\"/>\n        </group>\n        <text macro=\"point-locators\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"7\">\n    <sort>\n      <key macro=\"contributors\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"contributors\" suffix=\". \"/>\n      <text macro=\"date\" suffix=\". \"/>\n      <text macro=\"title\"/>\n      <text macro=\"description\"/>\n      <text macro=\"secondary-contributors\" prefix=\". \"/>\n      <text macro=\"container-contributors\"/>\n      <text macro=\"container-title\" prefix=\" \" suffix=\" \"/>\n      <text macro=\"edition\" prefix=\". \"/>\n      <text macro=\"locators\"/>\n      <text macro=\"collection-title\" prefix=\". \"/>\n      <text macro=\"locators-article\"/>\n      <text macro=\"publisher\"/>\n    </layout>\n  </bibliography>\n</style>\n"