"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-US\">\n  <info>\n    <title>Oil Shale</title>\n    <id>http://www.zotero.org/styles/oil-shale</id>\n    <link href=\"http://www.zotero.org/styles/oil-shale\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/proceedings-of-the-estonian-academy-of-sciences-numeric\" rel=\"template\"/>\n    <link href=\"https://kirj.ee/oil-shale-guidelines-for-authors/?v=04c19fa1e772\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>citationstyler@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"generic-base\"/>\n    <issn>0208-189X</issn>\n    <eissn>1736-7492</eissn>\n    <updated>2022-09-14T15:15:16+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editortranslator\" form=\"short\">\n        <single>ed. and translator</single>\n        <multiple>eds. and translators</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">\n        <single>translator</single>\n        <multiple>translators</multiple>\n      </term>\n      <term name=\"collection-editor\" form=\"short\">\n        <single>series ed.</single>\n        <multiple>series eds.</multiple>\n      </term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\" delimiter=\"; \">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis patent\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <text variable=\"collection-title\"/>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"full-issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n      <date-part name=\"day\" suffix=\", \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <text variable=\"URL\"/>\n      </if>\n      <else-if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"baseline\" delimiter=\", \" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <choose>\n          <if type=\"article-journal review\" match=\"any\">\n            <group delimiter=\" \">\n              <text macro=\"title\" suffix=\".\"/>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n                <text macro=\"issued\" font-weight=\"normal\"/>\n                <choose>\n                  <if variable=\"volume\">\n                    <group>\n                      <text variable=\"volume\" font-weight=\"bold\"/>\n                      <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                    </group>\n                  </if>\n                  <else>\n                    <group delimiter=\" \">\n                      <text term=\"issue\" form=\"short\" text-case=\"capitalize-first\"/>\n                      <text variable=\"issue\"/>\n                    </group>\n                  </else>\n                </choose>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"article-magazine article-newspaper article\" match=\"any\">\n            <group delimiter=\" \">\n              <text macro=\"title\" suffix=\".\"/>\n              <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n              <group delimiter=\", \">\n                <text macro=\"full-issued\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"genre\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"issued\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"patent\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <text term=\"patent\" text-case=\"capitalize-first\"/>\n                  <text variable=\"number\"/>\n                </group>\n                <date variable=\"issued\">\n                  <date-part name=\"year\" suffix=\"-\"/>\n                  <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n                  <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n                </date>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\" prefix=\" \"/>\n              <text macro=\"series\"/>\n              <choose>\n                <if type=\"report\">\n                  <group delimiter=\" \">\n                    <text variable=\"genre\"/>\n                    <text variable=\"number\"/>\n                  </group>\n                </if>\n              </choose>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"issued\"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"volume\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                  <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n                </group>\n                <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <choose>\n                <if type=\"entry-dictionary entry-encyclopedia\" match=\"none\"/>\n                <else>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </else>\n              </choose>\n              <text macro=\"series\"/>\n              <text macro=\"volume\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"issued\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"title\"/>\n              <text variable=\"URL\"/>\n              <date delimiter=\"\" variable=\"accessed\" prefix=\"(accessed \" suffix=\")\">\n                <date-part name=\"year\" suffix=\"-\"/>\n                <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n                <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n              </date>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <text variable=\"genre\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"issued\"/>\n                <group delimiter=\" \">\n                  <text variable=\"number-of-pages\"/>\n                  <label plural=\"never\" variable=\"number-of-pages\" form=\"short\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <group delimiter=\". \">\n                <text macro=\"title\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"issued\"/>\n                <text variable=\"volume\" font-style=\"italic\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"