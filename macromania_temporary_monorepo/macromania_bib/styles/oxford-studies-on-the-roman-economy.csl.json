"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"note\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\" page-range-format=\"expanded\">\n  <info>\n    <title>Oxford Studies on the Roman Economy</title>\n    <id>http://www.zotero.org/styles/oxford-studies-on-the-roman-economy</id>\n    <link href=\"http://www.zotero.org/styles/oxford-studies-on-the-roman-economy\" rel=\"self\"/>\n    <link href=\"http://oxrep.classics.ox.ac.uk/docs/OSREHouseStyle.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Nicolas Monteix</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <summary>Harvard author-date style edited for Oxford Studies on the Roman Economy</summary>\n    <updated>2013-05-23T11:00:00+01:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"editor-bib\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name initialize-with=\".\" and=\"text\" sort-separator=\", \" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <text macro=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <et-al font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"editor-short\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group prefix=\" (\" suffix=\")\">\n      <text term=\"accessed\" suffix=\" \"/>\n      <date variable=\"accessed\">\n        <date-part name=\"day\" suffix=\" \"/>\n        <date-part name=\"month\" suffix=\" \"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" form=\"long\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"webpage\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \" prefix=\", \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <layout delimiter=\"; \">\n      <group>\n        <text macro=\"author-short\" suffix=\" \"/>\n        <text macro=\"year-date\"/>\n        <choose>\n          <if locator=\"page\">\n            <text variable=\"locator\" prefix=\":&#160;\"/>\n          </if>\n          <else-if locator=\"paragraph\">\n            <text variable=\"locator\" prefix=\":&#160;ยง\"/>\n          </else-if>\n          <else>\n            <group prefix=\":&#160;\">\n              <label variable=\"locator\" form=\"short\" suffix=\".&#160;\"/>\n              <text variable=\"locator\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&#8212;\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"page-first\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <choose>\n        <if variable=\"author editor\" match=\"any\">\n          <text macro=\"author\"/>\n        </if>\n        <else>\n          <text variable=\"title\" form=\"short\"/>\n        </else>\n      </choose>\n      <text macro=\"year-date\" prefix=\" (\" suffix=\").\"/>\n      <choose>\n        <if type=\"thesis\">\n          <group prefix=\" \" delimiter=\", \" suffix=\".\">\n            <text macro=\"title\" font-style=\"italic\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"webpage\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"title\" quotes=\"true\" suffix=\",\"/>\n            <text variable=\"URL\"/>\n            <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n          </group>\n          <group prefix=\" \" delimiter=\" \" suffix=\".\">\n            <date variable=\"issued\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n            <text variable=\"container-title\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article-newspaper broadcast personal_communication thesis entry-dictionary entry-encyclopedia\" match=\"any\">\n          <group>\n            <text macro=\"title\" quotes=\"true\" prefix=\" \" suffix=\",\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\" prefix=\" \" suffix=\" \"/>\n          </group>\n          <group prefix=\" \">\n            <group suffix=\": \">\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\".\"/>\n            </group>\n            <group>\n              <text macro=\"pages\" suffix=\".\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"book graphic\" match=\"any\">\n          <group prefix=\" \" suffix=\".\">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"publisher\" prefix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" quotes=\"true\" prefix=\" \" suffix=\", \"/>\n          <group>\n            <text term=\"in\" suffix=\" \"/>\n            <text macro=\"editor-bib\" suffix=\", \"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"publisher\" prefix=\". \" suffix=\", \"/>\n            <text macro=\"pages\" suffix=\".\"/>\n          </group>\n        </else-if>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"