"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" initialize-with=\".\" page-range-format=\"expanded\" demote-non-dropping-particle=\"never\">\n  <!-- This style was edited with the Visual CSL Editor (https://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Société française d'égyptologie</title>\n    <title-short>SFE</title-short>\n    <id>http://www.zotero.org/styles/societe-francaise-degyptologie</id>\n    <link href=\"http://www.zotero.org/styles/societe-francaise-degyptologie\" rel=\"self\"/>\n    <link href=\"https://www.sfegyptologie.fr/index.php?option=com_content&amp;view=article&amp;id=207&amp;Itemid=550\" rel=\"documentation\"/>\n    <author>\n      <name>Nicolas Souchon</name>\n      <email>souchon.nicolas.ns@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <summary>Feuille de style pour les publications de la Société française d'égyptologie</summary>\n    <updated>2024-03-26T17:59:01+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"in\">dans</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name et-al-min=\"3\" et-al-use-first=\"1\" initialize-with=\".\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name et-al-min=\"3\" et-al-use-first=\"1\" initialize-with=\".\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" plural=\"never\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <text variable=\"collection-title\" form=\"short\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\"/\">\n          <number variable=\"volume\"/>\n          <number variable=\"issue\"/>\n        </group>\n      </if>\n      <else>\n        <number text-case=\"uppercase\" variable=\"volume\" form=\"roman\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if match=\"any\" variable=\"status\">\n        <text variable=\"status\"/>\n      </if>\n      <else>\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection-number\">\n    <number variable=\"collection-number\"/>\n  </macro>\n  <macro name=\"container-title\">\n    <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"locator\">\n    <group delimiter=\" \">\n      <label plural=\"never\" variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"title-short\">\n    <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"disambiguate-citation\">\n    <choose>\n      <if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\" \">\n          <text macro=\"container-title\" font-style=\"normal\"/>\n          <text macro=\"volume\"/>\n          <text macro=\"date\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n      <else-if type=\"chapter\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n            <text macro=\"collection\"/>\n            <text macro=\"collection-number\"/>\n          </group>\n          <text macro=\"date\"/>\n        </group>\n      </else-if>\n      <else-if type=\"book\" match=\"any\">\n        <text macro=\"title-short\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <citation name-delimiter=\" &#8211; \" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" initialize-with=\".\" near-note-distance=\"3\" disambiguate-add-givenname=\"true\" givenname-disambiguation-rule=\"all-names\">\n    <layout delimiter=\" ; \" suffix=\".\">\n      <group delimiter=\", \">\n        <text macro=\"author\"/>\n        <choose>\n          <if match=\"any\" position=\"ibid\">\n            <choose>\n              <if match=\"any\" disambiguate=\"true\">\n                <text macro=\"disambiguate-citation\"/>\n              </if>\n              <else>\n                <text term=\"ibid\" font-style=\"italic\" font-weight=\"normal\"/>\n              </else>\n            </choose>\n          </if>\n          <else-if match=\"any\" position=\"ibid-with-locator\">\n            <choose>\n              <if match=\"any\" disambiguate=\"true\">\n                <text macro=\"disambiguate-citation\"/>\n              </if>\n              <else>\n                <text term=\"ibid\" form=\"short\" font-style=\"italic\"/>\n              </else>\n            </choose>\n          </else-if>\n          <else-if match=\"any\" position=\"subsequent\">\n            <choose>\n              <if match=\"any\" disambiguate=\"true\">\n                <text macro=\"disambiguate-citation\"/>\n              </if>\n              <else>\n                <text value=\"op. cit.\" font-style=\"italic\" text-decoration=\"none\"/>\n              </else>\n            </choose>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <choose>\n                <if type=\"chapter\" match=\"any\">\n                  <group delimiter=\" \">\n                    <text term=\"in\"/>\n                    <text macro=\"editor\"/>\n                  </group>\n                </if>\n              </choose>\n              <choose>\n                <if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n                  <text macro=\"title\"/>\n                  <group delimiter=\" \">\n                    <text macro=\"container-title\"/>\n                    <text macro=\"volume\"/>\n                    <text macro=\"date\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                </if>\n                <else-if match=\"any\" variable=\"volume\">\n                  <choose>\n                    <if type=\"book\" match=\"any\">\n                      <text macro=\"title\"/>\n                    </if>\n                    <else-if type=\"chapter\" match=\"any\">\n                      <text macro=\"container-title\"/>\n                    </else-if>\n                  </choose>\n                  <group delimiter=\" \">\n                    <text macro=\"volume\"/>\n                    <choose>\n                      <if match=\"any\" variable=\"collection-title\">\n                        <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                          <text macro=\"collection\"/>\n                          <text macro=\"collection-number\"/>\n                        </group>\n                      </if>\n                    </choose>\n                  </group>\n                  <text macro=\"date\"/>\n                </else-if>\n                <else>\n                  <group delimiter=\" \">\n                    <choose>\n                      <if type=\"book\" match=\"any\">\n                        <text macro=\"title\"/>\n                      </if>\n                      <else-if type=\"chapter\" match=\"any\">\n                        <text macro=\"container-title\"/>\n                      </else-if>\n                    </choose>\n                    <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                      <text macro=\"collection\"/>\n                      <text macro=\"collection-number\"/>\n                    </group>\n                  </group>\n                  <text macro=\"date\"/>\n                </else>\n              </choose>\n            </group>\n          </else>\n        </choose>\n        <text macro=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n</style>\n"