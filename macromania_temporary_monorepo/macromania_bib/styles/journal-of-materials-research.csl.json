"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Materials Research</title>\n    <title-short>J. Mater. Res.</title-short>\n    <id>http://www.zotero.org/styles/journal-of-materials-research</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-materials-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-physics-society\" rel=\"template\"/>\n    <link href=\"http://www.mrs.org/jmr-format\" rel=\"documentation\"/>\n    <author>\n      <name>Sonal Dey</name>\n      <email>deysonal2012@gmail.com</email>\n      <uri>https://sites.google.com/site/deysonal2012</uri>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"physics\"/>\n    <issn>0884-2914</issn>\n    <eissn>2044-5326</eissn>\n    <summary>A citation style for the Journal of Materials Research.</summary>\n    <updated>2016-07-05T15:10:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\", \" initialize-with=\". \" and=\"text\"/>\n      <label form=\"long\" prefix=\", \" suffix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"verb\" suffix=\" \"/>\n      <name delimiter=\", \" initialize-with=\". \" and=\"text\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"day-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\" text-case=\"title\"/>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\" suffix=\": \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text variable=\"title\" text-case=\"title\"/>\n              <text macro=\"edition\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <text term=\"in\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n              <text macro=\"editor\"/>\n              <text macro=\"edition\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\" \">\n            <text variable=\"number\"/>\n            <text macro=\"day-date\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\" text-case=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal review\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            <group delimiter=\", \">\n              <choose>\n                <if variable=\"volume\">\n                  <group delimiter=\"\">\n                    <text variable=\"volume\" font-weight=\"bold\"/>\n                    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                </if>\n                <else-if variable=\"issue\">\n                  <group delimiter=\" \">\n                    <text term=\"issue\" form=\"short\" text-case=\"capitalize-first\"/>\n                    <text variable=\"issue\"/>\n                  </group>\n                </else-if>\n              </choose>\n              <text variable=\"page-first\"/>\n            </group>\n          </group>\n          <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\" text-case=\"title\"/>\n            <group delimiter=\", \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <group delimiter=\" \">\n                <text variable=\"page-first\" form=\"short\"/>\n                <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"