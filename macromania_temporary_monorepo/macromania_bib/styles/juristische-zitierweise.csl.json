"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"note\" default-locale=\"de-DE\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Juristische Zitierweise (Stüber) (Deutsch)</title>\n    <id>http://www.zotero.org/styles/juristische-zitierweise</id>\n    <link href=\"http://www.zotero.org/styles/juristische-zitierweise\" rel=\"self\"/>\n    <link href=\"www.niederle-media.de/Zitieren.pdf\" rel=\"documentation\"/>\n    <link href=\"https://forums.zotero.org/discussion/20886/citation-style-for-german-lawyers/\" rel=\"documentation\"/>\n    <author>\n      <name>Oliver Wolf</name>\n      <email>wolf.o@gmx.net</email>\n    </author>\n    <contributor>\n      <name>Philipp Zumstein</name>\n    </contributor>\n    <category citation-format=\"note\"/>\n    <category field=\"law\"/>\n    <summary>Juristische Zitierweise nach Stüber www.niederle-media.de/Zitieren.pdf</summary>\n    <updated>2018-07-21T09:10:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de-DE\">\n    <terms>\n      <term name=\"accessed\">besucht am</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" font-style=\"italic\">\n      <name delimiter=\"/ \" name-as-sort-order=\"all\" sort-separator=\", \" form=\"long\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-note\">\n    <names variable=\"author\" font-style=\"italic\">\n      <name form=\"short\" delimiter=\"/\" et-al-min=\"3\" et-al-use-first=\"1\" name-as-sort-order=\"all\"/>\n    </names>\n  </macro>\n  <macro name=\"locator-with-label\">\n    <group delimiter=\"&#160;\">\n      <label variable=\"locator\" form=\"symbol\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"autor-editor-note\">\n    <names variable=\"author\" font-style=\"italic\">\n      <name form=\"short\" delimiter=\"/\" et-al-min=\"3\" et-al-use-first=\"1\" sort-separator=\"\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"inbook\">\n    <group delimiter=\": \">\n      <text term=\"in\"/>\n      <group delimiter=\", \">\n        <names variable=\"editor\">\n          <name form=\"short\" delimiter=\"/\" initialize-with=\"\"/>\n        </names>\n        <text variable=\"container-title\" form=\"short\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"firstpage-locator\">\n    <group delimiter=\" \">\n      <text variable=\"page-first\"/>\n      <text variable=\"locator\" prefix=\"(\" suffix=\")\"/>\n    </group>\n  </macro>\n  <citation>\n    <layout delimiter=\"; \" suffix=\".\">\n      <choose>\n        <if type=\"article-journal\">\n          <group delimiter=\", \">\n            <text macro=\"author-note\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n            </group>\n            <group delimiter=\" \">\n              <text variable=\"page-first\"/>\n              <text variable=\"locator\" prefix=\"(\" suffix=\")\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"article-magazine\" match=\"all\" variable=\"volume\">\n          <group delimiter=\", \">\n            <text macro=\"author-note\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <text variable=\"volume\"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group delimiter=\" \">\n              <text variable=\"page-first\"/>\n              <text variable=\"locator\" prefix=\"(\" suffix=\")\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"book\">\n          <group delimiter=\", \">\n            <text macro=\"autor-editor-note\"/>\n            <text variable=\"title\" form=\"short\"/>\n            <text macro=\"locator-with-label\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group delimiter=\", \">\n            <text macro=\"author-note\"/>\n            <text macro=\"inbook\"/>\n            <group delimiter=\" \">\n              <group delimiter=\"&#160;\">\n                <text term=\"page\" form=\"short\"/>\n                <text variable=\"page-first\"/>\n              </group>\n              <text variable=\"locator\" prefix=\"(\" suffix=\")\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"entry-encyclopedia\">\n          <text value=\"Bearbeiter,\" font-style=\"italic\" suffix=\" \"/>\n          <text value=\"in: \"/>\n          <group delimiter=\", \">\n            <text variable=\"title\" form=\"short\"/>\n            <text variable=\"locator\"/>\n          </group>\n        </else-if>\n        <else-if type=\"legal_case\" match=\"any\">\n          <group delimiter=\", \">\n            <choose>\n              <if match=\"any\" variable=\"title-short\">\n                <text variable=\"title-short\"/>\n              </if>\n              <else>\n                <text variable=\"authority\"/>\n                <group delimiter=\" - \">\n                  <group delimiter=\" \">\n                    <text variable=\"genre\"/>\n                    <date form=\"numeric\" variable=\"issued\" prefix=\"v. \"/>\n                  </group>\n                  <text variable=\"number\"/>\n                </group>\n              </else>\n            </choose>\n            <group delimiter=\" \">\n              <text variable=\"container-title\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text macro=\"firstpage-locator\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author-note\"/>\n            <text variable=\"title\" form=\"short\"/>\n            <text macro=\"locator-with-label\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"article-journal\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\" suffix=\" \"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </if>\n        <else-if type=\"article-magazine\" match=\"all\" variable=\"volume\">\n          <group delimiter=\", \">\n            <text macro=\"author-note\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <text variable=\"volume\"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"book\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <text variable=\"edition\" suffix=\" \"/>\n            <group delimiter=\" \">\n              <text variable=\"publisher-place\" suffix=\" \"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <group delimiter=\":  \">\n              <text term=\"in\"/>\n              <group delimiter=\", \">\n                <names variable=\"editor\" font-style=\"italic\">\n                  <name delimiter=\"/\" name-as-sort-order=\"all\" sort-separator=\", \" form=\"long\"/>\n                  <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n                </names>\n                <text variable=\"container-title\"/>\n              </group>\n            </group>\n            <text variable=\"edition\"/>\n            <group delimiter=\" \">\n              <text variable=\"publisher-place\"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <group delimiter=\" \">\n              <text variable=\"publisher-place\"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"entry-encyclopedia\">\n          <text macro=\"author\"/>\n          <text variable=\"title\" prefix=\", \" suffix=\", \"/>\n          <text variable=\"edition\" suffix=\" \"/>\n          <text variable=\"publisher-place\" suffix=\" \"/>\n          <date variable=\"issued\" suffix=\" (Zit.:  \">\n            <date-part name=\"year\" form=\"long\"/>\n          </date>\n          <names variable=\"editor\" suffix=\"/\">\n            <name delimiter=\"; \" form=\"long\" initialize-with=\"\"/>\n          </names>\n          <text value=\"Bearbeiter,\" suffix=\" \" font-style=\"italic\"/>\n          <text value=\"in: \" suffix=\" \"/>\n          <text variable=\"title\" form=\"short\"/>\n          <text value=\")\" suffix=\" \"/>\n        </else-if>\n        <else-if type=\"webpage\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <date form=\"text\" variable=\"issued\" prefix=\"Stand: \"/>\n            <group delimiter=\" \">\n              <text variable=\"URL\"/>\n              <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                <text term=\"accessed\"/>\n                <date form=\"numeric\" variable=\"accessed\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"author\"/>\n          <text variable=\"title\" prefix=\", \" suffix=\", \"/>\n          <text variable=\"container-title\" suffix=\" \"/>\n          <date date-parts=\"year\" form=\"text\" variable=\"issued\" suffix=\", \"/>\n          <text variable=\"page\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"