"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>American School of Classical Studies at Athens</title>\n    <title-short>ASCSA</title-short>\n    <id>http://www.zotero.org/styles/american-school-of-classical-studies-at-athens</id>\n    <link href=\"http://www.zotero.org/styles/american-school-of-classical-studies-at-athens\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/ecology\" rel=\"template\"/>\n    <link href=\"https://www.ascsa.edu.gr/uploads/media/Guidelines10-29-18.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <summary>Reference style for ASCSA manuscripts and Hesperia journal articles.</summary>\n    <updated>2018-12-21T22:30:06+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor translator\" delimiter=\", \">\n          <label form=\"short\" plural=\"never\" suffix=\" \"/>\n          <name and=\"text\" initialize-with=\". \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \">\n          <label form=\"short\" suffix=\" \"/>\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"first\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <text variable=\"URL\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"book\" match=\"any\">\n          <text variable=\"publisher-place\" prefix=\", \"/>\n        </if>\n        <else-if type=\"thesis\" match=\"any\">\n          <text variable=\"publisher\" prefix=\" (diss. \" suffix=\")\"/>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"original-date\">\n      <date-part name=\"year\" prefix=\"[\" suffix=\"] \"/>\n    </date>\n    <choose>\n      <if variable=\"issued\">\n        <group prefix=\" \" suffix=\".\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <choose>\n            <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part prefix=\", \" name=\"month\"/>\n                <date-part prefix=\" \" name=\"day\"/>\n              </date>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text prefix=\" (\" term=\"no date\" suffix=\").\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\" \" prefix=\", \">\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n          <group delimiter=\", \">\n            <choose>\n              <if match=\"any\" variable=\"volume\">\n                <text variable=\"volume\" prefix=\" \"/>\n              </if>\n              <else>\n                <text macro=\"issued-year\"/>\n              </else>\n            </choose>\n            <text macro=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \" prefix=\", \" suffix=\",\">\n          <text macro=\"edition\"/>\n          <text macro=\"secondary-contributors\"/>\n          <text variable=\"container-title\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \" prefix=\", \">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text term=\"in\" font-style=\"normal\" suffix=\" \"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"collection\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text macro=\"container-contributors\"/>\n          </group>\n          <text macro=\"volumes\"/>\n          <text variable=\"publisher-place\"/>\n          <text macro=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"thesis\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-case=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\" \">\n      <text variable=\"collection-title\" font-style=\"italic\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-volumes\"/>\n      <label plural=\"always\" variable=\"volume\" form=\"short\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n        </group>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"1\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"issued\" suffix=\" \"/>\n      </group>\n      <group delimiter=\", \">\n        <text macro=\"title\" quotes=\"false\"/>\n      </group>\n      <text macro=\"locators\"/>\n      <group delimiter=\". \">\n        <text macro=\"publisher\"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"