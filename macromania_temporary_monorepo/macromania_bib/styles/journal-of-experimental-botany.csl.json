"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Journal of Experimental Botany</title>\n    <title-short>JXB</title-short>\n    <id>http://www.zotero.org/styles/journal-of-experimental-botany</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-experimental-botany\" rel=\"self\"/>\n    <link href=\"http://www.oxfordjournals.org/our_journals/exbotj/for_authors/index.html\" rel=\"documentation\"/>\n    <link href=\"https://academic.oup.com/jxb/pages/manuscript-preparation?login=false#References\" rel=\"documentation\"/>\n    <author>\n      <name>Johan Asplund</name>\n      <email>asplundj@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0022-0957</issn>\n    <eissn>1460-2431</eissn>\n    <updated>2023-07-18T12:32:23+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\"\" delimiter=\", \"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" initialize-with=\".\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal article\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"volume page\">\n            <choose>\n              <if match=\"any\" variable=\"DOI\">\n                <text variable=\"DOI\" prefix=\"doi: \"/>\n                <choose>\n                  <if type=\"article\" match=\"any\">\n                    <text term=\"preprint\" text-case=\"capitalize-first\" prefix=\". [\" suffix=\"]\"/>\n                  </if>\n                </choose>\n              </if>\n              <else>\n                <text term=\"in press\" font-style=\"italic\"/>\n              </else>\n            </choose>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date date-parts=\"year-month\" form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <citation collapse=\"year-suffix\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" year-suffix-delimiter=\",\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <group>\n          <text macro=\"year-date\"/>\n          <text variable=\"year-suffix\" font-style=\"italic\"/>\n        </group>\n        <group>\n          <label variable=\"locator\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n        <choose>\n          <if type=\"article\" match=\"any\">\n            <text term=\"preprint\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"3\" hanging-indent=\"false\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\" font-weight=\"bold\"/>\n        <group>\n          <text macro=\"year-date\"/>\n          <text variable=\"year-suffix\" font-style=\"italic\"/>\n        </group>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group suffix=\".\">\n              <text macro=\"editor-translator\" prefix=\" \"/>\n            </group>\n            <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <names variable=\"editor translator\">\n                  <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n                  <label form=\"short\" prefix=\", \"/>\n                </names>\n              </group>\n              <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n              <text variable=\"container-title\" prefix=\" \" suffix=\".\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"article\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"publisher\"/>\n              <text macro=\"access\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <text macro=\"access\"/>\n          </else-if>\n          <else>\n            <group delimiter=\". \" suffix=\".\">\n              <text macro=\"editor-translator\"/>\n              <group delimiter=\", \">\n                <group delimiter=\" \" prefix=\" \">\n                  <text variable=\"container-title\"/>\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text macro=\"access\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"