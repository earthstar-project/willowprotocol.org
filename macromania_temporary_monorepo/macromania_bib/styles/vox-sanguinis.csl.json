"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\" default-locale=\"en-US\">\n  <info>\n    <title>Vox Sanguinis</title>\n    <id>http://www.zotero.org/styles/vox-sanguinis</id>\n    <link href=\"http://www.zotero.org/styles/vox-sanguinis\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/biomed-central\" rel=\"template\"/>\n    <link href=\"https://onlinelibrary.wiley.com/page/journal/14230410/homepage/forauthors.html#preparing\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <category field=\"biology\"/>\n    <issn>0042-9007</issn>\n    <eissn>1423-0410</eissn>\n    <updated>2022-12-01T14:12:30+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <date form=\"text\" delimiter=\" \">\n      <date-part name=\"day\"/>\n      <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"collection-editor\" form=\"long\">\n        <single>Series editor</single>\n        <multiple>Series editors</multiple>\n      </term>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\".\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\"; \">\n      <group delimiter=\": \">\n        <choose>\n          <if type=\"thesis\" match=\"none\">\n            <text variable=\"publisher-place\"/>\n          </if>\n        </choose>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\" suffix=\".\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\">\n        <choose>\n          <if match=\"none\" variable=\"page volume\">\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"webpage post-weblog post\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\" \">\n            <text term=\"cited\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"year\"/>\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\" prefix=\" \"/>\n              <date-part name=\"day\" prefix=\" \"/>\n            </date>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group>\n        <group delimiter=\". \">\n          <text macro=\"author\"/>\n          <text variable=\"title\"/>\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n              <group delimiter=\". \" prefix=\" \">\n                <text macro=\"edition\"/>\n                <text variable=\"genre\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\". \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                  <group delimiter=\". \">\n                    <text macro=\"editor\"/>\n                    <text macro=\"container-title\"/>\n                  </group>\n                </group>\n                <text macro=\"edition\"/>\n                <group delimiter=\": \">\n                  <text macro=\"publisher\"/>\n                  <group delimiter=\" \">\n                    <label variable=\"page\" form=\"short\" plural=\"never\"/>\n                    <text variable=\"page\"/>\n                  </group>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <text macro=\"container-title\"/>\n              <group delimiter=\";\">\n                <text macro=\"year-date\"/>\n                <group>\n                  <text variable=\"volume\"/>\n                  <choose>\n                    <if is-numeric=\"issue\"/>\n                    <else>\n                      <text variable=\"issue\" prefix=\" \"/>\n                    </else>\n                  </choose>\n                  <text variable=\"page\" prefix=\":\"/>\n                </group>\n              </group>\n            </else>\n          </choose>\n          <text macro=\"access\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"