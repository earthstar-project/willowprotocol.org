"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>ACM SIGCHI Proceedings (2016)</title>\n    <id>http://www.zotero.org/styles/acm-sigchi-proceedings</id>\n    <link href=\"http://www.zotero.org/styles/acm-sigchi-proceedings\" rel=\"self\"/>\n    <link href=\"http://cscw.acm.org/2016/submit/sigchi-word-proceedings.zip\" rel=\"documentation\"/>\n    <author>\n      <name>Yevgeniy Medynskiy</name>\n      <email>eugenem@gatech.edu</email>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <contributor>\n      <name>Eric Baumer</name>\n      <email>ebaumer@ics.uci.edu</email>\n    </contributor>\n    <contributor>\n      <name>Svetlana Yarosh</name>\n      <email>lana@cc.gatech.edu</email>\n    </contributor>\n    <contributor>\n      <name>Lilly Irani</name>\n      <email>lirani@ics.uci.edu</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name delimiter=\", \" and=\"text\" sort-separator=\", \"/>\n        <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter=\", \" and=\"text\" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"journal\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n        <text variable=\"volume\"/>\n      </group>\n      <text variable=\"issue\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"book-publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else-if variable=\"URL\">\n        <group delimiter=\" \">\n          <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n          <text term=\"from\"/>\n          <text variable=\"URL\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <group delimiter=\":\">\n        <text variable=\"citation-number\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <group delimiter=\". \" suffix=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"year\"/>\n        <text macro=\"title\"/>\n      </group>\n      <group suffix=\".\">\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n            <text macro=\"book-publisher\" suffix=\".\"/>\n          </if>\n          <else-if type=\"paper-conference\">\n            <group suffix=\".\" delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <group delimiter=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <text variable=\"collection-title\" prefix=\"(\" suffix=\")\"/>\n                </group>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter\" match=\"any\">\n            <group delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <text macro=\"edition\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text macro=\"editor\"/>\n              </group>\n            </group>\n            <group suffix=\".\" delimiter=\", \" prefix=\". \">\n              <text macro=\"book-publisher\"/>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-journal\">\n            <group suffix=\".\" delimiter=\": \">\n              <text macro=\"journal\"/>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else>\n            <group suffix=\".\" delimiter=\", \">\n              <group delimiter=\" \" font-style=\"italic\">\n                <text variable=\"container-title\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"