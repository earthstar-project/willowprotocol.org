"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\" page-range-format=\"minimal\">\n  <info>\n    <title>Health Policy and Planning</title>\n    <id>http://www.zotero.org/styles/health-policy-and-planning</id>\n    <link href=\"http://www.zotero.org/styles/health-policy-and-planning\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/aquatic-conservation\" rel=\"template\"/>\n    <link href=\"http://www.oxfordjournals.org/our_journals/heapol/for_authors/index.html\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <issn>0268-1080</issn>\n    <eissn>1460-2237</eissn>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\"\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"version\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" collapse=\"year\" et-al-min=\"3\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\" line-spacing=\"2\" et-al-min=\"7\" et-al-use-first=\"3\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author-short\"/>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\" suffix=\". \"/>\n      <date variable=\"issued\" suffix=\".\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\", \"/>\n          </group>\n          <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"report\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\".\">\n            <group>\n              <group suffix=\".\">\n                <text variable=\"genre\" prefix=\" \"/>\n                <text variable=\"number\" prefix=\" No. \"/>\n              </group>\n              <group suffix=\".\">\n                <text variable=\"publisher\" prefix=\" \"/>\n                <text variable=\"publisher-place\" prefix=\", \"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\".\">\n            <group prefix=\" \">\n              <group suffix=\".\" delimiter=\", \">\n                <text variable=\"genre\"/>\n                <text variable=\"publisher\"/>\n                <text variable=\"publisher-place\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\". \">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <group delimiter=\". \">\n              <text macro=\"editor-translator\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <text variable=\"page\" prefix=\", \" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group prefix=\" \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n            </group>\n            <text variable=\"page\" prefix=\": \"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"