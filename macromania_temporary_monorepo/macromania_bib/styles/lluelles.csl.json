else>\n        </choose>\n        <text variable=\"references\" prefix=\" (\" suffix=\")\"/>\n      </else>\n    </choose>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"legislation-note\">\n    <group delimiter=\", \">\n      <text macro=\"contributors-note\"/>\n      <text variable=\"title\" font-style=\"italic\"/>\n      <choose>\n        <if variable=\"page\">\n          <group delimiter=\" \">\n            <text variable=\"section\"/>\n            <text variable=\"container-title\"/>\n            <text variable=\"page\"/>\n            <text macro=\"issued-year\" prefix=\" (\" suffix=\")\"/>\n          </group>\n          <text variable=\"number\"/>\n        </if>\n        <else>\n          <group delimiter=\" \">\n            <choose>\n              <if variable=\"container-title number\" match=\"none\">\n                <text macro=\"issued\"/>\n              </if>\n              <else>\n                <text macro=\"issued-year\" prefix=\" (\" suffix=\")\"/>\n              </else>\n            </choose>\n            <text variable=\"container-title\"/>\n          </group>\n          <text variable=\"section\"/>\n          <text variable=\"number\"/>\n        </else>\n      </choose>\n      <text variable=\"note\"/>\n    </group>\n    <text macro=\"point-locators\"/>\n    <text variable=\"references\" prefix=\" (\" suffix=\")\"/>\n    <text variable=\"title-short\" prefix=\" [\" suffix=\"]\" font-style=\"italic\"/>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"legislation-bib\">\n    <group delimiter=\", \">\n      <text macro=\"contributors\"/>\n      <text variable=\"title\" font-style=\"italic\"/>\n      <choose>\n        <if variable=\"page\">\n          <group delimiter=\" \">\n            <text variable=\"section\"/>\n            <text variable=\"container-title\"/>\n            <text variable=\"page\"/>\n            <text macro=\"issued-year\" prefix=\" (\" suffix=\")\"/>\n          </group>\n          <text variable=\"number\"/>\n        </if>\n        <else>\n          <group delimiter=\" \">\n            <choose>\n              <if variable=\"container-title number\" match=\"none\">\n                <text macro=\"issued\"/>\n              </if>\n              <else>\n                <text macro=\"issued-year\" prefix=\" (\" suffix=\")\"/>\n              </else>\n            </choose>\n            <text variable=\"container-title\"/>\n          </group>\n          <text variable=\"section\"/>\n          <text variable=\"number\"/>\n        </else>\n      </choose>\n    </group>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"bill-note\">\n    <group delimiter=\", \">\n      <text macro=\"contributors-note\"/>\n      <text variable=\"title\" font-style=\"italic\"/>\n      <group>\n        <choose>\n          <if variable=\"volume chapter-number\" match=\"any\">\n            <text variable=\"number\" prefix=\"projet de loi n°\"/>\n          </if>\n          <else>\n            <text variable=\"number\"/>\n          </else>\n        </choose>\n        <group prefix=\" (\" suffix=\")\" delimiter=\" - \">\n          <text variable=\"references\"/>\n          <text macro=\"issued\"/>\n        </group>\n      </group>\n      <choose>\n        <if variable=\"chapter-number\">\n          <group delimiter=\" \">\n            <text variable=\"chapter-number\"/>\n            <text variable=\"authority\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </if>\n        <else-if variable=\"volume\">\n          <group delimiter=\" \">\n            <text variable=\"volume\"/>\n            <text variable=\"container-title\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </else-if>\n        <else>\n          <text variable=\"authority\"/>\n          <text variable=\"container-title\"/>\n          <text variable=\"section\"/>\n        </else>\n      </choose>\n    </group>\n    <text macro=\"point-locators\"/>\n    <text variable=\"title-short\" prefix=\" [\" suffix=\"]\" font-style=\"italic\"/>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"bill-bib\">\n    <group delimiter=\", \">\n      <text macro=\"contributors-note\"/>\n      <text variable=\"title\" font-style=\"italic\"/>\n      <group>\n        <choose>\n          <if variable=\"volume chapter-number\" match=\"any\">\n            <text variable=\"number\" prefix=\"projet de loi n°\"/>\n          </if>\n          <else>\n            <text variable=\"number\"/>\n          </else>\n        </choose>\n        <group prefix=\" (\" suffix=\")\" delimiter=\" - \">\n          <text variable=\"references\"/>\n          <text macro=\"issued\"/>\n        </group>\n      </group>\n      <choose>\n        <if variable=\"chapter-number\">\n          <group delimiter=\" \">\n            <text variable=\"chapter-number\"/>\n            <text variable=\"authority\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </if>\n        <else-if variable=\"volume\">\n          <group delimiter=\" \">\n            <text variable=\"volume\"/>\n            <text variable=\"container-title\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </else-if>\n        <else>\n          <text variable=\"authority\"/>\n          <text variable=\"container-title\"/>\n          <text variable=\"section\"/>\n        </else>\n      </choose>\n    </group>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"entrydic-note\">\n    <group delimiter=\", \">\n      <text macro=\"contributors-note\"/>\n      <text variable=\"container-title\" font-style=\"italic\"/>\n      <text macro=\"edition\"/>\n      <text macro=\"translator\"/>\n      <text variable=\"volume\"/>\n      <text macro=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n      <text macro=\"issued-year\"/>\n      <group delimiter=\" \">\n        <text value=\"v°\"/>\n        <text variable=\"title\" quotes=\"true\"/>\n      </group>\n    </group>\n    <text macro=\"point-locators\"/>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"entrydic-bib\">\n    <group delimiter=\", \">\n      <text macro=\"contributors\"/>\n      <text variable=\"container-title\" font-style=\"italic\"/>\n      <text macro=\"edition\"/>\n      <text macro=\"translator\"/>\n      <text variable=\"volume\"/>\n      <text macro=\"collection-title\"/>\n      <text macro=\"collection-number\"/>\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n      <text macro=\"issued-year\"/>\n    </group>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"entryencyclo-note\">\n    <group delimiter=\", \">\n      <text macro=\"contributors-note\"/>\n      <group>\n        <text macro=\"title-chapter-special\"/>\n        <text variable=\"container-title\"/>\n      </group>\n      <text macro=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n      <text variable=\"volume\"/>\n      <text macro=\"edition\"/>\n      <text variable=\"page\"/>\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n    <text macro=\"point-locators\"/>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"entryencyclo-bib\">\n    <group delimiter=\", \">\n      <text macro=\"contributors\"/>\n      <group>\n        <text macro=\"title-chapter-special\"/>\n        <text variable=\"container-title\"/>\n      </group>\n      <text macro=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n      <text variable=\"volume\"/>\n      <text macro=\"edition\"/>\n      <text variable=\"page\"/>\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"point-locators\">\n    <choose>\n      <if variable=\"locator\" match=\"any\">\n        <choose>\n          <if locator=\"page\" type=\"chapter article-magazine article-newspaper\" match=\"all\">\n            <label variable=\"locator\" prefix=\" \" suffix=\"&#160;\" form=\"long\"/>\n            <text variable=\"locator\"/>\n          </if>\n          <else-if locator=\"page\" type=\"article-journal legal_case\" match=\"all\">\n            <text variable=\"locator\" prefix=\", \"/>\n          </else-if>\n          <else-if locator=\"sub-verbo\">\n            <text variable=\"locator\" quotes=\"true\" prefix=\", v° \"/>\n          </else-if>\n          <else-if type=\"legislation bill\" locator=\"section\" match=\"all\">\n            <text variable=\"locator\" prefix=\", art.&#160;\"/>\n          </else-if>\n          <else>\n            <label variable=\"locator\" prefix=\", \" suffix=\"&#160;\" form=\"short\"/>\n            <text variable=\"locator\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"URL\">\n    <choose>\n      <if variable=\"DOI\">\n        <text value=\", doi&#160;: \" font-variant=\"small-caps\"/>\n        <text variable=\"DOI\"/>\n      </if>\n      <else-if variable=\"URL\">\n        <text value=\", en ligne&#160;:\"/>\n        <text variable=\"URL\" prefix=\" &lt;\" suffix=\"&gt;\"/>\n        <date variable=\"accessed\" prefix=\" (consulté le \" suffix=\")\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"sort-by-contributors\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"sort-by-type\">\n    <choose>\n      <if type=\"bill legislation\" match=\"any\">\n        <text value=\"1\"/>\n      </if>\n      <else-if type=\"legal_case\">\n        <text value=\"2\"/>\n      </else-if>\n      <else-if type=\"book thesis entry-dictionary\" match=\"any\">\n        <text value=\"3\" font-weight=\"normal\"/>\n      </else-if>\n      <else-if type=\"chapter article-journal entry-encyclopedia\" match=\"any\">\n        <text value=\"4\"/>\n      </else-if>\n      <else>\n        <text value=\"5\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-subsequent-min=\"6\" et-al-subsequent-use-first=\"1\" disambiguate-add-names=\"true\" delimiter-precedes-et-al=\"never\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <text term=\"ibid\" form=\"short\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n          <text macro=\"point-locators\"/>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\" form=\"short\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <choose>\n              <if type=\"bill legal_case legislation entry-dictionary\" match=\"any\">\n                <choose>\n                  <if type=\"legal_case\">\n                    <choose>\n                      <if variable=\"author\">\n                        <text variable=\"authority\"/>\n                        <text macro=\"issued\"/>\n                      </if>\n                      <else-if variable=\"title\" match=\"none\">\n                        <text variable=\"authority\"/>\n                        <text macro=\"issued\"/>\n                      </else-if>\n                    </choose>\n                  </if>\n                </choose>\n                <choose>\n                  <if variable=\"title-short\" type=\"legislation bill entry-dictionary\" match=\"all\">\n                    <text variable=\"title-short\" font-style=\"italic\"/>\n                  </if>\n                  <else-if variable=\"title-short\" match=\"any\">\n                    <text variable=\"title-short\" font-style=\"italic\"/>\n                  </else-if>\n                  <else>\n                    <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n                  </else>\n                </choose>\n              </if>\n              <else>\n                <text macro=\"contributors-short\"/>\n              </else>\n            </choose>\n            <text value=\"préc.\" suffix=\", \"/>\n          </group>\n          <group delimiter=\" \">\n            <text value=\"note\"/>\n            <text variable=\"first-reference-note-number\"/>\n          </group>\n          <text macro=\"point-locators\"/>\n        </else-if>\n        <else-if type=\"legislation\">\n          <text macro=\"legislation-note\"/>\n        </else-if>\n        <else-if type=\"bill\">\n          <text macro=\"bill-note\"/>\n        </else-if>\n        <else-if type=\"legal_case\">\n          <text macro=\"case-note\"/>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"thesis-note\"/>\n        </else-if>\n        <else-if type=\"chapter\">\n          <text macro=\"chapter-note\"/>\n        </else-if>\n        <else-if type=\"book report\" match=\"any\">\n          <text macro=\"book-note\"/>\n        </else-if>\n        <else-if type=\"article-newspaper webpage post post-weblog\" match=\"any\">\n          <text macro=\"article-newspaper-note\"/>\n        </else-if>\n        <else-if type=\"article-magazine\">\n          <text macro=\"article-magazine-note\"/>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <text macro=\"article-journal-note\"/>\n        </else-if>\n        <else-if type=\"entry-dictionary\">\n          <text macro=\"entrydic-note\"/>\n        </else-if>\n        <else-if type=\"entry-encyclopedia\">\n          <text macro=\"entryencyclo-note\"/>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"contributors-note\"/>\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"translator\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"collection-title\"/>\n            <text macro=\"collection-number\"/>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"event\"/>\n            <text variable=\"issue\" prefix=\"n°\"/>\n            <text variable=\"volume\"/>\n            <text macro=\"issued\"/>\n            <text variable=\"page\" prefix=\"p.&#160;\"/>\n          </group>\n          <text macro=\"point-locators\"/>\n          <text macro=\"URL\"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"sort-by-type\"/>\n      <key macro=\"sort-by-contributors\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"legislation\">\n          <text macro=\"legislation-bib\"/>\n        </if>\n        <else-if type=\"bill\">\n          <text macro=\"bill-bib\"/>\n        </else-if>\n        <else-if type=\"legal_case\">\n          <text macro=\"case-bib\"/>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"thesis-bib\"/>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"chapter-bib\"/>\n        </else-if>\n        <else-if type=\"book report\" match=\"any\">\n          <text macro=\"book-bib\"/>\n        </else-if>\n        <else-if type=\"article-newspaper webpage post post-weblog\" match=\"any\">\n          <text macro=\"article-newspaper-bib\"/>\n        </else-if>\n        <else-if type=\"article-magazine\">\n          <text macro=\"article-magazine-bib\"/>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <text macro=\"article-journal-bib\"/>\n        </else-if>\n        <else-if type=\"entry-dictionary\">\n          <text macro=\"entrydic-bib\"/>\n        </else-if>\n        <else-if type=\"entry-encyclopedia\">\n          <text macro=\"entryencyclo-bib\"/>\n        </else-if>\n        <else>\n          <text macro=\"contributors\" suffix=\". \"/>\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\"/>\n            <text macro=\"translator\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"collection-title\"/>\n            <text macro=\"collection-number\"/>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"event\"/>\n            <text variable=\"issue\" prefix=\"n°\"/>\n            <text variable=\"volume\"/>\n            <text macro=\"issued\"/>\n            <text variable=\"page\" prefix=\"p.&#160;\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"