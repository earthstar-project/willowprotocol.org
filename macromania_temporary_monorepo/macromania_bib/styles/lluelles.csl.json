tributors\"/>\n      <text variable=\"title\" font-style=\"italic\"/>\n      <text variable=\"genre\"/>\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n      <text macro=\"issued-year\"/>\n    </group>\n    <text variable=\"note\" prefix=\" \"/>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"article-journal-note\">\n    <group delimiter=\" \">\n      <text macro=\"contributors-note\" suffix=\",\"/>\n      <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n      <choose>\n        <if variable=\"volume\">\n          <text macro=\"issued-year\" prefix=\"(\" suffix=\")\"/>\n          <group delimiter=\"-\">\n            <text variable=\"volume\"/>\n            <number variable=\"issue\"/>\n          </group>\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n          <text variable=\"page\"/>\n        </if>\n        <else>\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n          <group delimiter=\".\">\n            <text macro=\"issued-year\"/>\n            <number variable=\"issue\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </group>\n    <text macro=\"point-locators\"/>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"article-journal-bib\">\n    <group delimiter=\" \">\n      <text macro=\"contributors\" suffix=\",\"/>\n      <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n      <choose>\n        <if variable=\"volume\">\n          <text macro=\"issued-year\" prefix=\"(\" suffix=\")\"/>\n          <group delimiter=\"-\">\n            <text variable=\"volume\"/>\n            <number variable=\"issue\"/>\n          </group>\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n          <text variable=\"page\"/>\n        </if>\n        <else>\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n          <group delimiter=\".\">\n            <text macro=\"issued-year\"/>\n            <number variable=\"issue\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </group>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"article-magazine-note\">\n    <group delimiter=\" \">\n      <text macro=\"contributors-note\" suffix=\",\"/>\n      <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n      <text variable=\"container-title\" font-style=\"italic\"/>\n      <choose>\n        <if variable=\"volume\" match=\"any\">\n          <group delimiter=\"-\">\n            <text variable=\"volume\"/>\n            <number variable=\"issue\"/>\n          </group>\n        </if>\n        <else>\n          <group>\n            <text term=\"issue\" form=\"short\"/>\n            <number variable=\"issue\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n    </group>\n    <text variable=\"page\" prefix=\", p.&#160;\"/>\n    <text macro=\"point-locators\"/>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"article-magazine-bib\">\n    <group delimiter=\" \">\n      <text macro=\"contributors\" suffix=\",\"/>\n      <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n      <text variable=\"container-title\" font-style=\"italic\"/>\n      <choose>\n        <if variable=\"volume\" match=\"any\">\n          <group delimiter=\"-\">\n            <text variable=\"volume\"/>\n            <number variable=\"issue\"/>\n          </group>\n        </if>\n        <else>\n          <group>\n            <text term=\"issue\" form=\"short\"/>\n            <number variable=\"issue\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n    </group>\n    <text variable=\"page\" prefix=\", p.&#160;\"/>\n    <text macro=\"URL\"/>\n  </macro>\n  <macro name=\"article-newspaper-note\">\n    <group delimiter=\", \">\n      <t