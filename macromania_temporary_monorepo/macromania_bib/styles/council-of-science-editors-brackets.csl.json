"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Council of Science Editors, Citation-Sequence (numeric, brackets)</title>\n    <title-short>CSE C-S brackets</title-short>\n    <id>http://www.zotero.org/styles/council-of-science-editors-brackets</id>\n    <link href=\"http://www.zotero.org/styles/council-of-science-editors-brackets\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/council-of-science-editors-brackets\" rel=\"template\"/>\n    <link href=\"http://www.scientificstyleandformat.org/Tools/SSF-Citation-Quick-Guide.html\" rel=\"documentation\"/>\n    <author>\n      <name>Pavel Giverts</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <summary>The Council of Science Editors style 8th edition, Citation-Sequence system: numbers in text in brackets, sorted by order of appearance in text.</summary>\n    <updated>2021-08-12T04:37:16+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editortranslator\" form=\"long\">\n        <single>editor and translator</single>\n        <multiple>editors and translators</multiple>\n      </term>\n      <term name=\"collection-editor\" form=\"long\">\n        <single>editor</single>\n        <multiple>editors</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\" delimiter=\"; \" suffix=\".\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\"; \">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <text variable=\"URL\"/>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"thesis\" match=\"any\">\n          <text variable=\"genre\" form=\"long\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"review\">\n    <group delimiter=\". \">\n      <text variable=\"reviewed-title\"/>\n      <text variable=\"container-title\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <group delimiter=\" \">\n      <date variable=\"issued\" delimiter=\" \">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"patent article-newspaper webpage\" match=\"any\">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n        </if>\n        <else-if type=\"article-journal article-magazine\" match=\"any\">\n          <choose>\n            <if variable=\"volume issue\" match=\"none\">\n              <date variable=\"issued\" delimiter=\" \">\n                <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n                <date-part name=\"day\"/>\n              </date>\n            </if>\n          </choose>\n        </else-if>\n      </choose>\n      <group prefix=\" [\" suffix=\"]\" delimiter=\" \">\n        <text term=\"accessed\"/>\n        <date variable=\"accessed\" delimiter=\" \">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \" plural=\"never\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <choose>\n      <if type=\"report\">\n        <group prefix=\" \" suffix=\".\" delimiter=\" \">\n          <text variable=\"collection-title\"/>\n          <text variable=\"number\" prefix=\" Report No.: \"/>\n        </group>\n      </if>\n      <else>\n        <group prefix=\" (\" suffix=\").\" delimiter=\" \">\n          <names variable=\"collection-editor\" suffix=\". \">\n            <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n            <label prefix=\", \"/>\n          </names>\n          <text variable=\"collection-title\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n      <group delimiter=\" \" prefix=\",\">\n        <label variable=\"locator\" form=\"short\" strip-periods=\"true\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"margin\" hanging-indent=\"false\" et-al-min=\"11\" et-al-use-first=\"10\">\n    <layout>\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\".\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n          <group prefix=\" \" suffix=\".\" delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <group prefix=\" \" delimiter=\"; \">\n              <text macro=\"publisher\"/>\n              <group suffix=\".\" delimiter=\". \">\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </group>\n            <text macro=\"pages\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <group delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\" suffix=\".\"/>\n            </group>\n            <group delimiter=\" \" suffix=\".\">\n              <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <group suffix=\".\">\n              <group prefix=\" \" delimiter=\"; \">\n                <text macro=\"publisher\"/>\n                <group suffix=\".\" delimiter=\". \">\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                  <text macro=\"pages\"/>\n                </group>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"review review-book\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text macro=\"review\" suffix=\".\"/>\n            <group delimiter=\";\" prefix=\" \">\n              <text macro=\"issued\"/>\n              <group>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n            </group>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" suffix=\".\"/>\n            <group delimiter=\";\" prefix=\" \">\n              <text macro=\"issued\"/>\n              <group>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n            </group>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"collection\"/>\n      <text prefix=\" \" macro=\"access\"/>\n      <text variable=\"DOI\" prefix=\". doi:\"/>\n    </layout>\n  </bibliography>\n</style>\n"