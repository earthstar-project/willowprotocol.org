"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Orthopaedic Research</title>\n    <id>http://www.zotero.org/styles/journal-of-orthopaedic-research</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-orthopaedic-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/annals-of-neurology\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1002/(ISSN)1554-527X/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <category field=\"medicine\"/>\n    <issn>0736-0266</issn>\n    <eissn>1554-527X</eissn>\n    <updated>2017-08-10T12:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"no date\" form=\"long\">date unknown</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\".\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \" suffix=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \" suffix=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\". \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <choose>\n        <if type=\"book report\" match=\"any\">\n          <text variable=\"number-of-pages\" suffix=\" p.\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"page\" prefix=\"p \"/>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" prefix=\"[\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \">\n      <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n        <text term=\"cited\"/>\n        <date variable=\"accessed\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" prefix=\" \" form=\"short\" strip-periods=\"true\"/>\n          <date-part name=\"day\" prefix=\" \"/>\n        </date>\n      </group>\n      <group>\n        <text value=\"Available from: \"/>\n        <text variable=\"URL\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\", \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"book report\" match=\"any\">\n          <text macro=\"edition\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" et-al-min=\"5\" et-al-use-first=\"3\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"date\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"publisher\" prefix=\" \" suffix=\".\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\". \">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\"/>\n            <text macro=\"edition\" prefix=\", \"/>\n          </group>\n          <text macro=\"publisher\" prefix=\" \"/>\n        </else-if>\n        <else>\n          <text variable=\"container-title\" prefix=\" \" suffix=\" \" form=\"short\"/>\n          <text variable=\"volume\"/>\n          <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          <text variable=\"page\" prefix=\":\"/>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"