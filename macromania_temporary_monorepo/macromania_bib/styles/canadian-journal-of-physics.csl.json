"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\" default-locale=\"en-US\">\n  <info>\n    <title>Canadian Journal of Physics</title>\n    <title-short>Can J Phys</title-short>\n    <id>http://www.zotero.org/styles/canadian-journal-of-physics</id>\n    <link href=\"http://www.zotero.org/styles/canadian-journal-of-physics\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/institute-of-physics-numeric\" rel=\"template\"/>\n    <link href=\"http://www.nrcresearchpress.com/page/cjp/authors#9d\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"physics\"/>\n    <issn>0008-4204</issn>\n    <eissn>1208-6045</eissn>\n    <updated>2017-01-13T10:57:35+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">ed</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\" \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" suffix=\".\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" suffix=\".\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" suffix=\".\">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" strip-periods=\"true\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" et-al-min=\"10\" et-al-use-first=\"1\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\"/>\n      <group delimiter=\" \" prefix=\" \">\n        <text macro=\"author\"/>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"title\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text macro=\"publisher\" suffix=\". \"/>\n          </group>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"normal\" suffix=\".\"/>\n            <text variable=\"collection-title\"/>\n            <text variable=\"event\"/>\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"title\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\" \">\n            <date variable=\"issued\" suffix=\". \">\n              <date-part name=\"year\"/>\n            </date>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" form=\"short\" suffix=\". \"/>\n            <choose>\n              <if type=\"webpage post-weblog\" match=\"any\">\n                <text variable=\"URL\"/>\n              </if>\n            </choose>\n            <group suffix=\": \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page-first\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"