"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>European Journal of Anaesthesiology</title>\n    <title-short>EJA</title-short>\n    <id>http://www.zotero.org/styles/european-journal-of-anaesthesiology</id>\n    <link href=\"http://www.zotero.org/styles/european-journal-of-anaesthesiology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/mary-ann-liebert-vancouver\" rel=\"template\"/>\n    <link href=\"http://edmgr.ovid.com/eja/accounts/ifauth.htm\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0265-0215</issn>\n    <eissn>1365-2346</eissn>\n    <updated>2020-09-01T15:47:54+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" prefix=\" \" strip-periods=\"true\"/>\n        <et-al font-style=\"italic\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text macro=\"anon\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\"; \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"date\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal\" match=\"none\">\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"margin\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group suffix=\".\" prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text prefix=\" \" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\".\"/>\n            <text variable=\"volume\" prefix=\"Vol \" suffix=\".\"/>\n            <text macro=\"edition\" prefix=\" \"/>\n            <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\" prefix=\" \"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\". \"/>\n          <choose>\n            <if variable=\"URL\">\n              <group delimiter=\". \" suffix=\".\">\n                <text variable=\"URL\"/>\n                <group prefix=\"Published \">\n                  <date variable=\"issued\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n                <group>\n                  <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n                  <date variable=\"accessed\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\":\" suffix=\".\">\n                <group>\n                  <date variable=\"issued\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <group delimiter=\" \">\n            <text variable=\"URL\"/>\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <text term=\"accessed\"/>\n              <date variable=\"accessed\">\n                <date-part name=\"month\" form=\"short\"/>\n                <date-part name=\"day\"/>\n                <date-part name=\"year\" prefix=\" \"/>\n              </date>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"editor\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group delimiter=\"; \" prefix=\" \">\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"volume\" font-weight=\"bold\"/>\n            </group>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"