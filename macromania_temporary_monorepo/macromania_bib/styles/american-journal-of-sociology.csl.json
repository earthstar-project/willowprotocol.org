"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"chicago\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>American Journal of Sociology</title>\n    <title-short>AJS</title-short>\n    <id>http://www.zotero.org/styles/american-journal-of-sociology</id>\n    <link href=\"http://www.zotero.org/styles/american-journal-of-sociology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/conservation-and-society\" rel=\"template\"/>\n    <link href=\"https://www.journals.uchicago.edu/journals/ajs/instruct\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"sociology\"/>\n    <issn>0002-9602</issn>\n    <eissn>1537-5390</eissn>\n    <updated>2021-08-21T15:46:13+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\">accessed on</term>\n    </terms>\n  </locale>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \">\n          <label/>\n          <name prefix=\". \" and=\"text\" initialize-with=\". \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" name-as-sort-order=\"first\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book report\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" text-case=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"article-journal article-magazine\" match=\"none\">\n          <group delimiter=\". \">\n            <text variable=\"genre\"/>\n            <group delimiter=\": \">\n              <text variable=\"publisher-place\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </group>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <group prefix=\" \" suffix=\".\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <choose>\n            <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part prefix=\", \" name=\"month\"/>\n                <date-part prefix=\" \" name=\"day\"/>\n              </date>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text prefix=\" (\" term=\"no date\" suffix=\").\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <number variable=\"edition\" form=\"long-ordinal\" text-case=\"capitalize-first\"/>\n        <text term=\"edition\" form=\"long\" prefix=\" \" suffix=\".\"/>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\" \" prefix=\". \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <group delimiter=\": \">\n            <group>\n              <text variable=\"volume\" prefix=\" \"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\". \" prefix=\". \">\n          <text macro=\"secondary-contributors\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\". \" prefix=\". \">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <group delimiter=\" \">\n                <label text-case=\"capitalize-first\" variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n              <text term=\"in\" font-style=\"normal\"/>\n              <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n            </group>\n            <text macro=\"editors\"/>\n          </group>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage post post-weblog\" match=\"any\">\n        <text variable=\"container-title\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"editors\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <label form=\"verb\"/>\n      <name prefix=\" \" and=\"text\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n        </group>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-subsequent-min=\"4\" et-al-subsequent-use-first=\"1\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\".\"/>\n      <text macro=\"issued\" suffix=\" \"/>\n      <text macro=\"title\"/>\n      <text macro=\"locators\"/>\n      <group delimiter=\". \" prefix=\". \">\n        <text macro=\"edition\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"