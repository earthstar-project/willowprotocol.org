"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" page-range-format=\"expanded\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <!-- Polyglot; journal publishes in English and Indonesian. -->\n    <title>Mimbar Hukum</title>\n    <id>http://www.zotero.org/styles/mimbar-hukum</id>\n    <link href=\"http://www.zotero.org/styles/mimbar-hukum\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/international-review-of-the-red-cross\" rel=\"template\"/>\n    <link href=\"https://jurnal.ugm.ac.id/jmh/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"law\"/>\n    <issn>0852-100X</issn>\n    <eissn>2443-0994</eissn>\n    <updated>2019-06-11T14:55:35+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <style-options punctuation-in-quote=\"false\"/>\n    <terms>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"editortranslator\" form=\"short\">ed. and trans.</term>\n      <term name=\"chapter\" form=\"short\">c.</term>\n      <term name=\"accessed\">accessed on</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize=\"false\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-bib\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize=\"false\" name-as-sort-order=\"first\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-translator\">\n    <choose>\n      <if variable=\"editor translator\" match=\"any\">\n        <names variable=\"editor translator\">\n          <label form=\"short\" suffix=\" \"/>\n          <name and=\"text\" initialize-with=\".\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-editor\">\n    <group delimiter=\" \">\n      <text term=\"in\" suffix=\" \"/>\n      <names variable=\"editor\">\n        <name and=\"text\" name-as-sort-order=\"first\"/>\n        <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <names variable=\"translator\"/>\n          <names variable=\"container-author\"/>\n          <text macro=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book graphic legal_case motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"bill legislation treaty\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"paper-conference\" match=\"all\">\n        <group delimiter=\" \">\n          <text term=\"presented at\"/>\n          <text variable=\"event\"/>\n        </group>\n        <text variable=\"event-place\" prefix=\", \"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <date delimiter=\", \" variable=\"issued\">\n          <date-part name=\"month\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-volumes\"/>\n      <text term=\"volume\" form=\"short\" plural=\"true\" strip-periods=\"true\"/>\n    </group>\n  </macro>\n  <macro name=\"pageref\">\n    <choose>\n      <if variable=\"locator\">\n        <choose>\n          <if variable=\"page\">\n            <group>\n              <label variable=\"page\" form=\"short\" suffix=\" \"/>\n              <text variable=\"page\"/>\n              <text term=\"at\" prefix=\", \" suffix=\" \"/>\n            </group>\n          </if>\n        </choose>\n        <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n        <text variable=\"locator\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pageref-subsequent\">\n    <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"volref\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n        <text variable=\"volume\"/>\n      </group>\n      <group delimiter=\" \">\n        <label text-case=\"capitalize-first\" variable=\"issue\" form=\"short\"/>\n        <text variable=\"issue\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <choose>\n          <if variable=\"URL\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"URL\" prefix=\" \"/>\n              <group delimiter=\" \">\n                <text term=\"accessed\"/>\n                <date form=\"text\" variable=\"accessed\"/>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <choose>\n          <if match=\"any\" position=\"ibid-with-locator\">\n            <group delimiter=\", \">\n              <text term=\"ibid\"/>\n              <text macro=\"pageref-subsequent\"/>\n            </group>\n          </if>\n          <else-if match=\"any\" position=\"ibid\">\n            <text term=\"ibid\"/>\n          </else-if>\n          <else-if position=\"subsequent\">\n            <group delimiter=\", \">\n              <group>\n                <choose>\n                  <if match=\"any\" variable=\"author\">\n                    <text macro=\"author\"/>\n                  </if>\n                  <else>\n                    <text macro=\"publisher\"/>\n                  </else>\n                </choose>\n              </group>\n              <choose>\n                <if match=\"any\" disambiguate=\"true\">\n                  <text macro=\"title\"/>\n                </if>\n              </choose>\n              <group delimiter=\" \">\n                <text value=\"above note\"/>\n                <text variable=\"first-reference-note-number\"/>\n              </group>\n              <choose>\n                <if match=\"any\" locator=\"page chapter\">\n                  <text macro=\"pageref-subsequent\"/>\n                </if>\n              </choose>\n            </group>\n          </else-if>\n          <else-if type=\"treaty legislation\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text variable=\"container-title\"/>\n              <date delimiter=\" \" variable=\"issued\">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"note\" prefix=\"(entered into force \" suffix=\")\"/>\n            </group>\n          </else-if>\n          <else-if type=\"bill\" match=\"any\">\n            <group>\n              <text variable=\"container-title\"/>\n              <text variable=\"number\"/>\n              <date delimiter=\" \" variable=\"issued\">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n                <date-part name=\"year\"/>\n              </date>\n              <text macro=\"pageref\"/>\n            </group>\n          </else-if>\n          <else-if type=\"legal_case\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"authority\"/>\n              <text macro=\"title\"/>\n              <text variable=\"number\" prefix=\"Case No. \"/>\n              <text variable=\"references\"/>\n              <text variable=\"note\"/>\n              <date delimiter=\" \" variable=\"issued\">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n                <date-part name=\"year\"/>\n              </date>\n              <text macro=\"pageref\"/>\n            </group>\n          </else-if>\n          <else>\n            <text macro=\"author\"/>\n            <choose>\n              <if type=\"report thesis manuscript\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"date\"/>\n                  <text macro=\"title\"/>\n                  <text variable=\"genre\"/>\n                  <text variable=\"number\" prefix=\" \"/>\n                  <text variable=\"publisher\"/>\n                  <text variable=\"publisher-place\"/>\n                  <text variable=\"collection-title\"/>\n                </group>\n                <text macro=\"pageref\"/>\n              </if>\n              <else-if type=\"book graphic motion_picture song\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"date\"/>\n                  <text macro=\"title\"/>\n                  <text macro=\"editor-translator\"/>\n                  <text macro=\"container\"/>\n                  <text macro=\"volumes\"/>\n                  <text macro=\"collection\"/>\n                  <text macro=\"volref\"/>\n                </group>\n                <group delimiter=\", \">\n                  <group delimiter=\", \">\n                    <text macro=\"publisher\"/>\n                  </group>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else-if>\n              <else-if type=\"chapter paper-conference\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"container-editor\"/>\n                  <text macro=\"date\"/>\n                  <text macro=\"container\"/>\n                  <text macro=\"collection\"/>\n                  <text macro=\"volref\"/>\n                  <text macro=\"volumes\"/>\n                </group>\n                <group delimiter=\", \">\n                  <group delimiter=\", \">\n                    <text macro=\"publisher\"/>\n                  </group>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else-if>\n              <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"container\"/>\n                </group>\n                <text macro=\"date\"/>\n                <text macro=\"pageref\"/>\n              </else-if>\n              <else-if type=\"webpage post post-weblog\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"container\"/>\n                  <text macro=\"access\"/>\n                </group>\n              </else-if>\n              <else>\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <group delimiter=\" \">\n                    <text macro=\"publisher\"/>\n                    <text macro=\"container\" suffix=\",\"/>\n                    <text macro=\"collection\"/>\n                    <text macro=\"volref\"/>\n                  </group>\n                  <text macro=\"access\"/>\n                </group>\n                <group delimiter=\", \">\n                  <text macro=\"date\"/>\n                  <text macro=\"volumes\"/>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else>\n            </choose>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <layout suffix=\".\">\n      <group delimiter=\", \">\n        <choose>\n          <if type=\"treaty legislation\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text variable=\"container-title\"/>\n              <date delimiter=\" \" variable=\"issued\">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"note\" prefix=\"(entered into force \" suffix=\")\"/>\n            </group>\n          </if>\n          <else-if type=\"bill\" match=\"any\">\n            <group>\n              <text variable=\"container-title\"/>\n              <text variable=\"number\"/>\n              <date delimiter=\" \" variable=\"issued\">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n                <date-part name=\"year\"/>\n              </date>\n              <text macro=\"pageref\"/>\n            </group>\n          </else-if>\n          <else-if type=\"legal_case\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"authority\"/>\n              <text macro=\"title\"/>\n              <text variable=\"number\" prefix=\"Case No. \"/>\n              <text variable=\"references\"/>\n              <text variable=\"note\"/>\n              <date delimiter=\" \" variable=\"issued\">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n                <date-part name=\"year\"/>\n              </date>\n              <text macro=\"pageref\"/>\n            </group>\n          </else-if>\n          <else>\n            <text macro=\"author-bib\"/>\n            <choose>\n              <if type=\"report thesis manuscript\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"date\"/>\n                  <text macro=\"title\"/>\n                  <text variable=\"genre\"/>\n                  <text variable=\"number\" prefix=\" \"/>\n                  <text variable=\"publisher\"/>\n                  <text variable=\"publisher-place\"/>\n                  <text variable=\"collection-title\"/>\n                </group>\n                <text macro=\"pageref\"/>\n              </if>\n              <else-if type=\"book graphic motion_picture song\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"date\"/>\n                  <text macro=\"title\"/>\n                  <text macro=\"editor-translator\"/>\n                  <text macro=\"container\"/>\n                  <text macro=\"volumes\"/>\n                  <text macro=\"collection\"/>\n                  <text macro=\"volref\"/>\n                </group>\n                <group delimiter=\", \">\n                  <group delimiter=\", \">\n                    <text macro=\"publisher\"/>\n                  </group>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else-if>\n              <else-if type=\"chapter paper-conference\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"container-editor\"/>\n                  <text macro=\"date\"/>\n                  <text macro=\"container\"/>\n                  <text macro=\"collection\"/>\n                  <text macro=\"volref\"/>\n                  <text macro=\"volumes\"/>\n                </group>\n                <group delimiter=\", \">\n                  <group delimiter=\", \">\n                    <text macro=\"publisher\"/>\n                  </group>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else-if>\n              <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"container\"/>\n                </group>\n                <text macro=\"date\"/>\n                <text macro=\"pageref\"/>\n              </else-if>\n              <else-if type=\"webpage post post-weblog\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"container\"/>\n                  <text macro=\"access\"/>\n                </group>\n              </else-if>\n              <else>\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <group delimiter=\" \">\n                    <text macro=\"publisher\"/>\n                    <text macro=\"container\" suffix=\",\"/>\n                    <text macro=\"collection\"/>\n                    <text macro=\"volref\"/>\n                  </group>\n                  <text macro=\"access\"/>\n                </group>\n                <group delimiter=\", \">\n                  <text macro=\"date\"/>\n                  <text macro=\"volumes\"/>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else>\n            </choose>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"