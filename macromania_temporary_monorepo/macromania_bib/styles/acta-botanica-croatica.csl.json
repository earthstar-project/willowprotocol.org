"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Acta Botanica Croatica</title>\n    <id>http://www.zotero.org/styles/acta-botanica-croatica</id>\n    <link href=\"http://www.zotero.org/styles/acta-botanica-croatica\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/biosocieties\" rel=\"template\"/>\n    <link href=\"http://www.abc.botanic.hr/index.php/abc/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0365-0588</issn>\n    <eissn>1847-8476</eissn>\n    <updated>2019-06-16T19:10:45+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \">\n      <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n      <date form=\"text\" variable=\"accessed\"/>\n      <text term=\"from\"/>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author\"/>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\" \" suffix=\",\">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text macro=\"publisher\" prefix=\" \"/>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                  <text macro=\"editor\"/>\n                </group>\n                <text variable=\"container-title\" font-style=\"normal\"/>\n                <text variable=\"collection-title\" suffix=\".\"/>\n                <text variable=\"event\"/>\n                <text variable=\"page\"/>\n              </group>\n              <text macro=\"publisher\" prefix=\" \"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-newspaper\" match=\"any\">\n            <text macro=\"title\" suffix=\".\"/>\n            <group prefix=\" \" delimiter=\", \">\n              <text variable=\"container-title\" form=\"short\" suffix=\",\"/>\n              <text variable=\"collection-title\"/>\n            </group>\n            <group prefix=\" \" suffix=\":\">\n              <date variable=\"issued\">\n                <date-part name=\"day\" suffix=\" \" range-delimiter=\"-\"/>\n                <date-part name=\"month\"/>\n              </date>\n            </group>\n            <group delimiter=\" \">\n              <text macro=\"publisher\" prefix=\" \" suffix=\",\"/>\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post-weblog post\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text variable=\"container-title\"/>\n              </group>\n              <text macro=\"access\"/>\n            </group>\n          </else-if>\n          <else>\n            <group suffix=\".\">\n              <text macro=\"title\" prefix=\" \"/>\n              <text macro=\"editor\" prefix=\" \"/>\n            </group>\n            <group delimiter=\", \">\n              <group delimiter=\" \" prefix=\" \">\n                <text variable=\"container-title\" font-style=\"normal\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"