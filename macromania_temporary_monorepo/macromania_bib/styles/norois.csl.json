"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"fr-FR\">\n  <info>\n    <title>Norois (Français)</title>\n    <id>http://www.zotero.org/styles/norois</id>\n    <link href=\"http://www.zotero.org/styles/norois\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/universite-de-montreal-apa\" rel=\"template\"/>\n    <link href=\"https://journals.openedition.org/norois/1025#tocto1n11\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geography\"/>\n    <issn>0029-182X</issn>\n    <eissn>1760-8546</eissn>\n    <updated>2020-04-15T09:55:52+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"editor\" form=\"short\">dir.</term>\n      <term name=\"editortranslator\" form=\"short\">dir. et trad.</term>\n      <term name=\"translator\" form=\"short\">trad.</term>\n      <term name=\"no date\" form=\"short\">s. d.</term>\n      <term name=\"retrieved\">repéré</term>\n      <term name=\"from\">à</term>\n      <term name=\"presented at\">communication présentée au</term>\n      <term name=\"page\" form=\"short\">p.</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor translator\" delimiter=\", \" suffix=\", \">\n          <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\" sort-separator=\" \"/>\n          <et-al font-style=\"italic\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"article-journal chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \">\n          <label form=\"verb\" suffix=\" \"/>\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          <et-al font-style=\"italic\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\" \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\">\n            <text variable=\"publisher\"/>\n            <text macro=\"title\"/>\n          </if>\n          <else>\n            <text macro=\"title\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\">\n            <text variable=\"publisher\"/>\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else-if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </else-if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"thesis\">\n        <choose>\n          <if variable=\"archive\" match=\"any\">\n            <group>\n              <text term=\"retrieved\" text-case=\"capitalize-first\" suffix=\" \"/>\n              <text term=\"from\" suffix=\" \"/>\n              <text variable=\"archive\" suffix=\".\"/>\n              <text variable=\"archive_location\" prefix=\" (\" suffix=\")\"/>\n            </group>\n          </if>\n          <else>\n            <group>\n              <text term=\"retrieved\" text-case=\"capitalize-first\" suffix=\" \"/>\n              <text term=\"from\" suffix=\" \"/>\n              <text variable=\"URL\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"DOI : \"/>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n              <choose>\n                <if type=\"post\">\n                  <date variable=\"accessed\" form=\"text\" prefix=\"le \"/>\n                </if>\n              </choose>\n              <text term=\"from\"/>\n              <text variable=\"URL\"/>\n            </group>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"medium\"/>\n          <choose>\n            <if variable=\"URL\" match=\"any\">\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else-if type=\"report\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n          <text variable=\"genre\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n          <text variable=\"medium\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n          <choose>\n            <if variable=\"number\" match=\"any\">\n              <text term=\"issue\" form=\"short\"/>\n              <text variable=\"number\"/>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n      <else-if type=\"book graphic motion_picture report song manuscript speech\" match=\"any\">\n        <choose>\n          <if variable=\"version\">\n            <group delimiter=\" \">\n              <text variable=\"title\"/>\n              <text variable=\"genre\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n              <text variable=\"medium\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n              <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                <text term=\"version\"/>\n                <text variable=\"version\"/>\n              </group>\n            </group>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text variable=\"genre\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            <text variable=\"medium\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"paper-conference\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text variable=\"genre\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n        <text variable=\"medium\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n        <text variable=\"genre\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n        <text variable=\"medium\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <group delimiter=\" : \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n      <else-if type=\"thesis\" match=\"any\">\n        <choose>\n          <if variable=\"URL archive\" match=\"none\">\n            <group delimiter=\", \">\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <choose>\n            <if type=\"article-journal article-magazine paper-conference\" match=\"none\">\n              <group delimiter=\", \">\n                <group delimiter=\", \">\n                  <text variable=\"publisher-place\"/>\n                  <text variable=\"publisher\"/>\n                </group>\n                <choose>\n                  <if type=\"chapter paper-conference\" match=\"any\">\n                    <group>\n                      <label variable=\"page\" form=\"short\"/>\n                      <text variable=\"page\"/>\n                    </group>\n                  </if>\n                  <else>\n                    <group delimiter=\" \">\n                      <number variable=\"number-of-pages\"/>\n                      <label variable=\"number-of-pages\" form=\"short\"/>\n                    </group>\n                  </else>\n                </choose>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"container-title\" match=\"none\">\n        <choose>\n          <if variable=\"event\">\n            <choose>\n              <if variable=\"genre\" match=\"none\">\n                <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <text variable=\"event\"/>\n              </if>\n              <else>\n                <group delimiter=\" \">\n                  <text variable=\"genre\" text-case=\"capitalize-first\"/>\n                  <text term=\"presented at\"/>\n                  <text variable=\"event\"/>\n                </group>\n              </else>\n            </choose>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"bill legal_case legislation\" match=\"none\">\n        <choose>\n          <if variable=\"issued\">\n            <group prefix=\" \">\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"year-suffix\"/>\n              <choose>\n                <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n                  <date variable=\"issued\" prefix=\",\">\n                    <date-part prefix=\" \" name=\"day\"/>\n                    <date-part prefix=\" \" name=\"month\"/>\n                  </date>\n                </if>\n              </choose>\n            </group>\n          </if>\n          <else>\n            <group prefix=\" (\" suffix=\")\">\n              <text term=\"no date\" form=\"short\"/>\n              <text variable=\"year-suffix\" prefix=\"-\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued-sort\">\n    <choose>\n      <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"year-suffix\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n        <text variable=\"year-suffix\" prefix=\"-\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\", \" prefix=\", \">\n          <group>\n            <text variable=\"volume\" font-style=\"normal\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <group delimiter=\" \" prefix=\", \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"book graphic motion_picture report song chapter paper-conference\" match=\"any\">\n        <group delimiter=\"; \">\n          <group delimiter=\", \">\n            <text macro=\"edition\"/>\n            <group>\n              <text term=\"volume\" form=\"short\" plural=\"true\" suffix=\" \"/>\n              <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1-\"/>\n            </group>\n            <group>\n              <text term=\"volume\" form=\"short\" suffix=\" \"/>\n              <number variable=\"volume\" form=\"numeric\"/>\n            </group>\n          </group>\n          <text macro=\"secondary-contributors\"/>\n        </group>\n      </else-if>\n      <else-if type=\"legal_case\">\n        <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n          <text variable=\"authority\"/>\n          <date variable=\"issued\" form=\"text\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <choose>\n        <if locator=\"chapter\">\n          <label variable=\"locator\" form=\"short\"/>\n        </if>\n        <else>\n          <label variable=\"locator\" form=\"short\"/>\n        </else>\n      </choose>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <group>\n      <choose>\n        <if type=\"chapter entry-encyclopedia\" match=\"any\">\n          <text term=\"in\" font-style=\"italic\" suffix=\" \"/>\n        </if>\n      </choose>\n      <text macro=\"container-contributors\"/>\n      <choose>\n        <if type=\"paper-conference\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text value=\"Communication présentée au\"/>\n              <text macro=\"container-title\"/>\n            </group>\n            <text variable=\"publisher-place\"/>\n          </group>\n        </if>\n        <else>\n          <text macro=\"container-title\"/>\n        </else>\n      </choose>\n      <choose>\n        <if type=\"entry-encyclopedia\">\n          <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n            <text variable=\"volume\" prefix=\"Vol. \"/>\n            <text variable=\"page\" prefix=\"p. \"/>\n          </group>\n        </if>\n      </choose>\n    </group>\n    <choose>\n      <if type=\"manuscript\">\n        <text value=\"Document inédit\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"manuscript\"/>\n      <else-if type=\"paper-conference\">\n        <text variable=\"container-title\" text-case=\"title\"/>\n      </else-if>\n      <else-if type=\"bill legal_case legislation\" match=\"none\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <group delimiter=\" \" prefix=\", \">\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"volume\"/>\n              <text variable=\"container-title\"/>\n              <group delimiter=\" \">\n                <text term=\"section\" form=\"symbol\"/>\n                <text variable=\"section\"/>\n              </group>\n              <text variable=\"page\"/>\n            </if>\n            <else>\n              <choose>\n                <if type=\"legal_case\">\n                  <text term=\"issue\" form=\"short\"/>\n                  <text variable=\"number\"/>\n                </if>\n                <else>\n                  <text term=\"issue\" form=\"short\"/>\n                  <text variable=\"number\"/>\n                  <group delimiter=\" \">\n                    <text term=\"section\" form=\"symbol\"/>\n                    <text variable=\"section\"/>\n                  </group>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" givenname-disambiguation-rule=\"primary-name\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-sort\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued-year\"/>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-use-last=\"true\" entry-spacing=\"0\" line-spacing=\"2\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-sort\" sort=\"ascending\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <group suffix=\".\">\n        <group delimiter=\", \">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"issued\"/>\n          </group>\n          <text macro=\"title\" prefix=\" \"/>\n          <text macro=\"container\"/>\n        </group>\n        <text macro=\"locators\"/>\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"