"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>I.B. Tauris (note)</title>\n    <id>http://www.zotero.org/styles/ib-tauris-note</id>\n    <link href=\"http://www.zotero.org/styles/ib-tauris-note\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/art-history\" rel=\"template\"/>\n    <link href=\"https://github.com/citation-style-language/styles/files/1355290/IBT.House.Style.2016.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"generic-base\"/>\n    <summary>House style for the I.B. Tauris International Publishing company.</summary>\n    <updated>2017-11-11T23:01:01+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author-note\">\n    <names variable=\"author\">\n      <name and=\"text\"/>\n      <label form=\"short\" prefix=\", \" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"first\"/>\n      <label form=\"short\" prefix=\", \" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" quotes=\"false\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"publisher-place\"/>\n      <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"month\" suffix=\" \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-volumes\"/>\n      <text term=\"volume\" form=\"short\" plural=\"true\" strip-periods=\"true\"/>\n    </group>\n  </macro>\n  <macro name=\"pageno\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"pageref\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"volref\">\n    <group delimiter=\"/\">\n      <number variable=\"volume\" form=\"roman\"/>\n      <text variable=\"issue\"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <text variable=\"container-title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"editors\">\n    <names variable=\"editor translator\">\n      <name and=\"text\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author-subsequent\">\n    <names variable=\"author\">\n      <name and=\"text\">\n        <name-part name=\"family\"/>\n      </name>\n      <label form=\"short\" prefix=\", \" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <citation>\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"locator\"/>\n            </group>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else-if match=\"any\" position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-subsequent\"/>\n            <text macro=\"title\"/>\n            <text macro=\"pageref\"/>\n          </group>\n        </else-if>\n        <else>\n          <choose>\n            <if type=\"chapter\" match=\"any\">\n              <group delimiter=\" \">\n                <group delimiter=\", \">\n                  <text macro=\"author-note\"/>\n                  <text macro=\"title\"/>\n                  <group delimiter=\" \">\n                    <text term=\"in\"/>\n                    <text macro=\"editors\"/>\n                  </group>\n                  <text macro=\"container\"/>\n                </group>\n                <text macro=\"publisher\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </if>\n            <else-if type=\"article-magazine article-journal article-newspaper\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"author-note\"/>\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <group delimiter=\" \">\n                  <text macro=\"volref\"/>\n                  <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text macro=\"pageno\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </else-if>\n            <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"author-note\"/>\n                <group delimiter=\" \">\n                  <text macro=\"title\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n                <text macro=\"pageref\"/>\n              </group>\n            </else-if>\n            <else-if type=\"webpage post-weblog\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"author-note\"/>\n                <text macro=\"title\"/>\n                <group delimiter=\" \">\n                  <text term=\"available at\" text-case=\"capitalize-first\"/>\n                  <text variable=\"URL\"/>\n                  <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                    <text term=\"accessed\"/>\n                    <date form=\"text\" variable=\"accessed\"/>\n                  </group>\n                </group>\n                <text macro=\"pageref\"/>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"author-note\"/>\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"volref\"/>\n                <text macro=\"date\"/>\n                <text macro=\"volumes\"/>\n                <text macro=\"pageno\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </else>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&amp;#8212;&amp;#8212;&amp;#8212;\" subsequent-author-substitute-rule=\"partial-each\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"locator\"/>\n            </group>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else>\n          <choose>\n            <if type=\"chapter\" match=\"any\">\n              <group delimiter=\" \">\n                <group delimiter=\", \">\n                  <text macro=\"author\"/>\n                  <text macro=\"title\"/>\n                  <group delimiter=\" \">\n                    <text term=\"in\"/>\n                    <text macro=\"editors\"/>\n                  </group>\n                  <text macro=\"container\"/>\n                </group>\n                <text macro=\"publisher\"/>\n              </group>\n            </if>\n            <else-if type=\"article-magazine article-journal article-newspaper\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <group delimiter=\" \">\n                  <text macro=\"volref\"/>\n                  <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text macro=\"pageno\"/>\n              </group>\n            </else-if>\n            <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <group delimiter=\" \">\n                  <text macro=\"title\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"webpage post-weblog\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title\"/>\n                <group delimiter=\" \">\n                  <text term=\"available at\" text-case=\"capitalize-first\"/>\n                  <text variable=\"URL\"/>\n                  <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                    <text term=\"accessed\"/>\n                    <date form=\"text\" variable=\"accessed\"/>\n                  </group>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"volref\"/>\n                <text macro=\"date\"/>\n                <text macro=\"volumes\"/>\n                <text macro=\"pageno\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </else>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"