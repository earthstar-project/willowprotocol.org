"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Oxford Art Journal</title>\n    <id>http://www.zotero.org/styles/oxford-art-journal</id>\n    <link href=\"http://www.zotero.org/styles/oxford-art-journal\" rel=\"self\"/>\n    <link href=\"http://www.oxfordjournals.org/our_journals/oxartj/for_authors/index.html\" rel=\"documentation\"/>\n    <author>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <issn>0142-6540</issn>\n    <eissn>1741-7287</eissn>\n    <summary>Oxford Art journal style</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-editor\">\n    <group delimiter=\" \">\n      <text term=\"in\" suffix=\" \"/>\n      <names variable=\"editor\">\n        <name and=\"text\" delimiter=\", \"/>\n        <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <names variable=\"translator\"/>\n          <text macro=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"month\" suffix=\" \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-volumes\"/>\n      <text term=\"volume\" form=\"short\" plural=\"true\" strip-periods=\"true\"/>\n    </group>\n  </macro>\n  <macro name=\"pageno\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <label variable=\"page\" suffix=\" \" form=\"short\"/>\n        <text variable=\"page\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pageref\">\n    <label variable=\"locator\" suffix=\" \" form=\"short\"/>\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"volref\">\n    <group delimiter=\", \">\n      <text variable=\"volume\" prefix=\"Vol. \"/>\n      <text variable=\"issue\" prefix=\"no. \"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation>\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"pageref\"/>\n          </group>\n        </if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <choose>\n              <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"volumes\"/>\n                </group>\n                <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n                  <text macro=\"publisher\"/>\n                  <text macro=\"container\"/>\n                  <text macro=\"volref\"/>\n                  <text macro=\"date\"/>\n                </group>\n                <text macro=\"pageref\" prefix=\". \"/>\n              </if>\n              <else-if type=\"chapter paper-conference\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"container-editor\"/>\n                  <text macro=\"container\"/>\n                  <text macro=\"volref\"/>\n                  <text macro=\"volumes\"/>\n                  <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n                    <text macro=\"publisher\"/>\n                    <text macro=\"date\"/>\n                  </group>\n                  <text macro=\"pageno\"/>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else-if>\n              <else>\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"publisher\"/>\n                  <text macro=\"container\"/>\n                  <text macro=\"volref\"/>\n                  <text macro=\"date\"/>\n                  <text macro=\"volumes\"/>\n                  <text macro=\"pageno\"/>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"