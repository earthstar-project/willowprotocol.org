"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"minimal\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Karger journals</title>\n    <id>http://www.zotero.org/styles/karger-journals</id>\n    <link href=\"http://www.zotero.org/styles/karger-journals\" rel=\"self\"/>\n    <link href=\"http://www.karger.com/Journal/Guidelines/223832#05\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Sean Takats</name>\n      <email>stakats@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Michael Berkowitz</name>\n      <email>mberkowi@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Patrick O'Brien</name>\n      <email>citationstyler@gmail.com</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <summary>Vancouver style for Journals published by Karger</summary>\n    <updated>2019-07-08T19:49:43+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label strip-periods=\"true\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"publisher\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label strip-periods=\"false\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\"; \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n      <text macro=\"accessed-date\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\"/>\n      <else-if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\" DOI: \"/>\n      </else-if>\n      <else-if variable=\"URL\">\n        <group delimiter=\": \">\n          <text term=\"available at\" text-case=\"capitalize-first\"/>\n          <text variable=\"URL\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"DOI\"/>\n      <else-if variable=\"URL\">\n        <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n          <text term=\"cited\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"year\"/>\n            <date-part name=\"month\" prefix=\" \" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\" prefix=\" \"/>\n          </date>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"webpage post post-weblog\" match=\"any\">\n          <text term=\"internet\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\" \"/>\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". \" suffix=\".\">\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"container-title\"/>\n              <text macro=\"edition\" prefix=\", \"/>\n            </group>\n            <group delimiter=\"; \" suffix=\".\">\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n              <group delimiter=\";\">\n                <group>\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                    <date-part name=\"month\" form=\"short\" strip-periods=\"true\" prefix=\" \"/>\n                  </date>\n                  <text macro=\"accessed-date\" prefix=\" \" suffix=\". \"/>\n                </group>\n                <group delimiter=\":\" suffix=\". \">\n                  <group>\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"