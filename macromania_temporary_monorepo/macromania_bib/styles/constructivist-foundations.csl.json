"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>Constructivist Foundations</title>\n    <id>http://www.zotero.org/styles/constructivist-foundations</id>\n    <link href=\"http://www.zotero.org/styles/constructivist-foundations\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/karstenia\" rel=\"template\"/>\n    <link href=\"https://constructivist.info/guidelines/\" rel=\"documentation\"/>\n    <link href=\"https://constructivist.info/guidelines/template.docx\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1782-348X</issn>\n    <updated>2022-10-19T08:29:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"access\">\n    <text variable=\"URL\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor director\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter-precedes-last=\"never\" et-al-min=\"3\" et-al-use-first=\"1\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"symbol\" initialize-with=\". \" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \" suffix=\".\">\n        <group font-weight=\"bold\" delimiter=\" \">\n          <text macro=\"author\"/>\n          <date date-parts=\"year\" form=\"numeric\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n        </group>\n        <choose>\n          <if type=\"book thesis motion_picture article\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group delimiter=\" \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                  <text macro=\"editor\"/>\n                </group>\n                <text variable=\"container-title\"/>\n              </group>\n              <text variable=\"collection-title\"/>\n              <group delimiter=\": \">\n                <text macro=\"publisher\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group delimiter=\" \" prefix=\" \">\n                <text variable=\"container-title\"/>\n                <group delimiter=\": \">\n                  <group>\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"