"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Veterinary Record</title>\n    <id>http://www.zotero.org/styles/veterinary-record</id>\n    <link href=\"http://www.zotero.org/styles/veterinary-record\" rel=\"self\"/>\n    <link href=\"http://veterinaryrecord.bmj.com/\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>2042-7670</issn>\n    <summary>Style for the BMJ Veterinary Record journals</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" suffix=\" \" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n      <name and=\"text\" sort-separator=\" \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label form=\"short\" prefix=\", \" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <et-al term=\"and others\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"doi:\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <group delimiter=\". \">\n              <text variable=\"URL\"/>\n              <group>\n                <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"month\" suffix=\" \"/>\n                  <date-part name=\"day\" suffix=\", \"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if variable=\"accessed\">\n        <date variable=\"accessed\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\" year-suffix-delimiter=\",\">\n    <sort>\n      <key variable=\"issued\" sort=\"ascending\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"99\" et-al-use-first=\"98\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text term=\"in\" suffix=\" \" text-case=\"capitalize-first\"/>\n              <text variable=\"container-title\"/>\n            </group>\n            <text macro=\"editor\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n        </else-if>\n        <else-if type=\"report\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"title\"/>\n            <group delimiter=\", \" prefix=\" \">\n              <group delimiter=\" \" prefix=\" \">\n                <text variable=\"collection-title\" font-style=\"italic\"/>\n                <text variable=\"number\"/>\n              </group>\n              <text variable=\"page\" suffix=\" pp\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group delimiter=\", \" prefix=\" \" suffix=\".\">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"long\" font-style=\"italic\"/>\n              <text variable=\"volume\" font-weight=\"bold\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"