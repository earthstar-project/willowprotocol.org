"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\" default-locale=\"en-US\">\n  <info>\n    <title>Angiology</title>\n    <title-short>Angiology</title-short>\n    <id>http://www.zotero.org/styles/angiology</id>\n    <link href=\"http://www.zotero.org/styles/angiology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/sage-vancouver\" rel=\"template\"/>\n    <link href=\"https://journals.sagepub.com/author-instructions/ANG\" rel=\"documentation\"/>\n    <contributor>\n      <name>Andrew J. Sims</name>\n      <email>andrew.sims@newcastle.ac.uk</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0003-3197</issn>\n    <eissn>1940-1574</eissn>\n    <summary>Vancouver superscript style modified for Angiology</summary>\n    <updated>2022-07-05T15:11:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-newspaper report\" match=\"any\">\n        <date variable=\"issued\" form=\"text\"/>\n      </if>\n      <else>\n        <date variable=\"issued\" form=\"text\" date-parts=\"year\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <text variable=\"genre\" text-case=\"title\"/>\n        <text variable=\"number\"/>\n      </group>\n      <group delimiter=\": \">\n        <choose>\n          <if type=\"thesis\" match=\"none\">\n            <text variable=\"publisher-place\"/>\n          </if>\n        </choose>\n        <text variable=\"publisher\"/>\n      </group>\n      <choose>\n        <if variable=\"URL\" match=\"none\">\n          <text macro=\"date\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\" match=\"any\">\n            <group prefix=\". \" delimiter=\". \">\n              <date variable=\"issued\" form=\"text\" prefix=\"\"/>\n              <text variable=\"DOI\" prefix=\"DOI: \"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\" \" prefix=\", \">\n              <text variable=\"URL\"/>\n              <text macro=\"accessed-date\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"(\" suffix=\")\" delimiter=\", \">\n          <text macro=\"date\"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\" form=\"text\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" suffix=\".\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book patent report thesis\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" second-field-align=\"flush\" line-spacing=\"2\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture patent report song thesis\" match=\"any\">\n          <group delimiter=\". \" prefix=\". \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\", \" prefix=\". \">\n            <group delimiter=\". \">\n              <group delimiter=\" \">\n                <text term=\"in\" suffix=\":\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n                <text variable=\"container-title\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n          <group delimiter=\", \" prefix=\". \">\n            <text macro=\"container-title\"/>\n            <text macro=\"date\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\";\" prefix=\". \">\n            <group delimiter=\" \">\n              <text macro=\"container-title\"/>\n              <choose>\n                <if variable=\"page\" match=\"any\">\n                  <text macro=\"date\"/>\n                </if>\n              </choose>\n            </group>\n            <group delimiter=\":\">\n              <text variable=\"volume\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"