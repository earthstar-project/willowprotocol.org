"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"es-ES\">\n  <info>\n    <title>EUNOMÍA. Revista en Cultura de la Legalidad (Español)</title>\n    <id>http://www.zotero.org/styles/eunomia-revista-en-cultura-de-la-legalidad</id>\n    <link href=\"http://www.zotero.org/styles/eunomia-revista-en-cultura-de-la-legalidad\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/ecosistemas\" rel=\"template\"/>\n    <link href=\"https://e-revistas.uc3m.es/index.php/EUNOM/information/authors\" rel=\"documentation\"/>\n    <link href=\"https://e-revistas.uc3m.es/index.php/EUNOM/article/view/6807/5361\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"botany\"/>\n    <category field=\"science\"/>\n    <issn>2253-6655</issn>\n    <updated>2022-06-20T08:24:18+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"false\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\" type=\"webpage\">\n        <text term=\"available at\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <text variable=\"URL\"/>\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <date form=\"text\" variable=\"accessed\">\n            <date-part name=\"month\"/>\n            <date-part name=\"day\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" suffix=\" \" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"9\" et-al-use-first=\"7\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\" \">\n                <text macro=\"title\"/>\n                <group delimiter=\"; \" prefix=\"(\" suffix=\")\">\n                  <text macro=\"edition\"/>\n                  <text macro=\"editor\"/>\n                </group>\n              </group>\n              <text macro=\"publisher\" suffix=\".\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \" prefix=\" \">\n                <group delimiter=\" \">\n                  <text term=\"in\" text-case=\"capitalize-first\" font-style=\"normal\"/>\n                  <text macro=\"editor\"/>\n                </group>\n                <group delimiter=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                    <label variable=\"page\" form=\"short\"/>\n                    <text variable=\"page\"/>\n                  </group>\n                </group>\n                <text variable=\"collection-title\" suffix=\",\"/>\n              </group>\n              <text macro=\"publisher\" prefix=\" \"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \" prefix=\" \" suffix=\".\">\n              <group>\n                <text macro=\"title\" prefix=\" \"/>\n                <text macro=\"editor\" prefix=\" \"/>\n              </group>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <group>\n                  <text variable=\"volume\" font-style=\"italic\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"