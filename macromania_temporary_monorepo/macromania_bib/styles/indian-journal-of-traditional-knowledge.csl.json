"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Indian Journal of Traditional Knowledge</title>\n    <title-short>IJTK</title-short>\n    <id>http://www.zotero.org/styles/indian-journal-of-traditional-knowledge</id>\n    <link href=\"http://www.zotero.org/styles/indian-journal-of-traditional-knowledge\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-heart-association\" rel=\"template\"/>\n    <link href=\"http://nopr.niscair.res.in/ijtk_inst_auth.htm\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0972-5938</issn>\n    <eissn>0975-1068</eissn>\n    <updated>2017-07-04T13:56:55+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"verb\"/>\n      <name prefix=\" \" delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <group suffix=\",\">\n      <names variable=\"author\">\n        <name and=\"symbol\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n        <label form=\"short\" prefix=\" \" strip-periods=\"true\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text macro=\"anon\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"6\" et-al-use-first=\"5\" second-field-align=\"margin\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\"/>\n      <group delimiter=\", \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\", \" prefix=\" \">\n              <text macro=\"edition\"/>\n              <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n              <text macro=\"volume\"/>\n              <text macro=\"publisher\"/>\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\": \" prefix=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"volume\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"publisher\" prefix=\" \"/>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"article-newspaper\">\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\". \"/>\n            <choose>\n              <if variable=\"URL\">\n                <group delimiter=\". \" suffix=\".\">\n                  <text variable=\"URL\"/>\n                  <group prefix=\"Published \">\n                    <date variable=\"issued\">\n                      <date-part name=\"month\" suffix=\" \"/>\n                      <date-part name=\"day\" suffix=\", \"/>\n                      <date-part name=\"year\"/>\n                    </date>\n                  </group>\n                  <group>\n                    <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n                    <date variable=\"accessed\">\n                      <date-part name=\"month\" suffix=\" \"/>\n                      <date-part name=\"day\" suffix=\", \"/>\n                      <date-part name=\"year\"/>\n                    </date>\n                  </group>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\":\" suffix=\".\">\n                  <group>\n                    <date variable=\"issued\">\n                      <date-part name=\"month\" suffix=\" \"/>\n                      <date-part name=\"day\" suffix=\", \"/>\n                      <date-part name=\"year\"/>\n                    </date>\n                  </group>\n                  <text variable=\"page\"/>\n                </group>\n              </else>\n            </choose>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n              <date date-parts=\"year\" form=\"numeric\" variable=\"issued\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post-weblog\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"URL\" font-style=\"italic\" prefix=\"(\" suffix=\")\"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n            </group>\n          </else-if>\n          <else>\n            <text macro=\"editor\" suffix=\".\"/>\n            <group delimiter=\", \" prefix=\" \">\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n              <group delimiter=\" \">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"