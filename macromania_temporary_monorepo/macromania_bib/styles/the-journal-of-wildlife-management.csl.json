"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>The Journal of Wildlife Management</title>\n    <id>http://www.zotero.org/styles/the-journal-of-wildlife-management</id>\n    <link href=\"http://www.zotero.org/styles/the-journal-of-wildlife-management\" rel=\"self\"/>\n    <link href=\"http://wildlife.org/publications/jwm\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Rintze Zelle</name>\n      <uri>http://twitter.com/rintzezelle</uri>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0022-541X</issn>\n    <eissn>1937-2817</eissn>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\".\">\n      <name sort-separator=\", \" initialize-with=\". \" and=\"text\" name-as-sort-order=\"first\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\". \" and=\"text\" delimiter=\", \"/>\n      <label form=\"long\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"year-suffix\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"chapter-info\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <label variable=\"page\" text-case=\"capitalize-first\"/>\n            <text variable=\"page\"/>\n            <text term=\"in\" font-style=\"italic\"/>\n          </group>\n        </if>\n      </choose>\n      <text macro=\"editor\"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\"/>\n          <group delimiter=\":\">\n            <text variable=\"volume\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <text variable=\"container-title\"/>\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"day\"/>\n          <date-part name=\"month\"/>\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"section\" prefix=\"; section \"/>\n        <text variable=\"page\" prefix=\":\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"none\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"collection-title\"/>\n            <text variable=\"collection-number\"/>\n          </group>\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"long-ordinal\" text-case=\"capitalize-first\"/>\n          <text term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"none\">\n        <choose>\n          <if variable=\"volume\">\n            <group delimiter=\" \">\n              <text term=\"volume\" text-case=\"capitalize-first\"/>\n              <text variable=\"volume\"/>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\". \">\n          <text variable=\"URL\" prefix=\"&lt;\" suffix=\">\"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\" delimiter=\" \">\n              <date-part name=\"day\"/>\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-givenname=\"true\" givenname-disambiguation-rule=\"by-cite\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\":\">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n        <text variable=\"title\"/>\n        <text macro=\"chapter-info\"/>\n        <text macro=\"container\"/>\n        <text macro=\"edition\"/>\n        <text macro=\"volume\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"accessed\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"