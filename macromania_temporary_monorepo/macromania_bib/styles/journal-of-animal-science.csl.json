"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Animal Science</title>\n    <title-short>JAS</title-short>\n    <id>http://www.zotero.org/styles/journal-of-animal-science</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-animal-science\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/council-of-science-editors-author-date\" rel=\"template\"/>\n    <link href=\"https://www.animalsciencepublications.org/publications/jas/infora\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0021-8812</issn>\n    <eissn>1525-3163</eissn>\n    <updated>2012-09-09T21:58:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\". \" delimiter=\", \" and=\"text\"/>\n      <label form=\"long\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" prefix=\", \" strip-periods=\"true\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text variable=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\", \" initialize-with=\".\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text value=\"Available from:\" suffix=\" \"/>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"thesis\" match=\"any\">\n          <text variable=\"genre\" form=\"long\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <label plural=\"never\" suffix=\" \" variable=\"page\" form=\"short\"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"journal\">\n    <text variable=\"container-title\" form=\"short\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <text variable=\"locator\" prefix=\": \"/>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\" et-al-min=\"99\" et-al-use-first=\"98\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout>\n      <group suffix=\".\" delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n        <text macro=\"title\"/>\n      </group>\n      <group suffix=\".\">\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n            <group prefix=\" \" suffix=\".\" delimiter=\" \">\n              <text macro=\"edition\"/>\n              <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text prefix=\" \" macro=\"publisher\"/>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group prefix=\" \" delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n              <text macro=\"editor\"/>\n              <group suffix=\".\" delimiter=\". \">\n                <text variable=\"container-title\"/>\n                <text variable=\"volume\" prefix=\"Vol. \"/>\n                <text macro=\"edition\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group prefix=\" \" delimiter=\". \">\n              <text macro=\"editor\"/>\n              <text macro=\"journal\"/>\n              <group delimiter=\":\">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n              <text variable=\"DOI\" prefix=\"doi:\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"