"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"de-AT\">\n  <info>\n    <title>Wissenschaftlicher Industrielogistik-Dialog (Deutsch - Ã–sterreich)</title>\n    <title-short>WilD</title-short>\n    <id>http://www.zotero.org/styles/wissenschaftlicher-industrielogistik-dialog</id>\n    <link href=\"http://www.zotero.org/styles/wissenschaftlicher-industrielogistik-dialog\" rel=\"self\"/>\n    <link href=\"http://wild-leoben.jimdo.com/\" rel=\"documentation\"/>\n    <author>\n      <name>Christian Rainer</name>\n      <email>christian.rainer@outlook.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <updated>2013-07-16T03:53:13+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <group>\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" prefix=\" \" text-case=\"lowercase\" strip-periods=\"true\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\" delimiter=\", \" and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n    <text variable=\"publisher-place\" prefix=\". \"/>\n  </macro>\n  <macro name=\"page\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"5\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\" \"/>\n      <date variable=\"issued\">\n        <date-part name=\"year\" prefix=\"(\" suffix=\"): \"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" form=\"short\" suffix=\".\"/>\n            <text variable=\"volume\" prefix=\"Vol. \" suffix=\",\"/>\n            <text variable=\"issue\" prefix=\"Nr. \" suffix=\",\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"genre\" suffix=\",\"/>\n            <text variable=\"archive_location\" prefix=\"Doctoral Thesis #\"/>\n            <text variable=\"volume\" prefix=\"Vol. \" suffix=\",\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <choose>\n              <if variable=\"container-title\">\n                <text macro=\"editor\" prefix=\"In: \" suffix=\" \"/>\n                <text variable=\"container-title\" suffix=\". \"/>\n                <text macro=\"publisher\" suffix=\". \"/>\n                <text macro=\"page\"/>\n              </if>\n              <else>\n                <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <text variable=\"event\"/>\n                <text variable=\"event-place\" prefix=\", \"/>\n                <date variable=\"issued\" prefix=\" \">\n                  <date-part name=\"month\" suffix=\" \"/>\n                  <date-part name=\"day\" suffix=\" \"/>\n                </date>\n              </else>\n            </choose>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"URL\"/>\n            <date variable=\"accessed\" form=\"numeric\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"event\" suffix=\".\"/>\n            <text variable=\"event-place\" prefix=\", \"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"