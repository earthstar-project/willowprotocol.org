"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Socio-Economic Review</title>\n    <id>http://www.zotero.org/styles/socio-economic-review</id>\n    <link href=\"http://www.zotero.org/styles/socio-economic-review\" rel=\"self\"/>\n    <link href=\"http://www.oxfordjournals.org/our_journals/soceco/for_authors/examples_of_citations_and_refere.html\" rel=\"documentation\"/>\n    <author>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <issn>1475-1461</issn>\n    <eissn>1475-147X</eissn>\n    <updated>2012-09-09T21:58:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <group>\n      <names variable=\"editor translator\" prefix=\" \">\n        <name delimiter=\", \" name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\". \"/>\n        <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-citation\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <choose>\n          <if variable=\"URL\">\n            <text value=\"accessed at \"/>\n            <text variable=\"URL\"/>\n            <group prefix=\" on \">\n              <date variable=\"accessed\">\n                <date-part name=\"month\" suffix=\" \"/>\n                <date-part name=\"day\" suffix=\", \"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" form=\"long\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\" form=\"long\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"locator\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\">\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <text macro=\"author-citation\"/>\n        <text macro=\"date\"/>\n        <text macro=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"99\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <text macro=\"date\" prefix=\" (\" suffix=\")\"/>\n      <text macro=\"title\" prefix=\" \"/>\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\". \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text macro=\"editor-translator\"/>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n          </group>\n          <group prefix=\", \" delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </if>\n        <else-if type=\"article-journal\">\n          <group prefix=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"volume\" prefix=\", \" font-weight=\"bold\"/>\n            <text variable=\"page\" prefix=\", \"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n          <group delimiter=\", \" prefix=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"issue\"/>\n          </group>\n          <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n            <date-part name=\"day\" form=\"numeric\" prefix=\" \" suffix=\" \"/>\n            <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </else-if>\n        <else-if type=\"speech\">\n          <group prefix=\", \">\n            <text value=\"paper presented at the\" text-case=\"lowercase\"/>\n            <text variable=\"event\" prefix=\" \"/>\n            <text variable=\"event-place\" prefix=\", \"/>\n            <date variable=\"issued\" prefix=\", \">\n              <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n              <date-part name=\"day\" form=\"numeric\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n          <group prefix=\", \" delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text variable=\"volume\" prefix=\", \"/>\n        </else-if>\n        <else>\n          <group prefix=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"page\" prefix=\", \"/>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\", \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"