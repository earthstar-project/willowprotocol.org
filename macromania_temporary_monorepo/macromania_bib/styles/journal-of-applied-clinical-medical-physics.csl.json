"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <!-- This style was edited with the Visual CSL Editor (http://steveridout.com/csl/visualEditor/) -->\n  <info>\n    <title>Journal of Applied Clinical Medical Physics</title>\n    <id>http://www.zotero.org/styles/journal-of-applied-clinical-medical-physics</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-applied-clinical-medical-physics\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-medical-association\" rel=\"template\"/>\n    <link href=\"http://www.jacmp.org/index.php/jacmp/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Stefano Peca</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <category field=\"physics\"/>\n    <issn>1526-9914</issn>\n    <updated>2016-10-12T11:30:12+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" prefix=\", \"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text macro=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"publisher\" form=\"long\"/>\n      <text variable=\"publisher-place\" form=\"long\"/>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"article-journal chapter paper-conference article-magazine\" match=\"any\">\n        <text variable=\"title\" form=\"long\" quotes=\"false\"/>\n      </if>\n      <else>\n        <text variable=\"title\" form=\"long\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" entry-spacing=\"0\" et-al-min=\"7\" et-al-use-first=\"3\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" vertical-align=\"sup\" suffix=\" \"/>\n      <text macro=\"author\" suffix=\" \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text macro=\"title\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" suffix=\", \"/>\n          <text term=\"in\" suffix=\" \"/>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" suffix=\" \"/>\n          </group>\n          <text macro=\"publisher\"/>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\" prefix=\", \"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\" \">\n            <text variable=\"title\"/>\n            <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" form=\"short\"/>\n            <text macro=\"year-date\" prefix=\"\" suffix=\";\"/>\n            <group delimiter=\": \">\n              <group>\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <group delimiter=\" \">\n                <text variable=\"page\" form=\"short\"/>\n                <!-- text macro=\"year-date\" prefix=\"\" suffix=\"\"/ -->\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"