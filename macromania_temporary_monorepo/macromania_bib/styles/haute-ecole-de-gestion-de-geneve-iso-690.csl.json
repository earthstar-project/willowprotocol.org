delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"responsability\">\n    <names variable=\"author director\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n    <choose>\n      <if variable=\"author editor translator director\" match=\"any\">\n        <text macro=\"year-date\" prefix=\", \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author-citation\">\n    <names variable=\"author director\">\n      <name form=\"short\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"container-responsability\">\n    <choose>\n      <if variable=\"container-author\">\n        <text macro=\"container-author\"/>\n      </if>\n      <else-if variable=\"editor\">\n        <text macro=\"editor\"/>\n      </else-if>\n      <else>\n        <text macro=\"translator\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if is-uncertain-date=\"issued\">\n        <group prefix=\"[\" suffix=\"]\">\n          <choose>\n            <if variable=\"issued\">\n              <date variable=\"issued\">\n                <date-part name=\"year\" form=\"long\" suffix=\"?\"/>\n              </date>\n            </if>\n            <else>\n              <text term=\"no date\"/>\n            </else>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\">\n              <date-part name=\"year\" form=\"long\"/>\n            </date>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis map motion_picture song manuscript speech\" match=\"any\">\n        <choose>\n          <if variable=\"author editor translator director\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n            <text macro=\"year-date\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"software\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text term=\"software\" prefix=\" [\" suffix=\"]\"/>\n      </else-if>\n      <else-if type=\"paper-conference chapter article-journal article-magazine article-newspaper entry entry-dictionary entry-encyclopedia post-weblog post webpage broadcast\" match=\"any\">\n        <choose>\n          <if variable=\"author editor translator director\" match=\"any\">\n            <text variable=\"title\" suffix=\". \"/>\n          </if>\n          <else>\n            <text variable=\"title\" suffix=\", \"/>\n            <text macro=\"year-date\"/>\n          </else>\n        </choose>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\"&#160;: \"/>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"container-author editor translator\" match=\"any\">\n            <text macro=\"container-responsability\"/>\n            <choose>\n              <if variable=\"container-title event\" match=\"any\">\n                <text value=\", \"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <choose>\n              <if type=\"chapter paper-conference\" match=\"any\">\n                <group delimiter=\"&#160;\" prefix=\", \">\n                  <label variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </if>\n            </choose>\n          </if>\n          <else>\n            <text variable=\"event\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"report\">\n        <choose>\n          <if variable=\"author editor translator director\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"number\" suffix=\"&#160;: \"/>\n            <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n            <text macro=\"year-date\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"patent\">\n        <choose>\n          <if variable=\"author editor translator director\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" suffix=\", \"/>\n            <text macro=\"year-date\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <choose>\n          <if variable=\"author editor translator director\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n            <text macro=\"year-date\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n    <choose>\n      <if variable=\"DOI version\" match=\"none\">\n        <choose>\n          <if variable=\"URL\">\n            <text term=\"online\" prefix=\" [\" suffix=\"]\"/>\n          </if>\n          <else>\n            <choose>\n              <if type=\"broadcast motion_picture song book chapter article-journal article-magazine article-newspaper patent\" match=\"none\">\n                <text value=\". \"/>\n              </if>\n            </choose>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <text variable=\"authority\" suffix=\" \"/>\n    <text variable=\"number\"/>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n  </macro>\n  <macro name=\"legislation-number\">\n    <group delimiter=\" \">\n      <text variable=\"container-title\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"map\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n          <else>\n            <text value=\"map\" prefix=\"[\" suffix=\"]\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"genre\" suffix=\" \"/>\n        <text variable=\"number\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"patent\">\n        <text term=\"patent\" text-case=\"capitalize-firs