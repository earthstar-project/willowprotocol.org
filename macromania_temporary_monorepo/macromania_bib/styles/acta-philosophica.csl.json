"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" initialize-with-hyphen=\"true\" page-range-format=\"expanded\">\n  <info>\n    <title>Acta Philosophica</title>\n    <title-short>ActaPhil</title-short>\n    <id>http://www.zotero.org/styles/acta-philosophica</id>\n    <link href=\"http://www.zotero.org/styles/acta-philosophica\" rel=\"self\"/>\n    <link href=\"http://www.actaphilosophica.it/it/norme-i-collaboratori\" rel=\"documentation\"/>\n    <author>\n      <name>Francisco Fernández Labastida</name>\n      <email>ffernandez@pusc.it</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"humanities\"/>\n    <issn>1121-2179</issn>\n    <eissn>1825-6562</eissn>\n    <summary>Acta Philosophica journal’s citation style: footnotes citation style plus full bibliography (available customized locales:it,en,es,fr,de)</summary>\n    <published>2014-01-24T00:00:00+00:00</published>\n    <updated>2016-05-30T00:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"it\">\n    <terms>\n      <term name=\"editor\" form=\"verb\">curato da</term>\n      <term name=\"editor\" form=\"short\">a cura di</term>\n      <term name=\"collection-editor\" form=\"verb\">curato da</term>\n      <term name=\"collection-editor\" form=\"short\">a cura di</term>\n      <term name=\"translator\" form=\"short\">\n        <single>trad.</single>\n        <multiple>tradd.</multiple>\n      </term>\n      <term name=\"volume\" form=\"short\">\n        <single>Vol.</single>\n        <multiple>Voll.</multiple>\n      </term>\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>pp.</multiple>\n      </term>\n      <term name=\"paragraph\" form=\"short\">\n        <single>§</single>\n        <multiple>§§</multiple>\n      </term>\n      <term name=\"in\">in</term>\n      <term name=\"cited\" form=\"short\">cit.</term>\n      <term name=\"accessed\" form=\"long\">consultato il</term>\n      <term name=\"presented at\">pubblicato in</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"collection-editor\" form=\"verb\">edited by</term>\n      <term name=\"collection-editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"editor\" form=\"verb\">edited by</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">\n        <single>trad.</single>\n        <multiple>trads.</multiple>\n      </term>\n      <term name=\"paragraph\" form=\"short\">\n        <single>§</single>\n        <multiple>§§</multiple>\n      </term>\n      <term name=\"cited\" form=\"short\">cit.</term>\n      <term name=\"presented at\">published in</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"es\">\n    <terms>\n      <term name=\"collection-editor\" form=\"verb\">edición a cargo de</term>\n      <term name=\"collection-editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"editor\" form=\"verb\">edición a cargo de</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">\n        <single>trad.</single>\n        <multiple>trads.</multiple>\n      </term>\n      <term name=\"volume\" form=\"short\">\n        <single>Vol.</single>\n        <multiple>Vols.</multiple>\n      </term>\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>pp.</multiple>\n      </term>\n      <term name=\"paragraph\" form=\"short\">\n        <single>§</single>\n        <multiple>§§</multiple>\n      </term>\n      <term name=\"in\">en</term>\n      <term name=\"cited\" form=\"short\">cit.</term>\n      <term name=\"accessed\" form=\"long\">consultado el</term>\n      <term name=\"presented at\">publicado en</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"editor\" form=\"verb\">édité par</term>\n      <term name=\"editor\" form=\"short\">éd.</term>\n      <term name=\"collection-editor\" form=\"verb\">dirigé par</term>\n      <term name=\"collection-editor\" form=\"short\">dir.</term>\n      <term name=\"translator\" form=\"short\">trad.</term>\n      <term name=\"volume\" form=\"short\">Vol.</term>\n      <term name=\"page\" form=\"short\">p.</term>\n      <term name=\"paragraph\" form=\"short\">§</term>\n      <term name=\"in\">dans</term>\n      <term name=\"cited\" form=\"short\">op. cit.</term>\n      <term name=\"accessed\" form=\"long\">consulté le</term>\n      <term name=\"presented at\">publié dans</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"editor\" form=\"verb\">herausgegeben von</term>\n      <term name=\"editor\" form=\"short\">\n        <single>Hg.</single>\n        <multiple>Hgg.</multiple>\n      </term>\n      <term name=\"collection-editor\" form=\"verb\">herausgegeben von</term>\n      <term name=\"collection-editor\" form=\"short\">\n        <single>Hg.</single>\n        <multiple>Hgg.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">Übers.</term>\n      <term name=\"volume\" form=\"short\">Bd.</term>\n      <term name=\"page\" form=\"short\">S.</term>\n      <term name=\"paragraph\" form=\"short\">\n        <single>§</single>\n        <multiple>§§</multiple>\n      </term>\n      <term name=\"in\">in</term>\n      <term name=\"cited\" form=\"short\">cit.</term>\n      <term name=\"accessed\" form=\"long\">zuletz besucht</term>\n      <term name=\"presented at\">in</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" delimiter=\" &#8211; \" delimiter-precedes-last=\"always\" font-variant=\"small-caps\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-sort\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" initialize-with=\". \" delimiter=\" &#8211; \" delimiter-precedes-last=\"always\" font-variant=\"small-caps\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\". \" delimiter=\" &#8211; \" delimiter-precedes-last=\"always\" font-variant=\"small-caps\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"editor-sort\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" initialize-with=\". \" delimiter=\" &#8211; \" delimiter-precedes-last=\"always\" font-variant=\"small-caps\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name initialize-with=\". \" delimiter=\" &#8212; \" delimiter-precedes-last=\"always\" font-variant=\"small-caps\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"translator-sort\">\n    <names variable=\"translator\">\n      <name name-as-sort-order=\"all\" initialize-with=\". \" delimiter=\" &#8212; \" delimiter-precedes-last=\"always\" font-variant=\"small-caps\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"title-short\">\n    <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"series-title\">\n    <text variable=\"collection-title\" font-style=\"italic\" prefix=\"(\" suffix=\")\"/>\n  </macro>\n  <macro name=\"editor-container-title\">\n    <text term=\"in\" suffix=\" \"/>\n    <choose>\n      <if variable=\"container-author\">\n        <names variable=\"container-author\" suffix=\", \">\n          <name initialize-with=\". \" delimiter=\" &#8211; \" delimiter-precedes-last=\"always\" font-variant=\"small-caps\"/>\n        </names>\n      </if>\n      <else-if variable=\"collection-editor\">\n        <names variable=\"collection-editor\">\n          <name initialize-with=\". \" delimiter=\" &#8212; \" delimiter-precedes-last=\"always\" font-variant=\"small-caps\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\"), \"/>\n        </names>\n      </else-if>\n    </choose>\n    <choose>\n      <if variable=\"editor\">\n        <names variable=\"editor\">\n          <name initialize-with=\". \" delimiter=\" &#8211; \" delimiter-precedes-last=\"always\" font-variant=\"small-caps\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\"), \"/>\n        </names>\n      </if>\n    </choose>\n    <text variable=\"container-title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"day\" form=\"numeric\" suffix=\"/\" range-delimiter=\"-\"/>\n      <date-part name=\"month\" form=\"numeric\" suffix=\"/\"/>\n      <date-part name=\"year\" range-delimiter=\"&#8211;\"/>\n    </date>\n  </macro>\n  <macro name=\"accessed\">\n    <date variable=\"accessed\">\n      <date-part name=\"day\" suffix=\"/\" range-delimiter=\"-\"/>\n      <date-part name=\"month\" form=\"numeric\" suffix=\"/\"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"publisher-place-year\">\n    <text variable=\"publisher-place\" suffix=\" \"/>\n    <text macro=\"date\"/>\n    <number variable=\"edition\" vertical-align=\"sup\"/>\n  </macro>\n  <macro name=\"journal\">\n    <choose>\n      <if variable=\"container-title\">\n        <text variable=\"container-title\" prefix=\"«\" suffix=\"»\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" form=\"short\" prefix=\"«\" suffix=\"»\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volref\">\n    <choose>\n      <if variable=\"volume number-of-volumes\" match=\"all\">\n        <group delimiter=\" \">\n          <text term=\"volume\" form=\"short\" plural=\"false\"/>\n          <text variable=\"volume\"/>\n          <text variable=\"number-of-volumes\" prefix=\" (\"/>\n          <text term=\"volume\" form=\"short\" plural=\"true\" prefix=\" \" suffix=\")\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <text term=\"volume\" form=\"short\" plural=\"false\"/>\n          <text variable=\"volume\"/>\n        </group>\n        <group delimiter=\" \">\n          <text variable=\"number-of-volumes\"/>\n          <text term=\"volume\" form=\"short\" plural=\"true\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pageref\">\n    <label variable=\"locator\" form=\"short\" suffix=\" \" prefix=\", \"/>\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"pages\">\n    <text term=\"page\" form=\"short\" plural=\"true\" suffix=\" \" prefix=\", \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"urlref\">\n    <text variable=\"URL\" prefix=\" [\" suffix=\"]\"/>\n    <group prefix=\", \">\n      <text term=\"accessed\" form=\"long\" suffix=\" \"/>\n      <text macro=\"accessed\"/>\n    </group>\n  </macro>\n  <citation>\n    <layout prefix=\"\" suffix=\"\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <text term=\"ibid\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\".\"/>\n          <text macro=\"pageref\"/>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\".\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title-short\"/>\n            <text term=\"cited\" form=\"short\"/>\n          </group>\n          <text macro=\"pageref\"/>\n        </else-if>\n        <else-if type=\"book\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"title\"/>\n            <text macro=\"translator\"/>\n            <text macro=\"volref\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"publisher-place-year\"/>\n          </group>\n          <text macro=\"pageref\"/>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"journal\"/>\n          </group>\n          <text variable=\"volume\" prefix=\", \"/>\n          <text variable=\"issue\" prefix=\"/\"/>\n          <text macro=\"date\" prefix=\" (\" suffix=\")\"/>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n          <group delimiter=\", \" suffix=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n          </group>\n          <text term=\"presented at\" suffix=\" \"/>\n          <text variable=\"container-title\" font-style=\"italic\" suffix=\", \"/>\n          <group delimiter=\" \" suffix=\", \">\n            <text term=\"section\"/>\n            <text variable=\"section\" font-style=\"italic\"/>\n          </group>\n          <choose>\n            <if type=\"article-magazine\">\n              <text variable=\"issue\" suffix=\"/\"/>\n              <text macro=\"date\"/>\n            </if>\n            <else>\n              <text macro=\"date\"/>\n            </else>\n          </choose>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group delimiter=\", \" suffix=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"translator\"/>\n          </group>\n          <group delimiter=\" \" suffix=\", \">\n            <text macro=\"editor-container-title\"/>\n            <text macro=\"series-title\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"volref\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"publisher-place-year\"/>\n          </group>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n          </group>\n          <text variable=\"genre\" prefix=\", \" suffix=\", \"/>\n          <text variable=\"publisher\" suffix=\", \"/>\n          <text macro=\"publisher-place-year\"/>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else-if>\n        <else-if type=\"entry-encyclopedia entry-dictionary\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"editor-container-title\"/>\n            <text macro=\"volref\"/>\n          </group>\n          <choose>\n            <if variable=\"publisher publisher-place issued\" match=\"all\">\n              <text variable=\"publisher\" prefix=\", \" suffix=\", \"/>\n              <text macro=\"publisher-place-year\"/>\n            </if>\n            <else-if variable=\"issued\">\n              <text macro=\"date\" prefix=\", \" suffix=\", \"/>\n            </else-if>\n          </choose>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"date\" prefix=\" (\" suffix=\")\"/>\n          </group>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author-sort\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author-sort\" suffix=\", \"/>\n      <choose>\n        <if type=\"book\">\n          <group delimiter=\", \">\n            <text macro=\"editor-sort\"/>\n            <text macro=\"title\"/>\n            <text macro=\"translator-sort\"/>\n            <text macro=\"volref\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"publisher-place-year\"/>\n          </group>\n        </if>\n        <else-if type=\"thesis\">\n          <text macro=\"title\"/>\n          <text variable=\"genre\" prefix=\", \" suffix=\", \"/>\n          <text variable=\"publisher\" suffix=\", \"/>\n          <text macro=\"publisher-place-year\"/>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"journal\"/>\n          </group>\n          <text variable=\"volume\" prefix=\" \"/>\n          <text variable=\"issue\" prefix=\"/\"/>\n          <text macro=\"date\" prefix=\" (\" suffix=\")\"/>\n          <choose>\n            <if variable=\"page\">\n              <text macro=\"pages\"/>\n            </if>\n          </choose>\n          <text macro=\"urlref\"/>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n          <text macro=\"title\" suffix=\", \"/>\n          <text term=\"presented at\" suffix=\" \"/>\n          <text variable=\"container-title\" font-style=\"italic\" suffix=\", \"/>\n          <group delimiter=\" \" suffix=\", \">\n            <text term=\"section\"/>\n            <text variable=\"section\" font-style=\"italic\"/>\n          </group>\n          <choose>\n            <if type=\"article-magazine\">\n              <text variable=\"issue\" suffix=\"/\"/>\n              <text macro=\"date\"/>\n            </if>\n            <else>\n              <text macro=\"date\"/>\n            </else>\n          </choose>\n          <choose>\n            <if variable=\"page\">\n              <text macro=\"pages\"/>\n            </if>\n          </choose>\n          <text macro=\"urlref\"/>\n        </else-if>\n        <else-if type=\"chapter\">\n          <text macro=\"title\"/>\n          <group delimiter=\" \" prefix=\", \">\n            <text macro=\"editor-container-title\"/>\n            <text macro=\"series-title\"/>\n          </group>\n          <group delimiter=\", \" prefix=\", \">\n            <text macro=\"volref\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"publisher-place-year\"/>\n          </group>\n          <choose>\n            <if variable=\"page\">\n              <text macro=\"pages\"/>\n            </if>\n          </choose>\n          <text macro=\"urlref\"/>\n        </else-if>\n        <else-if type=\"entry-encyclopedia entry-dictionary\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"editor-container-title\"/>\n            <text macro=\"volref\"/>\n          </group>\n          <choose>\n            <if variable=\"publisher publisher-place issued\" match=\"all\">\n              <text variable=\"publisher\" prefix=\", \" suffix=\", \"/>\n              <text macro=\"publisher-place-year\"/>\n            </if>\n            <else-if variable=\"issued\">\n              <text macro=\"date\" prefix=\", \" suffix=\", \"/>\n            </else-if>\n          </choose>\n          <choose>\n            <if variable=\"page\">\n              <text macro=\"pages\"/>\n            </if>\n          </choose>\n          <text macro=\"urlref\"/>\n        </else-if>\n        <else>\n          <text macro=\"title\"/>\n          <text macro=\"date\" prefix=\" (\" suffix=\")\"/>\n          <choose>\n            <if variable=\"page\">\n              <text macro=\"pages\"/>\n            </if>\n          </choose>\n          <text macro=\"urlref\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"