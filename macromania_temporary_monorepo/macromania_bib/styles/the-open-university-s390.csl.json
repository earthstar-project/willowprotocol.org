"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>The Open University - S390</title>\n    <id>http://www.zotero.org/styles/the-open-university-s390</id>\n    <link href=\"http://www.zotero.org/styles/the-open-university-s390\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/the-open-university-harvard\" rel=\"template\"/>\n    <link href=\"http://www.open.ac.uk/libraryservices/documents/Harvard_citation_hlp.pdf\" rel=\"documentation\"/>\n    <link href=\"https://github.com/citation-style-language/styles/pull/4711#discussion_r411547690\" rel=\"documentation\"/>\n    <author>\n      <name>Neal Lyon</name>\n      <email>neallyon34@gmail.com/</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>The Open University Harvard author-date style-this version includes et al font style in italics and bibliography limited to 10 (ten) authors-for use with S390 modules</summary>\n    <updated>2020-03-26T21:53:41+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \" sort-separator=\", \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\" form=\"long\" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <choose>\n          <if type=\"webpage\">\n            <text variable=\"container-title\"/>\n            <text variable=\"URL\"/>\n          </if>\n          <else>\n            <names variable=\"editor\"/>\n            <text macro=\"anon\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <choose>\n          <if type=\"webpage\">\n            <text variable=\"container-title\"/>\n            <text variable=\"URL\"/>\n          </if>\n          <else>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"anon\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <label form=\"short\" plural=\"always\" suffix=\" \"/>\n      <name and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"director\">\n    <names variable=\"author\" prefix=\"Directed by \"/>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \">\n      <text term=\"online\" prefix=\"[\" suffix=\"].\" text-case=\"capitalize-first\"/>\n      <choose>\n        <if variable=\"DOI\">\n          <text variable=\"DOI\" prefix=\"DOI: \"/>\n        </if>\n        <else>\n          <group delimiter=\" \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </else>\n      </choose>\n      <group prefix=\"(\" delimiter=\" \" suffix=\").\">\n        <text term=\"accessed\" text-case=\"capitalize-first\"/>\n        <date variable=\"accessed\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song webpage\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <text term=\"volume\" form=\"short\"/>\n        <number variable=\"volume\" form=\"numeric\"/>\n      </group>\n      <group delimiter=\" \">\n        <text term=\"issue\" form=\"short\"/>\n        <number variable=\"issue\" form=\"numeric\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"published-date\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" form=\"ordinal\" suffix=\" \"/>\n          <date-part name=\"month\" form=\"long\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal\" match=\"any\">\n        <label variable=\"page\" form=\"short\" suffix=\" \"/>\n        <text variable=\"page\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-prefix\">\n    <choose>\n      <if type=\"chapter\">\n        <text term=\"in\"/>\n      </if>\n    </choose>\n  </macro>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"edition\" form=\"short\">edn</term>\n    </terms>\n  </locale>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\" sort=\"descending\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <choose>\n        <if type=\"motion_picture\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </if>\n        <else>\n          <group delimiter=\", \">\n            <group delimiter=\", \">\n              <text macro=\"author-short\"/>\n              <text macro=\"year-date\"/>\n            </group>\n            <group>\n              <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n              <text variable=\"locator\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"citation-number\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"motion_picture\">\n          <group delimiter=\" \">\n            <group delimiter=\" \" suffix=\" [Film].\">\n              <text macro=\"title\"/>\n              <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n              <text macro=\"director\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"webpage\">\n          <group delimiter=\" \">\n            <text macro=\"author\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            <text macro=\"title\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text macro=\"author\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            <group delimiter=\" \" suffix=\",\">\n              <text macro=\"title\"/>\n              <text macro=\"translator\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text macro=\"container-prefix\"/>\n            <group delimiter=\", \">\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"collection-title\"/>\n              <text variable=\"genre\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"locators\"/>\n              <text macro=\"published-date\"/>\n              <text macro=\"pages\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"