"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\" page-range-format=\"minimal\">\n  <info>\n    <title>Ecosystems</title>\n    <id>http://www.zotero.org/styles/ecosystems</id>\n    <link href=\"http://www.zotero.org/styles/ecosystems\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/council-of-science-editors\" rel=\"template\"/>\n    <link href=\"http://www.springer.com/life+sciences/ecology/journal/10021?detailsPage=pltci_721557#id54\" rel=\"documentation\"/>\n    <author>\n      <name>Charles Parnot</name>\n      <uri>http://twitter.com/cparnot</uri>\n      <email>charles.parnot@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1432-9840</issn>\n    <eissn>1435-0629</eissn>\n    <updated>2012-09-09T21:58:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"accessed\">last accessed</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"long\" prefix=\", \"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <names variable=\"translator\"/>\n          <text macro=\"anon\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\"\"/>\n      <et-al term=\"and others\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\". \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\" form=\"numeric\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"journal\">\n    <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" line-spacing=\"2\" et-al-min=\"99\" et-al-use-first=\"98\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"issued\"/>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\". \" suffix=\".\" delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text prefix=\" \" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\". \" suffix=\".\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" suffix=\".\"/>\n            <text variable=\"volume\" prefix=\"Vol. \" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text macro=\"pages\" suffix=\".\" prefix=\". \"/>\n        </else-if>\n        <else>\n          <group delimiter=\".\" prefix=\". \" suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text macro=\"journal\"/>\n            <group prefix=\" \">\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"page\" prefix=\":\" suffix=\".\"/>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"