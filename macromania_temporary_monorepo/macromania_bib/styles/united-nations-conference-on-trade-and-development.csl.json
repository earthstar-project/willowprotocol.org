"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>United Nations Conference on Trade and Development</title>\n    <title-short>UNCTAD</title-short>\n    <id>http://www.zotero.org/styles/united-nations-conference-on-trade-and-development</id>\n    <link href=\"http://www.zotero.org/styles/united-nations-conference-on-trade-and-development\" rel=\"self\"/>\n    <link href=\"https://storage.unctad.org/Zotero/Zotero_UNCTAD_User_Guide.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Carlos Razo</name>\n      <email>carlos.razo@unctad.org</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <summary>Bibliography style for use in UNCTAD, English version. CSL v1.0. This bibliographic style has been developed in the Division of Technology and Logistics and endorsed by the Documents Management Section, Division of Intergovernmental Support Services of UNCTAD.</summary>\n    <updated>2023-07-25T12:39:25+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"no date\" form=\"short\">n/d</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" prefix=\", \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text variable=\"publisher\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text variable=\"publisher\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name suffix=\",\" and=\"text\" initialize-with=\" \" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" \" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"publisher\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"call-number\">\n            <text variable=\"publisher-place\"/>\n          </if>\n          <else>\n            <group delimiter=\". \">\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"report\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access-date\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <group prefix=\"(\" suffix=\")\">\n          <text term=\"accessed\" suffix=\" \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else-if type=\"article-magazine article-newspaper\" match=\"any\">\n        <date variable=\"issued\" prefix=\" \">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </else-if>\n      <else-if type=\"article-journal bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" prefix=\" \"/>\n          <date-part name=\"month\" prefix=\" \"/>\n        </date>\n      </else-if>\n      <else>\n        <group delimiter=\" \" prefix=\" (\" suffix=\")\">\n          <text term=\"accessed\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <choose>\n          <if type=\"webpage\">\n            <choose>\n              <if variable=\"accessed\">\n                <date variable=\"accessed\">\n                  <date-part name=\"year\"/>\n                </date>\n              </if>\n              <else>\n                <text term=\"no date\" form=\"short\"/>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"extra\">\n    <choose>\n      <if variable=\"note\">\n        <text variable=\"note\" prefix=\". \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"un-sales\">\n    <choose>\n      <if variable=\"call-number\">\n        <text variable=\"call-number\" prefix=\"United Nations publication. Sales No. \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <group>\n          <text variable=\"volume\"/>\n          <text variable=\"issue\" prefix=\"(\" suffix=\"):\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"report\">\n        <text variable=\"number\" prefix=\" No. \"/>\n      </else-if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference song\" match=\"any\">\n        <choose>\n          <if variable=\"collection-title\">\n            <group delimiter=\", \">\n              <text variable=\"volume\" prefix=\" Vol. \"/>\n              <text variable=\"collection-number\" prefix=\" No. \"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <text variable=\"locator\" prefix=\": \"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \">\n      <text term=\"available at\" text-case=\"capitalize-first\"/>\n      <text variable=\"URL\" font-style=\"normal\"/>\n      <text macro=\"access-date\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <text macro=\"citation-locator\"/>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"6\" et-al-use-first=\"1\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\" \"/>\n      <text macro=\"year-date\" prefix=\"(\" suffix=\"). \"/>\n      <choose>\n        <if type=\"report\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\". \" delimiter=\" \">\n            <choose>\n              <if variable=\"collection-title\">\n                <text variable=\"collection-title\"/>\n              </if>\n              <else-if variable=\"genre\">\n                <text variable=\"genre\"/>\n              </else-if>\n            </choose>\n            <text macro=\"locators\"/>\n          </group>\n          <group prefix=\". \" delimiter=\", \">\n            <text macro=\"un-sales\"/>\n            <text macro=\"publisher\"/>\n            <text variable=\"page\" prefix=\" \"/>\n          </group>\n          <text macro=\"access-date\" prefix=\".\"/>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n          <text macro=\"title\"/>\n          <choose>\n            <if variable=\"collection-title\">\n              <text variable=\"collection-title\" prefix=\". \"/>\n              <text macro=\"locators\" prefix=\", \"/>\n            </if>\n          </choose>\n          <group prefix=\". \" delimiter=\". \">\n            <text macro=\"un-sales\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\"/>\n          <group prefix=\". \">\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\" suffix=\". \"/>\n            </group>\n            <text variable=\"container-title\" suffix=\". \" font-style=\"italic\" text-case=\"title\"/>\n            <text variable=\"collection-title\"/>\n            <text macro=\"un-sales\" prefix=\". \"/>\n            <group delimiter=\": \">\n              <text macro=\"publisher\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <text macro=\"title\"/>\n          <group prefix=\". \" delimiter=\". \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"collection-title\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <group prefix=\", \" delimiter=\", \">\n            <text macro=\"editor\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-magazine article-newspaper\" match=\"any\">\n          <text macro=\"title\"/>\n          <text macro=\"access-date\" prefix=\".\"/>\n        </else-if>\n        <else-if type=\"speech\">\n          <text macro=\"title\"/>\n          <text variable=\"event\" prefix=\". Presented at the \"/>\n          <text macro=\"extra\"/>\n          <text variable=\"publisher-place\" prefix=\". \"/>\n          <date variable=\"issued\">\n            <date-part name=\"day\" prefix=\". \"/>\n            <date-part name=\"month\" prefix=\" \"/>\n          </date>\n          <text macro=\"access\" prefix=\". \"/>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"container-title\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"title\"/>\n          <text variable=\"publisher\" prefix=\". \"/>\n          <text macro=\"extra\"/>\n          <text macro=\"access\" prefix=\". \"/>\n        </else-if>\n        <else>\n          <text macro=\"title\"/>\n          <text macro=\"editor\" prefix=\". \"/>\n          <text macro=\"extra\"/>\n          <date variable=\"issued\">\n            <date-part name=\"month\" prefix=\" \"/>\n          </date>\n          <text macro=\"access\" prefix=\". \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"