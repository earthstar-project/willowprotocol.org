"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Clinical Infectious Diseases</title>\n    <id>http://www.zotero.org/styles/clinical-infectious-diseases</id>\n    <link href=\"http://www.zotero.org/styles/clinical-infectious-diseases\" rel=\"self\"/>\n    <link href=\"http://www.oxfordjournals.org/our_journals/cid/for_authors/ms_prep.html#Article%20Types\" rel=\"documentation\"/>\n    <author>\n      <name>Michael Berkowitz</name>\n      <email>mberkowi@gmu.edu</email>\n    </author>\n    <contributor>\n      <name>Sean Takats</name>\n      <email>stakats@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1058-4838</issn>\n    <eissn>1537-6591</eissn>\n    <summary>Style for the Oxford Journal Clinical Infectious Diseases. Followed Journal practice rather than styleguide.</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\" suffix=\", \"/>\n    <group suffix=\". \">\n      <text macro=\"year-date\"/>\n      <text variable=\"page\" prefix=\": \"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text value=\"Available at: \"/>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group suffix=\" \">\n          <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" font-weight=\"bold\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song webpage\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\". \">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\"/>\n          </group>\n          <text macro=\"publisher\" prefix=\" \"/>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group suffix=\"; \">\n            <text macro=\"year-date\"/>\n          </group>\n          <group suffix=\". \">\n            <text variable=\"volume\"/>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else>\n      </choose>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"access\"/>\n        <text macro=\"accessed-date\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"