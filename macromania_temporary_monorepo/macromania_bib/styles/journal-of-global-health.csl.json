"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"minimal\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Global Health</title>\n    <title-short>JOGH</title-short>\n    <id>http://www.zotero.org/styles/journal-of-global-health</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-global-health\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/plos\" rel=\"template\"/>\n    <link href=\"https://jogh.org/information-for-contributors/\" rel=\"documentation\"/>\n    <link href=\"http://www.icmje.org/recommendations/browse/manuscript-preparation/preparing-for-submission.html#g\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>2047-2978</issn>\n    <eissn>2047-2986</eissn>\n    <updated>2021-12-10T10:40:56+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <date form=\"text\" delimiter=\" \">\n      <date-part name=\"day\"/>\n      <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"presented at\">presented at</term>\n      <term name=\"retrieved\">available</term>\n      <term name=\"section\" form=\"short\">sect.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <group delimiter=\": \">\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <text term=\"in\" text-case=\"capitalize-first\"/>\n        </if>\n      </choose>\n      <names variable=\"editor\" suffix=\".\">\n        <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"long\" prefix=\", \"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <group delimiter=\", \" suffix=\".\">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n      <else>\n        <choose>\n          <if type=\"article-newspaper article-journal article-magazine\" match=\"none\">\n            <group delimiter=\": \" suffix=\";\">\n              <choose>\n                <if type=\"speech\" match=\"any\"/>\n                <else>\n                  <text variable=\"publisher-place\"/>\n                </else>\n              </choose>\n              <text variable=\"publisher\"/>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI PMID\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"PMID\" prefix=\"Medline:\"/>\n          <text variable=\"DOI\" prefix=\"doi:\"/>\n        </group>\n      </if>\n      <else-if variable=\"URL\">\n        <group delimiter=\". \">\n          <group delimiter=\": \">\n            <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <choose>\n            <if type=\"article-newspaper article-magazine\" match=\"any\">\n              <group delimiter=\" \" suffix=\".\">\n                <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                <date variable=\"accessed\" form=\"text\"/>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n          <text term=\"cited\" text-case=\"lowercase\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal article-magazine chapter paper-conference article-newspaper post-weblog webpage post report\" match=\"any\">\n        <group suffix=\".\" delimiter=\" \">\n          <choose>\n            <if type=\"article-journal report\" match=\"any\">\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n            </if>\n            <else-if type=\"post-weblog webpage post\" match=\"any\">\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n                <text variable=\"container-title\"/>\n                <choose>\n                  <if variable=\"URL\">\n                    <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n                  </if>\n                </choose>\n              </group>\n            </else-if>\n            <else>\n              <text variable=\"container-title\" strip-periods=\"true\"/>\n            </else>\n          </choose>\n        </group>\n        <text macro=\"edition\" prefix=\" \"/>\n      </if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\". \">\n            <text variable=\"container-title\"/>\n            <group delimiter=\" \">\n              <text term=\"section\" form=\"short\" text-case=\"capitalize-first\"/>\n              <text variable=\"section\"/>\n            </group>\n          </group>\n          <text variable=\"number\"/>\n        </group>\n      </else-if>\n      <else-if type=\"speech\">\n        <group delimiter=\": \" suffix=\";\">\n          <group delimiter=\" \">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            <text term=\"presented at\"/>\n          </group>\n          <text variable=\"event\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <text macro=\"edition\" prefix=\". \"/>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"song motion_picture\" match=\"any\">\n        <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"volume page\">\n            <group delimiter=\" \">\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n              <text macro=\"accessed-date\"/>\n            </group>\n          </if>\n          <else>\n            <date date-parts=\"year\" form=\"text\" variable=\"issued\" suffix=\";\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"article-magazine article-newspaper\" match=\"any\">\n        <date variable=\"issued\" form=\"text\"/>\n      </else-if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <group delimiter=\", \">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"report\">\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n        </date>\n      </else-if>\n      <else-if type=\"patent\">\n        <group suffix=\".\">\n          <group delimiter=\", \">\n            <text variable=\"number\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"speech\">\n        <group delimiter=\"; \">\n          <group delimiter=\" \">\n            <date variable=\"issued\" delimiter=\" \">\n              <date-part name=\"year\"/>\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"day\"/>\n            </date>\n          </group>\n          <text variable=\"event-place\"/>\n        </group>\n      </else-if>\n      <else-if type=\"post-weblog webpage\" match=\"any\">\n        <date variable=\"issued\" form=\"text\"/>\n        <text macro=\"accessed-date\" prefix=\" \"/>\n      </else-if>\n      <else>\n        <group suffix=\".\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"page\" prefix=\":\"/>\n      </if>\n      <else>\n        <group prefix=\" \" delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"journal-location\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"volume\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"report-details\">\n    <choose>\n      <if type=\"report\">\n        <text variable=\"number\" prefix=\"Report No.: \"/>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\" \"/>\n      <group delimiter=\". \" suffix=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n      </group>\n      <group delimiter=\" \" suffix=\". \">\n        <text macro=\"editor\"/>\n        <text macro=\"container-title\"/>\n        <text macro=\"publisher\"/>\n        <group>\n          <text macro=\"date\"/>\n          <text macro=\"journal-location\"/>\n          <text macro=\"pages\"/>\n        </group>\n      </group>\n      <text macro=\"report-details\" suffix=\". \"/>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"