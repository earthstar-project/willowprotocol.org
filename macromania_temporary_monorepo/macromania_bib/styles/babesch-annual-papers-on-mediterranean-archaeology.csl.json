"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\">\n  <!-- Polyglot; journal publishes in English, French, German, and Italian -->\n  <info>\n    <title>BABESCH - Annual Papers on Mediterranean Archaeology</title>\n    <id>http://www.zotero.org/styles/babesch-annual-papers-on-mediterranean-archaeology</id>\n    <link href=\"http://www.zotero.org/styles/babesch-annual-papers-on-mediterranean-archaeology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/gallia\" rel=\"template\"/>\n    <link href=\"https://www.babesch.org/instructions.html\" rel=\"documentation\"/>\n    <author>\n      <name>Nicolas Monteix</name>\n      <email>nicolas.monteix@univ-rouen.fr</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"history\"/>\n    <category field=\"anthropology\"/>\n    <issn>0165-9367</issn>\n    <eissn>1783-1369</eissn>\n    <summary>Author-date style for BABESCH Annual papers on Mediterranean archaeology</summary>\n    <updated>2021-04-27T02:59:23+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"paragraph\" form=\"short\">ยง</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\", \">\n      <name name-as-sort-order=\"first\" sort-separator=\" \" initialize-with=\".\" delimiter=\"/\"/>\n      <substitute>\n        <text macro=\"editor-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-author\">\n    <choose>\n      <if variable=\"container-author\">\n        <text macro=\"container-author\"/>\n      </if>\n      <else-if variable=\"editor\">\n        <names variable=\"editor\">\n          <name name-as-sort-order=\"first\" sort-separator=\" \" initialize-with=\".\" delimiter=\"/\"/>\n          <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </else-if>\n      <else>\n        <text macro=\"title-short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <choose>\n      <if variable=\"container-author\">\n        <text macro=\"container-author\"/>\n      </if>\n      <else-if variable=\"editor\">\n        <names variable=\"editor\">\n          <name sort-separator=\" \" initialize-with=\".\" delimiter=\"/\"/>\n          <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\" delimiter=\", \">\n      <name sort-separator=\" \" initialize-with=\".\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <text variable=\"page\" prefix=\"p. \"/>\n    </group>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\", \"/>\n      <substitute>\n        <text macro=\"editor-short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <choose>\n      <if variable=\"editor\">\n        <names variable=\"editor\">\n          <name form=\"short\" delimiter=\", \"/>\n        </names>\n      </if>\n      <else>\n        <text macro=\"title-short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text term=\"accessed\" suffix=\" \"/>\n      <date variable=\"accessed\">\n        <date-part name=\"day\" suffix=\" \"/>\n        <date-part name=\"month\" suffix=\" \"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if variable=\"container-title\">\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <text variable=\"title-short\"/>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"webpage\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout delimiter=\"; \">\n      <choose>\n        <if variable=\"translator\">\n          <text macro=\"author-short\" suffix=\", \"/>\n          <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n        </if>\n        <else>\n          <text macro=\"author-short\" suffix=\" \"/>\n          <text macro=\"year-date\"/>\n        </else>\n      </choose>\n      <group prefix=\", \">\n        <choose>\n          <if locator=\"page\" match=\"none\">\n            <label variable=\"locator\" form=\"short\" suffix=\";\"/>\n            <text variable=\"locator\"/>\n          </if>\n          <else>\n            <text variable=\"locator\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"page-first\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <group prefix=\", \">\n        <choose>\n          <if type=\"thesis report\">\n            <group delimiter=\", \" suffix=\".\">\n              <text macro=\"title\" font-style=\"italic\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"webpage\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"container-title\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-journal article-magazine article-newspaper broadcast personal_communication\" match=\"any\">\n            <group delimiter=\", \" suffix=\".\">\n              <text macro=\"title\"/>\n              <text macro=\"container-title\"/>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"book graphic\" match=\"any\">\n            <group suffix=\".\">\n              <group suffix=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"collection\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n            <text macro=\"title\" suffix=\", \"/>\n            <group>\n              <text value=\"in\" suffix=\" \"/>\n              <text macro=\"editor\" suffix=\", \"/>\n              <group>\n                <text macro=\"container-title\"/>\n                <text macro=\"collection\"/>\n              </group>\n              <text macro=\"publisher\" prefix=\", \"/>\n              <text macro=\"pages\" prefix=\", \"/>\n            </group>\n          </else-if>\n          <else>\n            <group suffix=\".\">\n              <text macro=\"title\"/>\n              <group prefix=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"collection\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <group prefix=\" [\" suffix=\"]\" delimiter=\", \">\n        <text variable=\"URL\" prefix=\"URL: \"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"