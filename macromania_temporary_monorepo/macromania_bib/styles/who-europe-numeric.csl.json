"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>WHO Regional Office for Europe (numeric)</title>\n    <id>http://www.zotero.org/styles/who-europe-numeric</id>\n    <link href=\"http://www.zotero.org/styles/who-europe-numeric\" rel=\"self\"/>\n    <link href=\"https://github.com/citation-style-language/styles/pull/170\" rel=\"documentation\"/>\n    <author>\n      <name>Ivo Rakovac</name>\n      <email>irk@euro.who.int</email>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <summary>The WHO Regional Office for Europe numeric style, based on the second edition from 2017.</summary>\n    <updated>2024-02-12T11:17:06+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if variable=\"call-number\" match=\"none\">\n        <group suffix=\".\">\n          <names variable=\"author\">\n            <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n            <label strip-periods=\"true\" prefix=\", \"/>\n            <substitute>\n              <names variable=\"editor\"/>\n            </substitute>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </if>\n      <else-if match=\"any\" variable=\"URL\">\n        <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"accessSeries\">\n    <choose>\n      <if variable=\"URL collection-title collection-number\" match=\"any\">\n        <group delimiter=\"; \">\n          <text macro=\"series\"/>\n          <text macro=\"access\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" variable=\"archive_location\" match=\"all\">\n        <text variable=\"title\"/>\n        <text variable=\"archive_location\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n      <else-if type=\"thesis\" match=\"all\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text variable=\"genre\" prefix=\" [\" suffix=\"]\"/>\n      </else-if>\n      <else-if type=\"bill book graphic legal_case report\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"webpage\" match=\"all\">\n        <text variable=\"title\" suffix=\" [web site]\"/>\n      </else-if>\n      <else-if type=\"broadcast\" match=\"all\">\n        <text variable=\"title\"/>\n        <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n      </else-if>\n      <else-if type=\"song motion_picture\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"series\">\n    <choose>\n      <if variable=\"collection-title collection-number\" match=\"any\">\n        <group>\n          <text variable=\"collection-title\"/>\n          <text variable=\"collection-number\" prefix=\", No. \"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\",\" font-style=\"italic\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography delimiter-precedes-et-al=\"never\" et-al-min=\"7\" et-al-use-first=\"6\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group suffix=\".\">\n        <text macro=\"author\" suffix=\" \"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture song report thesis\" match=\"any\">\n            <group suffix=\".\">\n              <text macro=\"title\"/>\n              <text macro=\"edition\" prefix=\", \"/>\n              <text macro=\"editor\" prefix=\" (\" suffix=\")\"/>\n            </group>\n            <text prefix=\" \" macro=\"publisher\"/>\n            <group prefix=\", \">\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter\" match=\"any\">\n            <text macro=\"title\" suffix=\".\"/>\n            <group delimiter=\". \" prefix=\" \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\".\"/>\n              <group delimiter=\" \">\n                <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <text macro=\"edition\"/>\n              <text variable=\"collection-title\"/>\n              <group delimiter=\"; \">\n                <text macro=\"publisher\"/>\n                <group>\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                  <text variable=\"page\" prefix=\":\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\" match=\"any\">\n            <text macro=\"title\" suffix=\".\"/>\n            <group prefix=\" \">\n              <choose>\n                <if variable=\"editor\">\n                  <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \" prefix=\" \"/>\n                  <text macro=\"editor\" suffix=\" \"/>\n                  <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n                  <text variable=\"volume\" prefix=\"Vol\" suffix=\".\"/>\n                  <text macro=\"edition\" prefix=\" \"/>\n                  <text variable=\"collection-title\" suffix=\".\"/>\n                  <group>\n                    <text macro=\"publisher\" prefix=\" \"/>\n                    <group suffix=\".\" prefix=\", \">\n                      <date variable=\"issued\">\n                        <date-part name=\"year\"/>\n                      </date>\n                      <text variable=\"page\" prefix=\":\"/>\n                    </group>\n                  </group>\n                </if>\n                <else>\n                  <text variable=\"event\" font-style=\"italic\" suffix=\".\"/>\n                  <group>\n                    <text macro=\"publisher\" prefix=\" \"/>\n                    <group prefix=\", \">\n                      <date variable=\"issued\">\n                        <date-part name=\"year\"/>\n                      </date>\n                    </group>\n                  </group>\n                </else>\n              </choose>\n            </group>\n          </else-if>\n          <else-if type=\"webpage\" match=\"any\">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"note\" prefix=\" \"/>\n            <date variable=\"issued\" prefix=\", \">\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else-if type=\"article-newspaper broadcast\" match=\"any\">\n            <group suffix=\".\">\n              <text macro=\"title\" suffix=\". \"/>\n              <group suffix=\", \" delimiter=\" \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text variable=\"publisher-place\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <group>\n                <date variable=\"issued\">\n                  <date-part name=\"day\" suffix=\" \"/>\n                  <date-part name=\"month\" suffix=\" \"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n              <text variable=\"section\" prefix=\", Section \"/>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </else-if>\n          <else>\n            <group suffix=\".\">\n              <text macro=\"title\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <group delimiter=\" \" prefix=\" \" suffix=\".\">\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"normal\"/>\n              <group delimiter=\";\">\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n                <group delimiter=\":\">\n                  <text variable=\"volume\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else>\n        </choose>\n        <text prefix=\" \" macro=\"accessSeries\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"