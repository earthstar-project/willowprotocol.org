"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Pediatric Gastroenterology and Nutrition</title>\n    <title-short>JPGN</title-short>\n    <id>http://www.zotero.org/styles/journal-of-pediatric-gastroenterology-and-nutrition</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-pediatric-gastroenterology-and-nutrition\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/obesity\" rel=\"template\"/>\n    <link href=\"http://edmgr.ovid.com/jpgn/accounts/ifauth.htm\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>citationstyler@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0277-2116</issn>\n    <eissn>1536-4801</eissn>\n    <updated>2019-08-21T14:27:09+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"in press\">published</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" strip-periods=\"false\" prefix=\", \"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text macro=\"anon\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"citation-number\"/>\n      <group>\n        <label variable=\"locator\" form=\"short\" strip-periods=\"true\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"margin\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \" suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"edition\" prefix=\" \"/>\n            <text macro=\"editor\"/>\n            <group delimiter=\"; \">\n              <text prefix=\" \" macro=\"publisher\"/>\n              <group delimiter=\": \">\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\" prefix=\" \"/>\n            <group>\n              <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n            <text macro=\"volume\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"collection-title\"/>\n            <group delimiter=\"; \" suffix=\".\">\n              <text macro=\"publisher\"/>\n              <group delimiter=\":\">\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n                <text variable=\"page\" form=\"long\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <text variable=\"title\"/>\n            <text variable=\"container-title\"/>\n            <text variable=\"URL\"/>\n            <group delimiter=\" \">\n              <text term=\"in press\" text-case=\"capitalize-first\"/>\n              <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n            </group>\n            <group delimiter=\" \">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date form=\"text\" variable=\"accessed\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n            <group delimiter=\" \" prefix=\" \" suffix=\".\">\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n              <group delimiter=\":\">\n                <group delimiter=\"; \">\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                  <text variable=\"volume\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"