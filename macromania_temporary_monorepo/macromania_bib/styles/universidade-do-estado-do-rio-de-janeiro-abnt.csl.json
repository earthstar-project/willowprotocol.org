"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"never\" default-locale=\"pt-BR\">\n  <info>\n    <title>Universidade do Estado do Rio de Janeiro - ABNT (Português - Brasil)</title>\n    <title-short>UERJ-ABNT</title-short>\n    <id>http://www.zotero.org/styles/universidade-do-estado-do-rio-de-janeiro-abnt</id>\n    <link href=\"http://www.zotero.org/styles/universidade-do-estado-do-rio-de-janeiro-abnt\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/associacao-brasileira-de-normas-tecnicas\" rel=\"template\"/>\n    <link href=\"https://issuu.com/redesirius/docs/roteiro_uerj_web\" rel=\"documentation\"/>\n    <author>\n      <name>Caio Graco-Roza, PhD Candidate</name>\n      <email>caiogracor@gmail.com</email>\n      <uri>http://www.mendeley.com/profiles/caio-graco-roza2/</uri>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <updated>2021-02-04T08:14:12+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"pt-BR\">\n    <terms>\n      <term name=\"month-01\" form=\"short\">jan.</term>\n      <term name=\"month-02\" form=\"short\">fev.</term>\n      <term name=\"month-03\" form=\"short\">mar.</term>\n      <term name=\"month-04\" form=\"short\">abr.</term>\n      <term name=\"month-05\" form=\"short\">maio</term>\n      <term name=\"month-06\" form=\"short\">jun.</term>\n      <term name=\"month-07\" form=\"short\">jul.</term>\n      <term name=\"month-08\" form=\"short\">ago.</term>\n      <term name=\"month-09\" form=\"short\">set.</term>\n      <term name=\"month-10\" form=\"short\">out.</term>\n      <term name=\"month-11\" form=\"short\">nov.</term>\n      <term name=\"month-12\" form=\"short\">dez.</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"container-author\" form=\"short\">\n        <single>ed</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"collection-editor\" form=\"short\">\n        <single>ed</single>\n        <multiple>eds</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter\">\n        <names variable=\"container-author\" delimiter=\", \">\n          <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"always\">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n            <name-part name=\"given\" text-case=\"uppercase\"/>\n          </name>\n          <label form=\"short\" plural=\"never\" text-case=\"capitalize-first\" font-style=\"normal\" prefix=\" (\" suffix=\".).\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"collection-editor\"/>\n          </substitute>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter\" match=\"none\">\n        <names variable=\"editor\" delimiter=\"; \" prefix=\" (\" suffix=\")\">\n          <name initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\" suffix=\".\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"translator\">\n    <text value=\"Traducao \"/>\n    <names variable=\"translator\" delimiter=\"; \">\n      <name delimiter=\"; \" sort-separator=\" \" delimiter-precedes-last=\"always\">\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" font-variant=\"normal\">\n      <name font-style=\"normal\" font-variant=\"normal\" delimiter=\"; \" delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"all\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label form=\"short\" plural=\"never\" text-case=\"lowercase\" font-style=\"normal\" prefix=\" (\" suffix=\".)\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\" text-case=\"uppercase\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\" text-case=\"uppercase\"/>\n      </name>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"book\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <text variable=\"URL\" prefix=\"Disponível em: &lt;\" suffix=\"&gt;.\"/>\n    <date variable=\"accessed\" prefix=\". Acesso em: \" suffix=\".\">\n      <date-part name=\"day\" suffix=\" \"/>\n      <date-part name=\"month\" form=\"short\" suffix=\". \" text-case=\"lowercase\"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"chapter bill\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else-if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" font-weight=\"normal\"/>\n      </else-if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-weight=\"normal\"/>\n      </else-if>\n      <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n        <text variable=\"title\" text-case=\"capitalize-first\"/>\n      </else-if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <text variable=\"title\" suffix=\". \"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"normal\" prefix=\" \" suffix=\", \"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\" font-weight=\"normal\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if match=\"any\" variable=\"publisher\">\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\" suffix=\": \"/>\n          </if>\n          <else-if type=\"entry-encyclopedia\"/>\n          <else>\n            <text value=\"[s.l.] \"/>\n          </else>\n        </choose>\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\" suffix=\", \"/>\n            <text macro=\"issued\"/>\n          </if>\n          <else>\n            <text value=\"[s.n.]\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text value=\"[s.l: s.n.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <choose>\n          <if variable=\"genre\" match=\"none\">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <text variable=\"event\" text-case=\"uppercase\"/>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              <text term=\"presented at\"/>\n              <text variable=\"event\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\" match=\"any\">\n        <group>\n          <choose>\n            <if type=\"book chapter\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part name=\"day\" suffix=\" \"/>\n                <date-part name=\"month\" form=\"short\" suffix=\" \"/>\n              </date>\n            </if>\n          </choose>\n          <date variable=\"issued\" suffix=\". \">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <text value=\"[s.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\" match=\"all\">\n        <date date-parts=\"year\" form=\"numeric\" variable=\"issued\" prefix=\" \" suffix=\". \">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text value=\"[s.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"numeric\" suffix=\".\"/>\n              <text term=\"edition\" form=\"short\" suffix=\".\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\" ed.\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"paper-conference\" match=\"any\"/>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"bill\">\n        <group prefix=\". \" delimiter=\", \">\n          <date variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part prefix=\" \" name=\"month\" form=\"short\"/>\n            <date-part prefix=\" \" name=\"year\"/>\n          </date>\n          <text variable=\"section\" prefix=\"Sec. \"/>\n          <text variable=\"page\" prefix=\"p. \" suffix=\".\"/>\n        </group>\n      </if>\n      <else-if match=\"any\" type=\"article-journal article-magazine article-newspaper\">\n        <group delimiter=\", \">\n          <group delimiter=\", \">\n            <text variable=\"volume\" prefix=\"v. \"/>\n            <text variable=\"issue\" prefix=\"n. \"/>\n          </group>\n          <text variable=\"page\" prefix=\"p. \"/>\n        </group>\n      </else-if>\n      <else-if match=\"any\" type=\"book\">\n        <group delimiter=\", \">\n          <group>\n            <text variable=\"volume\" prefix=\"v. \"/>\n            <text variable=\"page\" suffix=\" p.\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"chapter\" match=\"any\">\n        <group>\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"page\" prefix=\"p. \"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"thesis\" match=\"any\">\n        <group>\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"page\" suffix=\" f.\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"paper-conference\" match=\"any\"/>\n    </choose>\n  </macro>\n  <macro name=\"collection-title\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"genre\">\n    <text variable=\"issue\"/>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"place\">\n    <choose>\n      <if match=\"any\" variable=\"publisher-place\">\n        <text variable=\"publisher-place\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group>\n      <text variable=\"archive\" prefix=\" \"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" collapse=\"year\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key macro=\"issued-year\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout font-variant=\"normal\" delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group>\n        <text suffix=\", \" macro=\"author-short\"/>\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n        <text prefix=\", \" macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <choose>\n        <if type=\"bill\">\n          <group>\n            <text macro=\"author\" text-case=\"uppercase\" font-variant=\"normal\" suffix=\". \"/>\n            <text variable=\"number\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text variable=\"references\" font-style=\"italic\"/>\n            <text variable=\"note\"/>\n            <text macro=\"locators\" suffix=\". \"/>\n          </group>\n        </if>\n        <else-if type=\"map\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text macro=\"issued\"/>\n            <text variable=\"note\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"book\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"translator\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"locators\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"secondary-contributors\" suffix=\". \"/>\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <text macro=\"container-contributors\" suffix=\". \"/>\n            <text macro=\"container-title\" suffix=\". \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"translator\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"locators\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine article-journal\" match=\"any\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"container-title\" suffix=\", \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\", \"/>\n            <text macro=\"locators\" suffix=\", \"/>\n            <text macro=\"issued\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"issued-year\"/>\n            <text macro=\"locators\"/>\n            <text variable=\"publisher\" prefix=\" \" suffix=\",\"/>\n            <text variable=\"publisher-place\" prefix=\" \" suffix=\",\"/>\n            <text macro=\"issued-year\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"manuscript\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"place\" suffix=\", \"/>\n            <text macro=\"issued\" suffix=\". \"/>\n            <text macro=\"access\" suffix=\". \"/>\n            <text macro=\"archive\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"access\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\"/>\n            <text macro=\"container-contributors\"/>\n            <text macro=\"secondary-contributors\"/>\n            <text macro=\"container-title\"/>\n            <text variable=\"collection-title\" prefix=\": \"/>\n            <text macro=\"locators\"/>\n            <text macro=\"event\"/>\n            <text macro=\"publisher\" prefix=\". \" suffix=\". \"/>\n            <text macro=\"access\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"entry-dictionary\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\"/>\n            <text macro=\"container-contributors\"/>\n            <text macro=\"secondary-contributors\"/>\n            <text macro=\"container-title\"/>\n            <text variable=\"collection-title\" prefix=\": \" suffix=\". \"/>\n            <text macro=\"locators\"/>\n            <text macro=\"event\"/>\n            <text macro=\"publisher\" prefix=\". \" suffix=\". \"/>\n            <text macro=\"collection-title\" prefix=\"(Texto para discussao, n. \" suffix=\").\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"entry-encyclopedia\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\"/>\n            <text variable=\"publisher-place\" prefix=\". \" suffix=\": \"/>\n            <text variable=\"publisher\" suffix=\", \"/>\n            <text macro=\"issued\" prefix=\", \" suffix=\". (Nota t?cnica).\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <text macro=\"author\" text-case=\"capitalize-all\" suffix=\". \"/>\n          <text macro=\"title\"/>\n          <text macro=\"container-contributors\"/>\n          <text macro=\"secondary-contributors\"/>\n          <text term=\"in\" text-case=\"capitalize-first\" prefix=\" \" suffix=\": \"/>\n          <text macro=\"container-title\"/>\n          <text variable=\"edition\" suffix=\"., \"/>\n          <text variable=\"issue\" suffix=\", \"/>\n          <text variable=\"publisher-place\" suffix=\". \"/>\n          <text value=\"Anais\" font-style=\"italic\"/>\n          <text value=\"...\"/>\n          <group delimiter=\". \" prefix=\". \" suffix=\". \">\n            <text macro=\"event\"/>\n          </group>\n          <text variable=\"publisher-place\" prefix=\" \" suffix=\": \"/>\n          <text variable=\"publisher\" suffix=\", \"/>\n          <text macro=\"issued\"/>\n          <text macro=\"access\"/>\n          <text macro=\"locators\"/>\n        </else-if>\n        <else-if type=\"legislation\" match=\"any\"/>\n        <else-if type=\"legal_case\" match=\"any\"/>\n        <else-if type=\"patent\" match=\"any\"/>\n        <else>\n          <text macro=\"author\" suffix=\". \"/>\n          <text macro=\"title\"/>\n          <text macro=\"container-contributors\"/>\n          <text macro=\"secondary-contributors\"/>\n          <text macro=\"container-title\"/>\n          <text variable=\"collection-title\" prefix=\": \" suffix=\".\"/>\n          <text macro=\"locators\"/>\n          <group delimiter=\". \" prefix=\". \" suffix=\". \">\n            <text macro=\"event\"/>\n          </group>\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\" suffix=\", \"/>\n          <text macro=\"issued\" prefix=\", \" suffix=\". \"/>\n          <text macro=\"access\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"