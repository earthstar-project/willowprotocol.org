"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>Physiotherapy Theory and Practice</title>\n    <id>http://www.zotero.org/styles/physiotherapy-theory-and-practice</id>\n    <link href=\"http://www.zotero.org/styles/physiotherapy-theory-and-practice\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/mycologia\" rel=\"template\"/>\n    <link href=\"https://www.tandfonline.com/action/authorSubmission?show=instructions&amp;journalCode=iptp20\" rel=\"documentation\"/>\n    <link href=\"https://files.taylorandfrancis.com/ref_iptp.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0959-3985</issn>\n    <eissn>1532-5040</eissn>\n    <updated>2022-09-05T16:08:42+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"et-al\">et al</term>\n    </terms>\n  </locale>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <text variable=\"URL\"/>\n  </macro>\n  <citation et-al-min=\"5\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <group delimiter=\" \">\n          <text macro=\"author\"/>\n          <text macro=\"issued\"/>\n          <text variable=\"title\"/>\n        </group>\n        <choose>\n          <if type=\"article article-magazine article-newspaper article-journal review review-book\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" text-case=\"title\" strip-periods=\"true\"/>\n              <group delimiter=\": \">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <group delimiter=\": \">\n                    <text term=\"in\" text-case=\"capitalize-first\"/>\n                    <text macro=\"editor\"/>\n                  </group>\n                  <text variable=\"container-title\" text-case=\"title\" strip-periods=\"true\"/>\n                </group>\n                <group delimiter=\" \">\n                  <label strip-periods=\"false\" variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text variable=\"container-title\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"access\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"