"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" names-delimiter=\". \" name-as-sort-order=\"all\" sort-separator=\" \" demote-non-dropping-particle=\"never\" initialize-with=\" \" initialize-with-hyphen=\"false\" page-range-format=\"expanded\" default-locale=\"zh-CN\">\n  <info>\n    <title>China National Standard GB/T 7714-2005 (numeric, 中文)</title>\n    <id>http://www.zotero.org/styles/chinese-gb7714-2005-numeric</id>\n    <link href=\"http://www.zotero.org/styles/chinese-gb7714-2005-numeric\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/china-national-standard-gb-t-7714-2015-numeric\" rel=\"template\"/>\n    <link href=\"http://std.samr.gov.cn/gb/search/gbDetailed?id=71F772D78562D3A7E05397BE0A0AB82A\" rel=\"documentation\"/>\n    <author>\n      <name>heromyth</name>\n      <email>zxpmyth@yahoo.com.cn</email>\n    </author>\n    <contributor>\n      <name>Zeping Lee</name>\n      <email>zepinglee@gmail.com</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"generic-base\"/>\n    <summary>The Chinese GB/T 7714-2005 numeric style</summary>\n    <updated>2022-01-20T00:00:00+08:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"zh-CN\">\n    <date form=\"text\">\n      <date-part name=\"year\" suffix=\"年\" range-delimiter=\"&#8212;\"/>\n      <date-part name=\"month\" form=\"numeric\" suffix=\"月\" range-delimiter=\"&#8212;\"/>\n      <date-part name=\"day\" suffix=\"日\" range-delimiter=\"&#8212;\"/>\n    </date>\n    <terms>\n      <term name=\"edition\" form=\"short\">版</term>\n      <term name=\"open-quote\">“</term>\n      <term name=\"close-quote\">”</term>\n      <term name=\"open-inner-quote\">‘</term>\n      <term name=\"close-inner-quote\">’</term>\n    </terms>\n  </locale>\n  <locale>\n    <date form=\"numeric\">\n      <date-part name=\"year\" range-delimiter=\"/\"/>\n      <date-part name=\"month\" form=\"numeric-leading-zeros\" prefix=\"-\" range-delimiter=\"/\"/>\n      <date-part name=\"day\" form=\"numeric-leading-zeros\" prefix=\"-\" range-delimiter=\"/\"/>\n    </date>\n    <terms>\n      <term name=\"page-range-delimiter\">-</term>\n    </terms>\n  </locale>\n  <!-- 引用日期 -->\n  <macro name=\"accessed-date\">\n    <date variable=\"accessed\" form=\"numeric\" prefix=\"[\" suffix=\"]\"/>\n  </macro>\n  <!-- 主要责任者 -->\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name>\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <substitute>\n        <names variable=\"composer\"/>\n        <names variable=\"illustrator\"/>\n        <names variable=\"director\"/>\n        <choose>\n          <if variable=\"container-title\" match=\"none\">\n            <names variable=\"editor\"/>\n          </if>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <!-- 书籍的卷号（“第 x 卷”或“第 x 册”） -->\n  <macro name=\"book-volume\">\n    <choose>\n      <if type=\"article article-journal article-magazine article-newspaper periodical\" match=\"none\">\n        <choose>\n          <if is-numeric=\"volume\">\n            <group delimiter=\" \">\n              <label variable=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n              <text variable=\"volume\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"volume\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <!-- 专著主要责任者 -->\n  <macro name=\"container-author\">\n    <names variable=\"editor\">\n      <name>\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <substitute>\n        <names variable=\"editorial-director\"/>\n        <names variable=\"collection-editor\"/>\n        <names variable=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <!-- 专著题名 -->\n  <macro name=\"container-title\">\n    <group delimiter=\", \">\n      <group delimiter=\": \">\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\"/>\n          </if>\n          <else>\n            <text variable=\"event\"/>\n          </else>\n        </choose>\n        <text macro=\"book-volume\"/>\n      </group>\n      <choose>\n        <if variable=\"event-date\">\n          <date variable=\"event-date\" form=\"text\"/>\n          <text variable=\"event-place\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <!-- 版本项 -->\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <!-- 电子资源的更新或修改日期 -->\n  <macro name=\"issued-date\">\n    <date variable=\"issued\" form=\"numeric\"/>\n  </macro>\n  <!-- 出版年 -->\n  <macro name=\"issued-year\">\n    <choose>\n      <if is-uncertain-date=\"issued\">\n        <date variable=\"issued\" prefix=\"[\" suffix=\"]\">\n          <date-part name=\"year\" range-delimiter=\"-\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\" range-delimiter=\"-\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <!-- 专著的出版项 -->\n  <macro name=\"publishing\">\n    <group delimiter=\": \">\n      <group delimiter=\", \">\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n        <!-- 非电子资源显示“出版年” -->\n        <choose>\n          <if variable=\"publisher page\" type=\"book chapter paper-conference thesis\" match=\"any\">\n            <text macro=\"issued-year\"/>\n          </if>\n          <else-if variable=\"URL DOI\" match=\"none\">\n            <text macro=\"issued-year\"/>\n          </else-if>\n        </choose>\n      </group>\n      <text variable=\"page\"/>\n    </group>\n    <choose>\n      <!-- 纯电子资源显示“更新或修改日期” -->\n      <if variable=\"publisher page\" type=\"book chapter paper-conference thesis\" match=\"none\">\n        <choose>\n          <if variable=\"URL DOI\" match=\"any\">\n            <text macro=\"issued-date\" prefix=\"(\" suffix=\")\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n    <text macro=\"accessed-date\"/>\n  </macro>\n  <!-- 其他责任者 -->\n  <macro name=\"secondary-contributor\">\n    <names variable=\"translator\">\n      <name>\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <!-- 连续出版物中的析出文献的出处项（年、卷、期等信息） -->\n  <macro name=\"periodical-publishing\">\n    <group>\n      <group delimiter=\": \">\n        <group>\n          <group delimiter=\", \">\n            <text macro=\"container-title\" text-case=\"title\"/>\n            <choose>\n              <if type=\"article-newspaper\">\n                <text macro=\"issued-date\"/>\n              </if>\n              <else>\n                <text macro=\"issued-year\"/>\n              </else>\n            </choose>\n            <text variable=\"volume\"/>\n          </group>\n          <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n        </group>\n        <text variable=\"page\"/>\n      </group>\n      <text macro=\"accessed-date\"/>\n    </group>\n  </macro>\n  <!-- 题名 -->\n  <macro name=\"title\">\n    <group delimiter=\", \">\n      <group delimiter=\": \">\n        <text variable=\"title\"/>\n        <group delimiter=\"&#8195;\">\n          <choose>\n            <if variable=\"container-title\" type=\"paper-conference\" match=\"none\">\n              <text macro=\"book-volume\"/>\n            </if>\n          </choose>\n          <choose>\n            <if type=\"patent\">\n              <group delimiter=\", \">\n                <choose>\n                  <!-- 专利的国别应使用 `jurisdiction`，但 Zotero 没有对应的域，所以 `publisher-place` 作为备选 -->\n                  <if variable=\"jurisdiction\">\n                    <text variable=\"jurisdiction\"/>\n                  </if>\n                  <else>\n                    <text variable=\"publisher-place\"/>\n                  </else>\n                </choose>\n                <text variable=\"number\"/>\n              </group>\n            </if>\n            <else-if type=\"bill legal_case legislation regulation report standard\" match=\"any\">\n              <text variable=\"number\"/>\n            </else-if>\n          </choose>\n        </group>\n      </group>\n      <choose>\n        <if variable=\"container-title\" type=\"paper-conference\" match=\"none\">\n          <choose>\n            <if variable=\"event-date\">\n              <text variable=\"event-place\"/>\n              <date variable=\"event-date\" form=\"text\"/>\n            </if>\n          </choose>\n        </if>\n      </choose>\n    </group>\n    <text macro=\"type-code\" prefix=\"[\" suffix=\"]\"/>\n  </macro>\n  <!-- 文献类型标识 -->\n  <macro name=\"type-code\">\n    <group delimiter=\"/\">\n      <choose>\n        <if type=\"article\">\n          <choose>\n            <if variable=\"archive\">\n              <text value=\"A\"/>\n            </if>\n            <else>\n              <text value=\"M\"/>\n            </else>\n          </choose>\n        </if>\n        <else-if type=\"article-journal article-magazine periodical\" match=\"any\">\n          <text value=\"J\"/>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <text value=\"N\"/>\n        </else-if>\n        <else-if type=\"book chapter\" match=\"any\">\n          <text value=\"M\"/>\n        </else-if>\n        <else-if type=\"dataset\">\n          <text value=\"DB\"/>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <text value=\"C\"/>\n        </else-if>\n        <else-if type=\"patent\">\n          <text value=\"P\"/>\n        </else-if>\n        <else-if type=\"post post-weblog webpage\" match=\"any\">\n          <text value=\"EB\"/>\n        </else-if>\n        <else-if type=\"report\">\n          <text value=\"R\"/>\n        </else-if>\n        <else-if type=\"software\">\n          <text value=\"CP\"/>\n        </else-if>\n        <else-if type=\"standard\">\n          <text value=\"S\"/>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text value=\"D\"/>\n        </else-if>\n        <else-if variable=\"URL\">\n          <text value=\"EB\"/>\n        </else-if>\n        <else>\n          <text value=\"M\"/>\n        </else>\n      </choose>\n      <choose>\n        <if variable=\"URL DOI\" match=\"any\">\n          <text value=\"OL\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <!-- 获取和访问路径以及 DOI -->\n  <macro name=\"url-doi\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"URL\"/>\n      </if>\n      <else>\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </else>\n    </choose>\n  </macro>\n  <!-- 连续出版物的年卷期 -->\n  <macro name=\"year-volume-issue\">\n    <group>\n      <group delimiter=\", \">\n        <text macro=\"issued-year\"/>\n        <text variable=\"volume\"/>\n      </group>\n      <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n    </group>\n  </macro>\n  <!-- 专著和电子资源 -->\n  <macro name=\"monograph-layout\">\n    <group delimiter=\". \" suffix=\".\">\n      <text macro=\"author\"/>\n      <text macro=\"title\"/>\n      <text macro=\"secondary-contributor\"/>\n      <text macro=\"edition\"/>\n      <text macro=\"publishing\"/>\n      <text macro=\"url-doi\"/>\n    </group>\n  </macro>\n  <!-- 专著中的析出文献 -->\n  <macro name=\"chapter-in-book-layout\">\n    <group delimiter=\". \" suffix=\".\">\n      <text macro=\"author\"/>\n      <group delimiter=\"//\">\n        <group delimiter=\". \">\n          <text macro=\"title\"/>\n          <text macro=\"secondary-contributor\"/>\n        </group>\n        <group delimiter=\". \">\n          <text macro=\"container-author\"/>\n          <text macro=\"container-title\"/>\n        </group>\n      </group>\n      <text macro=\"edition\"/>\n      <text macro=\"publishing\"/>\n      <text macro=\"url-doi\"/>\n    </group>\n  </macro>\n  <!-- 连续出版物 -->\n  <macro name=\"serial-layout\">\n    <group delimiter=\". \" suffix=\".\">\n      <text macro=\"author\"/>\n      <text macro=\"title\"/>\n      <text macro=\"year-volume-issue\"/>\n      <text macro=\"publishing\"/>\n      <text variable=\"URL\"/>\n      <text variable=\"DOI\" prefix=\"DOI:\"/>\n    </group>\n  </macro>\n  <!-- 连续出版物中的析出文献 -->\n  <macro name=\"article-in-periodical-layout\">\n    <group delimiter=\". \" suffix=\".\">\n      <text macro=\"author\"/>\n      <text macro=\"title\"/>\n      <text macro=\"periodical-publishing\"/>\n      <text macro=\"url-doi\"/>\n    </group>\n  </macro>\n  <!-- 专利文献 -->\n  <macro name=\"patent-layout\">\n    <group delimiter=\". \" suffix=\".\">\n      <text macro=\"author\"/>\n      <text macro=\"title\"/>\n      <group>\n        <text macro=\"issued-date\"/>\n        <text macro=\"accessed-date\"/>\n      </group>\n      <text macro=\"url-doi\"/>\n    </group>\n  </macro>\n  <!-- 正文中引用的文献标注格式 -->\n  <macro name=\"citation-layout\">\n    <group>\n      <text variable=\"citation-number\"/>\n    </group>\n  </macro>\n  <!-- 参考文献表格式 -->\n  <macro name=\"entry-layout\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text macro=\"article-in-periodical-layout\"/>\n      </if>\n      <else-if type=\"periodical\">\n        <text macro=\"serial-layout\"/>\n      </else-if>\n      <else-if type=\"patent\">\n        <text macro=\"patent-layout\"/>\n      </else-if>\n      <else-if type=\"paper-conference\" variable=\"container-title\" match=\"any\">\n        <text macro=\"chapter-in-book-layout\"/>\n      </else-if>\n      <else>\n        <text macro=\"monograph-layout\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\" after-collapse-delimiter=\",\">\n    <layout vertical-align=\"sup\" delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text macro=\"citation-layout\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <!-- 取消这部分注释可以使用 CSL-M 的功能支持双语 -->\n    <!-- <layout locale=\"en\"><text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/><text macro=\"entry-layout\"/></layout> -->\n    <layout>\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"entry-layout\"/>\n    </layout>\n  </bibliography>\n</style>\n"