"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" default-locale=\"fr-FR\">\n  <info>\n    <title>Université de Lausanne - Institut d'archéologie et des sciences de l'antiquité (Français)</title>\n    <title-short>UNIL - ASA</title-short>\n    <id>http://www.zotero.org/styles/universite-de-lausanne-institut-d-archeologie-et-des-sciences-de-l-antiquite</id>\n    <link href=\"http://www.zotero.org/styles/universite-de-lausanne-institut-d-archeologie-et-des-sciences-de-l-antiquite\" rel=\"self\"/>\n    <link href=\"https://lausannecitationstyle.github.io/support/style-asa.html\" rel=\"documentation\"/>\n    <author>\n      <name>Leonie Nussbaum</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <updated>2023-09-25T03:03:01+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"anonymous\">anonyme</term>\n      <term name=\"anonymous\" form=\"short\">anon.</term>\n      <term name=\"no date\">sans date</term>\n      <term name=\"no date\" form=\"short\">s. d.</term>\n      <term name=\"sub-verbo\">s.v.</term>\n    </terms>\n  </locale>\n  <macro name=\"sort-type\">\n    <choose>\n      <if type=\"book\">\n        <choose>\n          <if variable=\"translator\">\n            <text value=\"1\"/>\n          </if>\n          <else>\n            <text value=\"2\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"article-journal\">\n        <choose>\n          <if variable=\"reviewed-author\">\n            <text value=\"6\"/>\n          </if>\n          <else>\n            <text value=\"3\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"chapter\">\n        <text value=\"4\"/>\n      </else-if>\n      <else-if type=\"entry-dictionary entry-encyclopedia\">\n        <text value=\"5\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <text macro=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"reviewed-author\">\n    <names variable=\"reviewed-author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <text macro=\"author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <text variable=\"version\" suffix=\" \"/>\n    <names variable=\"translator\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <text macro=\"author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <group delimiter=\" \">\n      <names variable=\"editor\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter-precedes-last=\"never\" delimiter=\", \" initialize-with=\". \"/>\n      </names>\n      <text variable=\"language\" prefix=\"(\" suffix=\".)\"/>\n    </group>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\" delimiter=\", \">\n      <name form=\"short\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"editor-short\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <name form=\"short\" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <date variable=\"original-date\" form=\"text\" prefix=\" (\" suffix=\"¹)\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"2\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout delimiter=\" \">\n      <group suffix=\".\">\n        <choose>\n          <if position=\"ibid-with-locator\">\n            <group delimiter=\", \">\n              <text term=\"ibid\" font-style=\"italic\"/>\n              <text variable=\"locator\" prefix=\"p. \"/>\n            </group>\n          </if>\n          <else-if position=\"ibid\">\n            <text term=\"ibid\" font-style=\"italic\"/>\n          </else-if>\n          <else>\n            <group delimiter=\" \" suffix=\".\">\n              <text macro=\"author-short\"/>\n              <text macro=\"year-date\" suffix=\", \"/>\n              <choose>\n                <if variable=\"URL\">\n                  <text variable=\"URL\"/>\n                </if>\n                <else>\n                  <text variable=\"locator\" prefix=\"p. \"/>\n                </else>\n              </choose>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"sort-type\"/>\n      <key macro=\"reviewed-author\"/>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"title\"/>\n      <key variable=\"page-first\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group>\n        <choose>\n          <if type=\"article-journal\" match=\"any\">\n            <choose>\n              <if variable=\"reviewed-author\">\n                <group delimiter=\", \" suffix=\", \">\n                  <text macro=\"reviewed-author\"/>\n                  <text macro=\"title\" font-style=\"italic\"/>\n                  <text variable=\"archive\"/>\n                  <text macro=\"author\" prefix=\"compte rendu par \"/>\n                </group>\n              </if>\n              <else>\n                <group>\n                  <text macro=\"author\" suffix=\", \"/>\n                  <text macro=\"title\" quotes=\"true\" suffix=\", \"/>\n                </group>\n              </else>\n            </choose>\n            <group delimiter=\", \" suffix=\".\">\n              <group delimiter=\" \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <choose>\n                <if variable=\"URL\">\n                  <group delimiter=\" \">\n                    <text variable=\"URL\"/>\n                    <date variable=\"accessed\" prefix=\"(consulté le \" suffix=\")\">\n                      <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                      <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                      <date-part name=\"year\" form=\"short\"/>\n                    </date>\n                  </group>\n                </if>\n                <else>\n                  <text variable=\"page\" prefix=\"p. \"/>\n                </else>\n              </choose>\n            </group>\n          </if>\n          <else-if type=\"book\" match=\"any\">\n            <choose>\n              <if variable=\"translator\">\n                <group delimiter=\", \" suffix=\", \">\n                  <text macro=\"author-short\"/>\n                  <text macro=\"title\"/>\n                  <text variable=\"volume\"/>\n                  <text macro=\"translator\"/>\n                  <text variable=\"collection-title\"/>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\", \" suffix=\", \">\n                  <text macro=\"author\"/>\n                  <text macro=\"title\"/>\n                  <text variable=\"publisher\"/>\n                </group>\n              </else>\n            </choose>\n            <group delimiter=\", \" suffix=\".\">\n              <text variable=\"publisher-place\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter\" match=\"any\">\n            <group delimiter=\", \" suffix=\".\">\n              <text macro=\"author\"/>\n              <text macro=\"title\" quotes=\"true\"/>\n              <text macro=\"editor\" prefix=\"in \"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n              <text macro=\"year-date\"/>\n              <choose>\n                <if variable=\"URL\">\n                  <group delimiter=\" \">\n                    <text variable=\"URL\"/>\n                    <date variable=\"accessed\" prefix=\"(consulté le \" suffix=\")\">\n                      <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                      <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                      <date-part name=\"year\" form=\"short\"/>\n                    </date>\n                  </group>\n                </if>\n                <else>\n                  <text variable=\"page\" prefix=\"p. \"/>\n                </else>\n              </choose>\n            </group>\n          </else-if>\n          <else-if type=\"entry-encyclopedia entry-dictionary\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"author\"/>\n              <group delimiter=\" \">\n                <text term=\"sub-verbo\" font-style=\"italic\"/>\n                <text macro=\"title\" quotes=\"true\"/>\n              </group>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"year-date\"/>\n              <choose>\n                <if variable=\"URL\">\n                  <group delimiter=\" \">\n                    <text variable=\"URL\"/>\n                    <date variable=\"accessed\" prefix=\"(consulté le \" suffix=\")\">\n                      <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                      <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                      <date-part name=\"year\" form=\"short\"/>\n                    </date>\n                  </group>\n                </if>\n                <else>\n                  <text variable=\"page\" prefix=\"p. \"/>\n                </else>\n              </choose>\n            </group>\n          </else-if>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"