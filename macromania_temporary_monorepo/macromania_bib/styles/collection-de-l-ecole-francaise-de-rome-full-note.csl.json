"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"note\" demote-non-dropping-particle=\"sort-only\" default-locale=\"fr-FR\" page-range-format=\"expanded\" initialize-with-hyphen=\"true\">\n  <info>\n    <title>Collection de l'Ecole française de Rome (full note, Français)</title>\n    <id>http://www.zotero.org/styles/collection-de-l-ecole-francaise-de-rome-full-note</id>\n    <link href=\"http://www.zotero.org/styles/collection-de-l-ecole-francaise-de-rome-full-note\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/revue-archeologique\" rel=\"template\"/>\n    <link href=\"http://www.publications.efrome.it/opencms/export/sites/efrome/documenti/norme_fr.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Nicolas Monteix</name>\n      <email>nicolas.monteix@univ-rouen.fr</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <category field=\"anthropology\"/>\n    <issn>0223-5099</issn>\n    <summary>Feuille de style pour la Collection de l'Ecole française de Rome. Les références bibliographiques sont intégralement insérées en notes sans généralisation de l'abréviation</summary>\n    <updated>2015-12-06T04:18:39+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"editor\" form=\"short\">dir.</term>\n      <term name=\"paragraph\">§</term>\n      <term name=\"in\">dans</term>\n      <term name=\"online\">mis en ligne le</term>\n      <term name=\"anonymous\">anonyme</term>\n      <term name=\"anonymous\" form=\"short\">anon.</term>\n      <term name=\"no date\">sans date</term>\n      <term name=\"no date\" form=\"short\">s.&#160;d.</term>\n      <term name=\"cited\">op. cit.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\", \">\n      <name sort-separator=\", \" delimiter=\", \" initialize-with=\".\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name sort-separator=\", \" delimiter=\", \" initialize-with=\".\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" text-case=\"lowercase\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\" delimiter=\", \">\n      <name form=\"short\" et-al-min=\"3\" et-al-use-first=\"1\" delimiter=\" - \" delimiter-precedes-last=\"always\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"editor-short\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <name form=\"short\" et-al-min=\"4\" et-al-use-first=\"1\" delimiter=\" - \" delimiter-precedes-last=\"always\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\"&#160;\">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text term=\"accessed\" suffix=\"&#160;\"/>\n      <date variable=\"accessed\" form=\"numeric\">\n        <date-part name=\"day\"/>\n        <date-part name=\"month\"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"locator\">\n    <group delimiter=\"&#160;\">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"locator-cit\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper entry-dictionary entry-encyclopedia chapter paper-conference\" match=\"any\">\n        <group delimiter=\"&#160;\" prefix=\"en partic. \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </if>\n      <else>\n        <text macro=\"locator\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"webpage\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"lowercase\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"thesis\">\n    <group prefix=\" \" delimiter=\", \">\n      <text macro=\"title\" font-style=\"italic\"/>\n      <text variable=\"genre\"/>\n      <text variable=\"publisher\"/>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"articles\">\n    <group delimiter=\", \">\n      <text macro=\"title\" quotes=\"true\"/>\n      <group delimiter=\" \">\n        <text term=\"in\"/>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </group>\n      <text variable=\"volume\"/>\n      <text variable=\"issue\"/>\n      <group delimiter=\" \">\n        <text macro=\"year-date\"/>\n        <group prefix=\"(\" suffix=\")\" delimiter=\", \">\n          <text variable=\"URL\"/>\n          <text macro=\"access\"/>\n        </group>\n      </group>\n      <text macro=\"pages\"/>\n    </group>\n  </macro>\n  <macro name=\"book\">\n    <group delimiter=\", \">\n      <text macro=\"title\"/>\n      <text macro=\"edition\"/>\n      <text macro=\"publisher\"/>\n      <group delimiter=\" \">\n        <text macro=\"year-date\"/>\n        <text macro=\"collection\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"dictionary\">\n    <group delimiter=\", \">\n      <text macro=\"title\" quotes=\"true\"/>\n      <group delimiter=\" \">\n        <text term=\"in\"/>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </group>\n      <group delimiter=\"&#160;\">\n        <label variable=\"volume\" form=\"short\"/>\n        <text variable=\"volume\"/>\n      </group>\n      <text macro=\"year-date\"/>\n      <text macro=\"pages\"/>\n    </group>\n  </macro>\n  <macro name=\"chapter\">\n    <group delimiter=\" \">\n      <text macro=\"title\" quotes=\"true\" suffix=\",\"/>\n      <text term=\"in\"/>\n      <text macro=\"editor\" suffix=\",\"/>\n      <text variable=\"container-title\" font-style=\"italic\" suffix=\",\"/>\n      <text macro=\"publisher\" suffix=\",\"/>\n      <group delimiter=\" \" suffix=\",\">\n        <text macro=\"year-date\"/>\n        <text macro=\"collection\"/>\n      </group>\n      <text macro=\"pages\"/>\n    </group>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\">\n    <layout delimiter=\"&#160;; \">\n      <choose>\n        <if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\" form=\"short\" font-style=\"italic\" suffix=\"…\"/>\n            <group delimiter=\" \">\n              <text term=\"cited\" font-style=\"italic\"/>\n              <group delimiter=\"&#160;\">\n                <text term=\"note\" form=\"short\"/>\n                <text variable=\"first-reference-note-number\"/>\n              </group>\n            </group>\n            <text macro=\"locator\"/>\n          </group>\n        </if>\n        <else>\n          <text macro=\"author\" suffix=\", \"/>\n          <choose>\n            <if type=\"thesis\">\n              <text macro=\"thesis\"/>\n            </if>\n            <else-if type=\"webpage article-journal article-magazine article-newspaper broadcast personal_communication\" match=\"any\">\n              <text macro=\"articles\"/>\n            </else-if>\n            <else-if type=\"book graphic\" match=\"any\">\n              <text macro=\"book\"/>\n            </else-if>\n            <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n              <text macro=\"dictionary\"/>\n            </else-if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <text macro=\"chapter\"/>\n            </else-if>\n          </choose>\n          <text macro=\"locator-cit\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"page-first\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group display=\"block\">\n        <choose>\n          <if variable=\"author\" match=\"any\">\n            <group delimiter=\" \" suffix=\" = \">\n              <text macro=\"author-short\"/>\n              <text macro=\"year-date\"/>\n              <text variable=\"year-suffix\"/>\n            </group>\n          </if>\n          <else-if variable=\"editor\" match=\"any\">\n            <group delimiter=\" \" suffix=\" = \">\n              <text macro=\"editor-short\"/>\n              <text macro=\"year-date\"/>\n              <text variable=\"year-suffix\"/>\n            </group>\n          </else-if>\n          <else>\n            <text variable=\"title-short\"/>\n            <text macro=\"year-date\" prefix=\"&#160;\" suffix=\"&#160;= \"/>\n          </else>\n        </choose>\n        <text macro=\"author\" suffix=\", \"/>\n        <choose>\n          <if type=\"thesis\">\n            <text macro=\"thesis\"/>\n            <group delimiter=\"&#160;\">\n              <text variable=\"number-of-pages\"/>\n              <text term=\"page\" form=\"short\"/>\n            </group>\n          </if>\n          <else-if type=\"webpage article-journal article-magazine article-newspaper broadcast personal_communication\" match=\"any\">\n            <text macro=\"articles\"/>\n          </else-if>\n          <else-if type=\"book graphic\" match=\"any\">\n            <text macro=\"book\"/>\n          </else-if>\n          <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n            <text macro=\"dictionary\"/>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text macro=\"chapter\"/>\n          </else-if>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"