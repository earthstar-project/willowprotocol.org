"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Journal of Plankton Research</title>\n    <title-short>JPR</title-short>\n    <id>http://www.zotero.org/styles/journal-of-plankton-research</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-plankton-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/toxicological-sciences\" rel=\"template\"/>\n    <link href=\"https://academic.oup.com/plankt/pages/ms_prep_submission#references\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0142-7873</issn>\n    <eissn>1464-3774</eissn>\n    <updated>2022-08-03T14:00:21+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <et-al font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"9\" et-al-use-first=\"8\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\" prefix=\" \"/>\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"title\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"