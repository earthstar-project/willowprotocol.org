"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-US\">\n  <info>\n    <title>Stem Cell Reports</title>\n    <id>http://www.zotero.org/styles/stem-cell-reports</id>\n    <link href=\"http://www.zotero.org/styles/stem-cell-reports\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/cell\" rel=\"template\"/>\n    <link href=\"http://www.cell.com/stem-cell-reports/authors\" rel=\"documentation\"/>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"medicine\"/>\n    <eissn>2213-6711</eissn>\n    <updated>2016-12-13T12:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" initialize-with=\".\" and=\"text\" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group prefix=\"(\" delimiter=\": \" suffix=\")\">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\" and=\"text\" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"doi\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else-if type=\"webpage post post-weblog\" match=\"any\">\n        <text variable=\"URL\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author-short\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author\" names-min=\"3\" names-use-first=\"1\"/>\n      <key macro=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\">\n    <sort>\n      <key macro=\"author-short\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author\" names-min=\"3\" names-use-first=\"1\"/>\n      <key macro=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"issued\" prefix=\"(\" suffix=\").\"/>\n        <choose>\n          <if type=\"article article-magazine article-newspaper article-journal review\" match=\"any\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\"/>\n            <group delimiter=\", \" suffix=\". \">\n              <text variable=\"volume\" font-style=\"italic\"/>\n              <text variable=\"page\"/>\n            </group>\n            <group>\n              <text macro=\"doi\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" prefix=\"In \" suffix=\",\" text-case=\"title\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\" suffix=\",\"/>\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </else-if>\n          <else-if type=\"thesis\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"genre\" suffix=\".\"/>\n            <text variable=\"publisher\"/>\n          </else-if>\n          <else>\n            <text variable=\"title\"/>\n            <text macro=\"publisher\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"