"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" delimiter-precedes-et-al=\"always\" delimiter-precedes-last=\"never\" et-al-min=\"7\" et-al-use-first=\"6\" page-range-format=\"minimal\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Journal of Surgery and Medicine</title>\n    <title-short>JOSAM</title-short>\n    <id>http://www.zotero.org/styles/journal-of-surgery-and-medicine</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-surgery-and-medicine\" rel=\"self\"/>\n    <link href=\"http://dergipark.gov.tr/josam/page/3139\" rel=\"documentation\"/>\n    <author>\n      <name>Hakan Kocoglu</name>\n      <email>hakan@pooltext.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <eissn>2602-2079</eissn>\n    <summary>Provided by PoolText</summary>\n    <updated>2018-04-18T18:25:42+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <date form=\"text\" delimiter=\" \">\n      <date-part name=\"day\"/>\n      <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"collection-editor\" form=\"long\">\n        <single>Series editor</single>\n        <multiple>Series editors</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-et-al=\"always\" delimiter-precedes-last=\"never\" et-al-min=\"7\" et-al-use-first=\"6\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\".\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\"; \">\n      <group delimiter=\": \">\n        <choose>\n          <if type=\"thesis\" match=\"none\">\n            <text variable=\"publisher-place\"/>\n          </if>\n        </choose>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\" suffix=\".\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"doi:\"/>\n          </if>\n          <else>\n            <group delimiter=\". \">\n              <text variable=\"URL\"/>\n              <group delimiter=\" \">\n                <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                <date variable=\"accessed\" form=\"text\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group>\n        <group delimiter=\". \">\n          <text macro=\"author\"/>\n          <text variable=\"title\"/>\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n              <group delimiter=\". \" prefix=\" \">\n                <text macro=\"edition\"/>\n                <text variable=\"genre\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\". \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                  <group delimiter=\". \">\n                    <text macro=\"editor\"/>\n                    <text macro=\"container-title\"/>\n                  </group>\n                </group>\n                <text macro=\"edition\"/>\n                <text macro=\"publisher\"/>\n                <group delimiter=\" \">\n                  <label variable=\"page\" form=\"short\" plural=\"never\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <text macro=\"container-title\"/>\n              <group delimiter=\";\">\n                <text macro=\"year-date\"/>\n                <group>\n                  <text variable=\"volume\"/>\n                  <choose>\n                    <if is-numeric=\"issue\"/>\n                    <else>\n                      <text variable=\"issue\" prefix=\" \"/>\n                    </else>\n                  </choose>\n                  <text variable=\"page\" prefix=\":\"/>\n                </group>\n              </group>\n            </else>\n          </choose>\n          <text macro=\"access\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"