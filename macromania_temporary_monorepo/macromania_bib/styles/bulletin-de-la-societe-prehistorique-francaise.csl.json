"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"fr-FR\">\n  <info>\n    <title>Bulletin de la Société préhistorique française (Français)</title>\n    <title-short>BSPF</title-short>\n    <id>http://www.zotero.org/styles/bulletin-de-la-societe-prehistorique-francaise</id>\n    <link href=\"http://www.zotero.org/styles/bulletin-de-la-societe-prehistorique-francaise\" rel=\"self\"/>\n    <link href=\"http://www.frantiq.fr/\" rel=\"documentation\"/>\n    <author>\n      <name>Benedicte Macgregor</name>\n      <email>benedicte.macgregor@mae.u-paris10.fr</email>\n    </author>\n    <contributor>\n      <name>Dominique Commelin</name>\n      <email>commelin@mmsh.univ-aix.fr</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"anthropology\"/>\n    <issn>0037-9514</issn>\n    <eissn>1760-7361</eissn>\n    <summary>Style élaboré à partir du style ehess de F Heimburger</summary>\n    <updated>2018-05-22T00:59:23+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"editor\" form=\"short\">(dir.)</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\" \">\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"none\">\n        <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\".\" delimiter=\", \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\" \" delimiter-precedes-last=\"never\" initialize-with=\". \">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n      </name>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text value=\"&#160;\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <text variable=\"URL\"/>\n        <group prefix=\" [\" suffix=\"]\">\n          <text value=\"accédé le\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"&#160;; \">\n      <group delimiter=\"&#160;; \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"100\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\" \">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\" prefix=\"(\" suffix=\")&#160;‒\"/>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text macro=\"title\"/>\n            </group>\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\" text-case=\"lowercase\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"edition\"/>\n          </group>\n          <text macro=\"publisher\"/>\n          <group delimiter=\" \" prefix=\" (\" suffix=\")\">\n            <text variable=\"collection-title\"/>\n            <text variable=\"collection-number\"/>\n          </group>\n          <text variable=\"number-of-pages\" prefix=\", \" suffix=\"&#160;p.\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\",\"/>\n          <group prefix=\" \" suffix=\".\">\n            <text value=\"in\" font-style=\"italic\" suffix=\" \"/>\n            <text macro=\"editor\" suffix=\", \"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\", \"/>\n            <group prefix=\" \" suffix=\"\">\n              <text macro=\"publisher\"/>\n              <group prefix=\" (\" suffix=\"), \">\n                <group delimiter=\" \">\n                  <text variable=\"collection-title\"/>\n                  <text variable=\"collection-number\"/>\n                </group>\n              </group>\n              <text macro=\"pages\" prefix=\" &#160;\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\" \" delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n            <group>\n              <text variable=\"number-of-pages\" suffix=\"&#160;p.\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\",\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group prefix=\", \">\n              <text variable=\"volume\" prefix=\" \"/>\n              <text variable=\"issue\" prefix=\", \"/>\n            </group>\n            <group prefix=\", \">\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"page\" prefix=\"&#160;\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"