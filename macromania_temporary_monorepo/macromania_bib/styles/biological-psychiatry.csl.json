"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Biological Psychiatry</title>\n    <id>http://www.zotero.org/styles/biological-psychiatry</id>\n    <link href=\"http://www.zotero.org/styles/biological-psychiatry\" rel=\"self\"/>\n    <link href=\"http://www.elsevier.com/journals/biological-psychiatry/0006-3223/guide-for-authors\" rel=\"documentation\"/>\n    <author>\n      <name>Simon Kornblith</name>\n      <email>simon@simonster.com</email>\n    </author>\n    <author>\n      <name>Brenton M. Wiernik</name>\n      <email>zotero@wiernik.org</email>\n    </author>\n    <contributor>\n      <name>Bruce D'Arcus</name>\n    </contributor>\n    <contributor>\n      <name>Curtis M. Humphrey</name>\n    </contributor>\n    <contributor>\n      <name>Richard Karnesky</name>\n      <email>karnesky+zotero@gmail.com</email>\n      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"psychology\"/>\n    <category field=\"medicine\"/>\n    <issn>0006-3223</issn>\n    <eissn>1873-2402</eissn>\n    <updated>2015-03-18T20:24:20+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter entry-encyclopedia entry-dictionary paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \" prefix=\"(\" suffix=\")\">\n          <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n          <label prefix=\", \"/>\n          <et-al font-style=\"italic\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter entry-encyclopedia entry-dictionary paper-conference\" match=\"any\">\n        <group delimiter=\": \">\n          <text term=\"in\" text-case=\"capitalize-first\"/>\n          <names variable=\"editor translator\">\n            <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n            <label form=\"long\" prefix=\", \"/>\n            <et-al font-style=\"italic\"/>\n            <substitute>\n              <names variable=\"container-author\"/>\n            </substitute>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter-precedes-last=\"always\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          </if>\n          <else>\n            <choose>\n              <if variable=\"URL\">\n                <group delimiter=\" \">\n                  <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n                  <date variable=\"accessed\" suffix=\",\" delimiter=\" \">\n                    <date-part name=\"month\"/>\n                    <date-part name=\"day\" suffix=\",\"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                  <group delimiter=\" \">\n                    <text term=\"from\"/>\n                    <text variable=\"URL\"/>\n                  </group>\n                </group>\n              </if>\n            </choose>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n          <choose>\n            <if variable=\"container-title\" match=\"none\">\n              <group delimiter=\" \">\n                <group delimiter=\", \">\n                  <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n                  <text macro=\"edition\"/>\n                  <text macro=\"volume\"/>\n                </group>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\" \">\n                <text variable=\"title\"/>\n                <text macro=\"description\"/>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <else-if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"title\"/>\n              <text macro=\"description\"/>\n            </group>\n            <text macro=\"edition\"/>\n          </group>\n        </else-if>\n        <else>\n          <choose>\n            <if variable=\"container-title\" match=\"none\">\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <text variable=\"title\"/>\n                  <text macro=\"description\"/>\n                </group>\n                <text macro=\"edition\"/>\n                <text macro=\"volume\"/>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\" \">\n                <text variable=\"title\"/>\n                <text macro=\"description\"/>\n              </group>\n            </else>\n          </choose>\n        </else>\n      </choose>\n      <text macro=\"secondary-contributors\" prefix=\"(\" suffix=\")\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"event\">\n    <group delimiter=\" \">\n      <text term=\"presented at\"/>\n      <text variable=\"event\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <group>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <choose>\n            <if type=\"article-journal bill book chapter entry-encyclopedia entry-dictionary graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part prefix=\", \" name=\"month\"/>\n                <date-part prefix=\" \" name=\"day\"/>\n              </date>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if variable=\"edition\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <text term=\"version\"/>\n          <text variable=\"version\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if variable=\"volume\">\n        <group delimiter=\" \">\n          <text term=\"volume\" form=\"short\"/>\n          <number variable=\"volume\" form=\"numeric\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <text term=\"volume\" form=\"short\" plural=\"true\"/>\n          <group>\n            <text term=\"page-range-delimiter\" prefix=\"1\"/>\n            <number variable=\"number-of-volumes\" form=\"numeric\"/>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\" form=\"short\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"container-title\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"volume\"/>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\" \">\n          <text macro=\"container-title\"/>\n          <group delimiter=\": \" prefix=\" \">\n            <text variable=\"volume\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"paper-conference\">\n        <choose>\n          <if variable=\"editor translator\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"container-contributors\"/>\n              <text macro=\"container-title\"/>\n              <group delimiter=\", \" prefix=\" \">\n                <text macro=\"publisher\"/>\n                <group delimiter=\" \">\n                  <label strip-periods=\"true\" variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </if>\n          <else-if variable=\"volume page\" match=\"any\">\n            <group delimiter=\" \">\n              <text macro=\"container-title\"/>\n              <group delimiter=\": \" prefix=\" \">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"container-contributors\"/>\n              <text macro=\"container-title\"/>\n              <group delimiter=\", \">\n                <text macro=\"event\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <group delimiter=\". \">\n          <text macro=\"container-contributors\"/>\n          <text macro=\"container-title\"/>\n          <group delimiter=\", \">\n            <text macro=\"event\"/>\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <label strip-periods=\"true\" variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"description\">\n    <group prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <choose>\n        <if variable=\"page\" match=\"none\">\n          <text variable=\"status\"/>\n        </if>\n      </choose>\n      <text macro=\"genre\"/>\n      <text variable=\"medium\"/>\n      <choose>\n        <if variable=\"genre medium\" match=\"none\">\n          <choose>\n            <if type=\"dataset\">\n              <text value=\"dataset\"/>\n            </if>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"genre\">\n    <group delimiter=\" \">\n      <text variable=\"genre\"/>\n      <group delimiter=\" \">\n        <text term=\"issue\" form=\"short\"/>\n        <text variable=\"number\"/>\n      </group>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"8\" et-al-use-first=\"6\" entry-spacing=\"0\" line-spacing=\"2\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \">\n        <text variable=\"citation-number\" suffix=\".\"/>\n        <text macro=\"author\"/>\n        <text macro=\"issued\" prefix=\"(\" suffix=\"):\"/>\n        <group delimiter=\". \" suffix=\".\">\n          <text macro=\"title\"/>\n          <text macro=\"container\"/>\n        </group>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"