"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Seed Science and Technology</title>\n    <id>http://www.zotero.org/styles/seed-science-and-technology</id>\n    <link href=\"http://www.zotero.org/styles/seed-science-and-technology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/biometrics\" rel=\"template\"/>\n    <link href=\"https://www.seedtest.org/en/instructions-to-contributors-_content---1--1089.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>citationstyler@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <issn>0251-0952</issn>\n    <eissn>1819-5717</eissn>\n    <updated>2019-08-20T07:57:02+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <label form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n      <name and=\"text\" initialize-with=\"\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"genre\"/>\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\" suffix=\".\"/>\n      <date variable=\"issued\" prefix=\" (\" suffix=\") \">\n        <date-part name=\"year\"/>\n      </date>\n      <text macro=\"title\" suffix=\".\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"edition\" prefix=\", \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \" suffix=\".\">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n                <names variable=\"editor translator\" prefix=\"(\" suffix=\")\">\n                  <label form=\"short\" strip-periods=\"false\" suffix=\" \"/>\n                  <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n                </names>\n              </group>\n              <text macro=\"edition\" prefix=\", \"/>\n              <text macro=\"pages\"/>\n              <text macro=\"publisher\" prefix=\" \"/>\n            </group>\n            <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"publisher\"/>\n            <group prefix=\"URL \">\n              <text variable=\"URL\" suffix=\" \"/>\n              <date variable=\"accessed\" prefix=\"[accessed \" suffix=\"]\" delimiter=\" \">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"editor-translator\"/>\n          </group>\n          <group delimiter=\", \" prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"volume\" font-weight=\"bold\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"