"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" default-locale=\"en-US\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <!-- This style was edited with the Visual CSL Editor (http://steveridout.com/csl/visualEditor/) -->\n  <info>\n    <title>Acta Neurochirurgica</title>\n    <id>http://www.zotero.org/styles/acta-neurochirurgica</id>\n    <link href=\"http://www.zotero.org/styles/acta-neurochirurgica\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/springer-vancouver\" rel=\"template\"/>\n    <link href=\"http://www.springer.com/medicine/surgery/journal/701\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0001-6268</issn>\n    <eissn>0942-0940</eissn>\n    <summary>Springer Vancouver style - alphabetical</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"et-al\">et al</term>\n    </terms>\n  </locale>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\", \" suffix=\". \">\n          <text variable=\"title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\" suffix=\". \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-parenth\">\n    <date prefix=\"(\" suffix=\")\" variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year\"/>\n    </sort>\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"year-parenth\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if variable=\"page\">\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <text macro=\"editor\"/>\n              <group prefix=\" \" delimiter=\", \" suffix=\". \">\n                <text variable=\"container-title\" form=\"short\"/>\n                <text macro=\"edition\"/>\n              </group>\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \" prefix=\", \">\n                <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n                <text variable=\"page\"/>\n              </group>\n            </if>\n            <else>\n              <group>\n                <text variable=\"container-title\" suffix=\" \" form=\"short\" strip-periods=\"true\"/>\n                <group suffix=\":\">\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <else-if variable=\"DOI\">\n          <group delimiter=\". \">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n            <text prefix=\"doi: \" variable=\"DOI\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"publisher\"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group>\n            <text prefix=\"In: \" variable=\"container-title\" suffix=\". \" form=\"short\"/>\n            <text variable=\"URL\" suffix=\". \"/>\n            <date variable=\"accessed\">\n              <date-part prefix=\"Accessed \" name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group>\n            <text variable=\"container-title\" form=\"short\" suffix=\" \"/>\n            <text variable=\"volume\" suffix=\":\"/>\n            <text variable=\"page\" suffix=\".\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"