"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"de-DE\">\n  <info>\n    <title>EVA Berlin Konferenz (Deutsch)</title>\n    <id>http://www.zotero.org/styles/eva-berlin-konferenz</id>\n    <link href=\"http://www.zotero.org/styles/eva-berlin-konferenz\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/iso690-numeric-en\" rel=\"template\"/>\n    <link href=\"https://www.eva-berlin.de/en/participation/call-for-papers/\" rel=\"documentation\"/>\n    <author>\n      <name>Thomas Tunsch</name>\n      <email>thtbln@yahoo.de</email>\n      <uri>https://thtbln.solid.community/profile/card#me</uri>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"humanities\"/>\n    <category field=\"engineering\"/>\n    <summary>Style based on ISO 690:2010(E), V1.1; adapted for EVA Berlin conference publication. Citation style documentation from Word template</summary>\n    <updated>2018-10-08T17:31:56+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"no date\">[o.J.]</term>\n      <term name=\"in\">in</term>\n      <term name=\"online\">online</term>\n      <term name=\"accessed\">Online im Internet</term>\n      <term name=\"retrieved\">URL</term>\n      <term name=\"from\"/>\n      <term name=\"and\">und</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\"; \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\" \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"responsability\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <choose>\n          <if variable=\"author\">\n            <text macro=\"author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n          <else>\n            <text macro=\"translator\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"container-responsability\">\n    <choose>\n      <if variable=\"container-author editor translator\" match=\"any\">\n        <choose>\n          <if variable=\"container-author\">\n            <text macro=\"container-author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n          <else>\n            <text macro=\"translator\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis map motion_picture song manuscript\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"paper-conference speech chapter article-journal article-magazine article-newspaper entry entry-dictionary entry-encyclopedia post-weblog post webpage broadcast\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n        <choose>\n          <if type=\"chapter paper-conference entry-encyclopedia\" match=\"any\">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \" font-style=\"normal\"/>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"container-author editor translator\" match=\"any\">\n            <text macro=\"container-responsability\"/>\n            <choose>\n              <if variable=\"container-title event\" match=\"any\">\n                <text value=\", \"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\" font-style=\"normal\"/>\n          </if>\n          <else>\n            <text variable=\"event\" font-style=\"normal\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"report\">\n        <text variable=\"number\" suffix=\": \"/>\n        <text variable=\"title\" font-style=\"normal\"/>\n      </else-if>\n      <else-if type=\"patent\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n    <choose>\n      <if variable=\"URL\">\n        <text term=\"online\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <text variable=\"number\"/>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n  </macro>\n  <macro name=\"version\">\n    <text variable=\"version\"/>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"map\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n          <else>\n            <text value=\"map\" prefix=\"[\" suffix=\"]\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"genre\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\".\"/>\n          <date-part name=\"month\" form=\"numeric\" suffix=\".\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <text variable=\"edition\" form=\"long\"/>\n  </macro>\n  <macro name=\"publisher-group\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\", \">\n      <text variable=\"volume\" prefix=\"Vol. \"/>\n      <choose>\n        <if variable=\"volume\">\n          <text variable=\"issue\" prefix=\"no. \"/>\n          <text variable=\"page\" prefix=\"p. \"/>\n        </if>\n        <else-if variable=\"issue\">\n          <text variable=\"issue\" prefix=\"No. \"/>\n          <text variable=\"page\" prefix=\"p. \"/>\n        </else-if>\n        <else>\n          <text variable=\"page\" prefix=\"P. \"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"\" suffix=\":\">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\", \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"book thesis manuscript\" match=\"any\">\n        <text variable=\"number-of-pages\" suffix=\" p\"/>\n      </if>\n      <else-if type=\"chapter paper-conference article-newspaper\" match=\"any\">\n        <text variable=\"page\" prefix=\"p. \"/>\n      </else-if>\n      <else-if type=\"report patent\" match=\"any\">\n        <text variable=\"page\" suffix=\" p\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"isbn\">\n    <text variable=\"ISBN\" prefix=\"ISBN \"/>\n  </macro>\n  <macro name=\"doi\">\n    <text variable=\"DOI\" prefix=\"DOI \"/>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"URL\" suffix=\" \"/>\n        <group>\n          <text term=\"from\" suffix=\": \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" prefix=\"(\"/>\n            <date-part name=\"month\" form=\"numeric\" prefix=\".\"/>\n            <date-part name=\"year\" prefix=\".\" suffix=\"). \"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\": \">\n      <text variable=\"archive\"/>\n      <text macro=\"archive_location\"/>\n    </group>\n  </macro>\n  <macro name=\"archive_location\">\n    <choose>\n      <if variable=\"archive_location\">\n        <text variable=\"archive_location\"/>\n      </if>\n      <else>\n        <text variable=\"call-number\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"abstract\">\n    <text variable=\"abstract\"/>\n  </macro>\n  <macro name=\"note\">\n    <text variable=\"note\"/>\n  </macro>\n  <citation collapse=\"citation-number\" after-collapse-delimiter=\"; \">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <group delimiter=\", \">\n        <text variable=\"citation-number\"/>\n        <group>\n          <label variable=\"locator\" suffix=\". \" form=\"short\" strip-periods=\"true\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout>\n      <text variable=\"citation-number\" display=\"left-margin\" prefix=\"[\" suffix=\"] \"/>\n      <choose>\n        <if type=\"book map\" match=\"any\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\": \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text macro=\"genre\" suffix=\", \"/>\n            <text macro=\"edition\" suffix=\", \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\", \"/>\n            <text macro=\"collection\" suffix=\", \"/>\n            <text macro=\"accessed\" suffix=\" \"/>\n            <text macro=\"isbn\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </if>\n        <else-if type=\"article-journal article-magazine\" match=\"any\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\": \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text macro=\"edition\" suffix=\", \"/>\n            <text macro=\"date\" suffix=\", \"/>\n            <text macro=\"issue\" suffix=\", \"/>\n            <text macro=\"accessed\" suffix=\" \"/>\n            <text macro=\"doi\" suffix=\", \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\": \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text macro=\"edition\" suffix=\", \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"date\" suffix=\", \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter entry entry-dictionary entry-encyclopedia\" match=\"any\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\": \"/>\n            <text macro=\"title\" font-style=\"normal\" suffix=\", \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"year-date\" prefix=\"\" suffix=\", \"/>\n            <text macro=\"page\" suffix=\", \"/>\n            <text macro=\"collection\" suffix=\", \"/>\n            <text macro=\"isbn\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\": \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text macro=\"genre\" suffix=\", \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"date\" suffix=\", \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\": \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"date\" suffix=\", \"/>\n            <text macro=\"isbn\" suffix=\", \"/>\n            <text macro=\"page\" suffix=\", \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\": \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text macro=\"genre\" suffix=\", \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\", \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"post-weblog post webpage\" match=\"any\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\": \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text macro=\"date\" suffix=\", \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"broadcast motion_picture song\" match=\"any\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"medium\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"isbn\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\" match=\"any\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"manuscript\" match=\"any\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\", \"/>\n            <text macro=\"year-date\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"number\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\"/>\n            <text macro=\"archive\"/>\n            <text macro=\"abstract\"/>\n            <text macro=\"note\"/>\n          </group>\n        </else-if>\n        <else>\n          <group display=\"right-inline\">\n            <text macro=\"responsability\" suffix=\": \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text macro=\"version\" suffix=\", \"/>\n            <text macro=\"medium\" suffix=\", \"/>\n            <text macro=\"genre\" suffix=\", \"/>\n            <text macro=\"date\" suffix=\", \"/>\n            <text macro=\"edition\" suffix=\", \"/>\n            <text macro=\"publisher-group\" suffix=\". \"/>\n            <text macro=\"number\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"isbn\" suffix=\". \"/>\n            <text macro=\"url\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"