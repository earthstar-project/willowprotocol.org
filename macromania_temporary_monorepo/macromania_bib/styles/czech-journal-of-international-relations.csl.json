"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"expanded\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Czech Journal of International Relations</title>\n    <title-short>CJIR</title-short>\n    <id>http://www.zotero.org/styles/czech-journal-of-international-relations</id>\n    <link href=\"http://www.zotero.org/styles/czech-journal-of-international-relations\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/trabajos-de-prehistoria\" rel=\"template\"/>\n    <link href=\"https://cjir.iir.cz/index.php/cjir/guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"history\"/>\n    <issn>0323-1844</issn>\n    <eissn>2570-9429</eissn>\n    <updated>2023-03-25T13:10:04+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"es\">\n    <terms>\n      <term name=\"open-quote\">“</term>\n      <term name=\"close-quote\">”</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name delimiter=\" &#8211; \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\" &#8211; \" delimiter-precedes-last=\"never\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\" &#8211; \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog post\" match=\"any\">\n        <group delimiter=\" \" prefix=\", \">\n          <text term=\"available at\" text-case=\"capitalize-first\"/>\n          <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n          <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n      <else>\n        <text variable=\"DOI\" prefix=\", &lt;https://doi.org/\" suffix=\"&gt;\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\" text-case=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" et-al-use-last=\"true\" line-spacing=\"2\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\" \"/>\n      <choose>\n        <if match=\"any\" variable=\"author\">\n          <text macro=\"year-date\" prefix=\"(\" suffix=\"): \"/>\n        </if>\n        <else-if match=\"any\" variable=\"editor\">\n          <date date-parts=\"year\" form=\"text\" variable=\"issued\" suffix=\")\"/>\n        </else-if>\n        <else>\n          <text macro=\"year-date\" prefix=\"(\" suffix=\"): \"/>\n        </else>\n      </choose>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n          <choose>\n            <if match=\"any\" variable=\"collection-title\">\n              <group suffix=\",\">\n                <text macro=\"title\" font-style=\"italic\"/>\n                <text macro=\"edition\" prefix=\". \"/>\n                <text macro=\"editor\" prefix=\". \"/>\n                <text variable=\"collection-title\" prefix=\". \"/>\n                <text variable=\"collection-number\" prefix=\" \"/>\n              </group>\n            </if>\n            <else>\n              <group prefix=\" \" suffix=\".\">\n                <text macro=\"title\" font-style=\"italic\"/>\n                <text macro=\"edition\" prefix=\". \"/>\n                <text macro=\"editor\" prefix=\". \"/>\n                <text variable=\"collection-title\" prefix=\". \"/>\n                <text variable=\"collection-number\" prefix=\" \"/>\n              </group>\n            </else>\n          </choose>\n          <text macro=\"publisher\" prefix=\" \"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\" text-case=\"title\" quotes=\"false\" prefix=\" \"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\" suffix=\":\"/>\n              <text variable=\"container-title\" text-case=\"title\" quotes=\"false\" font-style=\"italic\"/>\n            </group>\n            <group delimiter=\", \" prefix=\" \">\n              <text variable=\"collection-title\" prefix=\" \" suffix=\" \"/>\n              <text variable=\"collection-number\" suffix=\".\"/>\n              <text variable=\"event\" suffix=\". \"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\" \" suffix=\".\" delimiter=\" \">\n            <text macro=\"title\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"genre\" text-case=\"capitalize-first\" suffix=\",\"/>\n            <text variable=\"publisher\" suffix=\".\"/>\n            <text variable=\"publisher-place\" suffix=\".\"/>\n            <text variable=\"URL\"/>\n            <group>\n              <text value=\"consulta\" prefix=\"(\"/>\n              <date form=\"numeric\" variable=\"accessed\" prefix=\" \" suffix=\")\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\". \" prefix=\" \" suffix=\".\">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <choose>\n                <if match=\"any\" variable=\"collection-title\">\n                  <text variable=\"collection-title\"/>\n                </if>\n                <else>\n                  <text variable=\"genre\"/>\n                </else>\n              </choose>\n              <choose>\n                <if match=\"any\" is-numeric=\"number\">\n                  <group delimiter=\" \">\n                    <text value=\"No.\"/>\n                    <text variable=\"number\"/>\n                  </group>\n                </if>\n                <else>\n                  <text variable=\"number\"/>\n                </else>\n              </choose>\n            </group>\n            <group delimiter=\", \">\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-magazine article-newspaper\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <date variable=\"issued\">\n                <date-part name=\"day\" suffix=\". \"/>\n                <date-part name=\"month\" form=\"numeric\" suffix=\". \"/>\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"title\" quotes=\"false\" prefix=\" \"/>\n            <group delimiter=\", \" prefix=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group delimiter=\" \">\n                <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <group delimiter=\" \">\n                <label text-case=\"capitalize-first\" variable=\"issue\" form=\"short\"/>\n                <text variable=\"issue\"/>\n              </group>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"