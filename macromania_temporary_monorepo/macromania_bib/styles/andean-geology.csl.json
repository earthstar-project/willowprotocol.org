"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\">\n  <info>\n    <title>Andean Geology</title>\n    <id>http://www.zotero.org/styles/andean-geology</id>\n    <link href=\"http://www.zotero.org/styles/andean-geology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/soil-science-and-plant-nutrition\" rel=\"template\"/>\n    <link href=\"http://www.andeangeology.cl/index.php/revista1/about/submissions\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geology\"/>\n    <issn>0718-7106</issn>\n    <updated>2012-09-14T21:22:32+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"es\">\n    <terms>\n      <term name=\"in\">in</term>\n      <term name=\"in press\">en prensa</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <name name-as-sort-order=\"all\" initialize-with=\".\" delimiter=\"; \" sort-separator=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\"; \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter=\"; \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <group delimiter=\". \">\n        <choose>\n          <if variable=\"number-of-pages\">\n            <group delimiter=\" \">\n              <text variable=\"number-of-pages\"/>\n              <text term=\"page\" plural=\"false\" form=\"short\"/>\n            </group>\n          </if>\n          <else-if type=\"book thesis\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"page\"/>\n              <text term=\"page\" plural=\"false\" form=\"short\"/>\n            </group>\n          </else-if>\n          <else>\n            <text variable=\"page\"/>\n          </else>\n        </choose>\n        <text variable=\"publisher-place\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"year-citation\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n      </if>\n      <else>\n        <text term=\"in press\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-bibliography\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n      </if>\n      <else>\n        <text term=\"in press\" text-case=\"capitalize-first\"/>\n      </else>\n    </choose>\n  </macro>\n  <!--  <macro name=\"access\">\n       <choose>\n\t <if variable=\"URL\">\n\t   <text variable=\"URL\"/>\n\t   <date variable=\"accessed\" prefix=\" (\" suffix=\")\" delimiter=\", \">\n\t     <date-part name=\"month\"/>\n\t     <date-part name=\"year\"/>\n\t   </date>\n\t </if>\n       </choose>\n       </macro> -->\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" suffix=\"n.\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"year\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-citation\"/>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"year-bibliography\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <group prefix=\" \" delimiter=\" \">\n              <text macro=\"edition\"/>\n              <text macro=\"editor-translator\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"thesis\">\n          <group prefix=\" \" delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\"/>\n              <text macro=\"editor-translator\"/>\n            </group>\n          </group>\n          <text macro=\"publisher\" prefix=\". \"/>\n        </else-if>\n        <else-if type=\"paper-conference\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text variable=\"container-title\"/>\n                <text macro=\"editor-translator\"/>\n              </group>\n              <text variable=\"volume\" prefix=\"No. \"/>\n              <group delimiter=\" \">\n                <text variable=\"collection-title\"/>\n                <text variable=\"issue\"/>\n              </group>\n            </group>\n          </group>\n          <text macro=\"publisher\" prefix=\": \"/>\n        </else-if>\n        <else-if type=\"article report\" match=\"any\">\n          <group delimiter=\", \" prefix=\" \">\n            <text variable=\"publisher\"/>\n            <text variable=\"container-title\"/>\n            <group delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <choose>\n                <if variable=\"collection-number\">\n                  <text variable=\"collection-number\"/>\n                </if>\n                <else>\n                  <text variable=\"number\"/>\n                </else>\n              </choose>\n            </group>\n          </group>\n          <group delimiter=\" \" prefix=\": \">\n            <choose>\n              <if variable=\"number-of-pages\">\n                <text variable=\"number-of-pages\"/>\n              </if>\n              <else>\n                <text variable=\"page\"/>\n              </else>\n            </choose>\n            <text term=\"page\" form=\"short\" plural=\"false\"/>\n          </group>\n          <text variable=\"genre\" prefix=\", \"/>\n          <text variable=\"publisher-place\" prefix=\". \"/>\n        </else-if>\n        <else-if type=\"map\">\n          <group delimiter=\", \" prefix=\" \">\n            <text variable=\"publisher\"/>\n            <group delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <text variable=\"collection-number\"/>\n            </group>\n          </group>\n          <group delimiter=\" \" prefix=\": \">\n            <text variable=\"number-of-pages\"/>\n            <text term=\"page\" form=\"short\" plural=\"false\"/>\n          </group>\n          <text variable=\"scale\" prefix=\", \"/>\n          <text variable=\"publisher-place\" prefix=\". \"/>\n        </else-if>\n        <else>\n          <group delimiter=\" \" prefix=\" \">\n            <text variable=\"container-title\"/>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\" prefix=\": \"/>\n        </else>\n      </choose>\n      <!--      <text macro=\"access\" prefix=\". \"/>-->\n    </layout>\n  </bibliography>\n</style>\n"