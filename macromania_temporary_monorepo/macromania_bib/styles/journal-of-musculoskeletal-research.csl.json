"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Journal of Musculoskeletal Research</title>\n    <title-short>JMR</title-short>\n    <id>http://www.zotero.org/styles/journal-of-musculoskeletal-research</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-musculoskeletal-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/nature\" rel=\"template\"/>\n    <link href=\"https://www.worldscientific.com/toc/jmr/23/01\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <category field=\"biology\"/>\n    <issn>0218-9577</issn>\n    <eissn>1793-6497</eissn>\n    <updated>2020-08-25T08:02:46+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\">accessed on</term>\n    </terms>\n  </locale>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <text macro=\"volume\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\", \"/>\n      <et-al font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi: \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issuance\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture song thesis chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\" form=\"long\"/>\n          <text macro=\"pages\"/>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else-if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\" suffix=\",\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor\">\n          <label form=\"verb\" suffix=\" \"/>\n          <name delimiter-precedes-last=\"never\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <group delimiter=\":\" suffix=\",\">\n          <group>\n            <text variable=\"volume\" font-weight=\"bold\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text macro=\"pages\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n          <text variable=\"volume\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"page\"/>\n      </if>\n      <else-if type=\"chapter\" match=\"any\">\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\" entry-spacing=\"0\" line-spacing=\"2\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <group delimiter=\" \">\n        <text macro=\"author\" suffix=\".\"/>\n        <text macro=\"title\" suffix=\".\"/>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n          </if>\n        </choose>\n        <text macro=\"container-title\"/>\n        <text macro=\"editor\" suffix=\",\"/>\n        <text macro=\"volume\"/>\n        <text macro=\"issuance\" suffix=\".\"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"