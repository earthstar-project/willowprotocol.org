      <name form=\"short\" initialize-with=\". \"/>\n      <et-al font-style=\"normal\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation delimiter-precedes-last=\"never\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\" year-suffix-delimiter=\"; \">\n    <sort>\n      <key variable=\"issued\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography sort-separator=\"\" entry-spacing=\"0\" line-spacing=\"2\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <date variable=\"issued\" prefix=\" \" suffix=\".\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \" suffix=\".\">\n            <text variable=\"title\" prefix=\" \"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"thesis\" match=\"any\">\n          <group suffix=\".\">\n            <text variable=\"title\" prefix=\" \" suffix=\".\"/>\n            <text variable=\"genre\" prefix=\" \" suffix=\" Thesis.\"/>\n            <text macro=\"publisher\" prefix=\" \"/>\n            <text variable=\"number-of-pages\" prefix=\", \" suffix=\"pp.\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" prefix=\" \" suffix=\". \"/>\n          <group delimiter=\". \">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text variable=\"container-title\" prefix=\" \"/>\n                <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"collection-title\" prefix=\" \"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n            <text macro=\"publisher\" prefix=\" \"/>\n          </group>\n        </else-if>\n        <else-if type=\"post-weblog webpage\" match=\"any\">\n          <group delimiter=\". \" prefix=\". \">\n            <text variable=\"title\"/>\n            <text variable=\"container-title\"/>\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                <date form=\"text\" variable=\"accessed\"/>\n              </group>\n              <group>\n                <text term=\"from\" text-case=\"lowercase\" font-style=\"normal\" suffix=\" \"/>\n                <text variable=\"URL\" font-style=\"italic\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text variable=\"title\" prefix=\" \"/>\n          </group>\n          <group delimiter=\", \" prefix=\" \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"