"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" delimiter-precedes-last=\"always\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Biotechnology and Bioprocess Engineering</title>\n    <title-short>BBE</title-short>\n    <id>http://www.zotero.org/styles/biotechnology-and-bioprocess-engineering</id>\n    <link href=\"http://www.zotero.org/styles/biotechnology-and-bioprocess-engineering\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/trends-journals\" rel=\"documentation\"/>\n    <link href=\"https://www.springer.com/journal/12257/submission-guidelines\" rel=\"documentation\"/>\n    <link href=\"https://media.springer.com/full/springer-instructions-for-authors-assets/pdf/1652521_173753306_12257_Instructions%20to%20Author.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <category field=\"medicine\"/>\n    <issn>1226-8372</issn>\n    <eissn>1976-3816</eissn>\n    <updated>2022-09-15T13:50:32+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \" prefix=\"(\" suffix=\") \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\" suffix=\"n\" strip-periods=\"true\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" text-case=\"capitalize-first\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize-with=\".\" name-as-sort-order=\"first\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\", \" initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"locators\">\n    <group delimiter=\", \">\n      <text macro=\"edition\" prefix=\"(\" suffix=\")\"/>\n      <group delimiter=\" \">\n        <number variable=\"volume\" form=\"numeric\"/>\n      </group>\n      <group delimiter=\" \">\n        <number variable=\"number-of-volumes\" form=\"numeric\"/>\n        <text term=\"volume\" form=\"short\" plural=\"true\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"publisher\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if type=\"paper-conference\">\n        <choose>\n          <if variable=\"container-title\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" prefix=\"in \" font-style=\"italic\"/>\n              <text variable=\"event-place\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\", \">\n              <text variable=\"event\" prefix=\"presented at the \"/>\n              <text variable=\"event-place\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <group delimiter=\". \">\n          <text value=\"[Online]\"/>\n          <text variable=\"URL\" prefix=\"Available: \"/>\n          <group prefix=\"[\" suffix=\"]\">\n            <date variable=\"accessed\" prefix=\"Accessed: \">\n              <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n              <date-part name=\"month\" form=\"short\" suffix=\"-\" strip-periods=\"true\"/>\n              <date-part name=\"year\" form=\"long\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\" plural=\"always\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"page\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"issued\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if type=\"article-journal\">\n          <group delimiter=\" \" prefix=\". \">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <choose>\n              <if variable=\"volume\">\n                <group delimiter=\": \">\n                  <text macro=\"locators\"/>\n                  <text macro=\"page\"/>\n                </group>\n              </if>\n              <else-if variable=\"DOI\">\n                <text variable=\"DOI\" prefix=\"DOI: \"/>\n              </else-if>\n              <else-if variable=\"URL\">\n                <text term=\"at\"/>\n                <text variable=\"URL\" prefix=\" &lt;\" suffix=\"&gt;\"/>\n              </else-if>\n            </choose>\n          </group>\n        </if>\n        <else-if type=\"paper-conference\">\n          <group delimiter=\", \" prefix=\". \">\n            <text macro=\"event\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\". \" prefix=\". \">\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <text variable=\"container-title\" font-style=\"italic\" prefix=\". \" suffix=\". \"/>\n          <text macro=\"access\"/>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\" \" prefix=\". \">\n            <text variable=\"authority\"/>\n            <text variable=\"number\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \" prefix=\". \">\n            <text macro=\"locators\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article-newspaper broadcast interview manuscript map patent personal_communication song speech thesis webpage\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \" prefix=\". \">\n            <text macro=\"page\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"