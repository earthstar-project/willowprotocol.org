"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>cultural geographies</title>\n    <title-short>cg</title-short>\n    <id>http://www.zotero.org/styles/cultural-geographies</id>\n    <link href=\"http://www.zotero.org/styles/cultural-geographies\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/international-journal-of-humanoid-robotics\" rel=\"template\"/>\n    <link href=\"https://uk.sagepub.com/en-gb/eur/journal/cultural-geographies#submission-guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"geography\"/>\n    <issn>1474-4740</issn>\n    <eissn>1477-0881</eissn>\n    <updated>2019-02-19T17:11:15+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"ibid\">op cit.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <label form=\"short\" strip-periods=\"false\" suffix=\" \"/>\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\". \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group prefix=\"[\" suffix=\"]\" delimiter=\": \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n              <date-part name=\"month\" form=\"short\" suffix=\"-\" strip-periods=\"true\"/>\n              <date-part name=\"year\" form=\"long\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\" (\" suffix=\")\">\n      <group delimiter=\": \">\n        <text variable=\"publisher\" form=\"long\"/>\n        <text variable=\"publisher-place\" form=\"long\"/>\n      </group>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher-short\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\" form=\"long\"/>\n      <text variable=\"publisher\" form=\"long\"/>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <number variable=\"number-of-volumes\"/>\n      <text variable=\"number-of-volumes\"/>\n    </group>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"article article-journal chapter\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\"/>\n          <number text-case=\"uppercase\" variable=\"volume\" form=\"roman\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation>\n    <layout vertical-align=\"baseline\" delimiter=\"; \">\n      <choose>\n        <if match=\"any\" position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title-short\"/>\n            <text macro=\"locator\"/>\n          </group>\n        </if>\n        <else-if match=\"any\" position=\"ibid\">\n          <group delimiter=\", \">\n            <text term=\"ibid\"/>\n            <text macro=\"locator\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text macro=\"author\"/>\n              <text macro=\"title-short\"/>\n              <text macro=\"editor-short\"/>\n              <text macro=\"locators\"/>\n            </group>\n            <group delimiter=\", \">\n              <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                <text macro=\"volumes\"/>\n                <text macro=\"publisher-short\"/>\n              </group>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n          <text macro=\"locator\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"31\" et-al-use-first=\"30\">\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group>\n            <group delimiter=\", \">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \" prefix=\", \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"number-of-pages\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" quotes=\"true\" suffix=\", \"/>\n          <text term=\"in\" suffix=\" \"/>\n          <group delimiter=\", \">\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n          </group>\n          <group delimiter=\", \" prefix=\" \">\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\". \">\n            <group delimiter=\", \">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"year-date\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\" \">\n            <text variable=\"title\"/>\n            <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article article-journal\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n            <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n            <text variable=\"volume\" font-weight=\"normal\"/>\n            <text macro=\"year-date\"/>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n            <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n            <text variable=\"volume\" font-weight=\"normal\"/>\n            <text macro=\"year-date\"/>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"