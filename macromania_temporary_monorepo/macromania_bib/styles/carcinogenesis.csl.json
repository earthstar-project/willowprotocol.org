"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Carcinogenesis</title>\n    <id>http://www.zotero.org/styles/carcinogenesis</id>\n    <link href=\"http://www.zotero.org/styles/carcinogenesis\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/trends-journals\" rel=\"template\"/>\n    <link href=\"http://www.oxfordjournals.org/our_journals/carcin/for_authors/general.html\" rel=\"documentation\"/>\n    <contributor>\n      <name>Jonas Bergan</name>\n      <uri>http://www.zotero.org/jonas.bergan</uri>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <category field=\"medicine\"/>\n    <issn>0143-3334</issn>\n    <eissn>1460-2180</eissn>\n    <summary>Style for the \"Carcinogenesis\" journal</summary>\n    <updated>2013-09-24T13:04:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <!-- Macros -->\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\" prefix=\"(\"/>\n              <text term=\"edition\" form=\"short\" suffix=\"n) \" strip-periods=\"true\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" text-case=\"capitalize-first\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-journal\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\" prefix=\"(\" suffix=\") \"/>\n        </date>\n      </if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\" prefix=\"(\" suffix=\") \"/>\n        </date>\n      </else-if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n          <date-part name=\"month\" form=\"short\" suffix=\"-\" strip-periods=\"true\"/>\n          <date-part name=\"year\" form=\"long\" prefix=\"(\" suffix=\") \"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\", \" initialize-with=\".\" name-as-sort-order=\"all\" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\", \" initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\", \" and=\"text\" prefix=\"(\"/>\n      <label form=\"short\" prefix=\", \" suffix=\"),\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"locators\">\n    <group delimiter=\", \">\n      <text macro=\"edition\"/>\n      <group delimiter=\" \">\n        <number variable=\"volume\" form=\"numeric\"/>\n      </group>\n      <group delimiter=\" \">\n        <number variable=\"number-of-volumes\" form=\"numeric\"/>\n        <text term=\"volume\" form=\"short\" plural=\"true\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\", \"/>\n      </if>\n      <else>\n        <text variable=\"title\" suffix=\". \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <text variable=\"publisher\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if type=\"paper-conference\">\n        <choose>\n          <!-- Published Conference Paper -->\n          <if variable=\"container-title\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" prefix=\"in \" font-style=\"italic\"/>\n              <text variable=\"event-place\"/>\n            </group>\n          </if>\n          <!-- Unpublished Conference Paper -->\n          <else>\n            <group delimiter=\", \">\n              <text variable=\"event\" prefix=\"presented at the \"/>\n              <text variable=\"event-place\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <group delimiter=\". \">\n          <text value=\"[Online]\"/>\n          <text variable=\"URL\" prefix=\"Available: \"/>\n          <group prefix=\"[\" suffix=\"]\">\n            <date variable=\"accessed\" prefix=\"Accessed: \">\n              <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n              <date-part name=\"month\" form=\"short\" suffix=\"-\" strip-periods=\"true\"/>\n              <date-part name=\"year\" form=\"long\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <label variable=\"page\" form=\"short\" plural=\"always\" suffix=\" \"/>\n        <text variable=\"page\"/>\n      </if>\n      <else>\n        <text variable=\"page\"/>\n      </else>\n    </choose>\n  </macro>\n  <!-- Citation -->\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <!-- Bibliography -->\n  <bibliography et-al-min=\"2\" et-al-use-first=\"1\" entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout>\n      <!-- Citation Number -->\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <!-- Author(s) -->\n      <text macro=\"author\" suffix=\" \"/>\n      <!-- Rest of Citation -->\n      <choose>\n        <!-- Specific Formats -->\n        <if type=\"article-journal\">\n          <text macro=\"issued\"/>\n          <text macro=\"title\"/>\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\" suffix=\", \"/>\n          <choose>\n            <if variable=\"volume\">\n              <text macro=\"locators\" suffix=\", \" font-weight=\"bold\"/>\n              <text macro=\"page\" suffix=\".\"/>\n            </if>\n            <else-if variable=\"DOI\">\n              <text variable=\"DOI\" prefix=\"DOI: \" suffix=\".\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text term=\"at\"/>\n              <text variable=\"URL\" prefix=\" &lt;\" suffix=\"&gt;.\"/>\n            </else-if>\n          </choose>\n        </if>\n        <else-if type=\"paper-conference\">\n          <group delimiter=\", \">\n            <text macro=\"issued\"/>\n            <text macro=\"title\"/>\n            <text macro=\"event\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"page\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text macro=\"issued\"/>\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\", \" suffix=\". \">\n            <text macro=\"issued\"/>\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </group>\n          <text macro=\"access\" suffix=\".\"/>\n        </else-if>\n        <else-if type=\"patent\">\n          <text macro=\"issued\"/>\n          <text macro=\"title\" suffix=\", \"/>\n          <text variable=\"number\" suffix=\".\"/>\n        </else-if>\n        <!-- Generic/Fallback Formats -->\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"issued\"/>\n          <text macro=\"title\"/>\n          <text macro=\"locators\"/>\n          <text macro=\"publisher\" suffix=\".\"/>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article-newspaper broadcast interview manuscript map patent personal_communication song speech thesis webpage\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"issued\"/>\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"issued\"/>\n          <text macro=\"title\"/>\n          <text variable=\"container-title\" prefix=\"In \" font-style=\"italic\" suffix=\" \"/>\n          <text macro=\"locators\"/>\n          <text macro=\"editor\" prefix=\" \" suffix=\" \"/>\n          <group delimiter=\", \">\n            <text macro=\"page\"/>\n            <text macro=\"publisher\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"issued\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\" suffix=\".\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"