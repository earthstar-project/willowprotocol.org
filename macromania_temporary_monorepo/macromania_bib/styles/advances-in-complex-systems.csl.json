"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Advances in Complex Systems</title>\n    <id>http://www.zotero.org/styles/advances-in-complex-systems</id>\n    <link href=\"http://www.zotero.org/styles/advances-in-complex-systems\" rel=\"self\"/>\n    <link href=\"http://www.worldscinet.com/acs/mkt/guidelines.shtml\" rel=\"documentation\"/>\n    <author>\n      <name>Michael Barton</name>\n      <email>michael.barton@asu.edu</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <issn>0219-5259</issn>\n    <eissn>1793-6802</eissn>\n    <updated>2012-09-09T21:58:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" prefix=\" \" suffix=\" \"/>\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" prefix=\"(\" suffix=\", \"/>\n    <text variable=\"publisher-place\" suffix=\", \"/>\n    <text macro=\"year-date\" suffix=\")\"/>\n  </macro>\n  <macro name=\"edition\">\n    <!--TODO: CSL should have low numeric be text (e.g. '3'->'third')-->\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"7\" et-al-use-first=\"6\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" suffix=\", \"/>\n          <text term=\"in\" suffix=\" \"/>\n          <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\" suffix=\", \"/>\n          <text macro=\"editor\" suffix=\", \"/>\n          <text macro=\"edition\" suffix=\", \"/>\n          <text macro=\"publisher\"/>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\" prefix=\", \"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text variable=\"title\" suffix=\",\"/>\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\" text-case=\"title\"/>\n            <text variable=\"volume\" font-weight=\"bold\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"