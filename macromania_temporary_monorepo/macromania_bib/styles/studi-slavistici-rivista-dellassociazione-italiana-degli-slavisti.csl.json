"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" delimiter-precedes-et-al=\"never\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Studi Slavistici Rivista dell’Associazione Italiana degli Slavisti</title>\n    <id>http://www.zotero.org/styles/studi-slavistici-rivista-dellassociazione-italiana-degli-slavisti</id>\n    <link href=\"http://www.zotero.org/styles/studi-slavistici-rivista-dellassociazione-italiana-degli-slavisti\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-neolithic-archaeology\" rel=\"template\"/>\n    <link href=\"https://oaj.fupress.net/index.php/ss/index\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <issn>1824-761X</issn>\n    <eissn>1824-7601</eissn>\n    <updated>2022-02-16T15:30:32+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"reviewed-author\" form=\"verb\">rev. of</term>\n      <term name=\"presented at\">talk</term>\n      <term name=\"accessed\">last accessed</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"reviewed-author\" form=\"verb\">Rez. zu</term>\n      <term name=\"presented at\">Vortrag</term>\n      <term name=\"accessed\">Zugriff: </term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"it\">\n    <terms>\n      <term name=\"reviewed-author\" form=\"verb\">recens. di</term>\n      <term name=\"presented at\">presentazione</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"reviewed-author\" form=\"verb\">rec. de</term>\n      <term name=\"presented at\">conférence</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"es\">\n    <terms>\n      <term name=\"reviewed-author\" form=\"verb\">recensión de</term>\n      <term name=\"presented at\">conferencia</term>\n    </terms>\n  </locale>\n  <macro name=\"short-container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group>\n          <choose>\n            <if variable=\"editor container-title\" match=\"any\">\n              <text term=\"in\" prefix=\". \" suffix=\": \"/>\n              <names variable=\"editor\">\n                <name delimiter=\",\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" initialize-with=\".\"/>\n                <et-al font-style=\"italic\"/>\n                <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\"),\"/>\n              </names>\n              <names variable=\"container-author\">\n                <name delimiter=\"/\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" initialize-with=\".\"/>\n              </names>\n              <text macro=\"collection-title\"/>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else-if type=\"article article-journal article-magazine article-newspaper\" match=\"any\">\n        <text macro=\"translator\"/>\n        <text variable=\"container-title\" quotes=\"true\" prefix=\", \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter=\",\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" initialize-with=\".\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <choose>\n      <if match=\"any\" variable=\"translator\">\n        <choose>\n          <if match=\"none\" variable=\"editor\">\n            <names variable=\"translator\" prefix=\", \">\n              <label form=\"verb-short\" suffix=\" \"/>\n              <name delimiter=\",\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" initialize-with=\".\"/>\n            </names>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"contributors\">\n    <group suffix=\", \">\n      <choose>\n        <if variable=\"author\">\n          <names variable=\"author\">\n            <name delimiter=\",\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" initialize-with=\".\"/>\n          </names>\n        </if>\n        <else-if variable=\"editor\">\n          <text macro=\"editor\"/>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"contributors-short-citation\">\n    <choose>\n      <if variable=\"author\">\n        <names variable=\"author\">\n          <name form=\"short\" delimiter=\",\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n          <et-al font-style=\"italic\"/>\n        </names>\n      </if>\n      <else-if variable=\"editor\">\n        <names variable=\"editor\">\n          <name form=\"short\" delimiter=\",\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n          <et-al font-style=\"italic\"/>\n        </names>\n      </else-if>\n      <else>\n        <text variable=\"title-short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"contributors-short-biblio\">\n    <choose>\n      <if variable=\"author\">\n        <names variable=\"author\">\n          <name form=\"short\" delimiter=\",\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n          <et-al font-style=\"italic\"/>\n        </names>\n      </if>\n      <else-if variable=\"editor\">\n        <names variable=\"editor\">\n          <name form=\"short\" delimiter=\",\" delimiter-precedes-last=\"always\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n          <et-al font-style=\"italic\"/>\n        </names>\n      </else-if>\n      <else>\n        <text variable=\"title-short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"personal_communication\" match=\"none\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\"/>\n        <text variable=\"volume\" prefix=\" \"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song chapter paper-conference\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <text variable=\"edition\" vertical-align=\"sup\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <number text-case=\"uppercase\" prefix=\" \" variable=\"volume\" form=\"roman\"/>\n        <text variable=\"issue\" prefix=\", \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-article\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"edition\" prefix=\", \"/>\n            <text term=\"edition\" prefix=\", \"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\" \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-journal chapter\" match=\"any\">\n        <group delimiter=\" \" prefix=\", \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"place-date\">\n    <choose>\n      <if type=\"book thesis chapter paper-conference\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"edition\">\n            <text value=\" \"/>\n          </if>\n        </choose>\n        <group delimiter=\" \" prefix=\", \">\n          <choose>\n            <if type=\"thesis\" match=\"any\">\n              <text variable=\"genre\" suffix=\" \"/>\n              <text variable=\"publisher\"/>\n            </if>\n            <else>\n              <choose>\n                <if match=\"any\" variable=\"original-date original-publisher-place\">\n                  <text variable=\"original-publisher-place\"/>\n                  <date date-parts=\"year\" form=\"text\" variable=\"original-date\" prefix=\" \"/>\n                  <text value=\"; \"/>\n                </if>\n              </choose>\n              <text variable=\"publisher-place\"/>\n            </else>\n          </choose>\n          <text macro=\"date\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-date\">\n    <choose>\n      <if type=\"article-journal\">\n        <text macro=\"date\" prefix=\", \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"day-month\">\n    <date variable=\"issued\">\n      <date-part name=\"month\"/>\n      <date-part name=\"day\" prefix=\" \"/>\n    </date>\n  </macro>\n  <macro name=\"collection-title\">\n    <text variable=\"container-title\" prefix=\" \"/>\n    <text variable=\"volume\" prefix=\" \"/>\n  </macro>\n  <macro name=\"event\">\n    <text term=\"presented at\" suffix=\" \"/>\n    <date form=\"text\" variable=\"issued\" suffix=\", \"/>\n    <text variable=\"event-place\" suffix=\". \"/>\n    <text variable=\"event\"/>\n  </macro>\n  <macro name=\"issue\">\n    <choose>\n      <if type=\"speech\" match=\"any\">\n        <text macro=\"event\" prefix=\" \"/>\n      </if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <text variable=\"event\" prefix=\" \"/>\n        <date form=\"text\" variable=\"event-date\" prefix=\" \"/>\n      </else-if>\n      <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n        <text macro=\"day-month\" prefix=\", \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"series\">\n    <choose>\n      <if match=\"any\" variable=\"collection-title\">\n        <text variable=\"collection-title\" prefix=\". \"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <text variable=\"volume\" prefix=\" \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if match=\"any\" variable=\"URL\">\n        <group delimiter=\" \" prefix=\", \">\n          <text variable=\"URL\" prefix=\"&lt;\" suffix=\">\"/>\n          <group delimiter=\": \" prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\".\"/>\n              <date-part name=\"month\" form=\"numeric\" suffix=\".\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"doi\">\n    <text variable=\"DOI\" prefix=\", DOI: https://doi.org/\"/>\n  </macro>\n  <macro name=\"review\">\n    <choose>\n      <if match=\"any\" variable=\"reviewed-author\">\n        <text term=\"reviewed-author\" form=\"verb\" prefix=\", \" suffix=\" \"/>\n        <names variable=\"reviewed-author\">\n          <name delimiter=\"/\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" initialize-with=\".\"/>\n        </names>\n        <text variable=\"reviewed-title\" prefix=\", \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"encyclopedia-article\">\n    <text variable=\"container-title\"/>\n    <number prefix=\" \" variable=\"volume\"/>\n    <choose>\n      <if match=\"none\" is-uncertain-date=\"issued\">\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\" (\" suffix=\")\"/>\n      </if>\n    </choose>\n    <text variable=\"page\" prefix=\" \"/>\n    <group>\n      <choose>\n        <if match=\"any\" variable=\"title\">\n          <text term=\"sub-verbo\" form=\"short\" prefix=\" \" suffix=\" \"/>\n          <text variable=\"title\"/>\n        </if>\n      </choose>\n    </group>\n    <text variable=\"edition\" prefix=\" [\" suffix=\"]\"/>\n    <choose>\n      <if match=\"any\" variable=\"author\">\n        <group prefix=\" (\" suffix=\")\">\n          <names variable=\"author\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"webpage-date\">\n    <choose>\n      <if type=\"webpage\" match=\"any\">\n        <choose>\n          <if match=\"none\" is-uncertain-date=\"issued\">\n            <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\", \"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"contributors-short-citation\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"contributors-short-citation\"/>\n          <text macro=\"date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\" line-spacing=\"1\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"contributors-short-biblio\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <group suffix=\".\">\n        <group suffix=\": \">\n          <text macro=\"contributors-short-biblio\" suffix=\" \"/>\n          <text macro=\"date\"/>\n        </group>\n        <group>\n          <choose>\n            <if type=\"entry-encyclopedia entry-dictionary\" match=\"any\">\n              <text macro=\"encyclopedia-article\"/>\n            </if>\n            <else>\n              <text macro=\"contributors\"/>\n              <text macro=\"title\" strip-periods=\"false\"/>\n              <text macro=\"review\"/>\n              <text macro=\"short-container-contributors\"/>\n              <text macro=\"locators\"/>\n              <text macro=\"issue\"/>\n              <text macro=\"series\"/>\n              <choose>\n                <if type=\"article article-journal article-magazine article-newspaper\" match=\"none\">\n                  <text macro=\"translator\" suffix=\" \"/>\n                </if>\n              </choose>\n              <text macro=\"webpage-date\"/>\n              <text macro=\"place-date\"/>\n              <text macro=\"journal-date\"/>\n              <text macro=\"locators-article\"/>\n            </else>\n          </choose>\n          <choose>\n            <if variable=\"DOI\" match=\"any\">\n              <text macro=\"doi\"/>\n            </if>\n            <else>\n              <text macro=\"url\"/>\n            </else>\n          </choose>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"