"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"sort-only\" default-locale=\"fr-FR\" page-range-format=\"expanded\">\n  <info>\n    <title>Techniques&amp;Culture (Français)</title>\n    <id>http://www.zotero.org/styles/techniques-et-culture</id>\n    <link href=\"http://www.zotero.org/styles/techniques-et-culture\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/ethnologie-francaise\" rel=\"template\"/>\n    <link href=\"https://journals.openedition.org/tc/1556#tocto3n5\" rel=\"documentation\"/>\n    <author>\n      <name>Nicolas Monteix</name>\n      <email>nicolas.monteix@univ-rouen.fr</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <category field=\"anthropology\"/>\n    <issn>0248-6016</issn>\n    <eissn>1952-420X</eissn>\n    <summary>Author-date style for Techniques &amp; culture. First edition year has to be inserted in extra field, see https://www.zotero.org/support/kb/item_types_and_fields#citing_fields_from_extra</summary>\n    <updated>2019-02-28T00:59:23+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <style-options limit-day-ordinals-to-day-1=\"true\"/>\n    <date form=\"text\">\n      <date-part name=\"day\" suffix=\" \"/>\n      <date-part name=\"month\" suffix=\" \"/>\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"online\">mis en ligne le </term>\n      <term name=\"page-range-delimiter\">-</term>\n      <term name=\"in\">in</term>\n      <term name=\"anonymous\">anonyme</term>\n      <term name=\"anonymous\" form=\"short\">anon.</term>\n      <term name=\"no date\">s.d.</term>\n      <term name=\"paragraph\" form=\"short\">§</term>\n      <term name=\"translator\" form=\"short\">trad.</term>\n      <term name=\"editor\" form=\"short\">dir.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" initialize-with=\". \" name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"symbol\" initialize-with=\". \" name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor2\">\n    <names variable=\"editor\">\n      <name and=\"symbol\" initialize-with=\". \" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\" prefix=\" : \"/>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\", \"/>\n      <et-al font-variant=\"normal\" font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"editor-short\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <name form=\"short\" delimiter=\", \"/>\n      <et-al font-variant=\"normal\" font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \" prefix=\". \">\n      <choose>\n        <if variable=\"DOI\">\n          <group delimiter=\" \">\n            <text value=\"doi :\"/>\n            <text variable=\"DOI\"/>\n          </group>\n        </if>\n        <else>\n          <group delimiter=\" \">\n            <text value=\"[En ligne] :\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"webpage\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\"/>\n      </else-if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\" : \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n      <text variable=\"collection-title\" quotes=\"true\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"loc-cite\">\n    <choose>\n      <if locator=\"page\">\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <group delimiter=\"\">\n          <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation and=\"text\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\" (\" suffix=\")\" delimiter=\" ; \">\n      <text macro=\"author-short\" suffix=\" \"/>\n      <text macro=\"year-date\"/>\n      <group>\n        <text macro=\"loc-cite\" prefix=\" : \"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&#8212;\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"page-first\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group>\n        <group delimiter=\" \" suffix=\" \">\n          <choose>\n            <if variable=\"author editor\" match=\"none\">\n              <text variable=\"title\" form=\"short\"/>\n            </if>\n            <else>\n              <text macro=\"author\"/>\n            </else>\n          </choose>\n          <text macro=\"year-date\"/>\n          <date variable=\"original-date\" prefix=\"[\" suffix=\"]\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n        <choose>\n          <if type=\"thesis\">\n            <group delimiter=\", \" suffix=\".\">\n              <text macro=\"title\" font-style=\"italic\"/>\n              <text variable=\"genre\"/>\n              <group delimiter=\" : \">\n                <text variable=\"publisher\"/>\n                <text variable=\"publisher-place\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\" quotes=\"true\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"volume\" prefix=\"vol. \"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage article-journal article-magazine article-newspaper broadcast personal_communication\" match=\"any\">\n            <group>\n              <text macro=\"title\" quotes=\"true\"/>\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n              <group prefix=\" \" delimiter=\".\">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"book graphic report\" match=\"any\">\n            <group suffix=\".\" delimiter=\" \">\n              <text macro=\"title\" suffix=\",\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"collection\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text macro=\"title\" quotes=\"true\" suffix=\", \"/>\n            <group delimiter=\" \">\n              <text value=\"in\"/>\n              <text macro=\"editor2\"/>\n              <text variable=\"container-title\" font-style=\"italic\" suffix=\",\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"collection\"/>\n            </group>\n          </else-if>\n        </choose>\n        <choose>\n          <if variable=\"DOI\">\n            <text macro=\"pages\"/>\n            <text macro=\"access\"/>\n          </if>\n          <else>\n            <text macro=\"access\"/>\n            <text macro=\"pages\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"