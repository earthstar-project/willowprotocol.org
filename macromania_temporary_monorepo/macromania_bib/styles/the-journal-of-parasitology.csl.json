"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" default-locale=\"en-US\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>The Journal of Parasitology</title>\n    <id>http://www.zotero.org/styles/the-journal-of-parasitology</id>\n    <link href=\"http://www.zotero.org/styles/the-journal-of-parasitology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-wildlife-diseases\" rel=\"template\"/>\n    <link href=\"http://www.journalofparasitology.org/page/author_instructions?code=asp-site\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0022-3395</issn>\n    <eissn>1937-2345</eissn>\n    <updated>2017-01-14T12:39:19+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"page\" form=\"short\">p</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor translator\" delimiter=\", \">\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\" (\" suffix=\").\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" font-variant=\"normal\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"genre\" suffix=\", \"/>\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n      <group delimiter=\" \">\n        <text variable=\"number-of-pages\"/>\n        <text term=\"page\" form=\"short\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <group prefix=\" \" suffix=\".\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <text prefix=\" (\" term=\"no date\" suffix=\").\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <number variable=\"edition\" form=\"ordinal\" prefix=\", \"/>\n            <text term=\"edition\" form=\"long\" prefix=\" \" suffix=\".\"/>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\"/>\n        <text variable=\"volume\" font-weight=\"bold\" prefix=\" \"/>\n        <text variable=\"page\" prefix=\": \"/>\n      </if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\" form=\"short\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"edited-volume\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" font-style=\"italic\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <group delimiter=\". \">\n          <text variable=\"container-title\"/>\n          <text macro=\"container-contributors\"/>\n          <text macro=\"secondary-contributors\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"11\" et-al-use-first=\"10\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued-year\"/>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"3\" et-al-use-first=\"1\" entry-spacing=\"0\" line-spacing=\"2\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\".\" font-variant=\"small-caps\"/>\n      <text macro=\"issued\" suffix=\" \"/>\n      <text variable=\"title\"/>\n      <text macro=\"edition\"/>\n      <group delimiter=\". \" prefix=\". \">\n        <text macro=\"edited-volume\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"locators\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"