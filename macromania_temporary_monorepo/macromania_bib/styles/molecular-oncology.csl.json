"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Molecular Oncology</title>\n    <id>http://www.zotero.org/styles/molecular-oncology</id>\n    <link href=\"http://www.zotero.org/styles/molecular-oncology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/archives-of-hand-and-microsurgery\" rel=\"documentation\"/>\n    <link href=\"https://febs.onlinelibrary.wiley.com/hub/journal/18780261/journal-menu/author-guidelines#refs\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1574-7891</issn>\n    <eissn>1878-0261</eissn>\n    <updated>2022-12-28T20:25:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name delimiter-precedes-et-al=\"always\" delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\" delimiter-precedes-et-al=\"never\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if type=\"article-journal\">\n            <choose>\n              <if match=\"none\" variable=\"page volume\">\n                <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n              </if>\n            </choose>\n          </if>\n          <else-if type=\"webpage post post-weblog\">\n            <group delimiter=\". \">\n              <text variable=\"URL\"/>\n              <group delimiter=\" \">\n                <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"day\" prefix=\" \"/>\n                  <date-part name=\"month\" strip-periods=\"true\" prefix=\" \"/>\n                  <date-part name=\"year\" prefix=\" \"/>\n                </date>\n              </group>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\" suffix=\".\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"baseline\" delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <group>\n              <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <label plural=\"never\" variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group delimiter=\";\" suffix=\".\">\n            <date variable=\"issued\" prefix=\" \">\n              <date-part name=\"year\"/>\n            </date>\n            <group delimiter=\":\">\n              <group>\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"