"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Corrosion</title>\n    <id>http://www.zotero.org/styles/corrosion</id>\n    <link href=\"http://www.zotero.org/styles/corrosion\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/elsevier-without-titles\" rel=\"template\"/>\n    <link href=\"http://www.nace.org/uploadedFiles/Publications/styleguideall.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <summary>Style following NACE style manual, ommitting titles for journals as specified for the Corrosion journal</summary>\n    <updated>2014-06-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\".\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"always\" name-as-sort-order=\"first\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"patent\">\n        <text variable=\"title\" text-case=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" suffix=\" \"/>\n      <name initialize-with=\".\" delimiter=\", \" and=\"text\" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <!--TODO: CSL should have low numeric be text (e.g. '3'->'third')-->\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"URL\"/>\n        <date variable=\"accessed\" prefix=\" (\" suffix=\")\">\n          <date-part name=\"month\" form=\"short\" suffix=\" \"/>\n          <date-part name=\"day\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\"/>\n          <group delimiter=\", \" prefix=\", \">\n            <group delimiter=\" \">\n              <text term=\"in\"/>\n              <text variable=\"container-title\" form=\"short\" text-case=\"title\"/>\n            </group>\n            <text macro=\"editor\"/>\n          </group>\n          <text macro=\"edition\" prefix=\", \"/>\n          <text macro=\"publisher\" prefix=\" \"/>\n          <group delimiter=\" \" prefix=\", \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"number\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n            <text variable=\"volume\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <group delimiter=\" \" prefix=\": \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text variable=\"container-title\"/>\n              <text macro=\"edition\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\", \"/>\n    </layout>\n  </bibliography>\n</style>\n"