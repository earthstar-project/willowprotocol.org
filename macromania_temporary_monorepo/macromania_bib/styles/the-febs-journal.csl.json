"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>The FEBS Journal</title>\n    <id>http://www.zotero.org/styles/the-febs-journal</id>\n    <link href=\"http://www.zotero.org/styles/the-febs-journal\" rel=\"self\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/(ISSN)1742-4658/homepage/ForAuthors.html#manuscript\" rel=\"documentation\"/>\n    <author>\n      <name>Rintze Zelle</name>\n      <uri>http://twitter.com/rintzezelle</uri>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>1742-464X</issn>\n    <eissn>1742-4658</eissn>\n    <summary>Style for FEBS Journal</summary>\n    <updated>2020-04-21T19:53:31+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\" \"/>\n      <text macro=\"author\" suffix=\" \"/>\n      <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"edition\" prefix=\", \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n            <text prefix=\" \" macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" suffix=\".\">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text variable=\"container-title\" prefix=\" \" font-style=\"italic\"/>\n            <names variable=\"editor\" prefix=\" (\" suffix=\"),\" delimiter=\", \">\n              <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\" \" initialize-with=\"\"/>\n              <label form=\"short\" prefix=\", \" strip-periods=\"true\"/>\n            </names>\n            <group prefix=\" \" suffix=\".\" delimiter=\", \">\n              <text macro=\"edition\"/>\n              <group>\n                <label variable=\"page\" form=\"short\" suffix=\" \"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n            <text macro=\"publisher\" prefix=\" \"/>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\" \" suffix=\".\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n            <group prefix=\" \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n            </group>\n            <group prefix=\", \">\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"