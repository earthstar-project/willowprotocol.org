"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"text\" delimiter-precedes-last=\"never\" page-range-format=\"expanded\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>University of York - Harvard - Archaeology</title>\n    <title-short>UoY Harvard Archaeology</title-short>\n    <id>http://www.zotero.org/styles/university-of-york-harvard-archaeology</id>\n    <link href=\"http://www.zotero.org/styles/university-of-york-harvard-archaeology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-imperial-college-london\" rel=\"template\"/>\n    <link href=\"https://www.york.ac.uk/students/studying/develop-your-skills/study-skills/study/integrity/referencing-styles/\" rel=\"documentation\"/>\n    <author>\n      <name>Peter L Jones</name>\n      <email>10101833@ex.uwl.ac.uk</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>University of York Author-Date Harvard, based on UoWL version.</summary>\n    <updated>2017-10-26T12:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name font-style=\"normal\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <et-al font-style=\"normal\" font-variant=\"normal\" font-weight=\"normal\" text-decoration=\"none\"/>\n      <label form=\"short\" font-style=\"normal\" prefix=\" \" suffix=\" \"/>\n      <substitute>\n        <names variable=\"translator\" font-style=\"normal\"/>\n        <text macro=\"editor\"/>\n        <text variable=\"publisher\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" font-style=\"normal\"/>\n      <et-al font-style=\"normal\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <text macro=\"editor-short\"/>\n        <text variable=\"publisher\" form=\"short\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <name form=\"short\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book graphic\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" font-weight=\"normal\" text-decoration=\"none\"/>\n      </if>\n      <else-if type=\"article-newspaper interview\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if type=\"webpage\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\" text-decoration=\"none\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-decoration=\"none\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"vol_iss\">\n    <group delimiter=\" \">\n      <text variable=\"volume\"/>\n      <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\" suffix=\" \"/>\n      <choose>\n        <if type=\"article-newspaper interview\" match=\"any\">\n          <text variable=\"publisher\" font-style=\"italic\"/>\n        </if>\n        <else>\n          <text variable=\"publisher\" suffix=\".\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \">\n      <text variable=\"archive\" font-style=\"italic\"/>\n      <text macro=\"location\"/>\n      <group prefix=\"[\" suffix=\"]\">\n        <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <date variable=\"accessed\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"location\">\n    <choose>\n      <if match=\"any\" variable=\"DOI URL\">\n        <choose>\n          <if type=\"article-newspaper\" match=\"any\">\n            <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]. \"/>\n            <text macro=\"issued\" suffix=\".\"/>\n            <group>\n              <text term=\"available at\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" \" suffix=\": \"/>\n            </group>\n          </if>\n          <else-if type=\"webpage figure graphic\" match=\"any\">\n            <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]. \"/>\n            <text macro=\"publisher\"/>\n            <text term=\"available at\" text-case=\"capitalize-first\" prefix=\" \" suffix=\": \"/>\n          </else-if>\n          <else>\n            <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]. \"/>\n            <text term=\"available at\" text-case=\"capitalize-first\" suffix=\": \"/>\n          </else>\n        </choose>\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"doi:\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <text variable=\"URL\"/>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"paper-conference broadcast article-newspaper\" match=\"any\">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n          </date>\n        </if>\n        <else-if type=\"interview\" match=\"any\">\n          <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n        </else-if>\n      </choose>\n      <choose>\n        <if type=\"paper-conference\" match=\"any\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\" \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if match=\"none\" is-uncertain-date=\"issued\">\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <group>\n        <choose>\n          <if locator=\"page\">\n            <text variable=\"locator\" prefix=\", \"/>\n          </if>\n          <else>\n            <label variable=\"locator\" form=\"short\" prefix=\", \"/>\n            <text variable=\"locator\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n        <choose>\n          <if type=\"book\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"collection\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <group delimiter=\", \">\n              <text macro=\"issued\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text macro=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <group delimiter=\". \">\n                <text macro=\"editor\"/>\n                <text variable=\"container-title\" font-style=\"italic\" text-decoration=\"none\"/>\n                <text macro=\"collection\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"vol_iss\"/>\n                <text macro=\"issued\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"article-newspaper\" match=\"any\">\n            <group>\n              <text macro=\"title\"/>\n              <text macro=\"collection\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <group>\n              <choose>\n                <if match=\"none\" variable=\"DOI URL\">\n                  <text macro=\"issued\"/>\n                </if>\n              </choose>\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage\" match=\"any\">\n            <group>\n              <text macro=\"title\"/>\n              <text macro=\"collection\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n          </else-if>\n          <else-if type=\"figure graphic\" match=\"any\">\n            <group>\n              <text macro=\"title\"/>\n              <text macro=\"collection\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <group>\n              <text macro=\"issued\"/>\n              <choose>\n                <if match=\"none\" variable=\"DOI URL\">\n                  <text macro=\"publisher\"/>\n                </if>\n              </choose>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"interview\" match=\"any\">\n            <group>\n              <text macro=\"title\"/>\n              <text macro=\"collection\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <group>\n              <names variable=\"editor\" prefix=\"Interview with \">\n                <name initialize-with=\".\"/>\n              </names>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"pages\"/>\n          </else-if>\n          <else-if type=\"musical_score\" match=\"any\">\n            <group>\n              <text macro=\"title\"/>\n              <text macro=\"collection\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <group>\n              <names variable=\"editor\">\n                <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <name initialize-with=\".\"/>\n              </names>\n            </group>\n            <group>\n              <text macro=\"issued\"/>\n              <text macro=\"publisher\"/>\n              <group prefix=\"(\" suffix=\")\">\n                <text variable=\"volume\" prefix=\"Original work published \"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <group delimiter=\", \">\n              <choose>\n                <if variable=\"author\" match=\"any\">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </if>\n              </choose>\n              <text macro=\"vol_iss\"/>\n              <text variable=\"genre\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"