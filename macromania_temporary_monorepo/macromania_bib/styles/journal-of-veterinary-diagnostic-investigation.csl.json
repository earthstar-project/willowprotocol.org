"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"expanded\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Veterinary Diagnostic Investigation</title>\n    <title-short>JVDI</title-short>\n    <id>http://www.zotero.org/styles/journal-of-veterinary-diagnostic-investigation</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-veterinary-diagnostic-investigation\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/the-oncologist\" rel=\"template\"/>\n    <link href=\"https://uk.sagepub.com/en-gb/eur/journal-of-veterinary-diagnostic-investigation/journal202058#submission-guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1040-6387</issn>\n    <eissn>1943-4936</eissn>\n    <updated>2018-09-19T19:24:30+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\"/>\n    <text variable=\"publisher\" prefix=\": \"/>\n    <group prefix=\", \">\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\" doi:\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <group delimiter=\". \">\n              <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n                <text term=\"cited\"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"year\"/>\n                  <date-part name=\"month\" form=\"short\" strip-periods=\"true\" prefix=\" \"/>\n                  <date-part name=\"day\" prefix=\" \"/>\n                </date>\n              </group>\n              <group delimiter=\": \">\n                <text term=\"available at\" text-case=\"capitalize-first\"/>\n                <text variable=\"URL\"/>\n              </group>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\", \">\n          <text variable=\"title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else-if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"title\"/>\n          <text term=\"internet\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography delimiter-precedes-et-al=\"always\" et-al-min=\"3\" et-al-use-first=\"1\" second-field-align=\"flush\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"publisher\"/>\n          </group>\n          <text variable=\"number-of-pages\" prefix=\": 1-\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\". \">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text macro=\"edition\"/>\n            </group>\n          </group>\n          <group delimiter=\":\" prefix=\" \" suffix=\".\">\n            <text macro=\"publisher\"/>\n            <text variable=\"page\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\" match=\"any\">\n          <group delimiter=\". \">\n            <group delimiter=\" \">\n              <text variable=\"authority\"/>\n              <text variable=\"number\"/>\n            </group>\n            <date form=\"text\" variable=\"issued\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group suffix=\".\">\n            <date variable=\"issued\" prefix=\" \">\n              <date-part name=\"year\"/>\n            </date>\n            <group prefix=\";\">\n              <text variable=\"volume\"/>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"