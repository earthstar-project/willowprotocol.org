"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\" default-locale=\"en-US\">\n  <info>\n    <title>International Journal of Exercise Science</title>\n    <title-short>IJES</title-short>\n    <id>http://www.zotero.org/styles/international-journal-of-exercise-science</id>\n    <link href=\"http://www.zotero.org/styles/international-journal-of-exercise-science\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/vancouver\" rel=\"template\"/>\n    <link href=\"http://digitalcommons.wku.edu/ijes/styleguide.html\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Sean Takats</name>\n      <email>stakats@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Michael Berkowitz</name>\n      <email>mberkowi@gmu.edu</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1939-795X</issn>\n    <summary>Vancouver style for the Journal IJES</summary>\n    <updated>2017-04-18T18:13:48+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"publisher-place\" prefix=\"[\" suffix=\"]: \"/>\n      </if>\n      <else>\n        <text variable=\"publisher-place\" suffix=\": \"/>\n      </else>\n    </choose>\n    <text variable=\"publisher\" suffix=\", \"/>\n    <group>\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n      <text macro=\"accessed-date\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n          <text term=\"cited\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"year\"/>\n            <date-part name=\"month\" prefix=\" \" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\" prefix=\" \"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" suffix=\" \"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"article-journal article-magazine\" match=\"none\">\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"thesis\">\n          <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n          <group delimiter=\". \" suffix=\".\">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group suffix=\". \" delimiter=\" \">\n            <text term=\"in\" suffix=\":\" text-case=\"capitalize-first\"/>\n            <group delimiter=\" \" suffix=\".\">\n              <text variable=\"container-title\"/>\n              <text variable=\"volume\" prefix=\"(Vol. \" suffix=\")\"/>\n            </group>\n            <text macro=\"editor\"/>\n          </group>\n          <text macro=\"publisher\" suffix=\".\"/>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group delimiter=\": \">\n            <group>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n          <date variable=\"issued\" prefix=\", \" suffix=\".\">\n            <date-part name=\"year\"/>\n          </date>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"