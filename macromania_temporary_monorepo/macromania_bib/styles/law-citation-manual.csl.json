"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" initialize-with=\". \" demote-non-dropping-particle=\"sort-only\" default-locale=\"zh-CN\">\n  <info>\n    <title>Manual of Legal Citation (法学注引手册, 中文)</title>\n    <id>http://www.zotero.org/styles/law-citation-manual</id>\n    <link href=\"http://www.zotero.org/styles/law-citation-manual\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/bluebook-law-review\" rel=\"template\"/>\n    <link href=\"https://www.pup.cn/bookDetail?id=910497ac470d4880ab56c6709bb1d7c5\" rel=\"documentation\"/>\n    <link href=\"https://colr.xmu.edu.cn/_upload/article/files/67/3c/bdf817f64039947a52276727ef01/082c41aa-fed1-4460-97fa-7f471901a879.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Zeping Lee</name>\n      <email>zepinglee@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"law\"/>\n    <updated>2022-09-06T12:51:00+08:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <date form=\"text\" delimiter=\" \">\n      <date-part name=\"day\"/>\n      <date-part name=\"month\"/>\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"accessed\">last visited</term>\n      <term name=\"page\" form=\"short\">p.</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"zh\">\n    <date form=\"text\">\n      <date-part name=\"year\" suffix=\"年\" range-delimiter=\"&#8212;\"/>\n      <date-part name=\"month\" form=\"numeric\" suffix=\"月\" range-delimiter=\"&#8212;\"/>\n      <date-part name=\"day\" suffix=\"日\" range-delimiter=\"&#8212;\"/>\n    </date>\n    <terms>\n      <term name=\"article-locator\" form=\"short\">条</term>\n      <term name=\"accessed\">访问</term>\n      <term name=\"compiler\">整理</term>\n      <term name=\"compiler\" form=\"short\">整理</term>\n      <term name=\"edition\" form=\"short\">版</term>\n      <term name=\"editor\" form=\"short\">主编</term>\n      <term name=\"ibid\">同上注</term>\n      <term name=\"in\">载</term>\n      <term name=\"op-cit\">同前注</term>\n      <term name=\"page\" form=\"short\">页</term>\n      <term name=\"paragraph\" form=\"short\">款</term>\n      <term name=\"rule\" form=\"short\">条</term>\n      <term name=\"section\" form=\"short\">条</term>\n      <term name=\"open-quote\">“</term>\n      <term name=\"close-quote\">”</term>\n      <term name=\"open-inner-quote\">‘</term>\n      <term name=\"close-inner-quote\">’</term>\n    </terms>\n  </locale>\n  <locale>\n    <style-options punctuation-in-quote=\"false\"/>\n    <terms>\n      <term name=\"page-range-delimiter\">-</term>\n    </terms>\n  </locale>\n  <macro name=\"author-zh\">\n    <names variable=\"author\">\n      <name delimiter=\"、\" delimiter-precedes-et-al=\"never\"/>\n      <label form=\"short\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <!-- 略写文献时，标示文献性质的“编”“主编”等可以省略 -->\n  <macro name=\"author-short-zh\">\n    <names variable=\"author\">\n      <name delimiter=\"、\" delimiter-precedes-et-al=\"never\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access-zh\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\"，\">\n          <text variable=\"URL\"/>\n          <choose>\n            <!-- 一般不要求注明“访问日期”。网页没有显示上传日期的，引用时可以标注最后访问日期 -->\n            <if variable=\"issued\" match=\"none\">\n              <group>\n                <date variable=\"accessed\" form=\"text\"/>\n                <text term=\"accessed\"/>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"source-zh\">\n    <group delimiter=\"：\">\n      <text macro=\"author-zh\"/>\n      <choose>\n        <if type=\"article-journal article-magazine\" match=\"any\">\n          <group delimiter=\"，\">\n            <text macro=\"title-zh\"/>\n            <text macro=\"secondary-contributor-zh\"/>\n            <text macro=\"container-zh\"/>\n            <text macro=\"locator-zh\"/>\n          </group>\n        </if>\n        <else-if type=\"article-newspaper\">\n          <group delimiter=\"，\">\n            <text macro=\"title-zh\"/>\n            <text macro=\"secondary-contributor-zh\"/>\n            <text macro=\"container-zh\"/>\n            <text macro=\"locator-or-page-zh\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\"，\">\n            <text macro=\"title-zh\"/>\n            <text macro=\"secondary-contributor-zh\"/>\n            <text macro=\"container-zh\"/>\n            <text macro=\"publisher-zh\"/>\n            <text macro=\"locator-zh\"/>\n          </group>\n        </else-if>\n        <else-if type=\"book thesis\" match=\"any\">\n          <group delimiter=\"，\">\n            <text macro=\"title-zh\"/>\n            <text macro=\"secondary-contributor-zh\"/>\n            <text macro=\"publisher-zh\"/>\n            <text macro=\"locator-zh\"/>\n          </group>\n        </else-if>\n        <!-- 49. 引用个人博客、微信公众号 -->\n        <else-if type=\"post post-weblog webpage\">\n          <group delimiter=\"，\">\n            <text macro=\"title-zh\"/>\n            <text macro=\"container-zh\"/>\n            <text macro=\"access-zh\"/>\n          </group>\n        </else-if>\n        <!-- (四)引用法律文件 -->\n        <else-if type=\"legislation\">\n          <group delimiter=\"，\">\n            <group>\n              <text macro=\"title-zh\"/>\n              <choose>\n                <!-- 优先使用 locator 的条文序号 -->\n                <if variable=\"locator\">\n                  <text macro=\"locator-zh\"/>\n                </if>\n                <else>\n                  <text macro=\"section-zh\"/>\n                </else>\n              </choose>\n            </group>\n            <text variable=\"number\"/>\n            <group>\n              <text macro=\"issuance-zh\"/>\n              <text value=\"发布\"/>\n            </group>\n          </group>\n        </else-if>\n        <!-- (五)引用司法案例 -->\n        <else-if type=\"legal_case\">\n          <group delimiter=\"，\">\n            <text macro=\"title-zh\"/>\n            <choose>\n              <if variable=\"container-title\">\n                <choose>\n                  <if variable=\"number\">\n                    <!-- 指导性案例 -->\n                    <group>\n                      <text macro=\"container-zh\"/>\n                      <choose>\n                        <if is-numeric=\"number\">\n                          <number variable=\"number\"/>\n                          <text value=\"号\"/>\n                        </if>\n                        <else>\n                          <text variable=\"number\"/>\n                        </else>\n                      </choose>\n                      <text macro=\"issuance-zh\" prefix=\"（\" suffix=\"）\"/>\n                    </group>\n                  </if>\n                  <else>\n                    <!-- 《最高人民法院公报》上的案例 -->\n                    <text macro=\"container-zh\"/>\n                  </else>\n                </choose>\n              </if>\n              <else>\n                <group delimiter=\"，\">\n                  <group>\n                    <text variable=\"authority\"/>\n                    <choose>\n                      <if variable=\"genre\">\n                        <!-- 用户需要在 genre 中填写司法案例的文书名称，如“民事判决书”、“行政判决书” -->\n                        <text variable=\"genre\"/>\n                      </if>\n                      <else>\n                        <text value=\"判决书\"/>\n                      </else>\n                    </choose>\n                    <text variable=\"number\"/>\n                  </group>\n                  <text macro=\"issuance-zh\"/>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\"，\">\n            <text macro=\"title-zh\"/>\n            <text macro=\"container-zh\"/>\n            <text macro=\"publisher-zh\"/>\n            <text macro=\"locator-zh\"/>\n          </group>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"title-zh\">\n    <choose>\n      <if type=\"webpage\">\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"title\" prefix=\"《\" suffix=\"》\"/>\n          </if>\n          <else>\n            <text variable=\"title\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"legal_case\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" prefix=\"《\" suffix=\"》\"/>\n      </else>\n    </choose>\n    <text macro=\"edition-zh\" prefix=\"（\" suffix=\"）\"/>\n  </macro>\n  <!-- 其他贡献者(翻译、 整理、校对) -->\n  <macro name=\"secondary-contributor-zh\">\n    <names variable=\"translator\" delimiter=\"，\">\n      <name delimiter=\"、\"/>\n      <label form=\"short\"/>\n      <substitute>\n        <names variable=\"compiler\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"edition-zh\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <text value=\"第\"/>\n        <number variable=\"edition\"/>\n        <label variable=\"edition\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor-zh\">\n    <names variable=\"editor\">\n      <name delimiter=\"、\"/>\n      <label form=\"short\"/>\n    </names>\n  </macro>\n  <macro name=\"container-zh\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text term=\"in\"/>\n        <text variable=\"container-title\" prefix=\"《\" suffix=\"》\"/>\n        <text macro=\"issuance-zh\"/>\n        <text macro=\"issue-zh\"/>\n      </if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\"/>\n        <group delimiter=\"：\">\n          <text macro=\"editor-zh\"/>\n          <text variable=\"container-title\" prefix=\"《\" suffix=\"》\"/>\n        </group>\n        <text macro=\"volume-zh\"/>\n        <text macro=\"issue-zh\"/>\n      </else-if>\n      <else-if type=\"legal_case\">\n        <choose>\n          <if variable=\"number\">\n            <!-- 指导性案例 -->\n            <text variable=\"container-title\"/>\n          </if>\n          <else>\n            <!-- 《最高人民法院公报》 -->\n            <text term=\"in\"/>\n            <text variable=\"container-title\" prefix=\"《\" suffix=\"》\"/>\n            <text macro=\"issuance-zh\"/>\n            <text macro=\"issue-zh\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"article-newspaper\">\n        <text term=\"in\"/>\n        <text variable=\"container-title\" prefix=\"《\" suffix=\"》\"/>\n        <text macro=\"issuance-zh\"/>\n      </else-if>\n      <else-if type=\"post post-weblog webpage\">\n        <!-- 2020 年版的网络文献和微信公众号文章的网站标题与上传日期之间均无逗号 -->\n        <text term=\"in\"/>\n        <text variable=\"container-title\"/>\n        <text macro=\"issuance-zh\"/>\n      </else-if>\n      <else>\n        <text term=\"in\"/>\n        <text variable=\"container-title\" prefix=\"《\" suffix=\"》\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume-zh\">\n    <choose>\n      <if is-numeric=\"volume\">\n        <text value=\"第\"/>\n        <number variable=\"volume\"/>\n        <label variable=\"volume\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"volume\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue-zh\">\n    <choose>\n      <if is-numeric=\"issue\">\n        <text value=\"第\"/>\n        <number variable=\"issue\"/>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <text value=\"辑\"/>\n          </if>\n          <else>\n            <label variable=\"issue\" form=\"short\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"issue\"/>\n      </else>\n    </choose>\n  </macro>\n  <!-- 条文序号 -->\n  <macro name=\"section-zh\">\n    <choose>\n      <if is-numeric=\"section\">\n        <text value=\"第\"/>\n        <number variable=\"section\"/>\n        <label variable=\"section\"/>\n      </if>\n      <else>\n        <text variable=\"section\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-zh\">\n    <text variable=\"publisher\"/>\n    <choose>\n      <if type=\"post webpage\">\n        <text macro=\"issuance-zh\"/>\n      </if>\n      <else>\n        <text macro=\"issuance-zh\"/>\n        <choose>\n          <if type=\"thesis\">\n            <choose>\n              <if variable=\"genre\">\n                <text variable=\"genre\"/>\n              </if>\n              <else>\n                <text value=\"博士学位论文\"/>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <text term=\"edition\" form=\"short\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issuance-zh\">\n    <choose>\n      <if type=\"article-magazine article-newspaper legal_case legislation post post-weblog webpage\" match=\"any\">\n        <date variable=\"issued\" form=\"text\"/>\n      </if>\n      <else>\n        <date variable=\"issued\" form=\"text\" date-parts=\"year\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator-zh\">\n    <choose>\n      <if is-numeric=\"locator\">\n        <text value=\"第\"/>\n        <text variable=\"locator\"/>\n        <choose>\n          <if locator=\"page\" type=\"article-newspaper\" match=\"all\">\n            <text value=\"版\"/>\n          </if>\n          <else>\n            <label variable=\"locator\" form=\"short\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"locator\"/>\n      </else>\n    </choose>\n  </macro>\n  <!-- 报纸的出版信息，一般注明年、月、日;必要时，注明版次。 -->\n  <macro name=\"locator-or-page-zh\">\n    <choose>\n      <if variable=\"locator\">\n        <text macro=\"locator-zh\"/>\n      </if>\n      <else-if variable=\"page\" type=\"article-newspaper\" match=\"all\">\n        <choose>\n          <if is-numeric=\"page\">\n            <text value=\"第\"/>\n            <text variable=\"page\"/>\n            <text value=\"版\"/>\n          </if>\n          <else>\n            <text variable=\"page\"/>\n          </else>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-layout-zh\">\n    <choose>\n      <if position=\"ibid-with-locator\">\n        <text term=\"ibid\"/>\n        <text macro=\"locator-zh\" prefix=\"，\"/>\n      </if>\n      <else-if position=\"ibid\">\n        <text term=\"ibid\"/>\n      </else-if>\n      <else-if position=\"subsequent\">\n        <choose>\n          <if disambiguate=\"true\">\n            <group delimiter=\"，\">\n              <text variable=\"first-reference-note-number\" prefix=\"同前注〔\" suffix=\"〕\"/>\n              <group delimiter=\"：\">\n                <text macro=\"author-short-zh\"/>\n                <text variable=\"title\" form=\"short\" prefix=\"《\" suffix=\"》\"/>\n              </group>\n              <text macro=\"locator-zh\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\"，\">\n              <text term=\"op-cit\"/>\n              <text macro=\"author-short-zh\" suffix=\"书\"/>\n              <text macro=\"locator-zh\"/>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text macro=\"source-zh\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"citation-number\">\n    <text variable=\"citation-number\" prefix=\"〔\" suffix=\"〕\"/>\n  </macro>\n  <!-- 作者人数为三人或者三人以上的，原则上第一次列明全部，再次引用时可以只列第一作者，后加“等”。 -->\n  <!-- 作者人数众多，不便全部列明的，可以根据情况列 1-2 个，后面加“等”。 -->\n  <citation et-al-min=\"6\" et-al-use-first=\"2\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\">\n    <layout suffix=\"。\" delimiter=\"；\">\n      <text macro=\"citation-layout-zh\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" et-al-min=\"6\" et-al-use-first=\"2\">\n    <layout suffix=\"。\">\n      <text macro=\"citation-number\"/>\n      <text macro=\"source-zh\"/>\n    </layout>\n  </bibliography>\n</style>\n"