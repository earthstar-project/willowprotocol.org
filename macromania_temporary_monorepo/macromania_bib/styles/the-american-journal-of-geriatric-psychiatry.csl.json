"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-US\">\n  <info>\n    <title>The American Journal of Geriatric Psychiatry</title>\n    <title-short>AJGP</title-short>\n    <id>http://www.zotero.org/styles/the-american-journal-of-geriatric-psychiatry</id>\n    <link href=\"http://www.zotero.org/styles/the-american-journal-of-geriatric-psychiatry\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/mary-ann-liebert-vancouver\" rel=\"template\"/>\n    <link href=\"https://www.editorialmanager.com/jgp/default.aspx\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1064-7481</issn>\n    <eissn>1545-7214</eissn>\n    <updated>2020-03-10T10:24:10+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"verb\" text-case=\"capitalize-first\"/>\n      <name prefix=\" \" delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <group>\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text macro=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-newspaper article-journal\" match=\"none\">\n        <choose>\n          <if variable=\"URL\">\n            <group delimiter=\". \">\n              <choose>\n                <if type=\"webpage post post-weblog\" match=\"any\">\n                  <date form=\"text\" variable=\"issued\"/>\n                </if>\n              </choose>\n              <group delimiter=\" \">\n                <text term=\"available at\" text-case=\"capitalize-first\"/>\n                <text variable=\"URL\"/>\n              </group>\n              <group>\n                <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"month\" suffix=\" \"/>\n                  <date-part name=\"day\" suffix=\", \"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"normal\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n      <group prefix=\"(\" suffix=\")\">\n        <label variable=\"locator\" form=\"short\" strip-periods=\"true\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" prefix=\": \"/>\n      <choose>\n        <if type=\"bill book graphic legislation motion_picture report song\" match=\"any\">\n          <group suffix=\".\" prefix=\". \" delimiter=\" \">\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n              <text variable=\"volume\" suffix=\".\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text macro=\"publisher\" prefix=\" \"/>\n          <group prefix=\", \" suffix=\".\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n          <group prefix=\", \" delimiter=\" \">\n            <text term=\"in\"/>\n            <text variable=\"container-title\" text-case=\"title\" font-style=\"normal\" suffix=\".\"/>\n            <text macro=\"editor\" suffix=\",\"/>\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n              <text variable=\"volume\" suffix=\".\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <group suffix=\".\">\n              <text macro=\"publisher\"/>\n              <group delimiter=\", \" prefix=\", \" suffix=\".\">\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n                <group delimiter=\" \">\n                  <label variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <text variable=\"container-title\" prefix=\". \" suffix=\". \"/>\n          <choose>\n            <if variable=\"URL\">\n              <group delimiter=\". \" suffix=\".\">\n                <text variable=\"URL\"/>\n                <group prefix=\"Published \">\n                  <date variable=\"issued\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n                <group>\n                  <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n                  <date variable=\"accessed\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\":\" suffix=\".\">\n                <group>\n                  <date variable=\"issued\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"legal_case\">\n          <group suffix=\",\" prefix=\". \" delimiter=\" \">\n            <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <group prefix=\" \" delimiter=\" \">\n            <text variable=\"container-title\"/>\n            <text variable=\"volume\"/>\n          </group>\n          <text variable=\"page\" prefix=\", \" suffix=\" \"/>\n          <group prefix=\"(\" suffix=\").\" delimiter=\" \">\n            <text variable=\"authority\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <text variable=\"container-title\" prefix=\". \" suffix=\".\"/>\n        </else-if>\n        <else>\n          <text macro=\"editor\" prefix=\". \" suffix=\".\"/>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n            <group delimiter=\"; \" prefix=\" \">\n              <choose>\n                <if variable=\"issue volume\" match=\"any\">\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                </if>\n                <else>\n                  <date variable=\"issued\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </else>\n              </choose>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"