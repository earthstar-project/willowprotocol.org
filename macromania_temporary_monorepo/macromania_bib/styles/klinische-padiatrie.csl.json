uffix=\". \" variable=\"editor\">\n      <name sort-separator=\" \" name-as-sort-order=\"all\" initialize-with=\"\" delimiter-precedes-last=\"always\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\" \">\n      <group delimiter=\", \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"DOI\" prefix=\", doi:\"/>\n      </if>\n      <else-if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\"; \" prefix=\". \">\n          <group delimiter=\": \">\n            <text term=\"internet\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group delimiter=\": \">\n            <text term=\"accessed\"/>\n            <date form=\"numeric\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <text form=\"short\" variable=\"container-title\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number form=\"ordinal\" variable=\"edition\"/>\n          <text form=\"short\" term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text suffix=\".\" variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation>\n    <layout delimiter=\", \" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout vertical-align=\"baseline\" font-variant=\"normal\">\n      <text variable=\"citation-number\" strip-periods=\"false\" quotes=\"false\" vertical-align=\"baseline\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\".\">\n            <text suffix=\": \" term=\"in\" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\"/>\n          </group>\n          <text prefix=\" \" macro=\"publisher\"/>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text macro=\"journal-title\"/>\n            <date suffix=\";\" variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n            <text variable=\"volume\"/>\n          </group>\n          <text prefix=\": \" variable=\"page\"/>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"