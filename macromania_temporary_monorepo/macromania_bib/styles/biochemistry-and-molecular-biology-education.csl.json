"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Biochemistry and Molecular Biology Education</title>\n    <title-short>BAMBED</title-short>\n    <id>http://www.zotero.org/styles/biochemistry-and-molecular-biology-education</id>\n    <link href=\"http://www.zotero.org/styles/biochemistry-and-molecular-biology-education\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-journal-of-climate-change\" rel=\"template\"/>\n    <link href=\"http://iubmb.onlinelibrary.wiley.com/hub/journal/10.1002/(ISSN)1539-3429/about/author-guidelines.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>1470-8175</issn>\n    <eissn>1539-3429</eissn>\n    <updated>2017-02-08T12:12:23+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\">accessed on</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\" \">\n      <name initialize-with=\". \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" text-case=\"capitalize-first\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" suffix=\", \"/>\n    <text variable=\"publisher-place\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"7\" et-al-use-first=\"6\">\n    <layout suffix=\". \">\n      <text variable=\"citation-number\"/>\n      <text macro=\"author\" prefix=\" \" suffix=\" \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"year-date\" prefix=\"(\" suffix=\") \"/>\n            <text variable=\"title\" suffix=\". \"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text term=\"in\" suffix=\" \"/>\n          <text macro=\"editor\" suffix=\" \"/>\n          <text macro=\"year-date\" prefix=\"(\" suffix=\") \"/>\n          <text variable=\"container-title\" form=\"short\" text-case=\"title\" suffix=\", \"/>\n          <text macro=\"edition\" suffix=\", \"/>\n          <text macro=\"publisher\" suffix=\", \"/>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\" \">\n            <text variable=\"title\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\") \"/>\n            <text variable=\"number\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"year-date\" prefix=\"(\" suffix=\") \"/>\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <group delimiter=\" \">\n            <text variable=\"title\"/>\n            <text macro=\"year-date\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <group delimiter=\" \" suffix=\".\">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date form=\"text\" variable=\"accessed\"/>\n            </group>\n            <group delimiter=\": \">\n              <text term=\"available at\" text-case=\"capitalize-first\"/>\n              <text variable=\"URL\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text variable=\"title\" suffix=\".\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\") \"/>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"volume\" font-weight=\"bold\" suffix=\",\"/>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"