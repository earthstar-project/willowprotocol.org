"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Peptide Science</title>\n    <title-short>J. Pept. Sci.</title-short>\n    <id>http://www.zotero.org/styles/journal-of-peptide-science</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-peptide-science\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-chemical-society\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1002/%28ISSN%291099-1387/homepage/ForAuthors.html#Ref\" rel=\"documentation\"/>\n    <link href=\"http://onlinelibrary.wiley.com/doi/10.1002/psc.2633/pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Philipp Zumstein</name>\n      <uri>https://github.com/zuphilip</uri>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <issn>1075-2617</issn>\n    <eissn>1099-1387</eissn>\n    <summary>Style for the Journal of Peptide Science how it seems mostly used in current articles (The style guide seems not to reflect the formating of bibliographies of current articles).</summary>\n    <updated>2019-03-25T12:01:26+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\".\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" suffix=\".\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <text variable=\"collection-title\"/>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"book-container\">\n    <group delimiter=\" \">\n      <text macro=\"title\" suffix=\".\"/>\n      <text term=\"in\" text-case=\"capitalize-first\"/>\n      <text variable=\"container-title\" font-style=\"italic\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"full-issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n      <date-part name=\"day\" suffix=\", \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\" suffix=\" \"/>\n      <choose>\n        <if type=\"article-magazine\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"full-issued\" suffix=\",\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \">\n            <group delimiter=\", \">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"volume\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text macro=\"series\"/>\n              <text macro=\"edition\"/>\n              <choose>\n                <if type=\"report\">\n                  <group delimiter=\" \">\n                    <text variable=\"genre\"/>\n                    <text variable=\"number\"/>\n                  </group>\n                </if>\n              </choose>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"number\"/>\n            </group>\n            <date variable=\"issued\" form=\"text\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <group delimiter=\", \">\n              <text macro=\"book-container\"/>\n              <text macro=\"volume\"/>\n              <text macro=\"series\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <group delimiter=\"; \">\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"issued\"/>\n              </group>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\" \">\n            <text variable=\"title\"/>\n            <text variable=\"URL\"/>\n            <date variable=\"accessed\" prefix=\"(accessed \" suffix=\")\" delimiter=\" \">\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n            <group delimiter=\", \">\n              <group delimiter=\";\">\n                <text macro=\"issued\" font-weight=\"normal\"/>\n                <group delimiter=\":\">\n                  <group>\n                    <text variable=\"volume\" font-style=\"normal\" font-weight=\"normal\"/>\n                    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"issued\"/>\n              <text variable=\"volume\" font-style=\"italic\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <group delimiter=\": \" prefix=\", \">\n        <text value=\"DOI\"/>\n        <text variable=\"DOI\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"