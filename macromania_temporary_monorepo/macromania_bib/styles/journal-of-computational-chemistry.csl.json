"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Computational Chemistry</title>\n    <title-short>JCC</title-short>\n    <id>http://www.zotero.org/styles/journal-of-computational-chemistry</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-computational-chemistry\" rel=\"self\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1002/(ISSN)1096-987X/homepage/jcc_author_guidelines.html\" rel=\"documentation\"/>\n    <author>\n      <name>Wouter Boomsma</name>\n      <email>wb@bio.ku.dk</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <issn>0192-8651</issn>\n    <eissn>1096-987X</eissn>\n    <updated>2013-05-17T18:57:42+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"book paper-conference report\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" text-case=\"capitalize-first\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"thesis patent\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n        </date>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"patent\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\", \"/>\n        </date>\n      </if>\n    </choose>\n    <date variable=\"issued\">\n      <date-part name=\"year\" form=\"long\"/>\n    </date>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\". \" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"locators\">\n    <group delimiter=\", \">\n      <text macro=\"edition\"/>\n      <group delimiter=\", \">\n        <choose>\n          <if type=\"book\">\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n              <number variable=\"volume\" form=\"numeric\"/>\n            </group>\n            <group delimiter=\" \">\n              <text term=\"chapter\" text-case=\"capitalize-first\"/>\n              <number variable=\"chapter-number\" form=\"numeric\"/>\n            </group>\n          </if>\n          <else>\n            <number font-style=\"italic\" variable=\"volume\"/>\n          </else>\n        </choose>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"book chapter paper-conference\" match=\"any\">\n        <text variable=\"publisher\" suffix=\", \"/>\n        <text variable=\"publisher-place\"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if type=\"paper-conference\">\n        <choose>\n          <if variable=\"container-title\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"normal\" prefix=\"in \"/>\n              <text variable=\"event-place\"/>\n              <date variable=\"event-date\">\n                <date-part name=\"month\" form=\"short\" strip-periods=\"true\" suffix=\" \"/>\n                <date-part name=\"day\" suffix=\", \"/>\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <group>\n      <choose>\n        <if type=\"book\">\n          <label strip-periods=\"true\" suffix=\" \" variable=\"page\" form=\"short\"/>\n        </if>\n      </choose>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"doi\">\n    <text variable=\"DOI\" prefix=\"DOI:\"/>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\", \">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"book\">\n          <group delimiter=\"; \" suffix=\", \">\n            <text macro=\"title\" prefix=\"in \"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"issued\" font-weight=\"bold\"/>\n          </group>\n          <group delimiter=\", \" prefix=\"; \">\n            <text macro=\"locators\"/>\n            <text macro=\"page\"/>\n          </group>\n        </if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\", \">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <text macro=\"issued\" font-weight=\"bold\"/>\n            <choose>\n              <if variable=\"DOI\">\n                <text macro=\"doi\"/>\n              </if>\n              <else>\n                <text macro=\"locators\"/>\n                <text macro=\"page\"/>\n              </else>\n            </choose>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <text macro=\"event\" suffix=\"; \"/>\n          <group delimiter=\", \">\n            <text macro=\"issued\"/>\n            <text macro=\"editor\" suffix=\"; \"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <text variable=\"number\" suffix=\", \"/>\n          <text macro=\"issued\"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"URL\"/>\n            <group prefix=\"(\" suffix=\")\">\n              <date variable=\"accessed\" prefix=\"accessed \">\n                <date-part name=\"month\" suffix=\" \"/>\n                <date-part name=\"day\" suffix=\", \"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"locators\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"