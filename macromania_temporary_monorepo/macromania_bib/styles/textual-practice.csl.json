"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" default-locale=\"en-GB\">\n  <info>\n    <title>Textual Practice</title>\n    <title-short>TextPrac</title-short>\n    <id>http://www.zotero.org/styles/textual-practice</id>\n    <link href=\"http://www.zotero.org/styles/textual-practice\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/acta-philosophica\" rel=\"template\"/>\n    <link href=\"https://www.tandf.co.uk//journals/authors/style/layout/style_rtpr.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Jon Rubin</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"humanities\"/>\n    <issn>0950-236X</issn>\n    <eissn>1470-1308</eissn>\n    <summary>Textual Practice journal's citation style: footnotes plus Ibid. citation style</summary>\n    <updated>2019-02-23T00:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"editortranslator\" form=\"short\">ed. and trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-short\">\n    <group delimiter=\" \">\n      <names variable=\"author\">\n        <name form=\"short\" and=\"text\" delimiter=\", \"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <names variable=\"translator\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <label form=\"short\" prefix=\" \" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \" prefix=\" \">\n      <label form=\"short\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"series-title\">\n    <text variable=\"collection-title\" font-style=\"italic\" prefix=\"(\" suffix=\")\"/>\n  </macro>\n  <macro name=\"editor-container-title\">\n    <text term=\"in\" suffix=\" \"/>\n    <choose>\n      <if variable=\"container-author\">\n        <names variable=\"container-author\" suffix=\", \">\n          <name and=\"text\" delimiter=\" \"/>\n        </names>\n      </if>\n      <else-if variable=\"collection-editor\">\n        <names variable=\"collection-editor\">\n          <name and=\"text\" delimiter=\" \"/>\n          <label form=\"short\" prefix=\" (\" suffix=\"), \"/>\n        </names>\n      </else-if>\n    </choose>\n    <choose>\n      <if variable=\"editor\">\n        <names variable=\"editor\">\n          <name and=\"text\" delimiter=\" \"/>\n          <label form=\"short\" prefix=\" (\" suffix=\"), \"/>\n        </names>\n      </if>\n    </choose>\n    <text variable=\"container-title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"day\" form=\"numeric\" suffix=\"/\" range-delimiter=\"-\"/>\n      <date-part name=\"month\" suffix=\" \"/>\n      <date-part name=\"year\" range-delimiter=\"&#8211;\"/>\n    </date>\n  </macro>\n  <macro name=\"accessed\">\n    <date variable=\"accessed\">\n      <date-part name=\"day\" suffix=\"/\" range-delimiter=\"-\"/>\n      <date-part name=\"month\" form=\"numeric\" suffix=\"/\"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" suffix=\", \"/>\n  </macro>\n  <macro name=\"publisher-place\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n  </macro>\n  <macro name=\"journal\">\n    <choose>\n      <if variable=\"container-title\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volref\">\n    <choose>\n      <if variable=\"volume number-of-volumes\" match=\"all\">\n        <group delimiter=\" \">\n          <text term=\"volume\" form=\"short\" plural=\"false\"/>\n          <text variable=\"volume\"/>\n          <text variable=\"number-of-volumes\" prefix=\" (\"/>\n          <text term=\"volume\" form=\"short\" plural=\"true\" prefix=\" \" suffix=\")\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <text term=\"volume\" form=\"short\" plural=\"false\"/>\n          <text variable=\"volume\"/>\n        </group>\n        <group delimiter=\" \">\n          <text variable=\"number-of-volumes\"/>\n          <text term=\"volume\" form=\"short\" plural=\"true\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pageref\">\n    <label variable=\"locator\" form=\"short\" suffix=\" \" prefix=\", \"/>\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"pages\">\n    <text term=\"page\" form=\"short\" plural=\"true\" suffix=\" \" prefix=\", \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"article-pageref-pages\">\n    <text macro=\"pages\"/>\n    <choose>\n      <if variable=\"locator\">\n        <text variable=\"locator\" prefix=\". \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"urlref\">\n    <text variable=\"URL\" prefix=\" [\" suffix=\"]\"/>\n    <group prefix=\", \">\n      <text term=\"accessed\" form=\"long\" suffix=\" \"/>\n      <text macro=\"accessed\"/>\n    </group>\n  </macro>\n  <citation>\n    <layout prefix=\"\" suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <text term=\"ibid\" font-style=\"italic\" suffix=\".\"/>\n          <text macro=\"pageref\"/>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\" font-style=\"italic\" suffix=\".\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"contributors-short\"/>\n            <text macro=\"title-short\"/>\n          </group>\n          <text macro=\"pageref\"/>\n        </else-if>\n        <else-if type=\"book\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"editor-translator\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"volref\"/>\n            <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n              <text macro=\"publisher-place\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"date\"/>\n            </group>\n          </group>\n          <text macro=\"pageref\"/>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"journal\"/>\n          </group>\n          <text variable=\"volume\" prefix=\", \"/>\n          <text variable=\"issue\" prefix=\".\"/>\n          <text macro=\"date\" prefix=\" (\" suffix=\")\"/>\n          <text macro=\"article-pageref-pages\"/>\n          <choose>\n            <if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </if>\n          </choose>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n          <group delimiter=\", \" suffix=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n          </group>\n          <text term=\"presented at\" suffix=\" \"/>\n          <text variable=\"container-title\" font-style=\"italic\" suffix=\", \"/>\n          <group delimiter=\" \" suffix=\", \">\n            <text term=\"section\"/>\n            <text variable=\"section\" font-style=\"italic\"/>\n          </group>\n          <choose>\n            <if type=\"article-magazine\">\n              <text variable=\"issue\" suffix=\"/\"/>\n              <text macro=\"date\"/>\n            </if>\n            <else>\n              <text macro=\"date\"/>\n            </else>\n          </choose>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group delimiter=\", \" suffix=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n          </group>\n          <group delimiter=\" \" suffix=\" \">\n            <text macro=\"editor-container-title\"/>\n            <text macro=\"series-title\"/>\n          </group>\n          <text macro=\"translator\" suffix=\" \"/>\n          <group delimiter=\", \">\n            <text macro=\"volref\"/>\n            <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n              <text macro=\"publisher-place\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"date\"/>\n            </group>\n          </group>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n          </group>\n          <text variable=\"genre\" prefix=\", \" suffix=\", \"/>\n          <text variable=\"publisher\" suffix=\", \"/>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else-if>\n        <else-if type=\"entry-encyclopedia entry-dictionary\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"editor-container-title\"/>\n            <text macro=\"volref\"/>\n          </group>\n          <choose>\n            <if variable=\"publisher publisher-place issued\" match=\"all\">\n              <text variable=\"publisher\" prefix=\", \" suffix=\", \"/>\n            </if>\n            <else-if variable=\"issued\">\n              <text macro=\"date\" prefix=\", \" suffix=\", \"/>\n            </else-if>\n          </choose>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"date\" prefix=\" (\" suffix=\")\"/>\n          </group>\n          <choose>\n            <if variable=\"locator\">\n              <text macro=\"pageref\"/>\n            </if>\n            <else-if variable=\"page\">\n              <text macro=\"pages\"/>\n            </else-if>\n            <else-if variable=\"URL\">\n              <text macro=\"urlref\"/>\n            </else-if>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"