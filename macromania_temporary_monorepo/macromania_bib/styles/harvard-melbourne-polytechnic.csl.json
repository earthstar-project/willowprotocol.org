"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Melbourne Polytechnic - Harvard</title>\n    <id>http://www.zotero.org/styles/harvard-melbourne-polytechnic</id>\n    <link href=\"http://www.zotero.org/styles/harvard-melbourne-polytechnic\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/style-manual-australian-government\" rel=\"template\"/>\n    <link href=\"https://melbournepolytechnic.spydus.com/reference.html\" rel=\"documentation\"/>\n    <author>\n      <name>mlewis</name>\n      <email>martinlewis@melbournepolytechnic.edu.au</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Harvard style for Melbourne Polytechnic</summary>\n    <updated>2016-08-02T01:55:02+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"edition\" form=\"short\">edn</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"symbol\" initialize-with=\"\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\"\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <choose>\n          <if type=\"broadcast\">\n            <text macro=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text macro=\"title\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <label form=\"short\" prefix=\" \" suffix=\" \"/>\n      <name form=\"short\" and=\"symbol\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text value=\"viewed\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" prefix=\" \"/>\n            <date-part name=\"month\" prefix=\" \"/>\n            <date-part name=\"year\" prefix=\" \"/>\n          </date>\n        </group>\n        <text variable=\"URL\" prefix=\",  \"/>\n      </if>\n      <else>\n        <group>\n          <text value=\"viewed\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" prefix=\" \"/>\n            <date-part name=\"month\" prefix=\" \"/>\n            <date-part name=\"year\" prefix=\" \"/>\n          </date>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis webpage\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"patent\">\n        <text variable=\"title\" font-style=\"italic\" quotes=\"false\"/>\n      </else-if>\n      <else-if type=\"broadcast\">\n        <text variable=\"title\" font-style=\"italic\" quotes=\"false\"/>\n      </else-if>\n      <else-if type=\"entry-encyclopedia\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"day-month\">\n    <date variable=\"issued\">\n      <date-part name=\"month\"/>\n      <date-part name=\"day\" prefix=\" \"/>\n    </date>\n  </macro>\n  <macro name=\"month-day\">\n    <date variable=\"issued\">\n      <date-part name=\"day\"/>\n      <date-part name=\"month\" prefix=\" \"/>\n    </date>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <choose>\n          <if type=\"personal_communication\">\n            <group prefix=\"pers-comm, \">\n              <text macro=\"month-day\"/>\n            </group>\n          </if>\n        </choose>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <choose>\n        <if type=\"broadcast\">\n          <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n        </if>\n      </choose>\n      <text macro=\"year-date\" prefix=\" \"/>\n      <choose>\n        <if type=\"bill book legal_case legislation report\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\" prefix=\", \"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text macro=\"publisher\"/>\n        </if>\n        <else-if type=\"song\">\n          <group delimiter=\", \">\n            <text macro=\"title\" prefix=\", \"/>\n          </group>\n          <group prefix=\", \">\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"motion_picture\">\n          <group delimiter=\", \">\n            <text macro=\"title\" prefix=\", \"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text variable=\"medium\" prefix=\", \"/>\n          <text macro=\"publisher\"/>\n          <text variable=\"note\" prefix=\", \"/>\n        </else-if>\n        <else-if type=\"graphic\">\n          <text macro=\"title\" prefix=\", \"/>\n          <group prefix=\", \">\n            <text variable=\"medium\"/>\n          </group>\n          <group prefix=\", held at \">\n            <text variable=\"archive\"/>\n          </group>\n          <group prefix=\", \">\n            <text variable=\"archive_location\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\",\">\n            <text macro=\"title\"/>\n            <text term=\"in\" prefix=\", \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n            <text variable=\"collection-title\" prefix=\", \"/>\n            <text variable=\"event\" prefix=\" \"/>\n            <group>\n              <text macro=\"publisher\"/>\n            </group>\n            <group prefix=\", \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\" prefix=\", \"/>\n            <text value=\"thesis\" prefix=\", \"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <group prefix=\", \">\n            <text macro=\"title\"/>\n          </group>\n          <group prefix=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group prefix=\", \">\n              <text variable=\"volume\" prefix=\"Vol. \"/>\n              <text variable=\"issue\" prefix=\", No. \"/>\n            </group>\n            <group prefix=\", \">\n              <text macro=\"day-month\"/>\n            </group>\n            <group prefix=\", \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\" prefix=\". \"/>\n            </group>\n          </group>\n          <group>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"entry-encyclopedia\">\n          <group prefix=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </group>\n          <group prefix=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </group>\n          <group>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group prefix=\", \">\n            <text macro=\"title\"/>\n          </group>\n          <group prefix=\", \">\n            <text variable=\"container-title\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <group prefix=\", \">\n            <text variable=\"title\" font-style=\"italic\" quotes=\"false\"/>\n          </group>\n          <text variable=\"genre\" prefix=\", \"/>\n          <text variable=\"event\" prefix=\", \"/>\n          <text variable=\"event-place\" prefix=\", \"/>\n        </else-if>\n        <else>\n          <group prefix=\", \">\n            <text macro=\"title\"/>\n          </group>\n          <group prefix=\", \">\n            <choose>\n              <if type=\"broadcast\">\n                <text variable=\"medium\"/>\n              </if>\n              <else>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n              </else>\n            </choose>\n            <text variable=\"genre\" prefix=\", \"/>\n            <group prefix=\" \">\n              <text macro=\"day-month\"/>\n            </group>\n            <group prefix=\", \">\n              <text variable=\"volume\" prefix=\"Vol. \"/>\n              <text variable=\"issue\" prefix=\", No. \"/>\n            </group>\n            <group prefix=\", \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\" prefix=\". \"/>\n            </group>\n          </group>\n          <group>\n            <text macro=\"publisher\"/>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\", \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"