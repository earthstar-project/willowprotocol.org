"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-GB\">\n  <info>\n    <title>Pediatric Allergy and Immunology</title>\n    <id>http://www.zotero.org/styles/pediatric-allergy-and-immunology</id>\n    <link href=\"http://www.zotero.org/styles/pediatric-allergy-and-immunology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/allergy\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/(ISSN)1399-3038/homepage/Society.html\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0905-6157</issn>\n    <eissn>1399-3038</eissn>\n    <updated>2013-04-02T02:07:39+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\" delimiter-precedes-et-al=\"never\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \" form=\"short\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\" \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"date\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\" doi:\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <text variable=\"URL\"/>\n            <group prefix=\" (\" suffix=\").\" delimiter=\" \">\n              <text term=\"accessed\"/>\n              <date variable=\"accessed\">\n                <date-part name=\"day\" prefix=\" \"/>\n                <date-part name=\"month\" prefix=\" \" form=\"short\" strip-periods=\"true\"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <!--Print full date as \"Published online first\" for\n        journal articles w/o volume and page range and with DOI or URL  -->\n    <choose>\n      <if type=\"article-journal\">\n        <choose>\n          <if variable=\"volume page\" match=\"none\">\n            <choose>\n              <if variable=\"DOI URL\" match=\"any\">\n                <group prefix=\"Published Online First: \">\n                  <date variable=\"issued\" delimiter=\" \">\n                    <date-part name=\"day\"/>\n                    <date-part name=\"month\" form=\"long\" text-case=\"capitalize-first\"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </if>\n              <else>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"edition\" prefix=\" \"/>\n          <text macro=\"publisher\" prefix=\" \"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\".\">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </group>\n          <group delimiter=\": \" prefix=\" \">\n            <text macro=\"publisher\"/>\n            <text variable=\"page\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group suffix=\".\">\n            <text macro=\"date\" prefix=\" \"/>\n            <group prefix=\": \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"