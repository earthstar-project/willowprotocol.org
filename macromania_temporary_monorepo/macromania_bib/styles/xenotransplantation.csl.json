"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Xenotransplantation</title>\n    <id>http://www.zotero.org/styles/xenotransplantation</id>\n    <link href=\"http://www.zotero.org/styles/xenotransplantation\" rel=\"self\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/%28ISSN%291399-3089/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0908-665X</issn>\n    <eissn>1399-3089</eissn>\n    <updated>2013-01-30T04:41:27+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name font-variant=\"small-caps\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n        <label form=\"short\" prefix=\", \"/>\n        <et-al font-variant=\"normal\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter-precedes-last=\"always\" initialize-with=\"\" delimiter=\", \"/>\n      <label form=\"long\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\" quotes=\"false\"/>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"journal\">\n    <group delimiter=\" \">\n      <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n      <text macro=\"year\" suffix=\";\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"conference\">\n    <group delimiter=\", \">\n      <text variable=\"container-title\"/>\n      <group delimiter=\" \">\n        <text variable=\"publisher\"/>\n        <text macro=\"year\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"book-publisher\">\n    <group delimiter=\", \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"year\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation>\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <group delimiter=\":\">\n        <text variable=\"citation-number\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"6\" et-al-use-first=\"3\" second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\" suffix=\". \"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"edition\" suffix=\" \"/>\n          <text macro=\"book-publisher\" suffix=\".\"/>\n        </if>\n        <else-if type=\"paper-conference\">\n          <group suffix=\".\" delimiter=\", \">\n            <text macro=\"conference\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n          <text macro=\"editor\" suffix=\" \"/>\n          <text variable=\"container-title\" suffix=\". \"/>\n          <text macro=\"edition\" suffix=\" \"/>\n          <group suffix=\".\">\n            <text macro=\"book-publisher\"/>\n            <text variable=\"page\" prefix=\": \"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group suffix=\".\">\n            <text macro=\"journal\"/>\n            <text variable=\"page\" prefix=\": \"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\" delimiter=\" \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text macro=\"year\"/>\n            <text variable=\"page\"/>\n          </group>\n          <text variable=\"URL\" prefix=\" \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"