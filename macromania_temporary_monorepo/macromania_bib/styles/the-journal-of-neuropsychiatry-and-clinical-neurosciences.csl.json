"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>The Journal of Neuropsychiatry and Clinical Neurosciences</title>\n    <id>http://www.zotero.org/styles/the-journal-of-neuropsychiatry-and-clinical-neurosciences</id>\n    <link href=\"http://www.zotero.org/styles/the-journal-of-neuropsychiatry-and-clinical-neurosciences\" rel=\"self\"/>\n    <link href=\"http://neuro.psychiatryonline.org/neuro_ifora.aspx\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0895-0172</issn>\n    <eissn>1545-7222</eissn>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"verb\" suffix=\" \"/>\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \">\n          <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n            <text term=\"cited\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"year\"/>\n              <date-part name=\"month\" prefix=\" \" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"day\" prefix=\" \"/>\n            </date>\n          </group>\n          <group>\n            <text value=\"Available from: \"/>\n            <text variable=\"URL\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if variable=\"URL\">\n          <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n    <text variable=\"page\"/>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"{\" suffix=\"}\" delimiter=\"; \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" et-al-min=\"4\" et-al-use-first=\"3\" line-spacing=\"2\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\": \"/>\n      <text macro=\"title\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \" prefix=\", \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\" prefix=\" \"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text term=\"in\" suffix=\" \" prefix=\", \"/>\n          <group delimiter=\", \" suffix=\". \">\n            <text variable=\"container-title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text macro=\"publisher\"/>\n          <text macro=\"pages\" prefix=\", \"/>\n        </else-if>\n        <else>\n          <text variable=\"container-title\" suffix=\" \" prefix=\". \" form=\"short\"/>\n          <date variable=\"issued\" suffix=\"; \">\n            <date-part name=\"year\"/>\n          </date>\n          <group delimiter=\":\">\n            <text variable=\"volume\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"