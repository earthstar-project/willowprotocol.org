"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"de-DE\">\n  <info>\n    <title>Westf채lische Wilhelms-Universit채t M체nster - Medizinische Fakult채t (Deutsch)</title>\n    <title-short>WWU</title-short>\n    <id>http://www.zotero.org/styles/westfalische-wilhelms-universitat-munster-medizinische-fakultat</id>\n    <link href=\"http://www.zotero.org/styles/westfalische-wilhelms-universitat-munster-medizinische-fakultat\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/pnas\" rel=\"template\"/>\n    <link href=\"https://campus.uni-muenster.de/fakultaet/fakultaet/dekanat/promotionen/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <updated>2018-12-05T15:40:18+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"et-al\">et al.</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" prefix=\" \" delimiter=\", \">\n      <name initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\" suffix=\". \"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"doi:\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <text value=\"Available at:\" suffix=\" \"/>\n            <text variable=\"URL\"/>\n            <group prefix=\" [\" suffix=\"]\">\n              <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n              <date variable=\"accessed\">\n                <date-part name=\"month\" suffix=\" \"/>\n                <date-part name=\"day\" suffix=\", \"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" strip-periods=\"true\"/>\n      <number variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography delimiter-precedes-et-al=\"always\" et-al-min=\"11\" et-al-use-first=\"10\" second-field-align=\"flush\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \" prefix=\" \">\n            <text variable=\"title\" font-style=\"normal\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\" strip-periods=\"false\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"edition\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\" prefix=\" \"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <group delimiter=\", \">\n                <text macro=\"editor\"/>\n                <text variable=\"container-title\" text-case=\"title\" font-style=\"normal\"/>\n                <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n              </group>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\" prefix=\" \"/>\n              <text macro=\"volume\"/>\n              <text macro=\"edition\" prefix=\" \"/>\n            </group>\n          </group>\n          <group prefix=\" \">\n            <label strip-periods=\"false\" suffix=\" \" variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\" match=\"any\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text variable=\"title\"/>\n            <group delimiter=\" \">\n              <choose>\n                <if variable=\"genre\">\n                  <text variable=\"genre\"/>\n                </if>\n                <else>\n                  <text value=\"Dissertation\"/>\n                </else>\n              </choose>\n              <group prefix=\"(\" suffix=\")\" delimiter=\", \">\n                <text variable=\"publisher\"/>\n                <text variable=\"publisher-place\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text variable=\"title\" prefix=\" \"/>\n            <group delimiter=\": \">\n              <group delimiter=\" \">\n                <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"normal\"/>\n                <group>\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"