"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"fr-FR\">\n  <!-- === Metadatas ======================================================= -->\n  <info>\n    <title>Presses universitaires de Strasbourg (note, Français)</title>\n    <title-short>PUS</title-short>\n    <id>http://www.zotero.org/styles/presses-universitaires-de-strasbourg-note</id>\n    <link href=\"http://www.zotero.org/styles/presses-universitaires-de-strasbourg-note\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/iso690-note-fr\" rel=\"template\"/>\n    <link href=\"http://pus.unistra.fr/html/WYSIWYGfiles/files/Note-aux-auteurs_finale.pdf\" rel=\"documentation\"/>\n    <link href=\"https://git.unistra.fr/dev-pus/styles-csl/blob/master/README.md#pus-note-ibid-france\" rel=\"documentation\"/>\n    <author>\n      <name>Étienne Nadji</name>\n      <email>etnadji@eml.cc</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"generic-base\"/>\n    <summary>Style for the Presses Universitaires de Strasbourg, \n      according to 2019 guidelines for authors.</summary>\n    <updated>2021-06-02T00:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <!-- ===================================================================== -->\n  <!-- === Terms =========================================================== -->\n  <!-- Seuls les termes différents || nouveaux sont listés ici -->\n  <!-- Cf. https://github.com/citation-style-language/locales/blob/master/locales-fr-FR.xml -->\n  <locale>\n    <terms>\n      <!-- Termes standards redéfinis -->\n      <term name=\"in\">dans</term>\n      <term name=\"online\">en&#160;ligne</term>\n      <term name=\"accessed\">consulté&#160;le</term>\n      <term name=\"available at\">disponible à l'adresse</term>\n      <term name=\"editor\" form=\"short\">dir.</term>\n      <term name=\"cited\">, cité</term>\n      <term name=\"translator\" form=\"verb-short\">traduction de</term>\n      <term name=\"translator\" form=\"verb\">traduction de</term>\n      <term name=\"no date\" form=\"short\"/>\n    </terms>\n  </locale>\n  <!-- ===================================================================== -->\n  <!-- === Reference fields ================================================ -->\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\" \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <!-- Called by secondary-responsability-->\n    <names variable=\"translator\">\n      <!-- <label text-case=\"capitalize-first\" form=\"verb-short\" suffix=\" \"/> -->\n      <label form=\"verb-short\" suffix=\" \"/>\n      <name and=\"text\" sort-separator=\" \" delimiter=\" \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"responsability\">\n    <choose>\n      <if type=\"patent\"/>\n      <else-if variable=\"author editor\" match=\"any\">\n        <choose>\n          <if variable=\"author\">\n            <text macro=\"author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"patent-inventor\">\n    <!-- Called by secondary-responsability-->\n    <text value=\"Inventeur(s)\" suffix=\"&#8239;: \"/>\n    <names variable=\"author\">\n      <name and=\"text\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"secondary-responsability\">\n    <!-- FIXME : improve management of secondary responsabiliy for other types -->\n    <choose>\n      <if type=\"patent\">\n        <text macro=\"patent-inventor\"/>\n      </if>\n    </choose>\n    <text macro=\"translator\"/>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\" \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"container-responsability\">\n    <choose>\n      <if variable=\"container-author editor\" match=\"any\">\n        <choose>\n          <if variable=\"container-author\">\n            <text macro=\"container-author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"paper-conference speech chapter article-journal article-magazine article-newspaper post-weblog post webpage broadcast\" match=\"any\">\n        <choose>\n          <if variable=\"container-title\">\n            <text term=\"open-quote\"/>\n            <text variable=\"title\"/>\n            <text term=\"close-quote\" suffix=\", \"/>\n            <choose>\n              <if type=\"article-journal article-magazine article-newspaper\" match=\"none\">\n                <text term=\"in\" suffix=\" \"/>\n              </if>\n            </choose>\n            <choose>\n              <if variable=\"container-author editor\" match=\"any\">\n                <text macro=\"container-responsability\"/>\n                <choose>\n                  <if variable=\"container-title event\" match=\"any\">\n                    <text value=\", \"/>\n                  </if>\n                </choose>\n              </if>\n            </choose>\n            <choose>\n              <if variable=\"container-title\">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text value=\", \"/>\n              </if>\n              <else>\n                <text variable=\"event\" font-style=\"italic\"/>\n                <!-- variable event: used for congresses-->\n                <text value=\",\"/>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text value=\",\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text value=\",\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"thesis-report-genre\">\n    <choose>\n      <if type=\"report\">\n        <choose>\n          <if variable=\"number\">\n            <choose>\n              <if variable=\"genre\">\n                <group delimiter=\" \">\n                  <text variable=\"genre\"/>\n                  <text term=\"issue\" form=\"short\"/>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\" \">\n                  <text value=\"Rapport\"/>\n                  <text term=\"issue\" form=\"short\"/>\n                </group>\n              </else>\n            </choose>\n            <text variable=\"number\"/>\n          </if>\n          <else-if variable=\"genre\">\n            <text variable=\"genre\"/>\n          </else-if>\n        </choose>\n      </if>\n      <else-if type=\"thesis\">\n        <text variable=\"genre\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <!-- If the volume field contains \"Tome 1, vol. 2\", this string is displayed. If it contains only a number, 'vol.' is added before-->\n    <choose>\n      <if type=\"article-newspaper article-journal article-magazine\" match=\"none\">\n        <!-- For article-newspaper article-journal article-magazine, volume is managed in 'issue-pagination-locator' macro-->\n        <choose>\n          <if is-numeric=\"volume\">\n            <!-- <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\"&#160;\"/> -->\n            <text term=\"volume\" form=\"short\" suffix=\"&#160;\"/>\n            <text variable=\"volume\"/>\n          </if>\n          <else>\n            <text variable=\"volume\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"numberOfVolumes\">\n    <!-- If the number-of-volume field contains '3 Tomes, 2 vol.', this string is displayed. If it contains only a number, 'vol.' is added after-->\n    <choose>\n      <if is-numeric=\"number-of-volumes\">\n        <text variable=\"number-of-volumes\"/>\n        <text term=\"volume\" form=\"short\" prefix=\"&#160;\"/>\n      </if>\n      <else>\n        <text variable=\"number-of-volumes\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <choose>\n      <if type=\"patent\" match=\"any\">\n        <text variable=\"number\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if variable=\"URL DOI\" match=\"any\">\n        <text term=\"online\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"medium_designation\">\n    <choose>\n      <if type=\"book\">\n        <choose>\n          <if variable=\"medium version\" match=\"any\">\n            <text value=\"logiciel\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"map\">\n        <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n          <choose>\n            <if variable=\"genre\">\n              <text variable=\"genre\"/>\n            </if>\n            <else>\n              <text value=\"carte\"/>\n            </else>\n          </choose>\n          <text macro=\"online\"/>\n        </group>\n      </else-if>\n      <else-if type=\"manuscript\">\n        <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n          <choose>\n            <if variable=\"genre\">\n              <text variable=\"genre\"/>\n            </if>\n            <else>\n              <text value=\"manuscrit\"/>\n            </else>\n          </choose>\n          <text macro=\"online\"/>\n        </group>\n      </else-if>\n      <else-if type=\"graphic\">\n        <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n          <choose>\n            <if variable=\"medium\">\n              <text variable=\"medium\"/>\n            </if>\n            <else>\n              <text value=\"image\"/>\n            </else>\n          </choose>\n          <text macro=\"online\"/>\n        </group>\n      </else-if>\n      <else-if type=\"song\">\n        <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n          <choose>\n            <if variable=\"medium\">\n              <text variable=\"medium\"/>\n            </if>\n          </choose>\n          <text macro=\"online\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"article-journal\">\n            <date variable=\"issued\">\n              <date-part name=\"month\" form=\"long\" text-case=\"capitalize-first\" suffix=\" \"/>\n              <date-part name=\"year\" form=\"long\"/>\n            </date>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n              <date-part name=\"year\" form=\"long\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" text-case=\"capitalize-first\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if type=\"book map thesis report chapter entry entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\"/>\n          </if>\n          <!-- <else> -->\n          <!-- <text value=\"s.&#160;l.\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/> -->\n          <!-- </else> -->\n        </choose>\n      </if>\n      <else-if type=\"article article-journal article-newspaper article-magazine\"/>\n      <else>\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\"/>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"book map thesis report chapter entry entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\"/>\n          </if>\n          <!-- <else> -->\n          <!-- <text value=\"s.&#160;n.\" prefix=\"[\" suffix=\"]\"/> -->\n          <!-- </else> -->\n        </choose>\n      </if>\n      <else-if type=\"article article-journal article-newspaper article-magazine\"/>\n      <else>\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\"/>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" prefix=\"&#160;\"/>\n            <date-part name=\"month\" prefix=\"&#160;\"/>\n            <date-part name=\"year\" prefix=\"&#160;\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\", \">\n      <text prefix=\"«&#160;\" suffix=\"&#160;»\" variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"issue-pagination-locator\">\n    <choose>\n      <if type=\"article-newspaper article-journal article-magazine\" match=\"any\">\n        <group delimiter=\", \">\n          <group>\n            <!-- <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\"&#160;\"/> -->\n            <text term=\"volume\" form=\"short\" suffix=\"&#160;\"/>\n            <text variable=\"volume\"/>\n          </group>\n          <group>\n            <text term=\"issue\" form=\"short\" suffix=\"&#160;\"/>\n            <text variable=\"issue\"/>\n          </group>\n          <group>\n            <text macro=\"date\"/>\n          </group>\n          <choose>\n            <if variable=\"locator\">\n              <group>\n                <label variable=\"locator\" form=\"short\" suffix=\"&#160;\"/>\n                <text variable=\"locator\"/>\n              </group>\n            </if>\n            <else>\n              <group>\n                <text term=\"page\" form=\"short\" suffix=\"&#160;\"/>\n                <text variable=\"page\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </if>\n      <else-if variable=\"locator\">\n        <group>\n          <label variable=\"locator\" form=\"short\" suffix=\"&#160;\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter entry entry-dictionary entry-encyclopedia paper-conference\" match=\"any\">\n        <group>\n          <text term=\"page\" form=\"short\" suffix=\"&#160;\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"report patent book thesis manuscript\" match=\"any\"/>\n    </choose>\n  </macro>\n  <!-- Macros appelées par la macro \"access\" -->\n  <macro name=\"isbn\">\n    <text variable=\"ISBN\" prefix=\"ISBN&#160;\"/>\n  </macro>\n  <macro name=\"doi\">\n    <text prefix=\"DOI&#160;:&#160;\" variable=\"DOI\"/>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <!-- <text term=\"available at\" suffix=\"&#8239;: \" text-case=\"capitalize-first\"/> -->\n          <text term=\"available at\" suffix=\"&#8239;: \"/>\n          <text variable=\"URL\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\"&#8239;:&#160;\">\n      <text variable=\"archive\" text-case=\"capitalize-first\"/>\n      <text variable=\"archive_location\" text-case=\"capitalize-first\"/>\n      <text variable=\"call-number\"/>\n    </group>\n  </macro>\n  <!-- +++++++++++++++++++++++++++++++++++++ -->\n  <macro name=\"access\">\n    <group delimiter=\". \">\n      <choose>\n        <if variable=\"DOI\">\n          <text macro=\"doi\"/>\n        </if>\n        <else>\n          <text macro=\"url\"/>\n        </else>\n      </choose>\n      <text macro=\"archive\"/>\n    </group>\n  </macro>\n  <macro name=\"note\">\n    <text variable=\"note\" text-case=\"capitalize-first\"/>\n  </macro>\n  <!-- ===================================================================== -->\n  <!-- === Citation reference layout macros ================================ -->\n  <!-- Standard citation reference layout -->\n  <macro name=\"citation-reference-normal\">\n    <group delimiter=\", \">\n      <text macro=\"responsability\" suffix=\",\"/>\n      <group delimiter=\" \">\n        <group>\n          <text macro=\"title\"/>\n          <!-- volume : used for books and monographs (a volume of a multivolume book)-->\n          <text macro=\"volume\"/>\n        </group>\n        <!-- medium : [online], [map], [software], etc.-->\n        <text macro=\"medium_designation\"/>\n      </group>\n      <text macro=\"thesis-report-genre\"/>\n      <text macro=\"edition\"/>\n      <!-- secondary-responsability : used for patent inventors and translators -->\n      <!--  <text macro=\"secondary-responsability\"/> -->\n      <group delimiter=\", \">\n        <group delimiter=\", \">\n          <!-- <group delimiter=\"&#8239;: \"> -->\n          <group delimiter=\", \">\n            <text macro=\"publisher-place\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text macro=\"number\"/>\n        </group>\n        <group delimiter=\", \">\n          <choose>\n            <if type=\"article-newspaper article-journal article-magazine\" match=\"any\"/>\n            <else>\n              <text macro=\"date\"/>\n            </else>\n          </choose>\n          <text macro=\"numberOfVolumes\"/>\n          <text macro=\"issue-pagination-locator\"/>\n        </group>\n      </group>\n      <text macro=\"accessed\"/>\n      <!--      <text macro=\"collection\"/> -->\n      <text macro=\"isbn\"/>\n      <text macro=\"access\"/>\n      <text macro=\"note\"/>\n    </group>\n  </macro>\n  <!-- In case of Ibid -->\n  <macro name=\"citation-reference-ibid\">\n    <text term=\"ibid\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n  </macro>\n  <!-- In case of Ibid + page -->\n  <macro name=\"citation-reference-ibid-locator\">\n    <group delimiter=\", \">\n      <text term=\"ibid\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\".\"/>\n      <text variable=\"locator\" prefix=\"p.&#160;\"/>\n    </group>\n  </macro>\n  <!-- In case of Op.cit / art.cité -->\n  <macro name=\"citation-reference-subsequent\">\n    <group>\n      <text macro=\"responsability\" suffix=\", \"/>\n      <choose>\n        <if type=\"paper-conference speech chapter article-journal article-magazine article-newspaper post-weblog post webpage broadcast\" match=\"any\">\n          <choose>\n            <if variable=\"container-title\">\n              <text prefix=\"«&#160;\" suffix=\"&#160;», \" variable=\"title\" form=\"short\"/>\n              <choose>\n                <if variable=\"locator\">\n                  <text value=\"art.cité\" suffix=\", \"/>\n                </if>\n                <else>\n                  <text value=\"art.cité\"/>\n                </else>\n              </choose>\n            </if>\n            <else>\n              <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n              <text value=\", \"/>\n              <choose>\n                <if variable=\"locator\">\n                  <text value=\"op.&#160;cit.\" font-style=\"italic\" suffix=\", \"/>\n                </if>\n                <else>\n                  <text value=\"op.&#160;cit.\" font-style=\"italic\"/>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </if>\n        <else>\n          <text variable=\"title\" font-style=\"italic\"/>\n          <text value=\", \"/>\n          <choose>\n            <if variable=\"locator\">\n              <text value=\"op.&#160;cit.\" font-style=\"italic\" suffix=\", \"/>\n            </if>\n            <else>\n              <text value=\"op.&#160;cit.\" font-style=\"italic\"/>\n            </else>\n          </choose>\n        </else>\n      </choose>\n      <choose>\n        <if variable=\"locator\">\n          <text variable=\"locator\" prefix=\"p.&#160;\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <!-- ===================================================================== -->\n  <!-- === Bibliography reference layout macro ============================= -->\n  <macro name=\"bibliography-reference\">\n    <group delimiter=\", \">\n      <text macro=\"responsability\"/>\n      <group delimiter=\" \">\n        <group delimiter=\". \">\n          <text macro=\"title\"/>\n          <text macro=\"volume\"/>\n        </group>\n      </group>\n      <text macro=\"thesis-report-genre\"/>\n      <text macro=\"edition\"/>\n      <!-- secondary-responsability : used for patent inventors and translators -->\n      <text macro=\"secondary-responsability\"/>\n      <group delimiter=\", \">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <text macro=\"publisher-place\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text macro=\"number\"/>\n        </group>\n        <text macro=\"collection\"/>\n        <group delimiter=\", \">\n          <choose>\n            <if type=\"article-newspaper article-journal article-magazine\" match=\"any\"/>\n            <else>\n              <text macro=\"date\"/>\n            </else>\n          </choose>\n          <text macro=\"numberOfVolumes\"/>\n          <text macro=\"issue-pagination-locator\"/>\n        </group>\n      </group>\n      <text macro=\"accessed\"/>\n      <text macro=\"isbn\"/>\n      <text macro=\"access\"/>\n      <text macro=\"note\"/>\n    </group>\n  </macro>\n  <!-- ===================================================================== -->\n  <!-- === Citation parameters ============================================= -->\n  <citation et-al-min=\"4\" et-al-use-first=\"3\">\n    <layout suffix=\".\" delimiter=\"&#8239;; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <text macro=\"citation-reference-ibid-locator\"/>\n        </if>\n        <else-if position=\"ibid\">\n          <text macro=\"citation-reference-ibid\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <text macro=\"citation-reference-subsequent\"/>\n        </else-if>\n        <else>\n          <text macro=\"citation-reference-normal\"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <!-- === Bibliography parameters ========================================= -->\n  <bibliography>\n    <sort>\n      <key macro=\"responsability\"/>\n      <key macro=\"date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <text macro=\"bibliography-reference\"/>\n    </layout>\n  </bibliography>\n  <!-- === “This is the end...” ============================================ -->\n</style>\n"