"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"text\" et-al-min=\"3\" et-al-use-first=\"2\" initialize=\"false\" demote-non-dropping-particle=\"never\">\n  <!-- Polyglot; journal publishes in English, French, and Spanish. -->\n  <info>\n    <title>Meta</title>\n    <id>http://www.zotero.org/styles/meta</id>\n    <link href=\"http://www.zotero.org/styles/meta\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/universite-de-montreal-apa\" rel=\"template\"/>\n    <link href=\"https://www.erudit.org/fr/revues/meta/#journal-info-editorial_policy\" rel=\"documentation\"/>\n    <author>\n      <name>Maya de Wit</name>\n      <email>mail@tolkngt.nl</email>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <issn>0026-0452</issn>\n    <eissn>1492-1421</eissn>\n    <updated>2022-06-13T09:02:50+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"editortranslator\" form=\"short\">ed. &amp; Trans.</term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"no date\" form=\"short\">n.d.</term>\n      <term name=\"in\">in</term>\n      <term name=\"retrieved\">retrieved</term>\n      <term name=\"accessed\">consulted on</term>\n      <term name=\"presented at\">presented at</term>\n      <term name=\"chapter-number\" form=\"short\">chap.</term>\n      <term name=\"section\">section</term>\n      <term name=\"number-of-volumes\" form=\"short\">vol.</term>\n      <term name=\"number\">number</term>\n      <term name=\"scale\">scale</term>\n      <term name=\"circa\" form=\"short\">v. </term>\n      <term name=\"interview\" form=\"verb\">interview</term>\n      <term name=\"author\" form=\"verb\">registered by</term>\n      <term name=\"number\" form=\"short\">n&amp;#7506;</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference entry-encyclopedia entry-dictionary map\" match=\"any\">\n        <names variable=\"editor\">\n          <name and=\"text\" initialize-with=\".\">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n        </names>\n      </if>\n      <else-if type=\"interview broadcast\" match=\"any\">\n        <choose>\n          <if match=\"any\" variable=\"container-title\">\n            <text variable=\"publisher\" suffix=\",\"/>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"article-journal book chapter paper-conference entry-encyclopedia entry-dictionary\" match=\"any\">\n        <names variable=\"editor\" prefix=\" \" suffix=\" \">\n          <name and=\"text\"/>\n          <label form=\"short\" prefix=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if match=\"any\" variable=\"composer\">\n        <names variable=\"composer\">\n          <name and=\"text\" delimiter-precedes-last=\"never\" et-al-use-last=\"true\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n        </names>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name and=\"text\" delimiter-precedes-last=\"never\" et-al-use-last=\"true\" initialize=\"false\" initialize-with=\". \" name-as-sort-order=\"all\">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n          <label form=\"short\" prefix=\", \"/>\n          <substitute>\n            <choose>\n              <if type=\"entry-dictionary entry-encyclopedia webpage manuscript legislation legal_case bill treaty article map article-magazine article-newspaper\" match=\"any\">\n                <text macro=\"title\"/>\n              </if>\n              <else-if type=\"book\" match=\"any\">\n                <names variable=\"editor\"/>\n                <text macro=\"title\" font-style=\"italic\"/>\n              </else-if>\n              <else>\n                <names variable=\"editor\"/>\n                <names variable=\"translator\"/>\n              </else>\n            </choose>\n            <choose>\n              <if type=\"report\">\n                <text variable=\"publisher\"/>\n                <text variable=\"title\" font-style=\"italic\" suffix=\".\"/>\n              </if>\n            </choose>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if match=\"any\" variable=\"composer\">\n        <names variable=\"composer\">\n          <name form=\"short\" and=\"text\" et-al-min=\"3\" et-al-use-first=\"2\" name-as-sort-order=\"all\"/>\n        </names>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name form=\"short\" and=\"text\" et-al-min=\"3\" et-al-use-first=\"2\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"2\" initialize=\"false\" name-as-sort-order=\"all\"/>\n          <et-al font-style=\"italic\"/>\n          <substitute>\n            <choose>\n              <if variable=\"container-title\">\n                <choose>\n                  <if type=\"bill legal_case legislation treaty article-journal\" match=\"none\">\n                    <text variable=\"title\" quotes=\"true\"/>\n                  </if>\n                </choose>\n              </if>\n            </choose>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <choose>\n              <if type=\"report\">\n                <text variable=\"publisher\"/>\n                <text variable=\"title\" font-style=\"italic\"/>\n              </if>\n              <else>\n                <choose>\n                  <if match=\"any\" variable=\"title-short\">\n                    <text variable=\"title-short\" quotes=\"false\"/>\n                  </if>\n                  <else>\n                    <text variable=\"title\" font-style=\"italic\"/>\n                  </else>\n                </choose>\n              </else>\n            </choose>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"thesis dataset\" match=\"any\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\"/>\n          </if>\n          <else-if variable=\"archive\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"archive\" suffix=\".\"/>\n              <text variable=\"URL\"/>\n            </group>\n          </else-if>\n          <else>\n            <choose>\n              <if type=\"dataset\" match=\"all\" variable=\"accessed\">\n                <group delimiter=\" \">\n                  <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n                  <date form=\"text\" variable=\"accessed\" prefix=\"le \"/>\n                  <text term=\"from\"/>\n                  <text variable=\"URL\"/>\n                </group>\n              </if>\n              <else>\n                <text variable=\"URL\"/>\n              </else>\n            </choose>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\"/>\n          </if>\n          <else>\n            <choose>\n              <if type=\"entry-encyclopedia entry-dictionary map article\" match=\"any\">\n                <choose>\n                  <if match=\"any\" variable=\"accessed\">\n                    <group delimiter=\" \">\n                      <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n                      <date form=\"text\" variable=\"accessed\" prefix=\"le \"/>\n                      <text term=\"from\"/>\n                      <text variable=\"URL\"/>\n                    </group>\n                  </if>\n                  <else>\n                    <text variable=\"URL\"/>\n                  </else>\n                </choose>\n              </if>\n              <else>\n                <group delimiter=\", \">\n                  <group delimiter=\" \">\n                    <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                    <date form=\"text\" variable=\"accessed\"/>\n                  </group>\n                  <text variable=\"URL\"/>\n                </group>\n              </else>\n            </choose>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"legislation bill\" match=\"any\">\n        <choose>\n          <if match=\"any\" variable=\"issued\">\n            <group delimiter=\". \">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text variable=\"container-title\"/>\n            </group>\n          </if>\n          <else-if match=\"none\" variable=\"number\">\n            <group delimiter=\". \">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text variable=\"container-title\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \" suffix=\", \">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text variable=\"container-title\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"interview\" match=\"any\">\n        <group delimiter=\" \" suffix=\". \">\n          <text term=\"interview\" form=\"verb\" text-case=\"capitalize-first\"/>\n          <names variable=\"interviewer\">\n            <name and=\"text\" initialize-with=\".\"/>\n          </names>\n        </group>\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if type=\"legal_case\" match=\"all\" variable=\"references\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\",\"/>\n      </else-if>\n      <else-if type=\"thesis report book graphic manuscript speech dataset legal_case treaty article webpage\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"motion_picture broadcast figure map article-journal entry-encyclopedia song musical_score\" match=\"any\">\n        <choose>\n          <if match=\"any\" variable=\"container-title\">\n            <text variable=\"title\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"patent\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\">\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n      <else-if type=\"post thesis\" match=\"none\">\n        <group delimiter=\", \">\n          <choose>\n            <if type=\"book chapter report\" match=\"any\">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\" suffix=\",\"/>\n              </group>\n            </if>\n            <else-if type=\"patent\">\n              <group delimiter=\", \">\n                <text variable=\"authority\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </else-if>\n            <else-if type=\"broadcast interview\" match=\"all\" variable=\"container-title\"/>\n            <else-if type=\"manuscript\" match=\"any\">\n              <group delimiter=\" \">\n                <text variable=\"archive\"/>\n                <text variable=\"archive_location\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"publisher-place\"/>\n            </else-if>\n            <else-if type=\"article-journal article-magazine\" match=\"any\">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </else-if>\n          </choose>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"container-title\" match=\"none\">\n        <choose>\n          <if variable=\"event\">\n            <text variable=\"event\"/>\n          </if>\n          <else-if type=\"speech\" match=\"any\">\n            <text variable=\"publisher\"/>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"extra\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\">\n        <choose>\n          <if match=\"any\" is-numeric=\"number-of-volumes\">\n            <group delimiter=\" \">\n              <number variable=\"number-of-volumes\"/>\n              <label variable=\"number-of-volumes\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"number-of-volumes\" prefix=\"[\" suffix=\"]\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"legal_case\" match=\"all\" variable=\"references\">\n        <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n          <text variable=\"authority\"/>\n          <date date-parts=\"year\" form=\"text\" variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <date delimiter=\" \" variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"container-title\">\n            <text variable=\"genre\"/>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if match=\"any\" variable=\"original-date\">\n        <choose>\n          <if type=\"musical_score book song\" match=\"any\">\n            <group prefix=\"(\" suffix=\")\">\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n              <text variable=\"year-suffix\"/>\n            </group>\n          </if>\n          <else>\n            <group prefix=\"(\" suffix=\")\">\n              <date delimiter=\"\" variable=\"original-date\">\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"year-suffix\"/>\n              <date delimiter=\" \" variable=\"original-date\" prefix=\", \">\n                <date-part name=\"day\" form=\"ordinal\"/>\n                <date-part name=\"month\"/>\n              </date>\n            </group>\n          </else>\n        </choose>\n      </if>\n      <else-if match=\"any\" is-uncertain-date=\"issued\">\n        <choose>\n          <if type=\"map\" match=\"any\">\n            <date form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\"?)\"/>\n          </if>\n          <else>\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <text term=\"circa\" form=\"short\"/>\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n      <else-if match=\"any\" variable=\"status\">\n        <group delimiter=\"-\" prefix=\"(\" suffix=\")\">\n          <text variable=\"status\"/>\n          <text variable=\"year-suffix\"/>\n        </group>\n      </else-if>\n      <else-if variable=\"issued\">\n        <choose>\n          <if type=\"legal_case\" match=\"all\" variable=\"references\"/>\n          <else-if type=\"bill legal_case\" match=\"any\">\n            <group prefix=\"(\" suffix=\").\">\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <date delimiter=\" \" variable=\"issued\" prefix=\", \">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n              </date>\n            </group>\n          </else-if>\n          <else-if type=\"legislation\" match=\"any\"/>\n          <else-if type=\"dataset\" match=\"any\">\n            <group prefix=\"(\" suffix=\")\">\n              <date delimiter=\"\" variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"year-suffix\" prefix=\"-\"/>\n              <date delimiter=\"\" variable=\"issued\" prefix=\", \">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\" prefix=\" \"/>\n              </date>\n            </group>\n          </else-if>\n          <else>\n            <group prefix=\"(\" suffix=\"):\">\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <text variable=\"year-suffix\"/>\n              <choose>\n                <if type=\"article-journal bill book chapter graphic legal_case legislation paper-conference thesis patent\" match=\"none\">\n                  <group prefix=\", \">\n                    <date variable=\"issued\">\n                      <date-part name=\"day\" form=\"ordinal\"/>\n                      <date-part prefix=\" \" name=\"month\"/>\n                    </date>\n                    <choose>\n                      <if type=\"entry-encyclopedia\" match=\"any\">\n                        <text variable=\"issue\" prefix=\", \"/>\n                      </if>\n                    </choose>\n                  </group>\n                </if>\n              </choose>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"manuscript\" match=\"all\" variable=\"issue\">\n        <text variable=\"issue\" prefix=\"[\" suffix=\"].\"/>\n      </else-if>\n      <else>\n        <group prefix=\" (\" suffix=\")\">\n          <choose>\n            <if type=\"legislation\" match=\"any\"/>\n            <else-if type=\"graphic\" match=\"none\">\n              <text term=\"no date\" form=\"short\"/>\n              <text variable=\"year-suffix\" prefix=\"-\"/>\n            </else-if>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-sort\">\n    <choose>\n      <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if match=\"any\" is-uncertain-date=\"issued\">\n        <choose>\n          <if type=\"map\" match=\"any\">\n            <group>\n              <date form=\"text\" variable=\"issued\" suffix=\"?\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text term=\"circa\" form=\"short\"/>\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </else>\n        </choose>\n        <text variable=\"year-suffix\"/>\n      </if>\n      <else-if match=\"any\" variable=\"status\">\n        <group delimiter=\"-\">\n          <text variable=\"status\"/>\n          <text variable=\"year-suffix\"/>\n        </group>\n      </else-if>\n      <else-if variable=\"issued\">\n        <choose>\n          <if type=\"legislation\" match=\"any\"/>\n          <else>\n            <group delimiter=\"/\">\n              <date variable=\"original-date\" form=\"text\"/>\n              <group>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n                <text variable=\"year-suffix\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <choose>\n          <if type=\"legislation\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"container-title\"/>\n              <text variable=\"section\" prefix=\"(\" suffix=\")\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\"-\">\n              <text term=\"no date\" form=\"short\"/>\n              <text variable=\"year-suffix\"/>\n            </group>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"translation-illustration\">\n    <choose>\n      <if variable=\"translator illustrator\" match=\"any\">\n        <group delimiter=\", \">\n          <names variable=\"translator\" delimiter=\", \" prefix=\" \">\n            <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n            <label form=\"short\" prefix=\", \"/>\n          </names>\n          <names variable=\"illustrator\">\n            <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n            <label form=\"short\" prefix=\", \"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine review-book interview article\" match=\"any\">\n        <group delimiter=\":\" prefix=\". \">\n          <group>\n            <text variable=\"volume\" font-style=\"normal\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n          <text variable=\"archive_location\"/>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <text variable=\"publisher-place\" prefix=\" (\" suffix=\")\"/>\n        <group delimiter=\", \" prefix=\", \">\n          <text variable=\"section\" prefix=\"section \"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"book paper-conference entry-encyclopedia chapter entry-dictionary musical_score map figure\" match=\"any\">\n        <group delimiter=\", \" prefix=\" \">\n          <group delimiter=\", \">\n            <text macro=\"edition\"/>\n            <text macro=\"translation-illustration\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"volume\" form=\"short\"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"chapter-number\" form=\"short\"/>\n            <number variable=\"chapter-number\"/>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"version\"/>\n            <text variable=\"version\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n        <choose>\n          <if type=\"book\" match=\"any\">\n            <text variable=\"number\" prefix=\" (\" suffix=\")\"/>\n            <group prefix=\" [\" suffix=\"]\">\n              <text variable=\"genre\"/>\n              <text variable=\"medium\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n      <else-if type=\"legal_case\" match=\"any\">\n        <group delimiter=\" \" prefix=\" \">\n          <text variable=\"number\"/>\n          <choose>\n            <if match=\"none\" variable=\"references page\">\n              <text variable=\"authority\" prefix=\"(\" suffix=\")\"/>\n            </if>\n            <else-if match=\"none\" variable=\"references\">\n              <text variable=\"authority\"/>\n            </else-if>\n          </choose>\n          <text variable=\"page\"/>\n        </group>\n        <text variable=\"source\" prefix=\". \"/>\n      </else-if>\n      <else-if type=\"treaty\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \" prefix=\", \">\n            <label variable=\"volume\" form=\"short\"/>\n            <text variable=\"volume\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n          <group delimiter=\" \" prefix=\" \">\n            <text term=\"issue\" form=\"short\"/>\n            <text variable=\"issue\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"song\" match=\"any\">\n        <text variable=\"number\" prefix=\" (épisode \" suffix=\")\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <choose>\n        <if locator=\"chapter\">\n          <label variable=\"locator\" form=\"long\" text-case=\"capitalize-first\"/>\n        </if>\n        <else>\n          <label variable=\"locator\" form=\"short\"/>\n        </else>\n      </choose>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <group>\n      <choose>\n        <if type=\"chapter paper-conference entry-encyclopedia entry-dictionary song musical_score figure broadcast interview\" match=\"any\">\n          <choose>\n            <if match=\"any\" variable=\"container-title\">\n              <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\": \"/>\n            </if>\n          </choose>\n        </if>\n        <else-if type=\"map\" match=\"all\" variable=\"container-author\">\n          <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n        </else-if>\n      </choose>\n      <text macro=\"container-contributors\"/>\n      <text macro=\"secondary-contributors\"/>\n      <text macro=\"container-title\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"bill legislation legal_case treaty report webpage\" match=\"none\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"legal_case treaty report\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"references\">\n            <text variable=\"container-title\"/>\n          </if>\n        </choose>\n      </else-if>\n      <else>\n        <group delimiter=\" \">\n          <choose>\n            <if variable=\"container-title\">\n              <choose>\n                <if type=\"legislation\" match=\"none\">\n                  <text variable=\"volume\"/>\n                  <text variable=\"container-title\"/>\n                  <group delimiter=\" \">\n                    <text term=\"section\" form=\"symbol\"/>\n                    <text variable=\"section\"/>\n                  </group>\n                </if>\n              </choose>\n            </if>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"original-date\">\n    <choose>\n      <if type=\"song musical_score book\" match=\"any\">\n        <group prefix=\"(\" suffix=\")\">\n          <text value=\"Publication 