"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\" page-range-format=\"expanded\">\n  <info>\n    <title>Organon</title>\n    <title-short>Organon</title-short>\n    <id>http://www.zotero.org/styles/organon</id>\n    <link href=\"http://www.zotero.org/styles/organon\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/revue-francaise-de-sociologie\" rel=\"template\"/>\n    <link href=\"http://www.ejournals.eu/Organon/menu/937/\" rel=\"documentation\"/>\n    <author>\n      <name>SÃ©bastien Plutniak</name>\n      <email>sebastien.plutniak@posteo.net</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"history\"/>\n    <issn>0078-6500</issn>\n    <eissn>2657-5337</eissn>\n    <summary>Style for English papers published in Organon.</summary>\n    <updated>2019-08-10T18:30:00+01:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-US\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>(ed.)</single>\n        <multiple>(eds.)</multiple>\n      </term>\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>pp.</multiple>\n      </term>\n      <term name=\"et-al\">&amp;&#160;al.</term>\n    </terms>\n  </locale>\n  <!-- Macros definition -->\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\" \">\n      <name sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"contextual\" initialize-with=\".&#160;\"/>\n      <label form=\"short\" prefix=\" \" suffix=\" \" text-case=\"lowercase\"/>\n      <substitute>\n        <names variable=\"editorial-director\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name and=\"symbol\" form=\"long\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\".&#160;\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-long\">\n    <names variable=\"author\">\n      <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"contextual\" initialize-with=\".\"/>\n      <label form=\"short\" prefix=\" \" text-case=\"lowercase\"/>\n      <substitute>\n        <names variable=\"editor\">\n          <substitute>\n            <names variable=\"editorial-director\"/>\n          </substitute>\n        </names>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"publisher-collection\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date-short\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\" suffix=\".&#160;\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <group>\n      <label variable=\"page\" suffix=\".&#160;\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"journal-issue\">\n    <choose>\n      <if variable=\"volume issue\" match=\"all\">\n        <text variable=\"volume\" suffix=\", \"/>\n        <text variable=\"issue\" suffix=\"/\"/>\n      </if>\n      <else-if variable=\"volume\">\n        <text variable=\"volume\" suffix=\", \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <!-- End of macros definition -->\n  <!-- Format of the short entries -->\n  <citation et-al-min=\"3\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author-long\"/>\n      <key macro=\"year-date-short\"/>\n    </sort>\n    <layout suffix=\"\" delimiter=\"&#160;; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <!-- Format of the full entries -->\n  <bibliography et-al-min=\"4\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author-long\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author-long\" suffix=\", \"/>\n      <choose>\n        <if type=\"article  article-newspaper article-magazine\" match=\"any\">\n          <group delimiter=\" \">\n            <text variable=\"title\" font-style=\"italic\" quotes=\"false\"/>\n            <text value=\"in\" suffix=\": \"/>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"page\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" font-style=\"italic\" quotes=\"false\"/>\n          <group delimiter=\" \">\n            <text value=\"in\" prefix=\" \" suffix=\": \"/>\n            <text variable=\"container-title\" suffix=\", \" font-style=\"italic\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"editor\"/>\n            <text variable=\"publisher\"/>\n            <group delimiter=\" \">\n              <text variable=\"publisher-place\"/>\n              <text macro=\"year-date-short\"/>\n            </group>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"book\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\" text-case=\"capitalize-first\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"publisher\" suffix=\", \"/>\n          </group>\n          <group delimiter=\" \">\n            <text variable=\"publisher-place\"/>\n            <text macro=\"year-date-short\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <choose>\n              <if variable=\"genre\">\n                <text variable=\"genre\"/>\n              </if>\n              <else>\n                <text value=\"PhD. thesis\"/>\n              </else>\n            </choose>\n            <text macro=\"publisher-collection\"/>\n            <text variable=\"number-of-pages\" suffix=\"&#160;p.\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"false\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"collection-title\" font-style=\"italic\"/>\n            <text variable=\"number\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n            <text macro=\"year-date-short\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text variable=\"title\" font-style=\"italic\" quotes=\"false\"/>\n            <text value=\"in\" suffix=\": \"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"journal-issue\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"year-date-short\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"