"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Plant Protection Research</title>\n    <title-short>JPPR</title-short>\n    <id>http://www.zotero.org/styles/journal-of-plant-protection-research</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-plant-protection-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/citizen-science-theory-and-practice\" rel=\"template\"/>\n    <link href=\"http://www.plantprotection.pl/Instructions-for-Authors,972.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1427-4345</issn>\n    <eissn>1899-007X</eissn>\n    <updated>2020-05-17T10:30:16+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">Available on</term>\n      <term name=\"accessed\">Accessed on</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter-precedes-last=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\" match=\"any\">\n            <text variable=\"publisher\"/>\n          </if>\n        </choose>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book webpage post post-weblog graphic\" match=\"any\">\n        <text variable=\"title\" suffix=\".\"/>\n      </if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" suffix=\".\">\n      <group delimiter=\", \">\n        <text variable=\"publisher\"/>\n        <text variable=\"publisher-place\"/>\n      </group>\n      <text macro=\"number-of-pages\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\" form=\"short\"/>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-newspaper paper-conference broadcast\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" prefix=\" \"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group delimiter=\": \" prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\"/>\n              <date-part name=\"month\" prefix=\" \"/>\n              <date-part name=\"year\" prefix=\" \"/>\n            </date>\n          </group>\n        </group>\n      </if>\n      <else>\n        <text variable=\"DOI\" prefix=\"DOI: https://doi.org/\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-pages\"/>\n      <label variable=\"number-of-pages\" form=\"short\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"1\" initialize-with=\".\" line-spacing=\"2\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\" suffix=\",\"/>\n      <text macro=\"year-date\" prefix=\" \" suffix=\".\"/>\n      <choose>\n        <if type=\"thesis\">\n          <group prefix=\" \" delimiter=\". \">\n            <text macro=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group prefix=\" \" delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n          </group>\n          <group delimiter=\", \" prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\"/>\n            <date variable=\"issued\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-newspaper broadcast personal_communication\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"editor\" prefix=\" \"/>\n            <choose>\n              <if variable=\"author\" match=\"any\">\n                <text variable=\"container-title\"/>\n              </if>\n            </choose>\n            <group prefix=\" \">\n              <group delimiter=\" \">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"issued\" suffix=\", \"/>\n              </group>\n              <text macro=\"pages\" prefix=\": \"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"book graphic\" match=\"any\">\n          <group prefix=\" \" delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <text variable=\"collection-number\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \" suffix=\".\">\n            <text macro=\"title\" suffix=\".\"/>\n            <group delimiter=\" \">\n              <label plural=\"never\" variable=\"page\" form=\"short\"/>\n              <text macro=\"pages\"/>\n            </group>\n            <group delimiter=\" \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text variable=\"container-title\" quotes=\"true\" suffix=\".\"/>\n              </group>\n              <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text macro=\"edition\" prefix=\" \" suffix=\".\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"publisher\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \" prefix=\" \" suffix=\".\">\n            <group delimiter=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group delimiter=\" \">\n              <group>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\" (\" suffix=\")\"/>\n                <text macro=\"issued\"/>\n              </group>\n              <group>\n                <label variable=\"page\" form=\"short\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"