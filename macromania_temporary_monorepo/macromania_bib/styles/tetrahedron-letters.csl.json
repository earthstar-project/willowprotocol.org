"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Tetrahedron Letters</title>\n    <id>http://www.zotero.org/styles/tetrahedron-letters</id>\n    <link href=\"http://www.zotero.org/styles/tetrahedron-letters\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-polymer-science-part-a-polymer-chemistry\" rel=\"template\"/>\n    <link href=\"https://www.elsevier.com/journals/tetrahedron-letters/0040-4039/guide-for-authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <issn>0040-4039</issn>\n    <updated>2017-07-06T16:05:59+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name suffix=\", \" and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <label form=\"short\" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"patent\">\n        <names variable=\"author\">\n          <name name-as-sort-order=\"all\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n          </substitute>\n        </names>\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"authority\">\n              <text variable=\"authority\"/>\n            </if>\n            <else>\n              <text variable=\"publisher\"/>\n            </else>\n          </choose>\n          <text variable=\"number\"/>\n        </group>\n      </if>\n      <else>\n        <names variable=\"author\" suffix=\".\">\n          <name delimiter-precedes-last=\"never\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"normal\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <label strip-periods=\"true\" suffix=\" \" variable=\"page\" form=\"short\"/>\n      </if>\n    </choose>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"book-container\">\n    <group delimiter=\"; \">\n      <group delimiter=\" \">\n        <text term=\"in\" text-case=\"capitalize-first\"/>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </group>\n      <text macro=\"editor\" suffix=\";\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date delimiter=\" \" variable=\"issued\" font-weight=\"normal\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text value=\"edn\" suffix=\".\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\" suffix=\" \"/>\n      <choose>\n        <if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"title\" suffix=\", \"/>\n          <group delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"issued\"/>\n            <text variable=\"volume\" prefix=\"vol. \"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"book-container\"/>\n            <text macro=\"publisher\" suffix=\", \"/>\n            <text macro=\"edition\"/>\n            <text macro=\"issued\" suffix=\";\"/>\n            <group delimiter=\" \" prefix=\" \" suffix=\", \">\n              <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\". \">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n            <group delimiter=\": \">\n              <group delimiter=\"; \">\n                <text macro=\"issued\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\" \">\n            <text macro=\"author\"/>\n            <text variable=\"container-title\" suffix=\".\" form=\"short\"/>\n            <text variable=\"URL\"/>\n            <text term=\"accessed\" suffix=\" \" prefix=\"(\"/>\n            <date variable=\"accessed\" suffix=\")\">\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <text macro=\"issued\"/>\n            <text variable=\"volume\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"