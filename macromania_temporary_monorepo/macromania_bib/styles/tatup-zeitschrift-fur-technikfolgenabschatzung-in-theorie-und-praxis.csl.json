"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <!-- Polyglot; journal publishes in English and German -->\n  <info>\n    <title>TATuP - Zeitschrift f체r Technikfolgenabsch채tzung in Theorie und Praxis</title>\n    <title-short>TATuP</title-short>\n    <id>http://www.zotero.org/styles/tatup-zeitschrift-fur-technikfolgenabschatzung-in-theorie-und-praxis</id>\n    <link href=\"http://www.zotero.org/styles/tatup-zeitschrift-fur-technikfolgenabschatzung-in-theorie-und-praxis\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-institut-fur-praxisforschung-de\" rel=\"template\"/>\n    <link href=\"http://www.tatup.de/index.php/tatup/about/submissions\" rel=\"documentation\"/>\n    <link href=\"http://www.tatup.de/downloads/TATuP_Zitationsstil_citation_style.pdf\" rel=\"documentation\"/>\n    <contributor>\n      <name>Patrick O'Brien</name>\n      <email>citationstyler@gmail.com</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"political_science\"/>\n    <category field=\"social_science\"/>\n    <issn>1619-7623</issn>\n    <eissn>2199-9201</eissn>\n    <updated>2021-02-24T13:32:59+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"anonymous\" form=\"short\">o. A.</term>\n      <term name=\"no date\" form=\"short\">o. J.</term>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"accessed\">zuletzt gepr체ft am</term>\n      <term name=\"available at\">Online verf체gbar unter</term>\n      <term name=\"editor\" form=\"short\">Hg.</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"anonymous\" form=\"short\">n.a.</term>\n      <term name=\"no date\" form=\"short\">n.d.</term>\n      <term name=\"accessed\">last accessed on</term>\n      <term name=\"available at\">available online at</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\"; \">\n      <name and=\"text\" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\"; \">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\"; \" delimiter-precedes-last=\"always\" form=\"long\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\" delimiter=\"; \">\n      <name form=\"short\" delimiter=\", \" and=\"text\" delimiter-precedes-last=\"contextual\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else>\n        <choose>\n          <if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\", \" suffix=\".\">\n              <group>\n                <text term=\"available at\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <text variable=\"URL\"/>\n              </group>\n              <group delimiter=\" \">\n                <text term=\"accessed\"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                  <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n                  <date-part name=\"year\" form=\"long\"/>\n                </date>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"report thesis\" match=\"any\">\n        <group prefix=\" (\" suffix=\")\">\n          <text variable=\"genre\"/>\n          <text variable=\"number\" prefix=\" Nr. \"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"paper-conference\" match=\"none\">\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"volume\" suffix=\" \"/>\n        <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locator-citation\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"published-date\">\n    <choose>\n      <if type=\"article-newspaper speech\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" form=\"numeric\" suffix=\".\"/>\n          <date-part name=\"month\" form=\"numeric\" suffix=\".\"/>\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"chapter article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group>\n          <number variable=\"edition\" form=\"numeric\" suffix=\". \"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-prefix\">\n    <choose>\n      <if type=\"chapter article-journal\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"container-title\"/>\n      </if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <group delimiter=\". \" suffix=\".\">\n          <text variable=\"event\"/>\n          <group delimiter=\", \">\n            <text variable=\"event-place\"/>\n            <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group>\n          <text macro=\"author-short\" suffix=\" \"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text macro=\"locator-citation\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"1\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \" suffix=\".\">\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\"; \">\n              <text macro=\"author\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\"; \">\n              <text macro=\"author\"/>\n              <text macro=\"editor\"/>\n            </group>\n          </else>\n        </choose>\n        <text macro=\"year-date\" prefix=\"(\" suffix=\"):\"/>\n        <text macro=\"title\" suffix=\".\"/>\n        <text macro=\"container-prefix\"/>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <text macro=\"editor\" suffix=\":\"/>\n          </if>\n        </choose>\n        <text macro=\"container-title\" font-style=\"normal\"/>\n        <text macro=\"edition\"/>\n        <text macro=\"genre\"/>\n        <group delimiter=\", \" suffix=\".\">\n          <text macro=\"publisher\"/>\n          <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n            <text variable=\"collection-title\"/>\n          </group>\n          <text macro=\"locator\"/>\n          <text macro=\"published-date\"/>\n          <text macro=\"pages\"/>\n        </group>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"