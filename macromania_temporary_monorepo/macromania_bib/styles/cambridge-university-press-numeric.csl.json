"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Cambridge University Press (numeric)</title>\n    <id>http://www.zotero.org/styles/cambridge-university-press-numeric</id>\n    <link href=\"http://www.zotero.org/styles/cambridge-university-press-numeric\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/ios-press-books\" rel=\"template\"/>\n    <link href=\"https://authornet.cambridge.org/information/academic/guidelines_for_authors.asp\" rel=\"documentation\"/>\n    <contributor>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"generic-base\"/>\n    <summary>Numeric style for Cambridge University Press.</summary>\n    <updated>2017-07-12T13:14:07+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\",\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <text variable=\"URL\"/>\n        <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n          <text term=\"accessed\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\"&#8211;\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography and=\"symbol\" second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" suffix=\". \"/>\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <group delimiter=\" \">\n              <group delimiter=\", \">\n                <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n                <text macro=\"edition\" suffix=\", \"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\",\"/>\n            <text variable=\"volume\" font-weight=\"bold\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            <text variable=\"page\" form=\"short\"/>\n          </group>\n        </else>\n      </choose>\n      <choose>\n        <if variable=\"DOI\">\n          <text variable=\"DOI\" prefix=\". https://doi.org/\"/>\n        </if>\n        <else>\n          <text macro=\"access\" prefix=\". \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"