"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"cs-CZ\">\n  <info>\n    <title>Stavební obzor (Čeština)</title>\n    <id>http://www.zotero.org/styles/stavebni-obzor</id>\n    <link href=\"http://www.zotero.org/styles/stavebni-obzor\" rel=\"self\"/>\n    <link href=\"http://www.stavebniobzor.cz/redakce-casopisu/pokyny-pro-autory/\" rel=\"documentation\"/>\n    <author>\n      <name>Libor Ansorge</name>\n      <email>libor.ansorge@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <issn>1210-4027</issn>\n    <eissn>1805-2576</eissn>\n    <summary>Style for journal Stavební obzor</summary>\n    <updated>2013-05-08T09:45:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\" - \"/>\n      <label form=\"short\" prefix=\" \" text-case=\"lowercase\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <text term=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\" \">\n      <text term=\"issue\" form=\"short\"/>\n      <text variable=\"issue\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\" after-collapse-delimiter=\"; \">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n      <group delimiter=\" \">\n        <label variable=\"locator\" form=\"short\" prefix=\" \"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"1\" second-field-align=\"flush\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout>\n      <!-- Citation Number -->\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <!-- Author(s) -->\n      <group delimiter=\" - \" suffix=\": \">\n        <text macro=\"author\"/>\n      </group>\n      <!-- Rest of Citation -->\n      <choose>\n        <!-- Specific Formats -->\n        <if type=\"book\">\n          <group delimiter=\". \" suffix=\". \">\n            <text variable=\"title\"/>\n            <text variable=\"edition\"/>\n            <group delimiter=\", \">\n              <text variable=\"publisher-place\"/>\n              <text variable=\"publisher\"/>\n            </group>\n            <text macro=\"year-date\"/>\n          </group>\n        </if>\n        <else-if type=\"paper-conference\">\n          <group delimiter=\". \" suffix=\". \">\n            <text variable=\"title\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" prefix=\"[\" suffix=\"]\"/>\n              <text variable=\"event\" prefix=\"konference \" quotes=\"true\"/>\n              <text variable=\"publisher\"/>\n              <text macro=\"year-date\"/>\n              <text macro=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <text variable=\"title\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text macro=\"year-date\"/>\n              <text macro=\"issue\"/>\n              <text macro=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text variable=\"title\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"