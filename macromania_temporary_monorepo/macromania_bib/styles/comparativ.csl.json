"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" page-range-format=\"minimal\" class=\"note\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Comparativ</title>\n    <id>http://www.zotero.org/styles/comparativ</id>\n    <link href=\"http://www.zotero.org/styles/comparativ\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/early-medieval-europe\" rel=\"template\"/>\n    <link href=\"https://www.comparativ.net/v2/about/submissions\" rel=\"documentation\"/>\n    <link href=\"https://www.comparativ.net/public/downloads/comparativ_english_styleguide.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <issn>0940-3566</issn>\n    <updated>2021-01-17T08:03:28+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <style-options punctuation-in-quote=\"false\"/>\n    <terms>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"editortranslator\" form=\"short\">ed. and trans.</term>\n      <term name=\"chapter\" form=\"short\">c.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\", \">\n      <name and=\"text\" initialize-with=\".\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-translator\">\n    <choose>\n      <if variable=\"editor translator\" match=\"any\">\n        <names variable=\"editor translator\">\n          <label form=\"short\" suffix=\". \" strip-periods=\"false\"/>\n          <name and=\"text\" initialize-with=\".\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-editor\">\n    <group delimiter=\": \">\n      <text term=\"in\" suffix=\" \"/>\n      <names variable=\"editor\">\n        <name and=\"text\" initialize-with=\".\" delimiter=\", \"/>\n        <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <names variable=\"translator\"/>\n          <names variable=\"container-author\"/>\n          <text macro=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"paper-conference\">\n        <group delimiter=\" \">\n          <text term=\"presented at\"/>\n          <text variable=\"event\"/>\n        </group>\n        <text variable=\"event-place\" prefix=\", \"/>\n      </if>\n      <else>\n        <text variable=\"publisher-place\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-volumes\"/>\n      <text term=\"volume\" form=\"short\" plural=\"true\" strip-periods=\"true\"/>\n    </group>\n  </macro>\n  <macro name=\"pageno\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pageref\">\n    <choose>\n      <if variable=\"locator\">\n        <choose>\n          <if variable=\"page\">\n            <group>\n              <label variable=\"page\" form=\"short\" suffix=\" \"/>\n              <text variable=\"page\"/>\n              <text term=\"at\" prefix=\", \" suffix=\" \"/>\n            </group>\n          </if>\n        </choose>\n        <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n        <text variable=\"locator\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pageref-subsequent\">\n    <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"volref\">\n    <group delimiter=\" \">\n      <text variable=\"volume\"/>\n      <text macro=\"date\" prefix=\"(\" suffix=\")\"/>\n      <text variable=\"issue\"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <text variable=\"container-title\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\" match=\"any\">\n        <choose>\n          <if variable=\"URL\" match=\"any\">\n            <text variable=\"URL\"/>\n            <group delimiter=\" \" prefix=\" (\" suffix=\")\">\n              <text term=\"accessed\"/>\n              <date variable=\"accessed\">\n                <date-part name=\"month\" form=\"numeric\" suffix=\"/\"/>\n                <date-part name=\"day\" suffix=\"/\"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\" match=\"any\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" form=\"short\"/>\n            <text macro=\"pageref-subsequent\"/>\n          </group>\n        </if>\n        <else-if match=\"any\" position=\"ibid\">\n          <text term=\"ibid\" form=\"short\"/>\n        </else-if>\n        <else-if match=\"any\" position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"pageref-subsequent\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"author\"/>\n          <choose>\n            <if type=\"report thesis\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text variable=\"genre\"/>\n                <text variable=\"publisher\"/>\n                <text variable=\"collection-title\"/>\n                <text variable=\"number\" prefix=\" \"/>\n                <text macro=\"date\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </if>\n            <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"editor-translator\"/>\n                <text macro=\"container\"/>\n                <text macro=\"volumes\"/>\n                <text macro=\"collection\"/>\n                <group delimiter=\" \">\n                  <text macro=\"publisher\"/>\n                  <text macro=\"date\"/>\n                </group>\n                <text macro=\"pageref\"/>\n              </group>\n            </else-if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"container-editor\"/>\n                <text macro=\"container\"/>\n                <text macro=\"collection\"/>\n                <text macro=\"volumes\"/>\n                <group delimiter=\" \">\n                  <text macro=\"publisher\"/>\n                  <text macro=\"date\"/>\n                </group>\n                <text macro=\"pageno\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <group delimiter=\" \">\n                  <text macro=\"publisher\"/>\n                  <group delimiter=\": \">\n                    <text term=\"in\"/>\n                    <text macro=\"container\"/>\n                  </group>\n                  <text macro=\"collection\"/>\n                  <text macro=\"volref\"/>\n                </group>\n                <text macro=\"access\"/>\n                <text macro=\"volumes\"/>\n                <text macro=\"pageno\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </else>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"