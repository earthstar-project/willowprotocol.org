"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\" page-range-format=\"minimal\">\n  <info>\n    <title>Anesthesiology</title>\n    <id>http://www.zotero.org/styles/anesthesiology</id>\n    <link href=\"http://www.zotero.org/styles/anesthesiology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/anesthesia-and-analgesia\" rel=\"template\"/>\n    <link href=\"http://journals.lww.com/anesthesiology/Pages/completeinstructionsforauthors.aspx#references\" rel=\"documentation\"/>\n    <author>\n      <name>Matt Levin</name>\n      <email>mlevin@mlevin.net</email>\n      <uri>http://www.mountsinai.org/profiles/matthew-a-levin</uri>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0003-3022</issn>\n    <eissn>1528-1175</eissn>\n    <updated>2015-12-22T17:55:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \" suffix=\".\">\n          <text variable=\"title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\", \" delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal article-newspaper article-magazine\" match=\"any\">\n        <text variable=\"container-title\" prefix=\". \" form=\"short\" strip-periods=\"true\"/>\n      </if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group prefix=\", \" delimiter=\", \" suffix=\".\">\n          <text variable=\"container-title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" prefix=\" \">\n      <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name delimiter=\", \" delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"volume\"/>\n      <else-if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </else-if>\n      <else-if variable=\"URL\">\n        <text term=\"at\"/>\n        <text variable=\"URL\" prefix=\" &lt;\" suffix=\"&gt;\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issuance\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <text macro=\"pages\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\":\">\n          <group delimiter=\"; \">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n            <text variable=\"volume\"/>\n          </group>\n          <text macro=\"pages\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"article-journal article-newspaper article-magazine\" match=\"any\">\n        <text variable=\"page\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"31\" et-al-use-first=\"30\" second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\" suffix=\":\"/>\n      <text macro=\"title\" prefix=\" \"/>\n      <text macro=\"container-title\"/>\n      <text macro=\"editor\" suffix=\".\"/>\n      <text macro=\"issuance\" prefix=\" \"/>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"