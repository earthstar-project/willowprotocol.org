"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Comparative Politics</title>\n    <id>http://www.zotero.org/styles/comparative-politics</id>\n    <link href=\"http://www.zotero.org/styles/comparative-politics\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/cultural-geographies\" rel=\"template\"/>\n    <link href=\"http://jcp.gc.cuny.edu/submission/style-sheet/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"political_science\"/>\n    <category field=\"social_science\"/>\n    <issn>0010-4159</issn>\n    <updated>2021-10-19T09:54:11+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize=\"false\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"article-journal\" match=\"any\">\n            <date date-parts=\"year-month\" form=\"text\" variable=\"issued\"/>\n          </if>\n          <else>\n            <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\". \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group prefix=\"[\" suffix=\"]\" delimiter=\": \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n              <date-part name=\"month\" form=\"short\" suffix=\"-\" strip-periods=\"true\"/>\n              <date-part name=\"year\" form=\"long\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\" (\" suffix=\")\">\n      <group delimiter=\": \">\n        <text variable=\"publisher\" form=\"long\"/>\n        <text variable=\"publisher-place\" form=\"long\"/>\n      </group>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"cite-locator\">\n    <group delimiter=\" \">\n      <choose>\n        <if match=\"none\" locator=\"page\">\n          <label variable=\"locator\" form=\"short\"/>\n        </if>\n      </choose>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <citation>\n    <layout delimiter=\"; \" suffix=\".\">\n      <choose>\n        <if match=\"any\" position=\"ibid-with-locator ibid\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" form=\"short\"/>\n            <text macro=\"cite-locator\"/>\n          </group>\n        </if>\n        <else-if match=\"any\" position=\"subsequent\">\n          <group delimiter=\", \">\n            <names variable=\"author\">\n              <name form=\"short\"/>\n            </names>\n            <choose>\n              <if match=\"any\" disambiguate=\"true\">\n                <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n              </if>\n            </choose>\n            <text macro=\"cite-locator\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"author\" suffix=\", \"/>\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group>\n                <group delimiter=\", \">\n                  <text variable=\"title\" font-style=\"italic\"/>\n                  <text macro=\"edition\"/>\n                </group>\n                <text macro=\"publisher\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <text variable=\"title\" quotes=\"true\" suffix=\", \"/>\n              <text term=\"in\" suffix=\" \"/>\n              <group delimiter=\", \">\n                <text macro=\"editor\"/>\n                <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n                <text macro=\"edition\"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text variable=\"page\"/>\n              </group>\n            </else-if>\n            <else-if type=\"webpage\">\n              <group delimiter=\", \" suffix=\".\">\n                <text variable=\"title\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text macro=\"year-date\"/>\n              </group>\n              <text macro=\"access\" prefix=\". \"/>\n            </else-if>\n            <else-if type=\"patent\">\n              <group delimiter=\", \">\n                <text variable=\"title\"/>\n                <text variable=\"number\"/>\n                <text macro=\"year-date\"/>\n              </group>\n            </else-if>\n            <else-if type=\"thesis\">\n              <group delimiter=\" \">\n                <text variable=\"title\"/>\n                <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                  <text variable=\"genre\"/>\n                  <text variable=\"publisher\"/>\n                  <text macro=\"year-date\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"article article-journal\" match=\"any\">\n              <group delimiter=\", \">\n                <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n                <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n                <group delimiter=\" \">\n                  <text variable=\"volume\" font-weight=\"normal\"/>\n                  <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\" form=\"short\"/>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\", \">\n                <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n                <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n                <text variable=\"volume\" font-weight=\"normal\"/>\n                <text macro=\"year-date\"/>\n                <text variable=\"page\" form=\"short\"/>\n              </group>\n            </else>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"