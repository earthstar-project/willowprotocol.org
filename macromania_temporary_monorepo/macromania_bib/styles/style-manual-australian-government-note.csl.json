"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" default-locale=\"en-GB\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Style Manual - Australian Government (note)</title>\n    <id>http://www.zotero.org/styles/style-manual-australian-government-note</id>\n    <link href=\"http://www.zotero.org/styles/style-manual-australian-government-note\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/university-of-south-australia-harvard-2013\" rel=\"template\"/>\n    <link href=\"https://www.tandfonline.com/action/authorSubmission?journalCode=raam20&amp;page=instructions#prep\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien, PhD</name>\n    </contributor>\n    <category citation-format=\"note\"/>\n    <category field=\"generic-base\"/>\n    <updated>2018-06-26T07:32:59+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">\n        <single>trans.</single>\n        <multiple>trans</multiple>\n      </term>\n      <term name=\"director\" form=\"short\">\n        <single>dir.</single>\n        <multiple>dirs</multiple>\n      </term>\n      <term name=\"edition\" form=\"short\">edn</term>\n      <term name=\"volume\" form=\"short\">vol.</term>\n      <term name=\"issue\" form=\"short\">no.</term>\n      <term name=\"accessed\" form=\"long\">accessed</term>\n      <term name=\"retrieved\" form=\"long\">viewed</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <choose>\n      <if variable=\"editor container-title\" match=\"all\">\n        <text term=\"in\" suffix=\" \"/>\n      </if>\n    </choose>\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\" delimiter=\", \">\n      <label form=\"short\" suffix=\" \"/>\n      <name and=\"symbol\" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"noauthor_title\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <names variable=\"author\">\n          <name/>\n          <substitute>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name/>\n          <substitute>\n            <text macro=\"title\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"noauthor_title\"/>\n      </substitute>\n    </names>\n    <choose>\n      <if type=\"motion_picture\">\n        <choose>\n          <if variable=\"author\">\n            <text term=\"director\" form=\"short\" prefix=\" (\" suffix=\")\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author-note\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"noauthor_title\"/>\n      </substitute>\n    </names>\n    <choose>\n      <if type=\"motion_picture\">\n        <choose>\n          <if variable=\"author\">\n            <text term=\"director\" form=\"short\" prefix=\" (\" suffix=\")\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <label form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n      <name form=\"short\" name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\"\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"noauthor_title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal article-magazine article-newspaper broadcast interview manuscript map personal_communication speech thesis map post post-weblog\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal article-magazine article-newspaper broadcast interview manuscript map personal_communication speech thesis map post post-weblog\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if variable=\"container-title\">\n        <text variable=\"publisher-place\"/>\n      </if>\n      <else-if variable=\"event\" match=\"none\">\n        <text variable=\"publisher-place\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date-note\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"article-newspaper report paper-conference post post-weblog song\" match=\"any\">\n            <date variable=\"issued\" form=\"text\"/>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"day-month-date\">\n    <choose>\n      <if type=\"article-newspaper report post post-weblog song\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" form=\"numeric\" suffix=\" \"/>\n          <date-part name=\"month\" form=\"long\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"case-date\">\n    <choose>\n      <if variable=\"volume\">\n        <date variable=\"issued\" date-parts=\"year\" form=\"text\" prefix=\"(\" suffix=\")\"/>\n      </if>\n      <else>\n        <date variable=\"issued\" date-parts=\"year\" form=\"text\" prefix=\"[\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"format\">\n    <choose>\n      <if type=\"song motion_picture\" match=\"any\">\n        <text variable=\"medium\"/>\n      </if>\n      <else>\n        <text variable=\"genre\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <choose>\n          <if variable=\"container-title\">\n            <choose>\n              <if variable=\"editor author\" match=\"all\"/>\n              <else>\n                <text term=\"in\" suffix=\" \"/>\n              </else>\n            </choose>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n          <else-if variable=\"event\">\n            <text value=\"paper presented at \"/>\n            <text variable=\"event\"/>\n            <text variable=\"event-place\" prefix=\", \"/>\n          </else-if>\n        </choose>\n      </if>\n      <else-if type=\"webpage\">\n        <text variable=\"container-title\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"case\">\n    <group delimiter=\" \">\n      <text variable=\"title\" font-style=\"italic\" strip-periods=\"true\"/>\n      <text macro=\"case-date\"/>\n      <text variable=\"volume\"/>\n      <text variable=\"container-title\"/>\n      <text variable=\"page-first\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\" \">\n      <text term=\"issue\" form=\"short\"/>\n      <text variable=\"issue\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL accessed\" match=\"all\">\n        <choose>\n          <if type=\"song\">\n            <text term=\"accessed\"/>\n          </if>\n          <else>\n            <text term=\"retrieved\"/>\n          </else>\n        </choose>\n        <date variable=\"accessed\" prefix=\" \">\n          <date-part name=\"day\" form=\"numeric\" suffix=\" \"/>\n          <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n        <text variable=\"URL\" prefix=\", &lt;\" suffix=\"&gt;\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"legal_case\" match=\"none\">\n          <label variable=\"locator\" form=\"short\"/>\n        </if>\n      </choose>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"99\" et-al-use-first=\"98\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout delimiter=\"; \" suffix=\".\">\n      <choose>\n        <if type=\"legal_case\">\n          <group delimiter=\", \">\n            <text macro=\"case\"/>\n            <text macro=\"locator\"/>\n          </group>\n        </if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"locator\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author-note\"/>\n            <text macro=\"title\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"translator\"/>\n            <text macro=\"format\"/>\n            <text macro=\"container\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"volume\"/>\n            <text variable=\"collection-title\"/>\n            <text variable=\"number\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"publisher-place\"/>\n            <text macro=\"issue\"/>\n            <text macro=\"date-note\"/>\n            <text macro=\"pages\"/>\n            <text macro=\"access\"/>\n            <text macro=\"locator\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"99\" et-al-use-first=\"98\" hanging-indent=\"false\" entry-spacing=\"1\" line-spacing=\"1\" subsequent-author-substitute=\"―\" subsequent-author-substitute-rule=\"complete-all\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"legal_case\">\n          <text macro=\"case\"/>\n        </if>\n        <else>\n          <group delimiter=\" \">\n            <text macro=\"author\"/>\n            <group delimiter=\", \">\n              <text macro=\"year-date\"/>\n              <text macro=\"title\"/>\n              <text macro=\"editor\"/>\n              <text macro=\"translator\"/>\n              <text macro=\"format\"/>\n              <text macro=\"container\"/>\n              <text macro=\"edition\"/>\n              <text variable=\"number\"/>\n              <text macro=\"day-month-date\"/>\n              <text macro=\"volume\"/>\n              <text variable=\"collection-title\"/>\n              <text variable=\"publisher\"/>\n              <text macro=\"publisher-place\"/>\n              <text macro=\"issue\"/>\n              <text macro=\"pages\"/>\n              <text macro=\"access\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"