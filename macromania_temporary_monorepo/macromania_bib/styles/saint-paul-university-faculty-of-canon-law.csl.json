"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Saint Paul University - Faculty of Canon Law</title>\n    <id>http://www.zotero.org/styles/saint-paul-university-faculty-of-canon-law</id>\n    <link href=\"http://www.zotero.org/styles/saint-paul-university-faculty-of-canon-law\" rel=\"self\"/>\n    <link href=\"https://ustpaul.ca/upload-files/CanonLaw/cours2015-2016/2015_-_Guidelines_-_12th_ed.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"law\"/>\n    <category field=\"theology\"/>\n    <summary/>\n    <updated>2020-11-28T12:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\".\" name-as-sort-order=\"first\" and=\"text\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-note\">\n    <names variable=\"author\">\n      <name initialize-with=\".\" and=\"text\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name initialize-with=\".\" and=\"text\" form=\"short\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"article-journal article-magazine article-newspaper chapter\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n      </else-if>\n      <else-if type=\"entry-encyclopedia entry-dictionary\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\" prefix=\"art. \"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"book\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\" form=\"short\"/>\n      </if>\n      <else-if type=\"article-journal article-magazine article-newspaper chapter\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\" form=\"short\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-case=\"title\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name initialize-with=\".\" and=\"text\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"container-title\">\n    <text variable=\"container-title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"numeric\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"series\">\n    <group delimiter=\", \">\n      <text variable=\"collection-title\"/>\n      <choose>\n        <if is-numeric=\"collection-number\">\n          <group delimiter=\" \">\n            <text term=\"issue\" form=\"short\"/>\n            <number variable=\"collection-number\"/>\n          </group>\n        </if>\n        <else>\n          <text variable=\"collection-number\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"book chapter entry-dictionary entry-encyclopedia paper-conference report thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <choose>\n            <if type=\"thesis report\" match=\"any\">\n              <text variable=\"genre\"/>\n            </if>\n          </choose>\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n          <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if type=\"article-journal\">\n        <group delimiter=\" \">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <group delimiter=\" \">\n              <text term=\"issue\" form=\"short\"/>\n              <text variable=\"issue\"/>\n            </group>\n          </group>\n          <date variable=\"issued\" form=\"numeric\" date-parts=\"year\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n      <else-if type=\"book chapter entry-dictionary entry-encyclopedia paper-conference report thesis\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"volume\" form=\"short\"/>\n          <text variable=\"volume\"/>\n        </group>\n      </else-if>\n      <else>\n        <date variable=\"issued\" form=\"text\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"article-journal article-magazine article-newspaper chapter entry-dictionary entry-encyclopedia paper-conference\" match=\"any\">\n          <text term=\"in\"/>\n        </if>\n      </choose>\n      <group delimiter=\", \">\n        <text macro=\"editor-translator\"/>\n        <text macro=\"container-title\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <date variable=\"accessed\" form=\"text\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if locator=\"page\">\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"locators-pages\">\n    <choose>\n      <if variable=\"locator\">\n        <text macro=\"locators\"/>\n      </if>\n      <else>\n        <text macro=\"pages\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\"/>\n            <text macro=\"locators\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"locators\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author-note\"/>\n            <text macro=\"title\"/>\n            <text macro=\"container\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"series\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"access\"/>\n            <text macro=\"locators-pages\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"4\" et-al-use-first=\"1\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\", \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <text macro=\"container\"/>\n        <text macro=\"edition\"/>\n        <text macro=\"series\"/>\n        <text macro=\"volume\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"access\"/>\n        <text macro=\"pages\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"