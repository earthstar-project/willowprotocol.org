"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" default-locale=\"pl-PL\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Studia BAS (Polski)</title>\n    <id>http://www.zotero.org/styles/studia-bas</id>\n    <link href=\"http://www.zotero.org/styles/studia-bas\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/polish-legal\" rel=\"template\"/>\n    <link href=\"http://orka.sejm.gov.pl/WydBAS.nsf/PDF/Studia-wsk/$File/StudiaBAS_wskazowki_dla_autorow.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Grzegorz Balcerowski</name>\n      <email>grzegorz.balcerowski@sejm.gov.pl</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"social_science\"/>\n    <category field=\"political_science\"/>\n    <issn>2080-2404</issn>\n    <eissn>2082-0658</eissn>\n    <summary>A style for Bureau of Research \"Studia BAS\" journal citation</summary>\n    <updated>2016-06-29T08:24:37+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\".\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-bibliography\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\".)\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \">\n      <date variable=\"accessed\" prefix=\"[\" suffix=\"]\">\n        <date-part name=\"day\" suffix=\" \"/>\n        <date-part name=\"month\" text-case=\"lowercase\" suffix=\" \"/>\n        <date-part name=\"year\" suffix=\" r.\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"false\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"title-short\">\n    <text variable=\"title\" form=\"short\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\", \"/>\n    <text value=\"op. cit.\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"container\">\n    <text term=\"in\" prefix=\" [\" suffix=\":] \"/>\n    <group delimiter=\", \">\n      <text variable=\"container-title\" quotes=\"false\" font-style=\"italic\"/>\n      <text macro=\"editor-translator\"/>\n      <text variable=\"collection-title\" quotes=\"true\" prefix=\"seria \"/>\n      <text variable=\"collection-number\" prefix=\" nr \"/>\n      <text variable=\"publisher\"/>\n      <text macro=\"publisher-date\"/>\n      <text macro=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher-date\">\n    <group delimiter=\" \">\n      <choose>\n        <if variable=\"publisher-place\">\n          <text variable=\"publisher-place\"/>\n        </if>\n        <else>\n          <text variable=\"publisher\"/>\n        </else>\n      </choose>\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group>\n      <text term=\"volume\" form=\"short\" text-case=\"lowercase\" suffix=\".\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"locator\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\" suffix=\".\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"journal\">\n    <group>\n      <text variable=\"container-title\" quotes=\"true\" font-style=\"normal\"/>\n      <choose>\n        <if match=\"any\" variable=\"issue\">\n          <date variable=\"issued\" prefix=\" \">\n            <date-part name=\"year\"/>\n          </date>\n        </if>\n        <else>\n          <date form=\"numeric\" variable=\"issued\" prefix=\", \"/>\n        </else>\n      </choose>\n      <choose>\n        <if match=\"any\" variable=\"volume\">\n          <text term=\"volume\" form=\"short\" text-case=\"lowercase\" prefix=\", \" suffix=\" \"/>\n          <text variable=\"volume\"/>\n        </if>\n      </choose>\n      <choose>\n        <if match=\"any\" variable=\"issue\">\n          <text term=\"issue\" form=\"short\" text-case=\"lowercase\" prefix=\", \" suffix=\" \"/>\n          <text variable=\"issue\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <citation et-al-min=\"5\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"first\">\n          <choose>\n            <if type=\"book\">\n              <choose>\n                <if match=\"any\" variable=\"editor\">\n                  <group delimiter=\", \">\n                    <text macro=\"title\"/>\n                    <text macro=\"author\"/>\n                    <text variable=\"collection-title\" quotes=\"true\" prefix=\"seria \"/>\n                    <text variable=\"collection-number\" prefix=\"nr \"/>\n                    <text variable=\"publisher\"/>\n                    <text macro=\"publisher-date\"/>\n                    <text macro=\"volume\"/>\n                    <text macro=\"locator\"/>\n                  </group>\n                </if>\n                <else>\n                  <group delimiter=\", \">\n                    <text macro=\"author\"/>\n                    <text macro=\"title\"/>\n                    <text variable=\"publisher\"/>\n                    <text macro=\"publisher-date\"/>\n                    <text macro=\"volume\"/>\n                    <text macro=\"locator\"/>\n                  </group>\n                </else>\n              </choose>\n            </if>\n            <else-if type=\"chapter\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title\"/>\n              </group>\n              <text macro=\"container\"/>\n              <text macro=\"locator\" prefix=\", \"/>\n            </else-if>\n            <else-if type=\"webpage post-weblog\" match=\"any\">\n              <group>\n                <text macro=\"author\" suffix=\", \"/>\n                <text macro=\"title\" suffix=\", \"/>\n                <text variable=\"URL\" suffix=\" \"/>\n                <text macro=\"access\"/>\n              </group>\n            </else-if>\n            <else-if type=\"article-journal article-newspaper\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <group delimiter=\" \">\n                  <text macro=\"title\" suffix=\",\"/>\n                  <text macro=\"journal\"/>\n                </group>\n                <text macro=\"locator\"/>\n              </group>\n            </else-if>\n            <else-if type=\"bill legislation\" match=\"any\">\n              <group>\n                <text variable=\"title\" suffix=\" \"/>\n                <text variable=\"number\" prefix=\"(\" suffix=\")\"/>\n              </group>\n            </else-if>\n            <else-if type=\"article\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text variable=\"title\"/>\n                <date form=\"text\" variable=\"issued\" suffix=\" r.\"/>\n                <text variable=\"call-number\"/>\n                <text variable=\"note\"/>\n              </group>\n            </else-if>\n            <else-if type=\"legal_case\" match=\"any\">\n              <group delimiter=\", \">\n                <text variable=\"title\"/>\n                <text variable=\"container-title\"/>\n                <text variable=\"URL\"/>\n                <text macro=\"access\"/>\n              </group>\n            </else-if>\n            <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n              <group delimiter=\", \">\n                <text variable=\"title\" font-style=\"italic\"/>\n                <group delimiter=\" \">\n                  <text term=\"in\" prefix=\" [\" suffix=\":]\"/>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </group>\n                <text variable=\"publisher\"/>\n                <text macro=\"publisher-date\"/>\n              </group>\n            </else-if>\n            <else-if type=\"report\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title\"/>\n                <text variable=\"genre\"/>\n                <text variable=\"number\" prefix=\"nr \"/>\n                <text variable=\"publisher\"/>\n                <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <group delimiter=\" \">\n                  <text macro=\"title\"/>\n                </group>\n                <text macro=\"locator\"/>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <else-if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" form=\"long\" font-style=\"normal\" text-case=\"capitalize-first\" suffix=\".\"/>\n            <group>\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\" prefix=\" \"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if position=\"ibid\">\n          <text value=\"Ibidem\" font-style=\"italic\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <choose>\n            <if type=\"book\">\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title-short\" font-style=\"italic\"/>\n                <text macro=\"volume\"/>\n                <text macro=\"locator\"/>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title-short\"/>\n                <text macro=\"locator\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"5\" et-al-use-first=\"1\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-bibliography\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"book\">\n          <choose>\n            <if match=\"any\" variable=\"editor\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"author\"/>\n                <text variable=\"collection-title\" quotes=\"true\" prefix=\"seria \"/>\n                <text variable=\"collection-number\" prefix=\"nr \"/>\n                <text variable=\"publisher\"/>\n                <text macro=\"publisher-date\"/>\n                <text macro=\"volume\"/>\n                <text macro=\"locator\"/>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"author-bibliography\"/>\n                <text macro=\"title\"/>\n                <text variable=\"publisher\"/>\n                <text macro=\"publisher-date\"/>\n                <text macro=\"volume\"/>\n                <text macro=\"locator\"/>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <else-if type=\"chapter\">\n          <group delimiter=\", \">\n            <text macro=\"author-bibliography\"/>\n            <text macro=\"title\"/>\n          </group>\n          <text macro=\"container\"/>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <group>\n            <text macro=\"author-bibliography\" suffix=\", \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text variable=\"URL\" suffix=\" \"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-newspaper\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"author-bibliography\"/>\n            <group delimiter=\" \">\n              <text macro=\"title\" suffix=\",\"/>\n              <text macro=\"journal\"/>\n            </group>\n            <text macro=\"locator\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill legislation\" match=\"any\">\n          <group>\n            <text variable=\"title\" suffix=\" \"/>\n            <text variable=\"number\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"author-bibliography\"/>\n            <text variable=\"title\"/>\n            <date form=\"text\" variable=\"issued\" suffix=\" r.\"/>\n            <text variable=\"call-number\"/>\n            <text variable=\"note\"/>\n          </group>\n        </else-if>\n        <else-if type=\"legal_case\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"container-title\"/>\n            <text variable=\"URL\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"author-bibliography\"/>\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"number\" prefix=\"nr \"/>\n            <text variable=\"publisher\"/>\n            <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author-bibliography\"/>\n            <group delimiter=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"container\"/>\n            </group>\n            <text macro=\"locator\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"