"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"note\" demote-non-dropping-particle=\"never\" page-range-format=\"expanded\" default-locale=\"fr-FR\">\n  <info>\n    <title>Bibliothèque d'archéologie méditerranéenne et africaine (BiAMA) (Français)</title>\n    <id>http://www.zotero.org/styles/bibliotheque-d-archeologie-mediterraneenne-et-africaine-biama</id>\n    <link href=\"http://www.zotero.org/styles/bibliotheque-d-archeologie-mediterraneenne-et-africaine-biama\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/collection-du-centre-jean-berard\" rel=\"template\"/>\n    <link href=\"http://ccj.cnrs.fr/spip.php?rubrique89\" rel=\"documentation\"/>\n    <author>\n      <name>Nicolas Monteix</name>\n      <email>nicolas.monteix@univ-rouen.fr</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <category field=\"anthropology\"/>\n    <issn>2101-2849</issn>\n    <summary>Feuille de style pour la Bibliothèque d'archéologie méditerranéenne et africaine</summary>\n    <updated>2020-05-09T05:39:57+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"editor\" form=\"short\">dir.</term>\n      <term name=\"in\">in</term>\n      <term name=\"anonymous\">anonyme</term>\n      <term name=\"page-range-delimiter\">-</term>\n      <term name=\"anonymous\" form=\"short\">anon.</term>\n      <term name=\"no date\">sans date</term>\n      <term name=\"no date\" form=\"short\">s.d.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\", \">\n      <name sort-separator=\", \" initialize-with=\".&#160;\" delimiter=\", \"/>\n      <et-al font-variant=\"normal\" font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name sort-separator=\", \" initialize-with=\".\" delimiter=\", \"/>\n      <et-al font-variant=\"normal\" font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" text-case=\"lowercase\" suffix=\")\"/>\n      <substitute>\n        <text macro=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\" delimiter=\", \">\n      <name sort-separator=\", \" initialize-with=\".\" delimiter=\", \"/>\n      <et-al font-style=\"italic\" font-variant=\"normal\"/>\n    </names>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\", \"/>\n      <et-al font-variant=\"normal\" font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title-short\" font-style=\"italic\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"collection\">\n    <group prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if locator=\"paragraph\">\n        <text variable=\"locator\" prefix=\", §&#160;\"/>\n      </if>\n      <else>\n        <group prefix=\",&#160;\">\n          <choose>\n            <if locator=\"page\">\n              <text variable=\"locator\"/>\n            </if>\n            <else>\n              <label variable=\"locator\" form=\"short\" suffix=\".&#160;\"/>\n              <text variable=\"locator\"/>\n            </else>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"webpage\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text macro=\"edition\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"event-date\">\n    <group prefix=\"(\" suffix=\")\">\n      <choose>\n        <if variable=\"event-date\">\n          <date variable=\"event-date\" form=\"text\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"year-date-short\">\n    <choose>\n      <if variable=\"event-date\">\n        <group delimiter=\" \">\n          <date variable=\"event-date\">\n            <date-part name=\"year\"/>\n          </date>\n          <date variable=\"issued\" prefix=\"[\" suffix=\"]\">\n            <date-part name=\"year\"/>\n          </date>\n          <text variable=\"year-suffix\"/>\n        </group>\n      </if>\n      <else-if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"year-suffix\"/>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <text variable=\"number-of-pages\" suffix=\"&#160;p.\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"lowercase\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"URL\">\n    <text variable=\"URL\" prefix=\" &lt;\" suffix=\"&gt;\"/>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <layout delimiter=\"&#160;; \">\n      <group>\n        <text macro=\"author-short\" suffix=\" \"/>\n        <text macro=\"year-date-short\"/>\n        <text macro=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"page-first\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group font-weight=\"bold\">\n        <choose>\n          <if variable=\"author\">\n            <names variable=\"author\">\n              <name form=\"short\" delimiter=\", \" et-al-min=\"4\" et-al-use-first=\"1\"/>\n              <et-al font-variant=\"normal\" font-style=\"italic\"/>\n            </names>\n          </if>\n          <else-if variable=\"editor\">\n            <names variable=\"editor\">\n              <name form=\"short\" delimiter=\", \" et-al-use-first=\"1\" et-al-min=\"4\"/>\n              <substitute>\n                <text macro=\"container-author\"/>\n              </substitute>\n            </names>\n          </else-if>\n          <else>\n            <text variable=\"title-short\"/>\n          </else>\n        </choose>\n        <text macro=\"year-date-short\" prefix=\" \" suffix=\"&#160;: \"/>\n      </group>\n      <group>\n        <choose>\n          <if variable=\"author\">\n            <text macro=\"author\" suffix=\", \"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\" suffix=\", \"/>\n          </else-if>\n        </choose>\n        <choose>\n          <if type=\"thesis\">\n            <group delimiter=\", \">\n              <text macro=\"title\" font-style=\"italic\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n              <text macro=\"year-date\"/>\n              <text macro=\"number-of-pages\"/>\n            </group>\n          </if>\n          <else-if type=\"webpage\">\n            <group delimiter=\" \">\n              <group suffix=\",\" delimiter=\", \">\n                <choose>\n                  <if variable=\"container-title\" match=\"none\">\n                    <text macro=\"title\" font-style=\"italic\"/>\n                  </if>\n                  <else>\n                    <text macro=\"title\"/>\n                    <text variable=\"container-title\" font-style=\"italic\"/>\n                  </else>\n                </choose>\n              </group>\n              <text macro=\"year-date\"/>\n              <text macro=\"URL\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\"/>\n              <text macro=\"year-date\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"book graphic\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text variable=\"genre\"/>\n                <text macro=\"event-date\"/>\n              </group>\n              <text macro=\"publisher\"/>\n              <text macro=\"collection\"/>\n              <text macro=\"year-date\"/>\n              <text macro=\"number-of-pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <choose>\n                <if variable=\"title-short\" match=\"any\">\n                  <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n                </if>\n                <else>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </else>\n              </choose>\n              <group delimiter=\"&#160;\">\n                <label variable=\"volume\" form=\"short\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <text macro=\"year-date\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <group delimiter=\"&#160;: \">\n                <text term=\"in\" font-style=\"italic\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group delimiter=\" \">\n                <text variable=\"genre\"/>\n                <text macro=\"event-date\"/>\n              </group>\n              <text macro=\"publisher\"/>\n              <text macro=\"collection\"/>\n              <text macro=\"year-date\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"collection\"/>\n              <text macro=\"year-date\"/>\n              <text macro=\"number-of-pages\"/>\n              <text macro=\"URL\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"