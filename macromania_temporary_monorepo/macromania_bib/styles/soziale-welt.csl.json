"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"de-DE\">\n  <!-- This style was edited with the Visual CSL Editor (http://steveridout.com/csl/visualEditor/) -->\n  <info>\n    <title>Soziale Welt (Deutsch)</title>\n    <id>http://www.zotero.org/styles/soziale-welt</id>\n    <link href=\"http://www.zotero.org/styles/soziale-welt\" rel=\"self\"/>\n    <link href=\"http://www.soziale-welt.nomos.de/fileadmin/soziale-welt/doc/Autorenhinweise_sw.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick Fick</name>\n      <email>patrick.fick@sowi.uni-goettingen.de</email>\n    </author>\n    <contributor>\n      <name>Frederik Elwert</name>\n      <email>frederik.elwert@rub.de</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <issn>0038-6073</issn>\n    <summary>Stil f√ºr Soziale Welt (Deutsch)</summary>\n    <updated>2015-11-23T15:49:26+01:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name delimiter=\"/\" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" text-case=\"capitalize-first\" suffix=\".),\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\"/\" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\"/\" delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text value=\"abrufbar unter:\" suffix=\" \"/>\n          <text variable=\"URL\"/>\n          <group prefix=\", \">\n            <text value=\"letzter Zugriff am\" suffix=\" \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\".\"/>\n              <date-part name=\"month\" suffix=\".\" form=\"numeric\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date date-parts=\"year-month-day\" form=\"text\" variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\" form=\"long\" prefix=\"S.\"/>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" givenname-disambiguation-rule=\"by-cite\" disambiguate-add-givenname=\"true\" collapse=\"year\" after-collapse-delimiter=\";\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" plural=\"never\" form=\"short\" suffix=\" \"/>\n          <text variable=\"locator\" form=\"long\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"9\" et-al-use-first=\"8\" entry-spacing=\"0\" line-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" (\" suffix=\"): \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \" suffix=\",\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text variable=\"genre\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text prefix=\" \" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\", \">\n            <text term=\"in\" text-case=\"lowercase\" suffix=\": \"/>\n            <text macro=\"editor\"/>\n            <group prefix=\" \" suffix=\". \" delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text variable=\"event\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\" \" suffix=\".\" delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\",\">\n            <group>\n              <text macro=\"title\" prefix=\" \"/>\n              <text macro=\"editor\" prefix=\" \"/>\n            </group>\n            <group prefix=\" \">\n              <text term=\"in\" text-case=\"lowercase\" suffix=\": \"/>\n              <text variable=\"container-title\"/>\n              <text variable=\"volume\" prefix=\" \"/>\n              <text variable=\"page\" form=\"long\" prefix=\", S. \"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\", \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"