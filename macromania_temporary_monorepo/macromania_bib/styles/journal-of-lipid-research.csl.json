"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Lipid Research</title>\n    <id>http://www.zotero.org/styles/journal-of-lipid-research</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-lipid-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-biological-chemistry\" rel=\"template\"/>\n    <link href=\"http://www.jlr.org/site/misc/ifora.xhtml\" rel=\"documentation\"/>\n    <author>\n      <name>Charles Parnot</name>\n      <uri>http://twitter.com/cparnot</uri>\n      <email>charles.parnot@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>0022-2275</issn>\n    <eissn>1539-7262</eissn>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" prefix=\"(\" suffix=\")\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text term=\"online\" prefix=\"[\" suffix=\"] \"/>\n        <text variable=\"URL\"/>\n        <group prefix=\" (\" suffix=\")\">\n          <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"day\" suffix=\", \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"capitalize-first\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"51\" et-al-use-first=\"50\" second-field-align=\"flush\" line-spacing=\"2\">\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text variable=\"citation-number\"/>\n        <text macro=\"author\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\" \">\n              <text term=\"in\" font-style=\"italic\" text-case=\"title\"/>\n              <text variable=\"container-title\"/>\n              <text variable=\"collection-title\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <group prefix=\" \" delimiter=\", \">\n              <text macro=\"edition\"/>\n              <group>\n                <label variable=\"page\" form=\"short\" suffix=\" \"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n            <text macro=\"publisher\" prefix=\", \"/>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n              <group delimiter=\": \">\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n        <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"