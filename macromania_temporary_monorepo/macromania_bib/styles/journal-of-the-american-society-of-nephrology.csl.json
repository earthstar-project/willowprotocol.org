"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of the American Society of Nephrology</title>\n    <id>http://www.zotero.org/styles/journal-of-the-american-society-of-nephrology</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-the-american-society-of-nephrology\" rel=\"self\"/>\n    <link href=\"http://jasn.asnjournals.org/site/misc/ifora.xhtml\" rel=\"documentation\"/>\n    <author>\n      <name>Michael Berkowitz</name>\n      <email>mberkowi@gmu.edu</email>\n    </author>\n    <contributor>\n      <name>Sean Takats</name>\n      <email>stakats@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <contributor>\n      <name>Richard Karnesky</name>\n      <email>karnesky+zotero@gmail.com</email>\n      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1046-6673</issn>\n    <eissn>1533-3450</eissn>\n    <updated>2021-03-19T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\": \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"verb\" suffix=\" \"/>\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\" suffix=\"; \"/>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text value=\"Available from: \"/>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n          <text term=\"cited\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"year\"/>\n            <date-part name=\"month\" prefix=\" \" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\" prefix=\" \"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group suffix=\" \">\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"internet\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"article-journal article-magazine\" match=\"none\">\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" et-al-min=\"7\" et-al-use-first=\"6\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\", \">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n            </group>\n          </group>\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n              <text variable=\"page\"/>\n            </group>\n            <group>\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <text variable=\"volume\" suffix=\": \"/>\n          <group delimiter=\", \">\n            <text variable=\"page\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n      <text macro=\"accessed-date\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"