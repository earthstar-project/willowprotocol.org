"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Molecular Nutrition &amp; Food Research</title>\n    <id>http://www.zotero.org/styles/molecular-nutrition-and-food-research</id>\n    <link href=\"http://www.zotero.org/styles/molecular-nutrition-and-food-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/angewandte-chemie\" rel=\"template\"/>\n    <link href=\"https://onlinelibrary.wiley.com/page/journal/16134133/homepage/2216_forauthors.html#refs\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <category field=\"chemistry\"/>\n    <issn>1613-4125</issn>\n    <eissn>1613-4133</eissn>\n    <updated>2019-10-10T08:29:44+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\" suffix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" text-case=\"capitalize-first\" suffix=\".: \" strip-periods=\"true\"/>\n      <name initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <group font-weight=\"bold\">\n      <choose>\n        <if variable=\"issued\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </if>\n        <else>\n          <text term=\"no date\" form=\"short\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"DOI \"/>\n          </if>\n        </choose>\n        <choose>\n          <if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\" \">\n              <text term=\"available at\"/>\n              <text variable=\"URL\"/>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\" text-case=\"capitalize-all\"/>\n      <text variable=\"publisher-place\" text-case=\"title\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text term=\"in\"/>\n              <group>\n                <text macro=\"volume\"/>\n                <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n              </group>\n              <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <text macro=\"year-date\"/>\n            </group>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"number\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text macro=\"access\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <group delimiter=\", \">\n              <text macro=\"year-date\"/>\n              <group>\n                <text variable=\"volume\" font-style=\"italic\"/>\n              </group>\n              <text variable=\"page\" form=\"short\"/>\n            </group>\n          </group>\n          <text macro=\"access\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"