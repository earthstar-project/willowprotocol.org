"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>The Canadian Journal of Chemical Engineering</title>\n    <id>http://www.zotero.org/styles/the-canadian-journal-of-chemical-engineering</id>\n    <link href=\"http://www.zotero.org/styles/the-canadian-journal-of-chemical-engineering\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/advanced-functional-materials\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1002/%28ISSN%291939-019X/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <issn>0008-4034</issn>\n    <eissn>1939-019X</eissn>\n    <updated>2018-03-22T14:36:33+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\">accessed on</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"patent\" match=\"none\">\n        <names variable=\"author\">\n          <name delimiter=\", \" delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <text variable=\"collection-title\"/>\n  </macro>\n  <macro name=\"pages\">\n    <label plural=\"never\" suffix=\" \" variable=\"page\" form=\"short\"/>\n    <text variable=\"page-first\"/>\n  </macro>\n  <macro name=\"book-container\">\n    <text variable=\"container-title\" quotes=\"false\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\"/>\n          <date-part name=\"month\" prefix=\" \"/>\n          <date-part name=\"year\" font-weight=\"bold\" prefix=\" \"/>\n        </date>\n      </if>\n      <else>\n        <date delimiter=\" \" variable=\"issued\" font-weight=\"bold\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"full-issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n      <date-part name=\"day\" suffix=\", \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"margin\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"] \"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"article-magazine\">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"full-issued\" suffix=\",\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"genre\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"editor\" prefix=\" \"/>\n            <text macro=\"series\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"pages\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\" font-weight=\"bold\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text macro=\"book-container\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"series\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\" font-weight=\"bold\"/>\n            </group>\n            <text macro=\"volume\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            <group delimiter=\", \">\n              <text macro=\"issued\" font-weight=\"bold\"/>\n              <text variable=\"volume\" font-style=\"italic\"/>\n              <text variable=\"page-first\" form=\"long\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"number\"/>\n              <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"publisher\"/>\n            <names variable=\"author\" prefix=\"invs.: \"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"issued\"/>\n            <group delimiter=\" \">\n              <text term=\"accessed\"/>\n              <date form=\"text\" variable=\"accessed\"/>\n            </group>\n            <text variable=\"URL\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"container-title\"/>\n            <text variable=\"event\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <text variable=\"URL\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"issued\" font-weight=\"bold\"/>\n              <text variable=\"volume\" font-style=\"italic\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"