"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Limnology and Oceanography</title>\n    <id>http://www.zotero.org/styles/limnology-and-oceanography</id>\n    <link href=\"http://www.zotero.org/styles/limnology-and-oceanography\" rel=\"self\"/>\n    <link href=\"http://www.aslo.org/lo/instructions/authors.html#References\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Carl J. Schreck, III</name>\n      <email>carl.schreck@gmail.com</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geology\"/>\n    <issn>0024-3590</issn>\n    <eissn>1939-5590</eissn>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter=\", \" initialize-with=\".\">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n      </name>\n      <label form=\"short\" prefix=\" [\" suffix=\"]\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\", \"/>\n      <et-al term=\"and others\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"day-month\">\n    <date variable=\"issued\">\n      <date-part name=\"month\"/>\n      <date-part name=\"day\" prefix=\" \"/>\n    </date>\n  </macro>\n  <macro name=\"page\">\n    <text variable=\"page\" prefix=\"p. \"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\" prefix=\"p. \"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" line-spacing=\"2\" hanging-indent=\"true\" et-al-min=\"9\" et-al-use-first=\"3\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <group>\n        <group delimiter=\" \">\n          <text macro=\"author\" suffix=\".\"/>\n          <text macro=\"year-date\" suffix=\".\"/>\n        </group>\n        <choose>\n          <if type=\"article-newspaper article-magazine\" match=\"any\">\n            <group delimiter=\" \">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n            </group>\n            <group prefix=\" \" delimiter=\", \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <text macro=\"day-month\"/>\n              <text variable=\"edition\"/>\n            </group>\n          </if>\n          <else-if type=\"thesis\">\n            <group prefix=\" \" delimiter=\". \" suffix=\".\">\n              <text macro=\"title\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n            <group delimiter=\" \" suffix=\".\">\n              <text macro=\"title\" prefix=\" \" suffix=\",\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\" suffix=\".\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\" \" suffix=\".\">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"number\" suffix=\".\"/>\n              <text macro=\"edition\"/>\n              <text variable=\"number\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\">\n            <group delimiter=\" \">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <group delimiter=\". \" suffix=\".\">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text variable=\"event\" prefix=\"Proceedings of the \"/>\n                <text variable=\"publisher\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"chapter\" match=\"any\">\n            <group delimiter=\" \" suffix=\".\">\n              <text macro=\"title\" prefix=\" \" suffix=\",\"/>\n              <text macro=\"page\" suffix=\".\"/>\n              <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n              <group delimiter=\". \">\n                <group delimiter=\", \">\n                  <text macro=\"editor\"/>\n                  <text variable=\"container-title\"/>\n                </group>\n                <text macro=\"publisher\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group suffix=\".\">\n              <text macro=\"title\" prefix=\" \"/>\n              <text macro=\"editor\" prefix=\" \"/>\n            </group>\n            <group delimiter=\". \">\n              <group prefix=\" \" suffix=\".\" delimiter=\" \">\n                <text variable=\"container-title\" form=\"short\"/>\n                <group delimiter=\": \">\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n              <text variable=\"DOI\" prefix=\"doi:\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"