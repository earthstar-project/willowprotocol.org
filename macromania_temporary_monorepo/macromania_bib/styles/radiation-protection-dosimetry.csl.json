"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-US\">\n  <info>\n    <title>Radiation Protection Dosimetry</title>\n    <title-short>RPD</title-short>\n    <id>http://www.zotero.org/styles/radiation-protection-dosimetry</id>\n    <link href=\"http://www.zotero.org/styles/radiation-protection-dosimetry\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/spie-proceedings\" rel=\"template\"/>\n    <link href=\"https://academic.oup.com/rpd/pages/General_Instructions\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0144-8420</issn>\n    <eissn>1742-3406</eissn>\n    <updated>2021-07-27T13:18:17+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"presented at\">presented at</term>\n    </terms>\n  </locale>\n  <macro name=\"type-number\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"patent\" match=\"none\">\n          <text variable=\"genre\"/>\n        </if>\n      </choose>\n      <text variable=\"number\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group prefix=\", \" delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\". \" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\", \" and=\"text\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n        <label form=\"short\" prefix=\", \"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text macro=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report\">\n        <text variable=\"title\" quotes=\"false\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\"/>\n        <text macro=\"edition\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"false\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"day\" suffix=\" \"/>\n      <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"issued-year\">\n    <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"accessed\">\n    <date variable=\"accessed\" prefix=\" (\" suffix=\")\">\n      <date-part name=\"day\" suffix=\" \"/>\n      <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n      <date-part name=\"year\" suffix=\" \"/>\n    </date>\n  </macro>\n  <macro name=\"page\">\n    <text variable=\"page\" prefix=\" \"/>\n  </macro>\n  <macro name=\"locators\">\n    <text variable=\"volume\" prefix=\" \" font-weight=\"bold\"/>\n    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if variable=\"container-title\">\n        <text variable=\"container-title\" form=\"short\"/>\n        <text macro=\"locators\"/>\n        <text macro=\"edition\"/>\n      </if>\n      <else-if type=\"paper-conference\">\n        <group delimiter=\" \">\n          <text term=\"presented at\"/>\n          <text variable=\"event\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\" prefix=\"(\" suffix=\")\">\n      <text variable=\"citation-number\"/>\n      <group>\n        <label variable=\"locator\" form=\"short\" strip-periods=\"true\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"20\" et-al-use-first=\"19\" entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\". \"/>\n      <choose>\n        <if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"container\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"access\"/>\n          </group>\n          <text macro=\"accessed\"/>\n        </if>\n        <else-if type=\"chapter\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" prefix=\"[\" suffix=\"]\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"type-number\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n          <text macro=\"access\"/>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <choose>\n            <if variable=\"container-title\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"page\"/>\n                <text macro=\"publisher\"/>\n              </group>\n              <text macro=\"issued-year\"/>\n            </if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <text macro=\"issued\"/>\n                <text variable=\"event-place\"/>\n                <text variable=\"page\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </else>\n          </choose>\n          <text macro=\"access\"/>\n        </else-if>\n        <else-if type=\"interview\">\n          <group delimiter=\", \">\n            <text variable=\"note\"/>\n            <text variable=\"medium\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n        </else-if>\n        <else-if type=\"book graphic motion_picture song legal_case legislation bill report\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n          <text variable=\"ISBN\" prefix=\" ISBN \"/>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"type-number\"/>\n            <text macro=\"container\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"page\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n          <text macro=\"access\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"