"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Bristol University Press</title>\n    <id>http://www.zotero.org/styles/bristol-university-press</id>\n    <link href=\"http://www.zotero.org/styles/bristol-university-press\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-university-of-birmingham\" rel=\"template\"/>\n    <link href=\"https://bristoluniversitypressdigital.com/view/journals/evp/evp-overview.xml?tab_body=instructions-for-authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <category field=\"humanities\"/>\n    <updated>2023-01-02T10:41:13+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"open-quote\">“</term>\n      <term name=\"close-quote\">”</term>\n      <term name=\"et-al\">et al</term>\n    </terms>\n    <style-options punctuation-in-quote=\"true\"/>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n    </names>\n  </macro>\n  <macro name=\"cite-editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"bookauthor\">\n    <names variable=\"container-author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <substitute>\n        <text macro=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"director\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <substitute>\n        <text macro=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"broadcast motion_picture\" match=\"any\">\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n          <else-if variable=\"title\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else-if>\n        </choose>\n      </if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <names variable=\"author\">\n          <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n          <label form=\"short\" prefix=\" \" strip-periods=\"false\"/>\n          <substitute>\n            <text macro=\"editor\"/>\n            <choose>\n              <if type=\"webpage\">\n                <text variable=\"title\" font-style=\"italic\"/>\n              </if>\n              <else-if variable=\"container-title\">\n                <text variable=\"title\" font-style=\"normal\"/>\n              </else-if>\n              <else>\n                <text variable=\"title\" font-style=\"italic\"/>\n              </else>\n            </choose>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"broadcast motion_picture\" match=\"any\">\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\"/>\n          </if>\n          <else-if variable=\"title\">\n            <text variable=\"title\"/>\n          </else-if>\n        </choose>\n      </if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <names variable=\"author\">\n          <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n          <substitute>\n            <text macro=\"cite-editor\"/>\n            <text variable=\"title\"/>\n            <names variable=\"translator\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\" match=\"none\">\n        <text variable=\"URL\"/>\n      </if>\n      <else>\n        <text variable=\"DOI\" prefix=\"doi: \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"author editor\" match=\"any\">\n        <choose>\n          <if type=\"bill book graphic legal_case legislation manuscript motion_picture report song webpage\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else-if variable=\"container-title\" match=\"none\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text variable=\"title\" quotes=\"true\"/>\n          </else-if>\n          <else>\n            <text variable=\"title\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"false\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <choose>\n          <if variable=\"volume issue\" match=\"any\">\n            <text variable=\"page\"/>\n          </if>\n          <else>\n            <label variable=\"page\" form=\"short\" suffix=\" \"/>\n            <text variable=\"page\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <group>\n          <label strip-periods=\"true\" suffix=\" \" variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volumes\">\n    <group>\n      <text variable=\"number-of-volumes\" suffix=\" \"/>\n      <label variable=\"volume\" form=\"short\" plural=\"always\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-newspaper article-magazine motion_picture paper-conference broadcast\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"motion_picture broadcast paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" prefix=\" \"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <choose>\n          <if type=\"bill legislation\" match=\"none\">\n            <text macro=\"year-date\"/>\n          </if>\n        </choose>\n        <group>\n          <label variable=\"locator\" form=\"short\" strip-periods=\"false\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\" et-al-min=\"4\" et-al-use-first=\"3\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\" sort=\"ascending\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <choose>\n          <if type=\"bill legislation\" match=\"any\">\n            <text macro=\"author\" suffix=\". \"/>\n          </if>\n          <else>\n            <text macro=\"author\"/>\n            <choose>\n              <if variable=\"author editor\" match=\"any\">\n                <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n              </if>\n              <else-if type=\"broadcast motion_picture\" match=\"any\">\n                <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n              </else-if>\n              <else>\n                <text macro=\"year-date\" prefix=\" (\" suffix=\").\"/>\n              </else>\n            </choose>\n          </else>\n        </choose>\n        <group delimiter=\", \">\n          <choose>\n            <if type=\"bill legislation\" match=\"any\">\n              <group delimiter=\", \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"editor\"/>\n                <text variable=\"number\" suffix=\")\" prefix=\"(\"/>\n                <text variable=\"note\"/>\n              </group>\n            </if>\n            <else-if type=\"thesis\">\n              <group delimiter=\", \">\n                <group prefix=\" \" delimiter=\" \">\n                  <text macro=\"title\" font-style=\"italic\"/>\n                  <text macro=\"edition\"/>\n                </group>\n                <group prefix=\" \" delimiter=\", \">\n                  <text variable=\"genre\"/>\n                  <text variable=\"publisher\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"webpage post post-weblog\" match=\"any\">\n              <group delimiter=\", \" prefix=\" \">\n                <text macro=\"title\"/>\n                <text variable=\"container-title\"/>\n              </group>\n            </else-if>\n            <else-if type=\"manuscript\">\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <text macro=\"title\"/>\n                  <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n                </group>\n                <text variable=\"archive\"/>\n                <text variable=\"archive_location\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </else-if>\n            <else-if type=\"article-journal article-magazine article-newspaper interview map patent personal_communication song speech\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\" prefix=\" \"/>\n                <text macro=\"editor\" prefix=\" \"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text macro=\"issued\"/>\n                <group>\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text macro=\"pages\"/>\n              </group>\n            </else-if>\n            <else-if type=\"broadcast motion_picture\" match=\"any\">\n              <group delimiter=\", \">\n                <group delimiter=\", \">\n                  <text variable=\"number\"/>\n                  <choose>\n                    <if variable=\"container-title\">\n                      <text variable=\"title\"/>\n                    </if>\n                  </choose>\n                </group>\n                <group delimiter=\" \">\n                  <choose>\n                    <if type=\"motion_picture\" match=\"any\">\n                      <text value=\"Film\"/>\n                    </if>\n                  </choose>\n                </group>\n                <group delimiter=\". \">\n                  <text macro=\"director\"/>\n                  <text macro=\"publisher\"/>\n                  <text macro=\"issued\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"bill book graphic legal_case report\" match=\"any\">\n              <group delimiter=\", \">\n                <group prefix=\" \">\n                  <text macro=\"title\"/>\n                  <group delimiter=\" \" prefix=\". \">\n                    <text variable=\"collection-title\"/>\n                    <text variable=\"collection-number\"/>\n                    <text variable=\"number\"/>\n                  </group>\n                  <group delimiter=\". \" prefix=\". \">\n                    <text macro=\"volumes\"/>\n                    <text macro=\"edition\"/>\n                    <text macro=\"editor\"/>\n                  </group>\n                </group>\n                <text macro=\"publisher\" prefix=\" \"/>\n              </group>\n            </else-if>\n            <else-if type=\"chapter\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n                <group prefix=\" \" delimiter=\" \">\n                  <text term=\"in\" text-decoration=\"none\" suffix=\" \"/>\n                  <text macro=\"bookauthor\"/>\n                </group>\n                <group prefix=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <group delimiter=\" \" prefix=\". \">\n                    <text variable=\"collection-title\"/>\n                    <text variable=\"collection-number\"/>\n                    <text variable=\"number\"/>\n                  </group>\n                  <group delimiter=\". \" prefix=\". \">\n                    <text macro=\"volumes\"/>\n                    <text macro=\"edition\"/>\n                  </group>\n                </group>\n                <group delimiter=\". \">\n                  <text macro=\"issued\"/>\n                  <text macro=\"publisher\"/>\n                  <text macro=\"pages\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"paper-conference\" match=\"any\">\n              <group>\n                <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n                <group delimiter=\". \">\n                  <group prefix=\" \" delimiter=\" \">\n                    <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \" text-decoration=\"underline\"/>\n                    <text macro=\"editor\" suffix=\".\"/>\n                    <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n                    <text variable=\"collection-title\"/>\n                    <group delimiter=\", \">\n                      <text variable=\"publisher-place\"/>\n                      <text macro=\"issued\"/>\n                    </group>\n                  </group>\n                  <group delimiter=\". \">\n                    <text variable=\"publisher\"/>\n                    <text macro=\"pages\"/>\n                  </group>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\". \">\n                <group>\n                  <text macro=\"title\" prefix=\" \"/>\n                  <text macro=\"editor\" prefix=\" \"/>\n                </group>\n                <group delimiter=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </group>\n                <group prefix=\" \">\n                  <group prefix=\" \">\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\" prefix=\" (\" suffix=\")\"/>\n                    <text macro=\"issued\"/>\n                  </group>\n                  <group prefix=\" \">\n                    <text macro=\"pages\"/>\n                  </group>\n                </group>\n              </group>\n            </else>\n          </choose>\n          <text macro=\"access\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"