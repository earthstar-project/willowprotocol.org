"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" default-locale=\"pt-PT\">\n  <info>\n    <title>Norma Portuguesa 405 (Português)</title>\n    <title-short>NP 405</title-short>\n    <id>http://www.zotero.org/styles/norma-portuguesa-405</id>\n    <link href=\"http://www.zotero.org/styles/norma-portuguesa-405\" rel=\"self\"/>\n    <link href=\"https://github.com/citation-style-language/styles/pull/413\" rel=\"documentation\"/>\n    <author>\n      <name>SBIDM. Universidade de Aveiro</name>\n      <uri>http://www.ua.pt/sbidm/biblioteca/</uri>\n      <email>sbidm-biblioteca@ua.pt</email>\n    </author>\n    <author>\n      <name>Cristina Cortês</name>\n      <email>ccortes@ua.pt</email>\n      <uri>http://www.mendeley.com/profiles/cristina-cortes/</uri>\n    </author>\n    <author>\n      <name>Diana Silva</name>\n      <email>dianasilva@ua.pt</email>\n    </author>\n    <author>\n      <name>Ana Bela Martins</name>\n      <email>abela@ua.pt</email>\n    </author>\n    <contributor>\n      <name>Susana Dias</name>\n      <email>sdias@ua.pt</email>\n    </contributor>\n    <contributor>\n      <name>Bella Nolasco</name>\n      <email>bellanolasco@ua.pt</email>\n    </contributor>\n    <contributor>\n      <name>Jonadabe PT</name>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <contributor>\n      <name>Rintze Zelle</name>\n      <uri>http://twitter.com/rintzezelle</uri>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>The Portuguese standard style</summary>\n    <updated>2013-10-03T11:50:09+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"pt-PT\">\n    <terms>\n      <term name=\"container-author\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"collection-editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"container-author\" delimiter=\", \">\n          <name name-as-sort-order=\"all\" sort-separator=\", \" initialize=\"false\" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"always\">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n            <name-part name=\"given\" text-case=\"uppercase\"/>\n          </name>\n          <et-al font-style=\"italic\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\".) -\" text-case=\"capitalize-first\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"collection-editor\"/>\n          </substitute>\n        </names>\n      </if>\n      <else-if type=\"patent\">\n        <names variable=\"container-author\" delimiter=\", \">\n          <name name-as-sort-order=\"all\" sort-separator=\", \" initialize=\"false\" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"always\">\n            <name-part name=\"family\"/>\n            <name-part name=\"given\"/>\n          </name>\n          <et-al font-style=\"italic\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"collection-editor\"/>\n          </substitute>\n        </names>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference patent\" match=\"none\">\n        <names variable=\"editor\" delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <name and=\"symbol\" initialize=\"false\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\" suffix=\".\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"translator\">\n    <text value=\"Trad. \"/>\n    <names variable=\"translator\" delimiter=\", \">\n      <name delimiter=\"; \" sort-separator=\" \" delimiter-precedes-last=\"always\">\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n      </name>\n      <et-al font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" initialize=\"false\" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\" text-case=\"capitalize-all\"/>\n      </name>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\".)\" text-case=\"uppercase\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" name-as-sort-order=\"all\" sort-separator=\", \" and=\"text\" initialize=\"false\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture song patent\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\" font-weight=\"normal\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"false\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"medium\">\n    <choose>\n      <if variable=\"URL\" match=\"any\">\n        <text value=\"[Em linha]\" prefix=\" \"/>\n      </if>\n      <else>\n        <text value=\"\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <date variable=\"accessed\" prefix=\"[Consult. \">\n      <date-part name=\"day\" suffix=\" \"/>\n      <date-part name=\"month\" form=\"short\" suffix=\". \" text-case=\"lowercase\"/>\n      <date-part name=\"year\" suffix=\"]. \"/>\n    </date>\n    <text variable=\"URL\" prefix=\"Disponível em WWW:&lt;URL:\" suffix=\"&gt;.\"/>\n  </macro>\n  <macro name=\"title\">\n    <group>\n      <choose>\n        <if type=\"book manuscript patent thesis webpage report\" match=\"any\">\n          <text variable=\"title\" form=\"long\" font-weight=\"bold\"/>\n        </if>\n        <else>\n          <text variable=\"title\" font-style=\"normal\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <group>\n      <choose>\n        <if type=\"article-magazine article-newspaper article-journal chapter entry-dictionary paper-conference article bill legislation\" match=\"any\">\n          <text variable=\"container-title\" form=\"long\" font-weight=\"bold\"/>\n        </if>\n        <else>\n          <text variable=\"container-title\" font-style=\"normal\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if match=\"any\" variable=\"publisher-place publisher\">\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\" suffix=\" : \"/>\n          </if>\n          <else>\n            <text value=\"[S.l.] \" suffix=\" : \"/>\n          </else>\n        </choose>\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\" suffix=\", \"/>\n            <text macro=\"issued\"/>\n          </if>\n          <else>\n            <text value=\"[s.n.]\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\" match=\"any\">\n        <group>\n          <choose>\n            <if type=\"book chapter webpage\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part name=\"day\" suffix=\" \"/>\n                <date-part name=\"month\" form=\"short\" suffix=\" \"/>\n              </date>\n            </if>\n          </choose>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <text value=\"[s.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text value=\"[s.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"numeric\" suffix=\".\"/>\n              <text term=\"edition\" form=\"short\" suffix=\".\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\" ed.\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"bill legislation\" match=\"any\">\n        <group>\n          <text variable=\"number\" prefix=\". \" suffix=\" \"/>\n          <date variable=\"issued\">\n            <date-part name=\"year\" form=\"short\" prefix=\" (\" suffix=\"-\"/>\n            <date-part name=\"month\" form=\"numeric-leading-zeros\" text-case=\"lowercase\" suffix=\"-\"/>\n            <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\") \"/>\n          </date>\n          <text variable=\"section\" prefix=\"Sec. \"/>\n          <text variable=\"page\" prefix=\" \" suffix=\".\"/>\n        </group>\n      </if>\n      <else-if type=\"patent\">\n        <group>\n          <date variable=\"issued\">\n            <date-part name=\"day\" prefix=\". \" suffix=\" \"/>\n            <date-part name=\"month\" form=\"short\" suffix=\". \" text-case=\"lowercase\"/>\n            <date-part name=\"year\" suffix=\", \"/>\n          </date>\n          <text variable=\"page\" prefix=\" \" suffix=\" p.\"/>\n        </group>\n      </else-if>\n      <else-if match=\"any\" type=\"article-journal article article-magazine\">\n        <text variable=\"volume\" prefix=\" \" suffix=\":\"/>\n        <text variable=\"issue\" suffix=\" (\"/>\n        <text suffix=\")\" macro=\"issued-year\"/>\n        <text variable=\"page\" prefix=\" \"/>\n      </else-if>\n      <else-if match=\"any\" type=\"chapter\">\n        <text variable=\"volume\" prefix=\"v. \"/>\n        <text variable=\"page\" prefix=\". p. \"/>\n      </else-if>\n      <else-if type=\"book\" match=\"any\">\n        <text variable=\"page\" prefix=\". \" suffix=\" p.\"/>\n      </else-if>\n      <else-if type=\"thesis\" match=\"any\">\n        <text variable=\"page\" prefix=\". \" suffix=\" f.\"/>\n      </else-if>\n      <else-if type=\"report\" match=\"any\">\n        <text variable=\"number\" prefix=\" (Relatório n.\" suffix=\").\"/>\n      </else-if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <date variable=\"issued\" prefix=\" (\" suffix=\"). \" delimiter=\" \">\n          <date-part name=\"day\"/>\n          <date-part name=\"month\" form=\"short\" suffix=\".\" text-case=\"lowercase\"/>\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"volume\" suffix=\":\"/>\n        <text variable=\"issue\"/>\n        <text variable=\"page\" prefix=\" \"/>\n      </else-if>\n      <else-if type=\"webpage\" match=\"any\">\n        <date variable=\"issued\" prefix=\", atual. \">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" form=\"short\" suffix=\". \" text-case=\"lowercase\"/>\n          <date-part name=\"year\" suffix=\". \"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"ISBN\">\n    <group>\n      <text variable=\"ISBN\" prefix=\". ISBN \"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group>\n        <text suffix=\", \" macro=\"author-short\"/>\n        <text macro=\"issued-year\"/>\n        <text prefix=\", \" macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\" et-al-min=\"4\" et-al-use-first=\"1\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <choose>\n        <if type=\"bill legislation\" match=\"any\">\n          <group>\n            <text variable=\"title\" suffix=\". \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"container-title\"/>\n            <text macro=\"container-contributors\" suffix=\". \"/>\n            <text macro=\"medium\"/>\n            <text macro=\"locators\" suffix=\" \"/>\n            <text macro=\"access\"/>\n          </group>\n        </if>\n        <else-if type=\"map\">\n          <group>\n            <text macro=\"author\" suffix=\" - \"/>\n            <text macro=\"title\"/>\n            <text macro=\"medium\"/>\n            <text macro=\"edition\" prefix=\". \"/>\n            <text macro=\"locators\"/>\n            <text macro=\"access\" prefix=\" \"/>\n            <text variable=\"note\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"book\">\n          <group>\n            <text macro=\"author\" suffix=\" - \"/>\n            <text macro=\"title\"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"medium\"/>\n            <text macro=\"edition\" prefix=\". \"/>\n            <text macro=\"publisher\" prefix=\". \"/>\n            <text macro=\"locators\"/>\n            <text macro=\"access\" prefix=\" \"/>\n            <text macro=\"ISBN\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n          <group>\n            <text macro=\"author\" suffix=\" - \"/>\n            <text variable=\"title\" suffix=\". \"/>\n            <text macro=\"secondary-contributors\" suffix=\". \"/>\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <text macro=\"container-contributors\" suffix=\" \"/>\n            <text macro=\"translator\" suffix=\" \"/>\n            <text macro=\"container-title\"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"medium\"/>\n            <text macro=\"edition\" prefix=\". \"/>\n            <text macro=\"publisher\" prefix=\". \"/>\n            <text macro=\"access\" prefix=\" \"/>\n            <text macro=\"ISBN\"/>\n            <text macro=\"locators\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article\" match=\"any\">\n          <group>\n            <text macro=\"author\" suffix=\" - \"/>\n            <text variable=\"title\" suffix=\". \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"container-title\" suffix=\". \"/>\n            <text variable=\"publisher-place\" suffix=\". \"/>\n            <text macro=\"ISSN\" suffix=\". \"/>\n            <text macro=\"locators\" suffix=\". \"/>\n            <text variable=\"DOI\" prefix=\"doi: \" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-magazine\" match=\"any\">\n          <group>\n            <text macro=\"author\" suffix=\" - \"/>\n            <text variable=\"title\" suffix=\". \"/>\n            <text macro=\"container-title\" suffix=\". \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text variable=\"publisher-place\" suffix=\". \"/>\n            <text macro=\"medium\"/>\n            <text macro=\"locators\" suffix=\".\"/>\n            <text macro=\"access\" prefix=\". \"/>\n            <text macro=\"ISSN\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\" match=\"any\">\n          <group>\n            <text macro=\"author\" suffix=\" - \"/>\n            <text variable=\"title\" suffix=\". \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"container-title\" suffix=\". \"/>\n            <text macro=\"medium\"/>\n            <text macro=\"ISSN\" suffix=\" \"/>\n            <text macro=\"locators\" suffix=\".\"/>\n            <text macro=\"access\" prefix=\" \"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group>\n            <text macro=\"author\" suffix=\" - \"/>\n            <text macro=\"title\"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"medium\"/>\n            <text macro=\"publisher\" prefix=\". \"/>\n            <text macro=\"locators\"/>\n            <text macro=\"access\" prefix=\" \"/>\n            <text variable=\"genre\" prefix=\" \" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"manuscript\">\n          <group>\n            <text macro=\"author\" suffix=\" - \"/>\n            <text macro=\"title\"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"medium\"/>\n            <text macro=\"publisher\" prefix=\". \"/>\n            <text macro=\"locators\"/>\n            <text macro=\"access\" prefix=\" \"/>\n            <text macro=\"archive\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group>\n            <text macro=\"author\" suffix=\" - \"/>\n            <text macro=\"title\"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"medium\"/>\n            <text macro=\"publisher\" prefix=\". \"/>\n            <text macro=\"locators\"/>\n            <text macro=\"access\" prefix=\" \"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group>\n            <text macro=\"author\" suffix=\" - \"/>\n            <text macro=\"title\"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"medium\"/>\n            <text macro=\"edition\" prefix=\". \"/>\n            <text macro=\"publisher\" prefix=\". \"/>\n            <text macro=\"locators\"/>\n            <text macro=\"access\" prefix=\" \"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group>\n            <text macro=\"container-contributors\" suffix=\" - \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"author\" suffix=\". \"/>\n            <text variable=\"publisher-place\" suffix=\" \"/>\n            <text variable=\"number\" prefix=\"patente \"/>\n            <text macro=\"locators\"/>\n            <text macro=\"access\" prefix=\" \"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"author\" suffix=\" - \"/>\n          <text macro=\"title\"/>\n          <text macro=\"container-contributors\"/>\n          <text macro=\"secondary-contributors\"/>\n          <text macro=\"container-title\"/>\n          <text variable=\"collection-title\" prefix=\": \" suffix=\".\"/>\n          <text macro=\"locators\"/>\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\" suffix=\", \"/>\n          <text macro=\"issued\" prefix=\", \" suffix=\". \"/>\n          <text macro=\"access\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"ISSN\">\n    <group>\n      <text variable=\"ISSN\" prefix=\". ISSN \"/>\n    </group>\n  </macro>\n  <macro name=\"archive\">\n    <group>\n      <text variable=\"archive\" prefix=\" \"/>\n    </group>\n  </macro>\n</style>\n"