"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Phyllomedusa</title>\n    <id>http://www.zotero.org/styles/phyllomedusa</id>\n    <link href=\"http://www.zotero.org/styles/phyllomedusa\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/limnology-and-oceanography\" rel=\"template\"/>\n    <link href=\"http://www.phyllomedusa.esalq.usp.br/instruction.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1519-1397</issn>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter=\", \" initialize-with=\".\">\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book report\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\". \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"day-month\">\n    <date variable=\"issued\">\n      <date-part name=\"month\"/>\n      <date-part name=\"day\" prefix=\" \"/>\n    </date>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <label variable=\"page\" text-case=\"capitalize-first\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\" prefix=\"p. \"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" line-spacing=\"2\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <group>\n        <group delimiter=\" \">\n          <text macro=\"author\" suffix=\".\"/>\n          <text macro=\"year-date\" suffix=\".\"/>\n        </group>\n        <choose>\n          <if type=\"article-newspaper article-magazine\" match=\"any\">\n            <group delimiter=\" \">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n            </group>\n            <group prefix=\" \" delimiter=\", \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <text macro=\"day-month\"/>\n              <text variable=\"edition\"/>\n            </group>\n          </if>\n          <else-if type=\"thesis\">\n            <group prefix=\" \" delimiter=\". \" suffix=\".\">\n              <text macro=\"title\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n            <group delimiter=\" \" suffix=\".\">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\" suffix=\".\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\" \" suffix=\".\">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"number\" suffix=\".\"/>\n              <text macro=\"edition\"/>\n              <text variable=\"number\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\">\n            <group delimiter=\" \">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <group delimiter=\". \" suffix=\".\">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text variable=\"event\" prefix=\"Proceedings of the \"/>\n                <text variable=\"publisher\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage\">\n            <group delimiter=\".\">\n              <text macro=\"title\"/>\n              <text variable=\"container-title\"/>\n              <text variable=\"URL\" prefix=\"Accessible at\"/>\n              <date variable=\"accessed\" prefix=\"Captured on\" delimiter=\" \">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </else-if>\n          <else-if type=\"chapter\" match=\"any\">\n            <group delimiter=\" \" suffix=\".\">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <text macro=\"page\"/>\n              <text term=\"in\" font-style=\"italic\"/>\n              <group delimiter=\". \">\n                <group delimiter=\", \">\n                  <text macro=\"editor\"/>\n                  <text variable=\"container-title\"/>\n                </group>\n                <text macro=\"publisher\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group suffix=\".\">\n              <text macro=\"title\" prefix=\" \"/>\n              <text macro=\"editor\" prefix=\" \"/>\n            </group>\n            <choose>\n              <if variable=\"page\">\n                <group prefix=\" \" suffix=\".\" delimiter=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <group delimiter=\": \">\n                    <text variable=\"volume\" font-style=\"italic\"/>\n                    <text variable=\"page\"/>\n                  </group>\n                </group>\n              </if>\n              <else>\n                <group prefix=\" \" delimiter=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <choose>\n                    <!--we use DOIs only for articles w/o page numbers conforming to published articles. \n                        No period after DOI -->\n                    <if variable=\"DOI\">\n                      <group>\n                        <text variable=\"volume\" font-style=\"italic\"/>\n                        <text variable=\"DOI\" prefix=\", doi:\"/>\n                      </group>\n                    </if>\n                    <else>\n                      <text variable=\"volume\" font-style=\"italic\" suffix=\".\"/>\n                    </else>\n                  </choose>\n                </group>\n              </else>\n            </choose>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"