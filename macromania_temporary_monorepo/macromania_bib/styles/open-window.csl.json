"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <!-- This style was edited with the Visual CSL Editor (https://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Open Window - Harvard</title>\n    <id>http://www.zotero.org/styles/open-window</id>\n    <link href=\"http://www.zotero.org/styles/open-window\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-cite-them-right\" rel=\"template\"/>\n    <link href=\"https://drive.google.com/file/d/1_8LqeaMsq5L6DRkgwhZwwSP4Rb0qUptR/view?usp=share_link\" rel=\"documentation\"/>\n    <author>\n      <name>Justin Hartman</name>\n      <email>j.hartman@ctca.co.za</email>\n    </author>\n    <author>\n      <email>karl@openwindow.co.za</email>\n      <name>Karl van Heerden</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <updated>2023-01-11T09:29:52+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-GB\">\n    <terms>\n      <term name=\"volume\" form=\"short\">v.</term>\n      <term name=\"available at\">accessed</term>\n      <term name=\"open-quote\">“</term>\n      <term name=\"close-quote\">”</term>\n      <term name=\"open-inner-quote\">‘</term>\n      <term name=\"close-inner-quote\">’</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <choose>\n      <if variable=\"editor translator interviewer\" match=\"any\">\n        <names variable=\"editor translator interviewer\" delimiter=\". \">\n          <label form=\"verb\" text-case=\"capitalize-first\" font-variant=\"normal\" suffix=\" \"/>\n          <name and=\"symbol\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" initialize-with=\"\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"never\" et-al-min=\"9\" et-al-use-first=\"8\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <names variable=\"director\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song webpage\" match=\"any\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" quotes=\"true\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" et-al-min=\"9\" et-al-use-first=\"1\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <names variable=\"director\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song webpage\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"date-issued\">\n    <choose>\n      <if type=\"article-journal interview\" match=\"none\">\n        <text macro=\"date-issued-year\"/>\n      </if>\n      <else-if type=\"interview\" match=\"any\">\n        <text macro=\"date-issue-long\"/>\n      </else-if>\n      <else-if variable=\"page\" match=\"none\">\n        <text macro=\"date-issued-year\"/>\n      </else-if>\n      <else-if is-numeric=\"page\">\n        <text macro=\"date-issued-year\"/>\n      </else-if>\n      <else>\n        <text term=\"in press\" prefix=\"(\" suffix=\")\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date-issued-no-parentheses\">\n    <choose>\n      <if type=\"article-journal\" match=\"none\">\n        <text macro=\"date-issued-year\"/>\n      </if>\n      <else-if variable=\"page\" match=\"none\">\n        <text macro=\"date-issued-year\"/>\n      </else-if>\n      <else-if is-numeric=\"page\">\n        <text macro=\"date-issued-year\"/>\n      </else-if>\n      <else>\n        <text term=\"in press\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date-issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date date-parts=\"year\" form=\"numeric\" variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date-access\">\n    <date variable=\"accessed\" prefix=\"(Accessed \" suffix=\")\">\n      <date-part name=\"day\" form=\"numeric-leading-zeros\" prefix=\"\"/>\n      <date-part name=\"month\" form=\"long\" prefix=\" \"/>\n      <date-part name=\"year\" prefix=\" \"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song webpage broadcast\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"book-details\">\n    <group delimiter=\". \">\n      <group delimiter=\" \">\n        <number vertical-align=\"baseline\" variable=\"edition\" form=\"ordinal\"/>\n        <label vertical-align=\"baseline\" variable=\"edition\" form=\"short\"/>\n      </group>\n      <group delimiter=\" \">\n        <label text-case=\"capitalize-first\" variable=\"volume\"/>\n        <text variable=\"volume\"/>\n      </group>\n      <group font-variant=\"normal\">\n        <text macro=\"editor-translator\" font-variant=\"normal\"/>\n      </group>\n      <group delimiter=\". \">\n        <choose>\n          <if type=\"broadcast\" match=\"any\">\n            <text variable=\"number\" prefix=\"Episode \"/>\n          </if>\n        </choose>\n        <text variable=\"version\"/>\n        <choose>\n          <if type=\"broadcast\" match=\"none\">\n            <text variable=\"medium\"/>\n          </if>\n        </choose>\n      </group>\n      <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n        <text variable=\"collection-title\"/>\n        <group delimiter=\" \">\n          <label variable=\"issue\" form=\"short\"/>\n          <text variable=\"collection-number\"/>\n        </group>\n      </group>\n      <choose>\n        <if type=\"paper-conference\" match=\"any\">\n          <date form=\"text\" variable=\"issued\"/>\n        </if>\n      </choose>\n      <text macro=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group>\n      <text variable=\"publisher-place\" suffix=\": \"/>\n      <text variable=\"publisher\"/>\n      <text variable=\"page\" prefix=\":\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"book chapter thesis\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <group delimiter=\" \">\n              <group prefix=\"https://doi.org/\">\n                <text variable=\"DOI\"/>\n              </group>\n            </group>\n          </if>\n          <else-if variable=\"URL\" match=\"all\">\n            <group delimiter=\" \">\n              <text variable=\"genre\" suffix=\".\"/>\n              <text variable=\"URL\"/>\n              <text macro=\"date-access\"/>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"date-issue-long\">\n    <choose>\n      <if match=\"any\" variable=\"issued\">\n        <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\">\n          <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n          <date-part name=\"month\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n    </names>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" year-suffix-delimiter=\",\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"false\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"date-issued-no-parentheses\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <choose>\n          <if type=\"motion_picture software\" match=\"none\">\n            <text macro=\"author-short\"/>\n          </if>\n          <else>\n            <text macro=\"title\" font-style=\"italic\"/>\n          </else>\n        </choose>\n        <text macro=\"date-issued-no-parentheses\"/>\n      </group>\n      <group>\n        <choose>\n          <if locator=\"page\" match=\"any\">\n            <text variable=\"locator\" prefix=\":\"/>\n          </if>\n          <else>\n            <label variable=\"locator\" form=\"short\" prefix=\", \"/>\n            <text variable=\"locator\" prefix=\" \"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author-short\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author\" names-min=\"3\" names-use-first=\"1\"/>\n      <key macro=\"date-issued-no-parentheses\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"date-issued\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song webpage manuscript\" match=\"any\">\n            <text macro=\"title\" font-style=\"italic\"/>\n            <text macro=\"book-details\"/>\n          </if>\n          <else-if type=\"article-journal\" match=\"any\">\n            <text macro=\"title\"/>\n            <group>\n              <text variable=\"container-title\" quotes=\"false\" font-style=\"italic\" suffix=\" \"/>\n              <choose>\n                <if variable=\"volume issue\" match=\"any\">\n                  <group>\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                </if>\n                <else>\n                  <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n                    <date-part name=\"month\" form=\"long\"/>\n                    <date-part name=\"day\" prefix=\", \"/>\n                  </date>\n                </else>\n              </choose>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n            <text macro=\"title\"/>\n            <group>\n              <text variable=\"container-title\" font-style=\"italic\" suffix=\" \"/>\n              <choose>\n                <if match=\"any\" variable=\"issued\">\n                  <date variable=\"issued\">\n                    <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n                    <date-part name=\"month\" prefix=\" \"/>\n                  </date>\n                </if>\n                <else>\n                  <text term=\"no date\" form=\"short\"/>\n                </else>\n              </choose>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </else-if>\n          <else-if type=\"chapter entry-encyclopedia entry-dictionary\" match=\"any\">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </group>\n            <text macro=\"book-details\"/>\n          </else-if>\n          <else-if type=\"paper-conference\" match=\"any\">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"normal\"/>\n            <text macro=\"book-details\"/>\n          </else-if>\n          <else-if type=\"patent\" match=\"any\">\n            <group font-style=\"italic\">\n              <text variable=\"number\" prefix=\"Patent No. \"/>\n            </group>\n            <group delimiter=\": \">\n              <text macro=\"publisher\"/>\n              <text variable=\"authority\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <text macro=\"title\"/>\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              <text variable=\"number\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </else-if>\n          <else-if type=\"software\" match=\"any\">\n            <text macro=\"title\" font-style=\"italic\"/>\n            <text macro=\"book-details\"/>\n          </else-if>\n          <else>\n            <text macro=\"title\" font-style=\"normal\"/>\n            <text macro=\"book-details\"/>\n          </else>\n        </choose>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"