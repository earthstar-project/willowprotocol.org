"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>American Physiological Society</title>\n    <title-short>APS</title-short>\n    <id>http://www.zotero.org/styles/american-physiological-society</id>\n    <link href=\"http://www.zotero.org/styles/american-physiological-society\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/vancouver\" rel=\"template\"/>\n    <link href=\"https://journals.physiology.org/author-info.references\" rel=\"documentation\"/>\n    <author>\n      <name>Michael Berkowitz</name>\n      <email>mberkowi@gmu.edu</email>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <contributor>\n      <name>Richard Karnesky</name>\n      <email>karnesky+zotero@gmail.com</email>\n      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>\n    </contributor>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <summary>A style for many APS journals</summary>\n    <updated>2020-05-21T09:18:04+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"no date\" form=\"long\">date unknown</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" font-weight=\"bold\" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \" suffix=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" prefix=\"edited by \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <text variable=\"publisher-place\" suffix=\": \"/>\n        <text variable=\"publisher\" suffix=\", \"/>\n        <text macro=\"date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"article-journal\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" prefix=\"[\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\"/>\n      <!--don't use if there is a DOI-->\n      <else>\n        <choose>\n          <if variable=\"URL\">\n            <group delimiter=\" \" prefix=\" \">\n              <group>\n                <text variable=\"URL\"/>\n              </group>\n              <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n                <date variable=\"accessed\">\n                  <date-part name=\"day\"/>\n                  <date-part name=\"month\" prefix=\" \" suffix=\" \" form=\"short\"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <!--this macro adds [online] to the end of an online ressource, but only when no DOI is given-->\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <choose>\n                <if variable=\"URL\">\n                  <text variable=\"title\"/>\n                </if>\n                <else>\n                  <text variable=\"title\" font-style=\"italic\"/>\n                </else>\n              </choose>\n            </if>\n            <else>\n              <text variable=\"title\"/>\n            </else>\n          </choose>\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"online\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \" suffix=\".\">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <group>\n              <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"editor\" prefix=\", \"/>\n            </group>\n            <text variable=\"event\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text variable=\"page\" prefix=\", p. \" suffix=\".\"/>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <choose>\n            <if variable=\"issue volume\" match=\"any\">\n              <text variable=\"container-title\" suffix=\" \" form=\"short\" font-style=\"italic\" strip-periods=\"true\"/>\n              <text variable=\"volume\"/>\n              <text variable=\"page\" prefix=\": \"/>\n              <text macro=\"date\" prefix=\", \" suffix=\".\"/>\n              <text variable=\"DOI\" prefix=\" doi: \"/>\n            </if>\n          </choose>\n        </else-if>\n        <else>\n          <group delimiter=\": \" suffix=\".\">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n              <text variable=\"volume\"/>\n              <text variable=\"publisher\"/>\n            </group>\n            <group delimiter=\", \">\n              <text variable=\"page\"/>\n              <text macro=\"date\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"