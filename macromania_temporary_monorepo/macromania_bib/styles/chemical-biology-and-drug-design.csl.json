"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>Chemical Biology &amp; Drug Design</title>\n    <id>http://www.zotero.org/styles/chemical-biology-and-drug-design</id>\n    <link href=\"http://www.zotero.org/styles/chemical-biology-and-drug-design\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/advanced-functional-materials\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/(ISSN)1747-0285/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <category field=\"chemistry\"/>\n    <category field=\"medicine\"/>\n    <issn>1747-0277</issn>\n    <eissn>1747-0285</eissn>\n    <updated>2017-08-03T14:07:03+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\", \" delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <text variable=\"collection-title\"/>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"book-container\">\n    <group delimiter=\" \">\n      <text term=\"in\" text-case=\"capitalize-first\"/>\n      <text variable=\"container-title\" font-style=\"italic\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"full-issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n      <date-part name=\"day\" suffix=\", \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"margin\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"] \"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"article-magazine\">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"full-issued\" suffix=\",\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"genre\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"editor\" prefix=\" \"/>\n            <text macro=\"series\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"book-container\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"series\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            <group delimiter=\", \">\n              <text macro=\"issued\" font-weight=\"bold\"/>\n              <text variable=\"volume\" font-style=\"italic\"/>\n              <text variable=\"page-first\" form=\"long\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"issued\" font-weight=\"bold\"/>\n              <text variable=\"volume\" font-style=\"italic\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"