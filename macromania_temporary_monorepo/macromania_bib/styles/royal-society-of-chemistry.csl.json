"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Royal Society of Chemistry</title>\n    <id>http://www.zotero.org/styles/royal-society-of-chemistry</id>\n    <link href=\"http://www.zotero.org/styles/royal-society-of-chemistry\" rel=\"self\"/>\n    <link href=\"http://www.rsc.org/learn-chemistry/resource/download/res00001664/cmp00004956/pdf\" rel=\"documentation\"/>\n    <link href=\"http://www.rsc.org/journals-books-databases/journal-authors-reviewers/prepare-your-article/#format-layout\" rel=\"documentation\"/>\n    <author>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Ivan Bushmarinov</name>\n      <email>ib@ineos.ac.ru</email>\n    </contributor>\n    <contributor>\n      <name>Steven R. Kirk</name>\n      <email>steven.kirk@cantab.net</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <summary>The Royal Society of Chemistry journal style.</summary>\n    <updated>2022-12-05T05:15:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" suffix=\" \"/>\n      <name initialize-with=\". \" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"presented at\">presented in part at the</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"patent\">\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"authority\">\n              <text variable=\"authority\"/>\n            </if>\n            <else>\n              <text variable=\"publisher\"/>\n            </else>\n          </choose>\n          <text variable=\"number\"/>\n        </group>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name initialize-with=\". \" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"book-container\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <text term=\"in\"/>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </group>\n      <text macro=\"editor\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"speech\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"month\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text value=\"edn\" suffix=\".\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"title\" suffix=\", \"/>\n          <group delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"issued\"/>\n            <text variable=\"volume\" prefix=\"vol. \"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"book-container\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"issued\"/>\n            <text variable=\"volume\" prefix=\"vol. \"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\", \">\n            <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            <choose>\n              <if variable=\"page\">\n                <text macro=\"issued\"/>\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text variable=\"page\"/>\n              </if>\n              <else>\n                <text variable=\"DOI\" prefix=\", DOI:\"/>\n              </else>\n            </choose>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" form=\"short\"/>\n            <text variable=\"URL\"/>\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <text term=\"accessed\"/>\n              <date form=\"text\" variable=\"accessed\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"software\" match=\"any\">\n          <group delimiter=\" \">\n            <text variable=\"title\" form=\"short\"/>\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <label variable=\"version\"/>\n              <text variable=\"version\"/>\n            </group>\n            <choose>\n              <if variable=\"publisher\">\n                <text macro=\"publisher\"/>\n              </if>\n              <else>\n                <choose>\n                  <if variable=\"DOI\">\n                    <text variable=\"DOI\" prefix=\", DOI:\"/>\n                  </if>\n                  <else-if variable=\"URL\">\n                    <text variable=\"URL\" prefix=\" \"/>\n                  </else-if>\n                </choose>\n              </else>\n            </choose>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text term=\"presented at\"/>\n              <text variable=\"event\"/>\n            </group>\n            <text variable=\"event-place\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <text macro=\"issued\"/>\n            <text variable=\"volume\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"