"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Journal of AOAC International</title>\n    <title-short>AOAC</title-short>\n    <id>http://www.zotero.org/styles/journal-of-aoac-international</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-aoac-international\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/photochemistry-and-photobiology\" rel=\"template\"/>\n    <link href=\"http://www.aoac.org/aoac_prod_imis/AOAC/AOAC_Member/PUBSCF/JAOACCF/JN_M.aspx\" rel=\"documentation\"/>\n    <link href=\"http://www.aoac.org/aoac_prod_imis/AOAC_Docs/Journal/Author_Resources.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>citationstyler@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <category field=\"biology\"/>\n    <issn>1060-3271</issn>\n    <eissn>1944-7922</eissn>\n    <updated>2019-07-17T12:01:30+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\">accessed on</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"symbol\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <choose>\n        <if type=\"thesis\" match=\"none\">\n          <text variable=\"publisher-place\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog post\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n      <else>\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography and=\"text\">\n    <layout>\n      <group delimiter=\". \">\n        <text variable=\"citation-number\"/>\n        <group display=\"block\">\n          <text macro=\"author\"/>\n          <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\", \" prefix=\" \">\n                <text macro=\"title\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\", \" prefix=\" \">\n                <group delimiter=\", \">\n                  <group delimiter=\" \">\n                    <text term=\"in\"/>\n                    <text variable=\"container-title\" font-style=\"italic\"/>\n                    <text macro=\"volume\" prefix=\"Vol. \"/>\n                  </group>\n                  <text macro=\"editor\"/>\n                  <text variable=\"collection-title\" prefix=\" \"/>\n                </group>\n                <text macro=\"publisher\" prefix=\" \"/>\n                <group delimiter=\" \">\n                  <label strip-periods=\"true\" variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"thesis\" match=\"any\">\n              <group delimiter=\". \">\n                <text macro=\"title\"/>\n                <group delimiter=\", \">\n                  <text variable=\"genre\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\" \" prefix=\" \">\n                <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n                <group delimiter=\", \">\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </else>\n          </choose>\n        </group>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"