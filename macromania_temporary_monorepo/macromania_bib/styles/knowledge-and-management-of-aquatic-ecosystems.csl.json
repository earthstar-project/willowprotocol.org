"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Knowledge &amp; Management of Aquatic Ecosystems</title>\n    <title-short>KMAE</title-short>\n    <id>http://www.zotero.org/styles/knowledge-and-management-of-aquatic-ecosystems</id>\n    <link href=\"http://www.zotero.org/styles/knowledge-and-management-of-aquatic-ecosystems\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-journal-of-enology-and-viticulture\" rel=\"template\"/>\n    <link href=\"https://www.kmae-journal.org/author-information/instructions-for-authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <eissn>1961-9502</eissn>\n    <updated>2019-08-26T12:51:37+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <text macro=\"editor-translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\"\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"version\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n        <text variable=\"title\" font-style=\"normal\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog post\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\" line-spacing=\"2\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author-short\"/>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"editor-translator\" prefix=\" \"/>\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <text variable=\"number-of-pages\"/>\n                <label plural=\"never\" variable=\"number-of-pages\" form=\"short\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\" prefix=\" \"/>\n              <group>\n                <text variable=\"genre\" prefix=\" \"/>\n                <text variable=\"number\" prefix=\" No. \"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n                <text variable=\"publisher\"/>\n                <text variable=\"publisher-place\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\" font-style=\"normal\"/>\n                <text macro=\"editor-translator\"/>\n              </group>\n              <group delimiter=\", \">\n                <text variable=\"container-title\"/>\n                <text macro=\"publisher\" prefix=\" \" suffix=\".\"/>\n                <text macro=\"pages\" prefix=\" \" suffix=\".\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"editor-translator\" prefix=\" \"/>\n              <group delimiter=\" \" prefix=\" \">\n                <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n                <group delimiter=\": \">\n                  <text variable=\"volume\"/>\n                  <text macro=\"pages\"/>\n                </group>\n              </group>\n              <text macro=\"access\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"