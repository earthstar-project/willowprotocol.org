"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"expanded\" default-locale=\"en-GB\">\n  <info>\n    <title>European Society of Cardiology</title>\n    <id>http://www.zotero.org/styles/european-society-of-cardiology</id>\n    <link href=\"http://www.zotero.org/styles/european-society-of-cardiology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/vancouver\" rel=\"template\"/>\n    <link href=\"https://academic.oup.com/eurheartj/pages/general_instructions#2.4\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Matthieu Depuydt</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-GB\">\n    <terms>\n      <term name=\"section\" form=\"short\">sect.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <group delimiter=\": \">\n      <choose>\n        <if type=\"chapter\" match=\"any\">\n          <text term=\"in\" text-case=\"capitalize-first\" prefix=\" \"/>\n        </if>\n      </choose>\n      <names variable=\"editor\" suffix=\".\">\n        <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n        <label form=\"short\" prefix=\", \"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group>\n      <choose>\n        <if type=\"book chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <group delimiter=\"; \">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n              <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n            </group>\n            <group>\n              <label variable=\"page\" form=\"short\" plural=\"never\" strip-periods=\"true\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </if>\n      </choose>\n    </group>\n    <text value=\"\" prefix=\" \" suffix=\";\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\" type=\"webpage\" match=\"all\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <date variable=\"accessed\" form=\"text\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"book article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n        <text macro=\"edition\" prefix=\" \"/>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\" prefix=\" \"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n  </macro>\n  <macro name=\"journal-location\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\":\">\n          <group delimiter=\";\">\n            <text macro=\"date\"/>\n            <text variable=\"volume\" font-weight=\"bold\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\". \">\n        <group delimiter=\". \">\n          <text macro=\"author\"/>\n          <text macro=\"title\"/>\n        </group>\n        <group delimiter=\" \" suffix=\". \">\n          <text macro=\"editor\"/>\n          <group delimiter=\". \">\n            <text macro=\"container-title\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text macro=\"journal-location\"/>\n        </group>\n      </group>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"