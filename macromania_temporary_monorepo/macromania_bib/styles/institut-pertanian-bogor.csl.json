uffix=\";\" font-style=\"italic\"/>\n                <text variable=\"event-place\" suffix=\".\"/>\n                <text macro=\"publisher\"/>\n                <text prefix=\" \" macro=\"collection\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"review review-book\" match=\"any\">\n            <text macro=\"editor\" prefix=\" \" suffix=\"[ulasan].\"/>\n            <group prefix=\" \" suffix=\".\">\n              <text macro=\"review\" suffix=\".\"/>\n              <group prefix=\" \">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </else-if>\n          <else-if type=\"personal_communication\" match=\"any\">\n            <text macro=\"editor\" suffix=\"[komunikasi singkat].\"/>\n          </else-if>\n          <else>\n            <text variable=\"original-title\" prefix=\" [\" suffix=\"].\"/>\n            <text macro=\"editor\" prefix=\" \" suffix=\"[editorial].\"/>\n            <group prefix=\" \" suffix=\".\">\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\" suffix=\".\"/>\n              <text variable=\"volume\" prefix=\" \"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              <text variable=\"page\" prefix=\":\"/>\n              <text variable=\"DOI\" prefix=\".doi:\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"