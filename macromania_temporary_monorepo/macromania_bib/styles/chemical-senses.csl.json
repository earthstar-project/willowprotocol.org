"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>Chemical Senses</title>\n    <id>http://www.zotero.org/styles/chemical-senses</id>\n    <link href=\"http://www.zotero.org/styles/chemical-senses\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/cell\" rel=\"template\"/>\n    <link href=\"http://www.oxfordjournals.org/our_journals/chemse/for_authors/instructions.html#9\" rel=\"documentation\"/>\n    <author>\n      <name>Charles Parnot</name>\n      <email>charles.parnot@gmail.com</email>\n      <uri>http://twitter.com/cparnot</uri>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"chemistry\"/>\n    <issn>0379-864X</issn>\n    <eissn>1464-3553</eissn>\n    <updated>2012-09-13T20:33:11+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" initialize-with=\".\" and=\"text\" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\" and=\"text\" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"issued\"/>\n        <choose>\n          <if type=\"article article-magazine article-newspaper article-journal review\" match=\"any\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" strip-periods=\"true\"/>\n            <group delimiter=\":\">\n              <text variable=\"volume\"/>\n              <text variable=\"page\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text variable=\"title\"/>\n            <group prefix=\"In: \" delimiter=\". \">\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\" text-case=\"title\" strip-periods=\"true\"/>\n              <text macro=\"publisher\"/>\n              <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"genre\" suffix=\".\"/>\n            <text variable=\"publisher\"/>\n          </else-if>\n          <else>\n            <text variable=\"title\"/>\n            <text macro=\"publisher\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"