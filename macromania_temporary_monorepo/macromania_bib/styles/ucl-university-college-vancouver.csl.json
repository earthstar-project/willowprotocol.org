"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" page-range-format=\"minimal\" default-locale=\"en-US\">\n  <info>\n    <title>UCL University College - Vancouver (English)</title>\n    <id>http://www.zotero.org/styles/ucl-university-college-vancouver</id>\n    <link href=\"http://www.zotero.org/styles/ucl-university-college-vancouver\" rel=\"self\"/>\n    <link href=\"https://sites.google.com/ucl.dk/ucl-formelle-krav/vancouver\" rel=\"documentation\"/>\n    <author>\n      <name>Roald Erik Fr√∏sig</name>\n      <email>roaldfrosig@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Camilla K. Andresen</name>\n      <email>ckan1@ucl.dk</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <updated>2014-12-17T13:56:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"online\">internet</term>\n      <term name=\"accessed\">cited</term>\n      <term name=\"available at\">available from</term>\n      <term name=\"no date\">date unknown</term>\n      <term name=\"page\" form=\"short\">p.</term>\n    </terms>\n  </locale>\n  <!--                                                 CITATION MACRO -->\n  <macro name=\"page\">\n    <choose>\n      <if is-numeric=\"locator\">\n        <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <text variable=\"locator\"/>\n      </else>\n    </choose>\n  </macro>\n  <!--                                                    BIBLIOGRAPHY MACROS -->\n  <macro name=\"author\">\n    <choose>\n      <if type=\"legislation\">\n      </if>\n      <else-if type=\"entry-encyclopedia\" variable=\"author\" match=\"all\">\n        <names variable=\"author\">\n          <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\"/>\n        </names>\n      </else-if>\n      <else-if variable=\"author editor composer\" match=\"any\">\n        <names variable=\"author\">\n          <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\"/>\n          <label prefix=\", \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"composer\"/>\n          </substitute>\n        </names>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if variable=\"author\">\n            <text variable=\"title\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"post-weblog\" match=\"none\">\n        <text variable=\"title\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"editor-container\">\n    <group delimiter=\": \">\n      <text term=\"in\" text-case=\"capitalize-first\"/>\n      <group delimiter=\". \">\n        <names variable=\"editor\">\n          <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\"/>\n          <label prefix=\", \"/>\n        </names>\n        <text variable=\"container-title\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\">\n        <choose>\n          <if variable=\"container-title-short\">\n            <text variable=\"container-title-short\" strip-periods=\"true\"/>\n          </if>\n          <else>\n            <text variable=\"container-title\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"entry-encyclopedia\" variable=\"author\">\n        <text macro=\"editor-container\"/>\n      </else-if>\n      <else-if type=\"chapter\">\n        <text macro=\"editor-container\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"material\">\n    <choose>\n      <if type=\"report\">\n        <text macro=\"online\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n      <else-if type=\"thesis\" variable=\"genre\">\n        <group delimiter=\", \">\n          <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          <text macro=\"online\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </else-if>\n      <else-if variable=\"medium\">\n        <group delimiter=\", \">\n          <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n          <text macro=\"online\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"online\" prefix=\" [\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if variable=\"URL\">\n        <choose>\n          <if type=\"legislation bill\" match=\"any\"/>\n          <else>\n            <text term=\"online\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </if>\n        <else>\n          <text variable=\"edition\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"city\">\n    <text variable=\"publisher-place\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"year\">\n    <choose>\n      <if type=\"legislation bill\" match=\"any\"/>\n      <else-if variable=\"issued accessed\" match=\"none\">\n        <text term=\"no date\" prefix=\"[\" suffix=\"]\"/>\n      </else-if>\n      <else-if type=\"article-newspaper broadcast\" match=\"any\">\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </else-if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if type=\"legislation bill\" match=\"any\"/>\n      <else-if variable=\"URL\">\n        <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n          <text term=\"accessed\"/>\n          <date variable=\"accessed\" delimiter=\" \">\n            <date-part name=\"year\"/>\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"entry\">\n    <choose>\n      <if type=\"entry-encyclopedia\">\n        <choose>\n          <if variable=\"author\"/>\n          <else>\n            <text variable=\"title\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"post-weblog\">\n        <text variable=\"title\" suffix=\";\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\":\">\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <group delimiter=\":\">\n          <text variable=\"section\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\" \" suffix=\", \">\n          <text term=\"book\" form=\"short\"/>\n          <text variable=\"volume\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"legislation\">\n        <choose>\n          <if variable=\"number\">\n            <text variable=\"number\"/>\n          </if>\n          <else>\n            <text variable=\"volume\"/>\n          </else>\n        </choose>\n        <date variable=\"issued\" prefix=\" af \" delimiter=\"/\">\n          <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n          <date-part name=\"month\" form=\"numeric-leading-zeros\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else-if type=\"bill\">\n        <number variable=\"number\"/>\n        <date variable=\"issued\" prefix=\". \" delimiter=\"/\">\n          <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n          <date-part name=\"month\" form=\"numeric-leading-zeros\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if type=\"legislation bill\" match=\"none\">\n        <group delimiter=\": \">\n          <text term=\"available at\" text-case=\"capitalize-first\"/>\n          <text variable=\"URL\" text-decoration=\"underline\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <!--                                                                END OF MACROS -->\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n      <text macro=\"page\" prefix=\", \"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\". \" suffix=\".\">\n        <group delimiter=\" \">\n          <group delimiter=\". \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"container-title\"/>\n          </group>\n          <text macro=\"material\"/>\n        </group>\n        <text macro=\"edition\"/>\n        <group delimiter=\"; \">\n          <group delimiter=\": \">\n            <text macro=\"city\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <group delimiter=\" \">\n            <group delimiter=\". \">\n              <text macro=\"year\"/>\n              <text macro=\"entry\"/>\n            </group>\n            <text macro=\"accessed\"/>\n          </group>\n          <text macro=\"locator\"/>\n        </group>\n        <text macro=\"url\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"