"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Journal of Tropical Life Science</title>\n    <title-short>J Trop. Life. Science</title-short>\n    <id>http://www.zotero.org/styles/journal-of-tropical-life-science</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-tropical-life-science\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/springer-basic-brackets\" rel=\"template\"/>\n    <link href=\"http://www.jtrolis.ub.ac.id/index.php/jtrolis/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>JTLS</name>\n      <email>jtls@ub.ac.id</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>2087-5517</issn>\n    <eissn>2527-4376</eissn>\n    <updated>2017-05-24T09:31:29+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\". \" suffix=\". \">\n          <text variable=\"title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\" suffix=\". \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-parenth\">\n    <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"5\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"year-parenth\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if variable=\"page\">\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\" \" suffix=\". \">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n                <names variable=\"editor\">\n                  <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n                  <label form=\"short\" strip-periods=\"true\" prefix=\" \" suffix=\"\"/>\n                </names>\n                <group delimiter=\", \">\n                  <text variable=\"container-title\" form=\"short\"/>\n                  <text macro=\"edition\"/>\n                </group>\n              </group>\n              <group delimiter=\". \">\n                <text macro=\"publisher\"/>\n                <group delimiter=\" \">\n                  <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n                  <text variable=\"page\" suffix=\".\"/>\n                </group>\n              </group>\n            </if>\n            <else>\n              <group>\n                <text variable=\"container-title\" strip-periods=\"true\" suffix=\" \"/>\n                <text variable=\"volume\" suffix=\" \"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\"): \"/>\n                <text variable=\"page\" suffix=\".\"/>\n                <text variable=\"DOI\" prefix=\" doi: \" suffix=\".\"/>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <else-if variable=\"DOI\">\n          <group delimiter=\". \">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n            <text prefix=\"doi: \" variable=\"DOI\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"publisher\" suffix=\".\"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group>\n            <text variable=\"URL\" suffix=\". \"/>\n            <text macro=\"year-parenth\"/>\n            <text value=\"Accessed\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <date date-parts=\"year-month\" form=\"text\" variable=\"accessed\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <group delimiter=\". \">\n              <group delimiter=\"; \">\n                <text variable=\"event\"/>\n                <text variable=\"event-place\"/>\n              </group>\n              <names variable=\"editor\">\n                <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n              </names>\n              <text macro=\"publisher\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\": \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"