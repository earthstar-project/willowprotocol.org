"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" default-locale=\"en-GB\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\">\n  <info>\n    <title>British Journal of Cancer</title>\n    <title-short>BJC</title-short>\n    <id>http://www.zotero.org/styles/british-journal-of-cancer</id>\n    <link href=\"http://www.zotero.org/styles/british-journal-of-cancer\" rel=\"self\"/>\n    <link href=\"http://www.nature.com/bjc/authors/submit.html#how-to-submit\" rel=\"documentation\"/>\n    <author>\n      <name>Iain Gallagher</name>\n      <email>iaingallagher@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Julie Courraud</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <issn>0007-0920</issn>\n    <eissn>1532-1827</eissn>\n    <summary>Style for the British Journal of Cancer</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-US\">\n    <terms>\n      <term name=\"et-al\">et al</term>\n    </terms>\n  </locale>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\" \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter-precedes-last=\"never\" delimiter-precedes-et-al=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group prefix=\"(\" delimiter=\": \" suffix=\")\">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\" and=\"text\" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"DOI\">\n    <group prefix=\"doi:\">\n      <text variable=\"DOI\"/>\n    </group>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if type=\"webpage\">\n        <text variable=\"URL\"/>\n        <group delimiter=\": \" prefix=\" (\" suffix=\")\">\n          <text term=\"accessed\"/>\n          <date variable=\"accessed\" form=\"numeric\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n        <choose>\n          <if type=\"article article-magazine article-newspaper article-journal review\" match=\"any\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\" strip-periods=\"true\"/>\n            <text variable=\"volume\" font-style=\"normal\" font-weight=\"bold\" suffix=\":\"/>\n            <group delimiter=\", \">\n              <text variable=\"page\"/>\n              <text macro=\"DOI\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" prefix=\"In \" suffix=\",\" text-case=\"title\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\" suffix=\",\"/>\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </else-if>\n          <else-if type=\"thesis\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"genre\" suffix=\".\"/>\n            <text variable=\"publisher\"/>\n          </else-if>\n          <else>\n            <text variable=\"title\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"accessed\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"