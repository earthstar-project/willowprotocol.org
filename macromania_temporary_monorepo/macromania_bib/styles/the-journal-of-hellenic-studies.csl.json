"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" initialize-with-hyphen=\"true\" page-range-format=\"chicago\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>The Journal of Hellenic Studies</title>\n    <id>http://www.zotero.org/styles/the-journal-of-hellenic-studies</id>\n    <link href=\"http://www.zotero.org/styles/the-journal-of-hellenic-studies\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/cardiff-university-harvard\" rel=\"template\"/>\n    <link href=\"http://www.hellenicsociety.org.uk/publications/\" rel=\"documentation\"/>\n    <author>\n      <name>Polly Low</name>\n    </author>\n    <author>\n      <name>Zoe Young</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <issn>0075-4269</issn>\n    <eissn>2041-4099</eissn>\n    <summary>JHS author-date style</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-GB\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">tr.</term>\n      <term name=\"chapter\" form=\"short\">\n        <single>chapter </single>\n        <multiple>chapters </multiple>\n      </term>\n      <term name=\"note\" form=\"short\">\n        <single>n.</single>\n        <multiple>nn.</multiple>\n      </term>\n      <term name=\"edition\" form=\"short\">\n        <single>edn</single>\n        <multiple>edns</multiple>\n      </term>\n      <term name=\"open-quote\">‘</term>\n      <term name=\"close-quote\">’</term>\n      <term name=\"open-inner-quote\">“</term>\n      <term name=\"close-inner-quote\">”</term>\n    </terms>\n    <style-options punctuation-in-quote=\"false\"/>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-contributors\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" sort-separator=\", \" delimiter-precedes-last=\"never\" initialize-with=\".\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\"), \" strip-periods=\"false\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\" delimiter=\", \">\n      <label form=\"short\" prefix=\", \" suffix=\" \" strip-periods=\"false\"/>\n      <name and=\"text\" sort-separator=\", \" delimiter-precedes-last=\"never\" initialize-with=\".\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text value=\"Available at: \"/>\n        <text variable=\"URL\"/>\n        <group prefix=\" [\" suffix=\"]\">\n          <text value=\"Accessed:\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group>\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" prefix=\" \" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"additional-info\">\n    <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n      <group>\n        <text variable=\"collection-title\" suffix=\" \"/>\n        <text variable=\"collection-number\"/>\n      </group>\n      <group>\n        <number variable=\"number-of-volumes\" form=\"numeric\"/>\n        <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\" strip-periods=\"true\"/>\n      </group>\n      <text macro=\"edition\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" prefix=\" (\" suffix=\") \"/>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\" prefix=\" \"/>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if locator=\"page\">\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <layout delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n        <text macro=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"8\" et-al-use-first=\"1\" subsequent-author-substitute=\"-----\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \">\n            <text macro=\"title\"/>\n            <number variable=\"volume\" form=\"numeric\" prefix=\" \" suffix=\" \"/>\n            <text macro=\"editor\"/>\n            <text macro=\"translator\"/>\n            <text macro=\"additional-info\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\",\"/>\n          <group prefix=\" \">\n            <text term=\"in\" suffix=\" \"/>\n            <text macro=\"container-contributors\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <number variable=\"volume\" form=\"numeric\" prefix=\" \" suffix=\" \"/>\n            <text macro=\"translator\"/>\n            <text macro=\"additional-info\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <group>\n              <text macro=\"title\" suffix=\", \"/>\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\" suffix=\" \"/>\n              <text variable=\"volume\" suffix=\",\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\" \" delimiter=\", \" suffix=\" \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group prefix=\" \">\n            <text macro=\"title\" suffix=\" [Online]\"/>\n            <text macro=\"edition\"/>\n            <text prefix=\" \" macro=\"access\"/>\n          </group>\n        </else-if>\n        <else>\n          <group>\n            <text macro=\"title\" prefix=\". \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group prefix=\" \">\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group prefix=\", \">\n              <label variable=\"locator\" form=\"short\"/>\n              <text macro=\"pages\"/>\n            </group>\n            <group prefix=\". \">\n              <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"