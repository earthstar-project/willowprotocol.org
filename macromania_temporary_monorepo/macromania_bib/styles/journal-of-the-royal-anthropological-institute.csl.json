"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Journal of the Royal Anthropological Institute</title>\n    <title-short>JRAI</title-short>\n    <id>http://www.zotero.org/styles/journal-of-the-royal-anthropological-institute</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-the-royal-anthropological-institute\" rel=\"self\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/(ISSN)1467-9655/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>James Johnston</name>\n      <email>thejamesjohnston@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"anthropology\"/>\n    <issn>1359-0987</issn>\n    <eissn>1467-9655</eissn>\n    <summary>The JRAI style.</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" prefix=\"(\" suffix=\") \" strip-periods=\"true\"/>\n      <name and=\"symbol\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"secondary-contributors-chapter\">\n    <group delimiter=\". \">\n      <choose>\n        <if variable=\"author editor\" match=\"any\">\n          <names variable=\"translator\" prefix=\"(\" suffix=\")\">\n            <label form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n            <name and=\"symbol\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          </names>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"editor-translator\">\n    <group delimiter=\"; \" prefix=\" (\" suffix=\")\">\n      <choose>\n        <if variable=\"author\" match=\"any\">\n          <names variable=\"editor\">\n            <label form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n            <name and=\"symbol\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          </names>\n        </if>\n      </choose>\n      <choose>\n        <if variable=\"author editor\" match=\"any\">\n          <names variable=\"translator\">\n            <label form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n            <name and=\"symbol\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          </names>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\">\n      <label form=\"verb\" prefix=\" \" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"symbol\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"first\" and=\"symbol\" sort-separator=\", \" font-variant=\"small-caps\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\" (available on-line: \" suffix=\")\">\n          <text variable=\"URL\" font-style=\"italic\"/>\n          <text term=\"accessed\" prefix=\", \" suffix=\" \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text variable=\"volume\" prefix=\", vol. \"/>\n        <text macro=\"editor-translator\"/>\n      </if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"title\"/>\n        <text variable=\"volume\" prefix=\", vol. \"/>\n        <text macro=\"secondary-contributors-chapter\" prefix=\" \"/>\n      </else-if>\n      <else-if type=\"article-journal\">\n        <text variable=\"title\"/>\n        <text macro=\"secondary-contributors-chapter\" prefix=\" \"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n        <text variable=\"volume\" prefix=\", vol. \"/>\n        <text macro=\"editor-translator\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group prefix=\", \">\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" text-case=\"capitalize-first\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <group>\n      <text term=\"presented at\" suffix=\" \"/>\n      <text variable=\"event\"/>\n      <text variable=\"event-place\" prefix=\", \"/>\n      <text macro=\"day-month\" prefix=\", \"/>\n    </group>\n  </macro>\n  <macro name=\"day-month\">\n    <date variable=\"issued\">\n      <date-part name=\"day\"/>\n      <date-part name=\"month\" prefix=\" \"/>\n    </date>\n  </macro>\n  <macro name=\"locators\">\n    <group prefix=\". \" delimiter=\" \">\n      <text variable=\"container-title\" font-style=\"italic\"/>\n      <group delimiter=\", \">\n        <text variable=\"volume\" font-weight=\"bold\"/>\n        <text variable=\"page\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <group prefix=\". (\" suffix=\")\">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\" prefix=\" \" font-weight=\"bold\"/>\n    </group>\n  </macro>\n  <macro name=\"chapter\">\n    <group delimiter=\" \" prefix=\".\">\n      <text term=\"in\" text-case=\"capitalize-first\" prefix=\" \"/>\n      <text variable=\"container-title\" font-style=\"italic\"/>\n    </group>\n  </macro>\n  <macro name=\"genre\">\n    <text variable=\"genre\" text-case=\"capitalize-first\" prefix=\". \"/>\n  </macro>\n  <macro name=\"thesis\">\n    <group prefix=\". \" delimiter=\", \">\n      <text variable=\"genre\"/>\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"news\">\n    <group prefix=\". \" delimiter=\", \">\n      <text variable=\"container-title\" font-style=\"italic\"/>\n      <text macro=\"day-month\"/>\n      <text macro=\"edition\"/>\n      <text variable=\"section\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\" prefix=\" (\" suffix=\")\" text-case=\"lowercase\"/>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"6\" et-al-use-first=\"3\" subsequent-author-substitute=\"&#8211;&#8211;&#8211;&#8211;&#8211;&#8211;&#8211;\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\" \"/>\n      <text macro=\"year-date\" suffix=\".\"/>\n      <text macro=\"title\" prefix=\" \"/>\n      <choose>\n        <if type=\"article-newspaper article-magazine\" match=\"any\">\n          <text macro=\"news\"/>\n        </if>\n        <else-if type=\"thesis\">\n          <text macro=\"thesis\"/>\n        </else-if>\n        <else-if type=\"speech\">\n          <text macro=\"genre\"/>\n          <text macro=\"event\" prefix=\" \"/>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"edition\" prefix=\". (\" suffix=\").\"/>\n          <text macro=\"series\"/>\n          <text macro=\"publisher\" prefix=\". \"/>\n        </else-if>\n        <else-if type=\"manuscript\">\n          <text macro=\"genre\"/>\n        </else-if>\n        <else-if type=\"interview\">\n          <text macro=\"interviewer\" prefix=\".\"/>\n          <text macro=\"medium\"/>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"chapter\"/>\n          <text macro=\"editor\" prefix=\" \"/>\n          <text macro=\"pages\"/>\n          <text macro=\"edition\" prefix=\". (\" suffix=\")\"/>\n          <text macro=\"series\"/>\n          <text macro=\"publisher\" prefix=\". \"/>\n        </else-if>\n        <else>\n          <text macro=\"locators\"/>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"