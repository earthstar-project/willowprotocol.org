"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" default-locale=\"en-GB\" page-range-format=\"expanded\" demote-non-dropping-particle=\"sort-only\" and=\"text\" delimiter-precedes-last=\"never\">\n  <info>\n    <title>St Patrick's College</title>\n    <id>http://www.zotero.org/styles/st-patricks-college</id>\n    <link href=\"http://www.zotero.org/styles/st-patricks-college\" rel=\"self\"/>\n    <link href=\"https://github.com/citation-style-language/styles/pull/370\" rel=\"documentation\"/>\n    <link href=\"http://www.st-patricks.ac.uk/\" rel=\"documentation\"/>\n    <author>\n      <name>Chris Maugino</name>\n      <email>christophe.maugino@st-patricks.ac.uk</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Harvard author-date style edited for St Patrick's College specifics.</summary>\n    <updated>2013-01-30T02:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" \" suffix=\" \"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <text macro=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <text macro=\"editor-short\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <name form=\"short\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book graphic\" match=\"any\">\n        <!--  webpage  -->\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" prefix=\"'\" suffix=\"'\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"vol_iss\">\n    <group delimiter=\" \">\n      <text variable=\"volume\"/>\n      <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \">\n      <text variable=\"archive\" font-style=\"italic\"/>\n      <text macro=\"location\" prefix=\"[Online]. \"/>\n      <group prefix=\"(\" suffix=\")\">\n        <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <date variable=\"accessed\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"location\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </if>\n      <else-if variable=\"URL\">\n        <text variable=\"URL\" prefix=\"Available at: \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"article-newspaper paper-conference broadcast\" match=\"any\">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n          </date>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"paper-conference\" match=\"any\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <!-- <label variable=\"page\" form=\"short\"/> -->\n      <text variable=\"page\" form=\"short\" prefix=\"pp.\"/>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\" \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" collapse=\"year\" et-al-min=\"3\" et-al-use-first=\"1\">\n    <!--  sort as cited  -->\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <!--  Does this next group do anything?  -->\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \">\n        <!--\n <text variable=\"type\" prefix=\"[DEBUG: \" suffix=\"]\"/> \n-->\n        <text macro=\"author\"/>\n        <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n        <choose>\n          <if type=\"book graphic\" match=\"any\">\n            <!-- <text variable=\"note\"/> -->\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"collection\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <group delimiter=\", \">\n              <text macro=\"issued\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text macro=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <group delimiter=\", \">\n                <text macro=\"editor\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text macro=\"collection\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"vol_iss\"/>\n                <text macro=\"issued\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <!--\n <else-if type=\"article-journal article-newspaper thesis webpage\" match=\"any\">broadcast personal_communication \n-->\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <group delimiter=\", \">\n              <choose>\n                <if variable=\"author\" match=\"any\">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <!--   -->\n                </if>\n              </choose>\n              <text macro=\"vol_iss\"/>\n              <text variable=\"genre\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n            <!-- </else-if> -->\n          </else>\n          <!--\n<else>\n        <text macro=\"title\"/>\n\t      <text macro=\"editor\"/>\n\t      <text variable=\"container-title\" font-style=\"italic\"/>\n\t      <group delimiter=\", \">\n              <text macro=\"issued\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n\t      </group>\n\t      </else>\n-->\n        </choose>\n      </group>\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"