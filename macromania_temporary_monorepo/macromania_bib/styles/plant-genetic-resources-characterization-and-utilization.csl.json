"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Plant Genetic Resources - Characterization and Utilization</title>\n    <id>http://www.zotero.org/styles/plant-genetic-resources-characterization-and-utilization</id>\n    <link href=\"http://www.zotero.org/styles/plant-genetic-resources-characterization-and-utilization\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/united-nations-conference-on-trade-and-development\" rel=\"template\"/>\n    <link href=\"https://www.cambridge.org/core/journals/plant-genetic-resources/information/instructions-contributors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>citationstyler@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1479-2621</issn>\n    <eissn>1479-263X</eissn>\n    <updated>2019-08-20T08:15:28+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"webpage\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <names variable=\"author\">\n          <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text variable=\"publisher\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"webpage\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <names variable=\"author\">\n          <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\"/>\n          <et-al font-style=\"italic\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text variable=\"publisher\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"publisher\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <group delimiter=\": \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access-date\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text value=\"See\"/>\n          <text variable=\"URL\"/>\n          <group prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\" suffix=\" \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-magazine article-newspaper\" match=\"any\">\n        <date variable=\"issued\" prefix=\" \">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </else-if>\n      <else>\n        <group delimiter=\" \" prefix=\" \">\n          <group prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\" suffix=\" \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <choose>\n          <if type=\"webpage\">\n            <choose>\n              <if variable=\"accessed\">\n                <date variable=\"accessed\">\n                  <date-part name=\"year\"/>\n                </date>\n              </if>\n              <else>\n                <text term=\"no date\" form=\"short\"/>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <group delimiter=\": \">\n          <text variable=\"volume\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"report\">\n        <group delimiter=\" \" prefix=\" \">\n          <label text-case=\"capitalize-first\" variable=\"number\" form=\"short\"/>\n          <text variable=\"number\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference song\" match=\"any\">\n        <choose>\n          <if variable=\"collection-title\">\n            <group delimiter=\", \" prefix=\" \">\n              <group delimiter=\" \">\n                <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <group>\n                <label text-case=\"capitalize-first\" variable=\"collection-number\" form=\"short\"/>\n                <text variable=\"collection-number\"/>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"6\" et-al-use-first=\"1\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\" \"/>\n      <text macro=\"year-date\" prefix=\"(\" suffix=\") \"/>\n      <choose>\n        <if type=\"report\">\n          <text macro=\"title\"/>\n          <group prefix=\". \" delimiter=\" \">\n            <choose>\n              <if variable=\"collection-title\">\n                <text variable=\"collection-title\"/>\n              </if>\n              <else-if variable=\"genre\">\n                <text variable=\"genre\"/>\n              </else-if>\n            </choose>\n            <text macro=\"locators\"/>\n          </group>\n          <group prefix=\". \" delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text variable=\"page\" prefix=\" \"/>\n          </group>\n          <text macro=\"access-date\" prefix=\".\"/>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n          <text macro=\"title\"/>\n          <choose>\n            <if variable=\"collection-title\">\n              <text variable=\"collection-title\" prefix=\". \"/>\n              <text macro=\"locators\" prefix=\", \"/>\n            </if>\n          </choose>\n          <text macro=\"publisher\" prefix=\". \"/>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n            <text variable=\"collection-title\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"locators\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-magazine article-newspaper\" match=\"any\">\n          <text macro=\"title\"/>\n          <text macro=\"access-date\" prefix=\".\"/>\n        </else-if>\n        <else-if type=\"speech\">\n          <text macro=\"title\"/>\n          <text variable=\"event\" prefix=\". Presented at the \"/>\n          <text variable=\"publisher-place\" prefix=\". \"/>\n          <date variable=\"issued\">\n            <date-part name=\"day\" prefix=\". \"/>\n            <date-part name=\"month\" prefix=\" \"/>\n          </date>\n          <text variable=\"URL\" prefix=\". Available at \" font-style=\"normal\"/>\n          <text macro=\"access-date\"/>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"container-title\"/>\n            <text macro=\"access-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </group>\n          <text macro=\"access-date\"/>\n        </else-if>\n        <else>\n          <text macro=\"title\" prefix=\" \"/>\n          <text macro=\"editor\" prefix=\". \"/>\n          <text macro=\"access-date\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"