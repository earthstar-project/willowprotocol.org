"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Forensic Anthropology</title>\n    <id>http://www.zotero.org/styles/forensic-anthropology</id>\n    <link href=\"http://www.zotero.org/styles/forensic-anthropology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/new-zealand-plant-protection\" rel=\"template\"/>\n    <link href=\"http://journals.upress.ufl.edu/fa/guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>2573-5020</issn>\n    <eissn>2573-5039</eissn>\n    <updated>2020-09-16T11:02:45+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"translator\" form=\"verb-short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" prefix=\" In: \"/>\n      <label form=\"verb-short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\"\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"thesis\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine paper-conference\" match=\"any\">\n        <group delimiter=\":\">\n          <group delimiter=\";\">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"year-date\"/>\n            </group>\n            <group>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"chapter\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\":\">\n      <group delimiter=\"; \">\n        <group delimiter=\", \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n        <choose>\n          <if type=\"article-journal\" match=\"none\">\n            <text macro=\"year-date\"/>\n          </if>\n        </choose>\n      </group>\n      <text macro=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"chapter\" match=\"any\">\n        <text variable=\"page\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"volume page\">\n            <choose>\n              <if match=\"any\" variable=\"DOI\">\n                <text variable=\"DOI\" prefix=\"doi: \"/>\n              </if>\n              <else>\n                <text variable=\"URL\"/>\n              </else>\n            </choose>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"year-suffix\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <text macro=\"edition\"/>\n        <text macro=\"editor-translator\"/>\n        <text macro=\"locators\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"