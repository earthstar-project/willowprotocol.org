"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Hydrological Processes</title>\n    <id>http://www.zotero.org/styles/hydrological-processes</id>\n    <link href=\"http://www.zotero.org/styles/hydrological-processes\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/aquatic-conservation\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1002/%28ISSN%291099-1085/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Aditi Bhaskar</name>\n      <email>abhaskar@usgs.gov</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geology\"/>\n    <issn>0885-6087</issn>\n    <eissn>1099-1085</eissn>\n    <updated>2015-08-04T18:06:31+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\").\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\"\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\"\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"version\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n        <text variable=\"title\" strip-periods=\"false\" quotes=\"false\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"DOI: \"/>\n      </if>\n      <else-if variable=\"URL\">\n        <group delimiter=\": \">\n          <text term=\"available at\" text-case=\"capitalize-first\"/>\n          <text variable=\"URL\"/>\n        </group>\n        <group prefix=\" [\" suffix=\"]\" delimiter=\" \">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" collapse=\"year\" et-al-min=\"3\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\" line-spacing=\"2\" et-al-min=\"11\" et-al-use-first=\"10\">\n    <sort>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author-short\"/>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n          <group delimiter=\". \" suffix=\".\">\n            <group suffix=\".\" prefix=\" \" delimiter=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"editor-translator\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"report\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\". \" delimiter=\". \" suffix=\".\">\n            <group delimiter=\" \">\n              <text variable=\"genre\" prefix=\" \"/>\n              <text variable=\"number\"/>\n            </group>\n            <group delimiter=\", \">\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group suffix=\".\" delimiter=\", \" prefix=\".\">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\".\">\n            <group>\n              <group suffix=\".\">\n                <text variable=\"container-title\" font-style=\"italic\" prefix=\" In \" text-case=\"title\"/>\n                <names variable=\"editor\">\n                  <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" prefix=\", \"/>\n                  <label form=\"short\" prefix=\" (\" suffix=\").\"/>\n                </names>\n                <group delimiter=\"; \">\n                  <text macro=\"publisher\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\" \" delimiter=\". \">\n            <group delimiter=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"editor-translator\"/>\n            </group>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"issue\" form=\"short\" prefix=\"(\" suffix=\")\"/>\n            </group>\n          </group>\n          <text variable=\"page\" prefix=\": \"/>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"