"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" page-range-format=\"minimal\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>London Review of International Law</title>\n    <title-short>LRIL</title-short>\n    <id>http://www.zotero.org/styles/london-review-of-international-law</id>\n    <link href=\"http://www.zotero.org/styles/london-review-of-international-law\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/the-journal-of-egyptian-archaeology\" rel=\"template\"/>\n    <link href=\"https://academic.oup.com/lril/pages/General_Instructions\" rel=\"documentation\"/>\n    <link href=\"https://static.primary.prod.gcms.the-infra.com/static/site/lril/document/style-guide.docx?node=b9d2bf7e238350df2230&amp;version=104499:13d1826d3ed8adee4689\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"law\"/>\n    <issn>2050-6325</issn>\n    <eissn>2050-6333</eissn>\n    <updated>2020-08-11T16:25:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <style-options punctuation-in-quote=\"false\"/>\n    <terms>\n      <term name=\"edition\" form=\"short\">edn</term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"editortranslator\" form=\"short\">ed. and trans.</term>\n      <term name=\"chapter\" form=\"short\">c.</term>\n      <term name=\"open-quote\">‘</term>\n      <term name=\"close-quote\">’</term>\n      <term name=\"open-inner-quote\">“</term>\n      <term name=\"close-inner-quote\">”</term>\n      <term name=\"accessed\">last visited</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\", \">\n      <name and=\"symbol\" initialize-with=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-translator\">\n    <choose>\n      <if variable=\"editor translator\" match=\"any\">\n        <names variable=\"editor translator\">\n          <label form=\"short\" suffix=\". \" strip-periods=\"false\"/>\n          <name and=\"text\" initialize-with=\".\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-editor\">\n    <group delimiter=\" \">\n      <text term=\"in\" suffix=\" \"/>\n      <names variable=\"editor\">\n        <name and=\"symbol\" initialize-with=\" \"/>\n        <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <names variable=\"translator\"/>\n          <names variable=\"container-author\"/>\n          <text macro=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"container-editor-short\">\n    <group delimiter=\" \">\n      <text term=\"in\" suffix=\" \"/>\n      <names variable=\"editor\">\n        <name form=\"short\" and=\"text\" initialize-with=\". \"/>\n        <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <names variable=\"translator\"/>\n          <names variable=\"container-author\"/>\n          <text macro=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"paper-conference\">\n        <group delimiter=\" \">\n          <text term=\"presented at\"/>\n          <text variable=\"event\"/>\n        </group>\n        <text variable=\"event-place\" prefix=\", \"/>\n      </if>\n      <else>\n        <text variable=\"publisher-place\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-volumes\"/>\n      <text term=\"volume\" form=\"short\" plural=\"true\" strip-periods=\"true\"/>\n    </group>\n  </macro>\n  <macro name=\"pageno\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <text variable=\"page-first\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pageref\">\n    <choose>\n      <if variable=\"locator\">\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"page-first\">\n              <text variable=\"page-first\"/>\n            </if>\n          </choose>\n          <text variable=\"locator\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pageref-subsequent\">\n    <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"volref\">\n    <group delimiter=\" \">\n      <text variable=\"volume\"/>\n      <text term=\"number\" form=\"short\" text-case=\"capitalize-first\" suffix=\".\"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-short\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"container-editor-short\"/>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <text macro=\"pageno\"/>\n        </group>\n      </if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" form=\"short\"/>\n            <text macro=\"volref\"/>\n          </group>\n          <text macro=\"pageno\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <number vertical-align=\"sup\" variable=\"edition\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"available at\"/>\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\" suffix=\" \"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <text macro=\"container-short\"/>\n            <text macro=\"pageref-subsequent\"/>\n          </group>\n        </if>\n        <else>\n          <text macro=\"author\"/>\n          <choose>\n            <if type=\"report thesis\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text variable=\"genre\"/>\n                <text variable=\"publisher\"/>\n                <text variable=\"collection-title\"/>\n              </group>\n              <text variable=\"number\" prefix=\" \"/>\n              <group delimiter=\", \">\n                <text macro=\"date\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </if>\n            <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"editor-translator\"/>\n                <text macro=\"container\"/>\n                <text macro=\"volumes\"/>\n                <text macro=\"volref\"/>\n              </group>\n              <group delimiter=\", \">\n                <group delimiter=\"; \" prefix=\" (\" suffix=\")\">\n                  <text macro=\"collection\"/>\n                  <group delimiter=\", \">\n                    <group delimiter=\" \">\n                      <number variable=\"edition\" form=\"ordinal\"/>\n                      <label strip-periods=\"true\" variable=\"edition\" form=\"short\"/>\n                    </group>\n                    <text macro=\"publisher\"/>\n                    <text macro=\"date\"/>\n                  </group>\n                </group>\n                <text macro=\"pageref\"/>\n              </group>\n            </else-if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"container-editor\"/>\n                <text macro=\"container\"/>\n                <text macro=\"collection\"/>\n                <text macro=\"volref\"/>\n                <text macro=\"volumes\"/>\n              </group>\n              <group delimiter=\", \">\n                <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n                  <text macro=\"publisher\"/>\n                  <text macro=\"date\"/>\n                </group>\n                <text macro=\"pageno\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </else-if>\n            <else-if type=\"post post-weblog webpage\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <text macro=\"date\"/>\n                <text macro=\"access\"/>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <group delimiter=\" \">\n                  <text macro=\"publisher\"/>\n                  <text macro=\"container\"/>\n                  <text macro=\"collection\"/>\n                  <text macro=\"volref\"/>\n                </group>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"access\"/>\n                <text macro=\"date\" prefix=\" (\" suffix=\")\"/>\n                <text macro=\"volumes\"/>\n                <text macro=\"pageno\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </else>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"