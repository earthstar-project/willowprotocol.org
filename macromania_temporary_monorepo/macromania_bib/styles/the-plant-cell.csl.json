"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>The Plant Cell</title>\n    <id>http://www.zotero.org/styles/the-plant-cell</id>\n    <link href=\"http://www.zotero.org/styles/the-plant-cell\" rel=\"self\"/>\n    <link href=\"https://academic.oup.com/plcell/pages/General-Instructions\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Richard Karnesky</name>\n      <email>karnesky+zotero@gmail.com</email>\n      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>\n    </contributor>\n    <contributor>\n      <name>Gal Haspel</name>\n      <email>gal.haspel@gmail.com</email>\n    </contributor>\n    <contributor>\n      <name>Charles Parnot</name>\n      <uri>http://twitter.com/cparnot</uri>\n      <email>charles.parnot@gmail.com</email>\n    </contributor>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1040-4651</issn>\n    <eissn>1531-298X</eissn>\n    <updated>2024-02-19T10:45:26+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed. by</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed</single>\n        <multiple>eds</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\",\">\n      <name and=\"text\" initialize-with=\"\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\", \" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \" prefix=\"(\" suffix=\")\">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \">\n            <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n      <else>\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\" font-weight=\"bold\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\" \" prefix=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text macro=\"title\" prefix=\" \" suffix=\". \"/>\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <group prefix=\" \" delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text macro=\"editor\"/>\n              <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n            </group>\n            <group suffix=\".\">\n              <text macro=\"publisher\" prefix=\" \"/>\n              <group prefix=\", \">\n                <label variable=\"page\" suffix=\" \" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\" prefix=\" \"/>\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n              <group delimiter=\":\" prefix=\" \">\n                <text macro=\"year-date\"/>\n                <group>\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"