"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-US\">\n  <info>\n    <title>Cell</title>\n    <id>http://www.zotero.org/styles/cell</id>\n    <link href=\"http://www.zotero.org/styles/cell\" rel=\"self\"/>\n    <link href=\"https://www.cell.com/cell/authors\" rel=\"documentation\"/>\n    <author>\n      <name>Adam Mark</name>\n      <email>a.mark@uoguelph.ca</email>\n    </author>\n    <contributor>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </contributor>\n    <contributor>\n      <name>Aurimas Vinckevicius</name>\n      <email>aurimas.dev@gmail.com</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>0092-8674</issn>\n    <eissn>1097-4172</eissn>\n    <summary>The Cell journal style, switched to numeric superscript as per the Cell editorial office in Sept. 2022</summary>\n    <updated>2022-09-23T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\"/>\n      </if>\n      <else-if match=\"any\" type=\"webpage dataset entry-dictionary entry-encyclopedia post post-weblog\">\n        <text variable=\"URL\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" prefix=\"(\" suffix=\")\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\" second-field-align=\"flush\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\"/>\n      <date variable=\"issued\" prefix=\" (\" suffix=\").\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text variable=\"container-title\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n          </group>\n          <text macro=\"editor\" prefix=\", \"/>\n          <group suffix=\".\">\n            <text macro=\"publisher\" prefix=\" \"/>\n            <group prefix=\", \">\n              <label variable=\"page\" suffix=\" \" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article\">\n          <text macro=\"title\" prefix=\" \"/>\n          <text term=\"article\" text-case=\"capitalize-first\" prefix=\". \"/>\n          <text variable=\"publisher\" prefix=\" at \"/>\n          <text macro=\"access\" prefix=\", \"/>\n        </else-if>\n        <else-if type=\"dataset software\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <group delimiter=\" \">\n                <text term=\"version\" text-case=\"capitalize-first\"/>\n                <text variable=\"version\"/>\n              </group>\n              <text variable=\"publisher\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group delimiter=\", \" prefix=\" \" suffix=\".\">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <text variable=\"volume\" font-style=\"italic\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"