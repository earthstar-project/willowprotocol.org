"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>American Society for Horticultural Science</title>\n    <title-short>ASHS</title-short>\n    <id>http://www.zotero.org/styles/american-society-for-horticultural-science</id>\n    <link href=\"http://www.zotero.org/styles/american-society-for-horticultural-science\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/springer-basic-author-date\" rel=\"template\"/>\n    <link href=\"https://cdn.ymaws.com/ashs.org/resource/resmgr/publications/ashspubsstylemanualweb.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"botany\"/>\n    <updated>2023-10-07T12:02:26+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"edition\" form=\"short\">edn.</term>\n    </terms>\n  </locale>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\", \">\n          <text variable=\"title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-parenth\">\n    <date date-parts=\"year\" form=\"numeric\" variable=\"issued\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else-if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"author\" sort=\"ascending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"year-parenth\"/>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <group delimiter=\" \">\n                  <label plural=\"never\" strip-periods=\"true\" variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n                <names variable=\"editor\">\n                  <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n                  <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n                </names>\n              </group>\n              <text variable=\"container-title\"/>\n              <text macro=\"edition\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"article-journal\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\". \">\n                <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n                <group delimiter=\":\" prefix=\" \">\n                  <group>\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                  <text variable=\"page\" prefix=\":\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post-weblog post\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"container-title\" form=\"short\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\". \">\n              <group delimiter=\" \">\n                <text macro=\"title\"/>\n                <text variable=\"genre\" text-case=\"capitalize-first\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"patent\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group delimiter=\" \">\n                <text variable=\"authority\"/>\n                <text variable=\"number\"/>\n              </group>\n              <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                <date form=\"text\" variable=\"submitted\" prefix=\"Filed \"/>\n                <date form=\"text\" variable=\"issued\" prefix=\"granted \"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"container-title\" form=\"short\"/>\n              <group delimiter=\":\">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"