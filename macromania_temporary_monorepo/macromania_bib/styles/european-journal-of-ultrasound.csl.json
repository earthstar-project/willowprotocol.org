"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"expanded\" default-locale=\"en-GB\">\n  <info>\n    <title>European Journal of Ultrasound (Ultraschall in der Medizin)</title>\n    <id>http://www.zotero.org/styles/european-journal-of-ultrasound</id>\n    <link href=\"http://www.zotero.org/styles/european-journal-of-ultrasound\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/current-gene-therapy\" rel=\"template\"/>\n    <link href=\"http://ultraschall.thieme.de/documents/10157/10820/UiM-Autorenrichtlinien2016.pdf/8cf116e9-c862-4626-bc36-945a971495af\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0172-4614</issn>\n    <eissn>1438-8782</eissn>\n    <updated>2017-01-31T10:07:42+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\">cited</term>\n    </terms>\n  </locale>\n  <macro name=\"page\">\n    <choose>\n      <if is-numeric=\"locator\">\n        <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <text variable=\"locator\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"legislation\"/>\n      <else-if type=\"entry-encyclopedia\" variable=\"author\" match=\"all\">\n        <names variable=\"author\">\n          <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\"/>\n        </names>\n      </else-if>\n      <else-if variable=\"author editor composer\" match=\"any\">\n        <names variable=\"author\" font-style=\"italic\">\n          <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\"/>\n          <label prefix=\", \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"composer\"/>\n          </substitute>\n        </names>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if variable=\"author\">\n            <text variable=\"title\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"book\" match=\"any\">\n        <text variable=\"title\" suffix=\",\"/>\n      </else-if>\n      <else-if type=\"post-weblog\" match=\"none\">\n        <text variable=\"title\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"editor-container\">\n    <group delimiter=\": \">\n      <text term=\"in\" text-case=\"capitalize-first\"/>\n      <group delimiter=\" \" suffix=\".\">\n        <names variable=\"editor\">\n          <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\"/>\n          <label form=\"short\" text-case=\"capitalize-first\" prefix=\" (\" suffix=\")\"/>\n        </names>\n        <text variable=\"container-title\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\">\n        <choose>\n          <if variable=\"container-title-short\">\n            <text variable=\"container-title-short\" strip-periods=\"true\"/>\n          </if>\n          <else>\n            <text variable=\"container-title\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"entry-encyclopedia\" variable=\"author\" match=\"any\">\n        <text macro=\"editor-container\"/>\n      </else-if>\n      <else-if type=\"chapter\">\n        <text macro=\"editor-container\"/>\n      </else-if>\n      <else-if type=\"patent\" match=\"any\">\n        <text variable=\"authority\" suffix=\" \"/>\n        <text variable=\"number\" suffix=\",\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" suffix=\" \"/>\n        <text macro=\"year\"/>\n        <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"material\">\n    <choose>\n      <if type=\"report\">\n        <text macro=\"online\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n      <else-if type=\"thesis\" variable=\"genre\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          <text macro=\"online\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </else-if>\n      <else-if variable=\"medium\">\n        <group delimiter=\", \">\n          <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n          <text macro=\"online\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"online\" prefix=\" [\" suffix=\"]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if variable=\"URL\">\n        <choose>\n          <if type=\"legislation bill\" match=\"any\"/>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </if>\n        <else>\n          <text variable=\"edition\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"city\">\n    <text variable=\"publisher-place\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"year\">\n    <choose>\n      <if type=\"legislation bill patent\" match=\"any\">\n        <date form=\"text\" variable=\"issued\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else-if type=\"article-newspaper broadcast\" match=\"any\">\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </else-if>\n      <else-if type=\"chapter article-journal\" match=\"any\">\n        <date variable=\"issued\" suffix=\";\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if type=\"legislation bill\" match=\"any\"/>\n      <else-if variable=\"URL\">\n        <group delimiter=\" \" prefix=\"[\" suffix=\"].\">\n          <text term=\"accessed\"/>\n          <date delimiter=\" \" variable=\"accessed\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"entry\">\n    <choose>\n      <if type=\"entry-encyclopedia\">\n        <choose>\n          <if variable=\"author\"/>\n          <else>\n            <text variable=\"title\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"post-weblog\">\n        <text variable=\"title\" suffix=\";\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\": \">\n          <text variable=\"volume\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <group delimiter=\":\">\n          <text variable=\"section\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\" \" suffix=\", \">\n          <text term=\"book\" form=\"short\"/>\n          <text variable=\"volume\"/>\n        </group>\n        <group delimiter=\" \">\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"legislation\">\n        <number variable=\"volume\"/>\n        <date variable=\"issued\" prefix=\" af \" delimiter=\"/\">\n          <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n          <date-part name=\"month\" form=\"numeric-leading-zeros\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else-if type=\"bill\">\n        <number variable=\"number\"/>\n        <date variable=\"issued\" prefix=\". \" delimiter=\"/\">\n          <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n          <date-part name=\"month\" form=\"numeric-leading-zeros\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if type=\"legislation bill\" match=\"none\">\n        <group delimiter=\": \">\n          <text term=\"available at\" text-case=\"capitalize-first\"/>\n          <text variable=\"URL\" text-decoration=\"none\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-label\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n      <text macro=\"page\" prefix=\", \"/>\n    </layout>\n  </citation>\n  <bibliography delimiter-precedes-et-al=\"never\" et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\"/>\n      <group delimiter=\" \" suffix=\".\">\n        <group delimiter=\" \">\n          <group delimiter=\". \">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"container-title\"/>\n          </group>\n          <text macro=\"material\"/>\n        </group>\n        <text macro=\"edition\"/>\n        <group delimiter=\" \">\n          <group delimiter=\": \">\n            <choose>\n              <if type=\"book\">\n                <text macro=\"city\"/>\n                <text macro=\"publisher\" suffix=\",\"/>\n              </if>\n              <else>\n                <text macro=\"city\"/>\n                <text macro=\"publisher\" suffix=\";\"/>\n              </else>\n            </choose>\n          </group>\n          <group delimiter=\" \">\n            <group delimiter=\". \">\n              <text macro=\"year\"/>\n              <text macro=\"entry\"/>\n            </group>\n          </group>\n          <text macro=\"locator\"/>\n        </group>\n        <choose>\n          <if type=\"webpage post-weblog\" match=\"any\">\n            <text macro=\"accessed\"/>\n            <text macro=\"url\"/>\n          </if>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"