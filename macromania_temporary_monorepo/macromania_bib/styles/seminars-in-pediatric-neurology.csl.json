"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Seminars in Pediatric Neurology</title>\n    <id>http://www.zotero.org/styles/seminars-in-pediatric-neurology</id>\n    <link href=\"http://www.zotero.org/styles/seminars-in-pediatric-neurology\" rel=\"self\"/>\n    <link href=\"http://www.elsevier.com/journals/seminars-in-pediatric-neurology/1071-9091/guide-for-authors\" rel=\"documentation\"/>\n    <author>\n      <name>Crystal Ruff</name>\n      <email>Crystal.Ruff@ucl.ac.uk</email>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1071-9091</issn>\n    <eissn>1558-0776</eissn>\n    <updated>2013-01-16T22:20:27+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \" suffix=\"; \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\". \">\n          <group>\n            <text value=\"Available at: \"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\" form=\"text\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\": \"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\". \">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <group delimiter=\". \">\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\"/>\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\" plural=\"never\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n            <group delimiter=\":\">\n              <text variable=\"volume\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n          <date variable=\"issued\" prefix=\", \">\n            <date-part name=\"year\"/>\n          </date>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"