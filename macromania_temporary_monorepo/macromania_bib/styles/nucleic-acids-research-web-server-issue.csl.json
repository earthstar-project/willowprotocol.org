"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Nucleic Acids Research - Web Server Issue</title>\n    <id>http://www.zotero.org/styles/nucleic-acids-research-web-server-issue</id>\n    <link href=\"http://www.zotero.org/styles/nucleic-acids-research-web-server-issue\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/nucleic-acids-research\" rel=\"template\"/>\n    <link href=\"http://www.oxfordjournals.org/nar/for_authors/msprep_submission.html\" rel=\"documentation\"/>\n    <link href=\"https://academic.oup.com/nar/pages/submission_webserver\" rel=\"documentation\"/>\n    <author>\n      <name>Sjoerd de Vries</name>\n      <email>sjdv1982@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>0305-1048</issn>\n    <eissn>1362-4962</eissn>\n    <summary>Nucleic Acids Research Web Server Issue style.</summary>\n    <updated>2017-02-19T11:21:49+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\",\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\",\" initialize-with=\".\" delimiter=\", \" and=\"text\" delimiter-precedes-last=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"cite-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"11\" et-al-use-first=\"10\">\n    <layout>\n      <group display=\"block\" suffix=\".\">\n        <text variable=\"citation-number\" suffix=\". \"/>\n        <text macro=\"author\"/>\n        <text macro=\"cite-date\" prefix=\" (\" suffix=\")\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\" \" prefix=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group prefix=\" \" delimiter=\" \" suffix=\".\">\n              <text macro=\"title\" suffix=\".\"/>\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <group delimiter=\", \">\n                <text macro=\"editor\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n              </group>\n            </group>\n            <group suffix=\".\" delimiter=\", \">\n              <text macro=\"publisher\" prefix=\" \"/>\n              <text variable=\"volume\" prefix=\"Vol. \"/>\n              <group>\n                <label variable=\"page\" suffix=\" \" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n            <group delimiter=\", \" prefix=\" \" suffix=\".\">\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n              <choose>\n                <if variable=\"volume\">\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text variable=\"page\"/>\n                </if>\n                <else>\n                  <text variable=\"DOI\"/>\n                </else>\n              </choose>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <group display=\"block\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </group>\n      <group display=\"block\">\n        <choose>\n          <if variable=\"PMCID\">\n            <text variable=\"PMCID\" prefix=\"http://www.ncbi.nlm.nih.gov/pmc/articles/\"/>\n          </if>\n          <else>\n            <text variable=\"PMID\" prefix=\"http://www.ncbi.nlm.nih.gov/pubmed/\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"