"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>UNESCO International Institute for Educational Planning</title>\n    <title-short>UNESCO IIEP</title-short>\n    <id>http://www.zotero.org/styles/unesco-international-institute-for-educational-planning</id>\n    <link href=\"http://www.zotero.org/styles/unesco-international-institute-for-educational-planning\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/human-resource-management-journal\" rel=\"template\"/>\n    <link href=\"https://box.iiep.unesco.org/s/od697NSMAiocAKX\" rel=\"documentation\"/>\n    <link href=\"https://box.iiep.unesco.org/s/nPi9co5n2LEf43f\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <summary>Citation style for all publications from UNESCO's International Institute for Educational Planning; tested in English and French</summary>\n    <updated>2017-07-03T03:48:18+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <style-options punctuation-in-quote=\"false\"/>\n    <date form=\"text\">\n      <date-part name=\"day\" suffix=\" \"/>\n      <date-part name=\"month\" suffix=\" \"/>\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"open-quote\">‘</term>\n      <term name=\"close-quote\">’</term>\n      <term name=\"open-inner-quote\">“</term>\n      <term name=\"close-inner-quote\">”</term>\n      <term name=\"accessed\">last accessed</term>\n      <term name=\"no date\" form=\"short\">n.d.</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"in\">dans</term>\n      <term name=\"accessed\">Accès le</term>\n      <term name=\"no date\" form=\"short\">s.d.</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>pp.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"secondary-contributors\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\".\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter=\"; \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else>\n        <choose>\n          <if type=\"report\" match=\"any\">\n            <text variable=\"URL\"/>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date variable=\"accessed\" form=\"text\" suffix=\":\"/>\n              <text variable=\"URL\"/>\n            </group>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\". \">\n          <choose>\n            <if type=\"report\" match=\"any\">\n              <text variable=\"genre\"/>\n            </if>\n          </choose>\n          <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"title\" quotes=\"true\"/>\n          <choose>\n            <if type=\"webpage post-weblog\" match=\"any\">\n              <text variable=\"container-title\"/>\n            </if>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <group delimiter=\", \">\n          <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n      <else-if type=\"speech\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            <choose>\n              <if variable=\"genre\">\n                <group delimiter=\" \">\n                  <text term=\"presented at\"/>\n                  <text variable=\"event\"/>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\" \">\n                  <text term=\"presented at\" text-case=\"capitalize-first\"/>\n                  <text variable=\"event\"/>\n                </group>\n              </else>\n            </choose>\n          </group>\n          <text variable=\"event-place\"/>\n          <date variable=\"issued\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\"/>\n          </date>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <group delimiter=\": \">\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <group delimiter=\", \" prefix=\", \">\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"article-magazine article-newspaper\" match=\"any\">\n        <date variable=\"issued\" prefix=\", \">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </else-if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"edition\"/>\n          <group>\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n        </group>\n        <group prefix=\" (\" suffix=\")\">\n          <label variable=\"page\" form=\"short\" suffix=\" \"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"none\">\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group delimiter=\" \">\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <label variable=\"locator\" form=\"short\"/>\n        </if>\n      </choose>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <group delimiter=\", \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n        </group>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"issued-year\"/>\n        <group delimiter=\". \">\n          <text macro=\"title\"/>\n          <group>\n            <choose>\n              <if type=\"chapter paper-conference article-journal\" match=\"any\">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n              </if>\n            </choose>\n            <group delimiter=\", \">\n              <text macro=\"secondary-contributors\"/>\n              <text macro=\"container-title\"/>\n            </group>\n            <text macro=\"locators\"/>\n          </group>\n        </group>\n        <group delimiter=\". \">\n          <text macro=\"series\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"access\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"