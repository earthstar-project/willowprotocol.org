"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"chicago\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>European Journal of Public Health</title>\n    <title-short>EJPH</title-short>\n    <id>http://www.zotero.org/styles/european-journal-of-public-health</id>\n    <link href=\"http://www.zotero.org/styles/european-journal-of-public-health\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/british-journal-of-dermatology\" rel=\"template\"/>\n    <link href=\"https://academic.oup.com/eurpub/pages/Instructions_For_Authors#References\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1101-1262</issn>\n    <eissn>1464-360X</eissn>\n    <updated>2017-06-02T09:07:42+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <group>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\" doi:\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <text variable=\"URL\" prefix=\"URL \"/>\n            <group delimiter=\" \" prefix=\" \">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date variable=\"accessed\" form=\"text\"/>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\". \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\", \" suffix=\".\">\n            <group delimiter=\" \">\n              <text term=\"in\" suffix=\":\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </group>\n            <text macro=\"edition\"/>\n          </group>\n          <group delimiter=\"; \" suffix=\".\" prefix=\" \">\n            <text macro=\"publisher\"/>\n            <text variable=\"page\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group delimiter=\";\" suffix=\".\" prefix=\" \">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n            <group delimiter=\":\">\n              <text variable=\"volume\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"