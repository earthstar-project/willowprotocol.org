"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Elementa</title>\n    <title-short>Elementa</title-short>\n    <id>http://www.zotero.org/styles/elementa</id>\n    <link href=\"http://www.zotero.org/styles/elementa\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/council-of-science-editors-author-date\" rel=\"template\"/>\n    <link href=\"http://www.zotero.org/styles/apa\" rel=\"template\"/>\n    <link href=\"https://home.elementascience.org/for-authors/style-guide/\" rel=\"documentation\"/>\n    <link href=\"http://lynn-library.libguides.com/cse\" rel=\"documentation\"/>\n    <link href=\"http://writing.wisc.edu/Handbook/DocCSE_CitationSystems.html\" rel=\"documentation\"/>\n    <author>\n      <name>Akos Kokai</name>\n      <email>akokai@berkeley.edu</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <eissn>2325-1026</eissn>\n    <summary>Based on The Council of Science Editors style, Name-Year system: author-date in text, sorted in alphabetical order by author.</summary>\n    <updated>2016-07-26T01:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editortranslator\" form=\"long\">\n        <single>editor and translator</single>\n        <multiple>editors and translators</multiple>\n      </term>\n      <term name=\"collection-editor\" form=\"long\">\n        <single>editor</single>\n        <multiple>editors</multiple>\n      </term>\n      <term name=\"no date\">n.d.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\"; \">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor translator\"/>\n        <names variable=\"editor\"/>\n        <names variable=\"collection-editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\">\n            <text variable=\"publisher\"/>\n            <text macro=\"title\"/>\n          </if>\n          <else>\n            <text macro=\"title\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\", \" initialize-with=\"\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"collection-editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\">\n            <text variable=\"publisher\"/>\n            <text macro=\"title\"/>\n          </if>\n          <else>\n            <text macro=\"title\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-contributors\">\n    <names variable=\"editor translator\" delimiter=\"; \">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <group delimiter=\" \">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"year\"/>\n          </date>\n          <choose>\n            <if type=\"patent article-newspaper webpage\" match=\"any\">\n              <date variable=\"issued\" delimiter=\" \">\n                <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n                <date-part name=\"day\"/>\n              </date>\n            </if>\n            <else-if type=\"article-journal article-magazine\" match=\"any\">\n              <choose>\n                <if variable=\"volume issue\" match=\"none\">\n                  <date variable=\"issued\" delimiter=\" \">\n                    <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n                    <date-part name=\"day\"/>\n                  </date>\n                </if>\n              </choose>\n            </else-if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"book\">\n          <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n        </if>\n        <else>\n          <text variable=\"title\"/>\n        </else>\n      </choose>\n      <choose>\n        <if type=\"thesis dataset speech interview broadcast\" match=\"any\">\n          <text variable=\"genre\" form=\"long\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n      </choose>\n      <choose>\n        <!-- Book reviews -->\n        <if variable=\"reviewed-author\">\n          <choose>\n            <if variable=\"reviewed-title\">\n              <group delimiter=\" \">\n                <text variable=\"title\"/>\n                <group delimiter=\", \" prefix=\"[\" suffix=\"]\">\n                  <text variable=\"reviewed-title\" font-style=\"italic\" prefix=\"Review of \"/>\n                  <names variable=\"reviewed-author\" delimiter=\", \">\n                    <label form=\"verb-short\" suffix=\" \"/>\n                    <name and=\"text\" initialize-with=\"\" delimiter=\", \"/>\n                  </names>\n                </group>\n              </group>\n            </if>\n            <else>\n              <!-- assume `title` is title of reviewed work -->\n              <group delimiter=\", \" prefix=\"[\" suffix=\"]\">\n                <text variable=\"title\" font-style=\"italic\" prefix=\"Review of \"/>\n                <names variable=\"reviewed-author\" delimiter=\", \">\n                  <label form=\"verb-short\" suffix=\" \"/>\n                  <name and=\"text\" initialize-with=\"\" delimiter=\", \"/>\n                </names>\n              </group>\n            </else>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter\">\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"article-journal\">\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\" strip-periods=\"true\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container\">\n    <group>\n      <choose>\n        <if type=\"chapter entry-dictionary entry-encyclopedia\" match=\"any\">\n          <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"bill legal_case legislation\" match=\"none\">\n          <group delimiter=\". \">\n            <text macro=\"container-contributors\"/>\n            <text macro=\"container-title\"/>\n          </group>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <label variable=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"chapter entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\"; \">\n          <group>\n            <label variable=\"page\" form=\"short\" suffix=\" \" plural=\"never\"/>\n            <text variable=\"page\"/>\n          </group>\n          <group>\n            <text variable=\"number-of-pages\"/>\n            <choose>\n              <if is-numeric=\"number-of-pages\">\n                <label variable=\"number-of-pages\" form=\"short\" prefix=\" \" plural=\"never\"/>\n              </if>\n            </choose>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"journal-locators\">\n    <group suffix=\".\">\n      <choose>\n        <if variable=\"volume issue page\" match=\"none\">\n          <choose>\n            <if type=\"article-journal review review-book\" match=\"any\">\n              <text term=\"in press\" prefix=\", \"/>\n            </if>\n          </choose>\n        </if>\n        <else>\n          <group>\n            <text variable=\"volume\" prefix=\" \" font-weight=\"bold\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            <text variable=\"page\" prefix=\": \"/>\n          </group>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper review review-book\" match=\"any\">\n        <text macro=\"journal-locators\"/>\n      </if>\n      <else-if type=\"paper-conference\">\n        <choose>\n          <if variable=\"container-title\">\n            <text macro=\"journal-locators\"/>\n          </if>\n          <else>\n            <date variable=\"issued\" delimiter=\" \" suffix=\".\">\n              <date-part name=\"year\"/>\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"day\"/>\n            </date>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"bill legal_case legislation\" match=\"none\">\n        <text macro=\"edition\" suffix=\".\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"legal-cites\">\n    <!-- Bluebook style; adapted from apa.csl -->\n    <choose>\n      <if type=\"bill legal_case legislation\" match=\"any\">\n        <group delimiter=\" \">\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"volume\"/>\n              <text variable=\"container-title\"/>\n              <group delimiter=\" \">\n                <!--change to label variable=\"section\" as that becomes available -->\n                <text term=\"section\" form=\"symbol\"/>\n                <text variable=\"section\"/>\n              </group>\n              <text variable=\"section\"/>\n              <text variable=\"page\"/>\n            </if>\n            <else>\n              <choose>\n                <if type=\"legal_case\">\n                  <text variable=\"number\" prefix=\"No. \"/>\n                </if>\n                <else>\n                  <text variable=\"number\" prefix=\"Pub. L. No. \"/>\n                  <group delimiter=\" \">\n                    <!--change to label variable=\"section\" as that becomes available -->\n                    <text term=\"section\" form=\"symbol\"/>\n                    <text variable=\"section\"/>\n                  </group>\n                  <text variable=\"section\"/>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <choose>\n          <if variable=\"container-title\" match=\"none\">\n            <!-- Don't give event info if part of a publication\n               (e.g., conference proceedings) -->\n            <choose>\n              <if type=\"paper-conference\" match=\"none\">\n                <group delimiter=\" \">\n                  <text variable=\"genre\" text-case=\"capitalize-first\"/>\n                  <text term=\"presented at\" text-case=\"capitalize-first\"/>\n                </group>\n              </if>\n            </choose>\n            <group prefix=\" \" suffix=\".\" delimiter=\"; \">\n              <text variable=\"event\"/>\n              <text variable=\"event-place\"/>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <choose>\n        <if type=\"thesis\">\n          <text variable=\"publisher-place\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n        <else-if variable=\"event\" match=\"none\">\n          <text variable=\"publisher-place\"/>\n        </else-if>\n      </choose>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <!-- This is to appear at the end of the citation:\n         after publisher, before access. -->\n    <choose>\n      <if type=\"report\">\n        <group prefix=\" \" suffix=\".\" delimiter=\" \">\n          <text variable=\"collection-title\"/>\n          <text variable=\"number\" prefix=\"Report No.: \"/>\n        </group>\n      </if>\n      <else-if type=\"book\">\n        <group prefix=\" (\" suffix=\").\" delimiter=\" \">\n          <names variable=\"collection-editor\" suffix=\". \">\n            <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n            <label prefix=\"; \"/>\n          </names>\n          <group delimiter=\"; \">\n            <text variable=\"collection-title\"/>\n            <choose>\n              <if variable=\"collection-number\">\n                <group>\n                  <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\". \"/>\n                  <text variable=\"collection-number\"/>\n                </group>\n              </if>\n              <else>\n                <group>\n                  <label variable=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\". \"/>\n                  <text variable=\"volume\"/>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </group>\n      </else-if>\n      <!-- Add other types that need collection info after publisher. -->\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi: \"/>\n      </if>\n      <else-if variable=\"URL\">\n        <group suffix=\". \" delimiter=\" \">\n          <text term=\"available at\" text-case=\"capitalize-first\"/>\n          <text variable=\"URL\"/>\n        </group>\n        <group suffix=\". \" delimiter=\" \">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\" delimiter=\" \">\n            <date-part name=\"year\"/>\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year\"/>\n      </group>\n      <text variable=\"locator\" prefix=\": \"/>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"11\" et-al-use-first=\"10\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n        <text macro=\"title\"/>\n        <group delimiter=\"\">\n          <text macro=\"container\"/>\n          <text macro=\"locators\"/>\n        </group>\n        <text macro=\"legal-cites\"/>\n        <text macro=\"event\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"pages\"/>\n        <text macro=\"collection\"/>\n      </group>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"