"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Environmental Conservation</title>\n    <id>http://www.zotero.org/styles/environmental-conservation</id>\n    <link href=\"http://www.zotero.org/styles/environmental-conservation\" rel=\"self\"/>\n    <link href=\"https://www.cambridge.org/core/journals/environmental-conservation/information/author-instructions/preparing-your-materials\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </contributor>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0376-8929</issn>\n    <eissn>1469-4387</eissn>\n    <summary>Style for the Cambridge journal Environmental Conservation</summary>\n    <updated>2022-11-22T10:39:42+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name initialize-with=\"\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" initialize-with=\"\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog report\" match=\"any\">\n        <group>\n          <text value=\"URL\" suffix=\" \"/>\n          <text variable=\"URL\"/>\n        </group>\n      </if>\n      <else-if type=\"article-journal\" match=\"any\"/>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n    <choose>\n      <if type=\"webpage post post-weblog report\">\n        <text value=\"WWW document\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <layout delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \" suffix=\".\">\n        <text macro=\"author\"/>\n        <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\" \" prefix=\" \">\n              <text macro=\"title\" suffix=\".\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \" prefix=\" \">\n              <text macro=\"title\" suffix=\".\"/>\n              <group delimiter=\", \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n                  <text macro=\"editor\"/>\n                </group>\n                <group delimiter=\", \" suffix=\".\">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <group>\n                    <label variable=\"page\" suffix=\" \" form=\"short\"/>\n                    <text variable=\"page\"/>\n                  </group>\n                </group>\n              </group>\n              <text macro=\"publisher\" prefix=\" \" suffix=\".\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group prefix=\" \">\n                <group delimiter=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <text variable=\"volume\"/>\n                </group>\n                <text variable=\"page\" prefix=\": \"/>\n                <choose>\n                  <if type=\"article-journal\" match=\"any\">\n                    <choose>\n                      <if match=\"none\" variable=\"volume page\">\n                        <text variable=\"DOI\" prefix=\"(epub ahead of print) doi: \"/>\n                      </if>\n                    </choose>\n                  </if>\n                </choose>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"