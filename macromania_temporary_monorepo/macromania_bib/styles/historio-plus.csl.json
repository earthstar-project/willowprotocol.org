"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"de-AT\">\n  <info>\n    <title>historioPLUS (Deutsch - Österreich)</title>\n    <id>http://www.zotero.org/styles/historio-plus</id>\n    <link href=\"http://www.zotero.org/styles/historio-plus\" rel=\"self\"/>\n    <link href=\"https://www.historioplus.at/?page_id=49\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <issn>2313-2450</issn>\n    <summary>With generous support from historioPLUS, Universität Salzburg</summary>\n    <updated>2021-08-16T10:44:33+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"editor\" form=\"short\">Hg.</term>\n      <term name=\"et-al\">u. a.</term>\n      <term name=\"volume\" form=\"short\">\n        <single>Bd.</single>\n        <multiple>Bde.</multiple>\n      </term>\n      <term name=\"online\">online unter</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\" / \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\", \" plural=\"never\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\"/\" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <choose>\n      <if variable=\"author editor\" match=\"none\">\n        <choose>\n          <if type=\"webpage\">\n            <text variable=\"title\"/>\n          </if>\n          <else>\n            <text value=\"N. N.\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <text variable=\"title\" form=\"short\"/>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\">\n      <name delimiter=\" / \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\", \" plural=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"post-weblog webpage\" match=\"any\"/>\n      <else-if variable=\"container-title\">\n        <group delimiter=\" \">\n          <text term=\"in\" suffix=\":\"/>\n          <group delimiter=\", \">\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\"/>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"editor\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator-periodical\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"article-journal\" match=\"any\">\n          <group delimiter=\" \">\n            <group delimiter=\"/\">\n              <text variable=\"volume\"/>\n              <text variable=\"issue\"/>\n            </group>\n            <date variable=\"issued\" form=\"numeric\" date-parts=\"year\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </if>\n        <else-if type=\"article-magazine article-newspaper\">\n          <group delimiter=\", \">\n            <text variable=\"volume\"/>\n            <date variable=\"issued\" form=\"numeric\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume-book\">\n    <choose>\n      <if type=\"book chapter entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if variable=\"volume\" match=\"any\">\n            <group delimiter=\" \">\n              <label variable=\"volume\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text variable=\"number-of-volumes\"/>\n              <text term=\"volume\" form=\"short\" plural=\"true\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if variable=\"publisher-place\">\n        <text variable=\"publisher-place\"/>\n      </if>\n      <else-if type=\"book chapter paper-conference\" match=\"any\">\n        <text value=\"[o. O.]\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n      </if>\n      <else>\n        <text value=\"[o. J.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <group delimiter=\" \">\n            <text variable=\"publisher\"/>\n            <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-journal article-magazine article-newspaper\" match=\"none\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text macro=\"publisher-place\"/>\n            <text macro=\"year\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\", \">\n      <text variable=\"archive\"/>\n      <text variable=\"archive_location\"/>\n    </group>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if locator=\"page\">\n        <choose>\n          <if variable=\"page\">\n            <text variable=\"locator\" prefix=\"hier \"/>\n          </if>\n          <else>\n            <text variable=\"locator\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator-subsequent\">\n    <choose>\n      <if locator=\"page\">\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"online\" suffix=\":\"/>\n          <group delimiter=\", \">\n            <text variable=\"container-title\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <date variable=\"accessed\" form=\"numeric\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n      <else-if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"DOI: \"/>\n      </else-if>\n      <else-if variable=\"URL\">\n        <group delimiter=\" \">\n          <text term=\"online\" suffix=\":\"/>\n          <text variable=\"URL\"/>\n          <date variable=\"accessed\" form=\"numeric\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"interview-short\">\n    <names variable=\"author\" prefix=\"Interview mit \">\n      <name delimiter=\" / \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"interview-long\">\n    <group delimiter=\", \">\n      <names variable=\"author\" prefix=\"Interview mit \">\n        <name delimiter=\" / \" delimiter-precedes-last=\"always\">\n          <name-part name=\"family\" font-variant=\"small-caps\"/>\n        </name>\n      </names>\n      <text variable=\"event-place\"/>\n      <date variable=\"issued\" form=\"numeric\"/>\n    </group>\n  </macro>\n  <macro name=\"sort-key\">\n    <choose>\n      <if variable=\"archive\">\n        <text value=\"1\"/>\n      </if>\n      <else-if type=\"interview\">\n        <text value=\"2\"/>\n      </else-if>\n      <else>\n        <text value=\"3\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\" \">\n            <text term=\"ibid\"/>\n            <text macro=\"locator-subsequent\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"locator-subsequent\"/>\n          </group>\n        </else-if>\n        <else-if type=\"interview\">\n          <text macro=\"interview-short\"/>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\" text-case=\"title\"/>\n            <group delimiter=\" \">\n              <text macro=\"container-title\"/>\n              <text macro=\"locator-periodical\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"volume-book\"/>\n            <text variable=\"medium\"/>\n            <text variable=\"dimensions\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"archive\"/>\n            <text macro=\"access\"/>\n            <text macro=\"locator\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"1\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"sort-key\"/>\n      <key macro=\"author\"/>\n      <key macro=\"year\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"interview\">\n          <text macro=\"interview-long\"/>\n        </if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\" text-case=\"title\"/>\n            <group delimiter=\" \">\n              <text macro=\"container-title\"/>\n              <text macro=\"locator-periodical\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"volume-book\"/>\n            <text variable=\"medium\"/>\n            <text variable=\"dimensions\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"archive\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"