"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Arachnologische Mitteilungen</title>\n    <title-short>AraMit</title-short>\n    <id>http://www.zotero.org/styles/arachnologische-mitteilungen</id>\n    <link href=\"http://www.zotero.org/styles/arachnologische-mitteilungen\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/zootaxa\" rel=\"template\"/>\n    <link href=\"https://arages.de/zeitschrift/hinweise-fuer-autoren\" rel=\"documentation\"/>\n    <author>\n      <name>Hubert HÃ¶fer</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <summary>Zitierstil der Arachnologischen Mitteilungen (Arachnolgy Letters)</summary>\n    <updated>2023-01-16T08:06:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-US\">\n    <date form=\"text\">\n      <date-part name=\"month\" suffix=\" \"/>\n      <date-part name=\"day\" suffix=\", \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\",\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" et-al-use-last=\"true\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" et-al-use-last=\"true\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" \" suffix=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"&#8211; doi: \"/>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"URL\">\n            <group delimiter=\" \" prefix=\"&#8211; \">\n              <text term=\"internet\" text-case=\"capitalize-first\" suffix=\":\"/>\n              <text variable=\"URL\"/>\n              <group prefix=\"(\" suffix=\")\">\n                <date form=\"numeric\" variable=\"accessed\"/>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"legal_case\">\n    <group prefix=\" \" delimiter=\" \">\n      <text variable=\"volume\"/>\n      <text variable=\"container-title\"/>\n    </group>\n    <text variable=\"authority\" prefix=\" (\" suffix=\")\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\" match=\"none\">\n        <group delimiter=\", \" suffix=\".\">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n        <text variable=\"genre\" prefix=\". \"/>\n      </if>\n      <else>\n        <group delimiter=\". \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <group>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"issue\" suffix=\":\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if locator=\"page\">\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-volume\">\n    <text variable=\"container-title\"/>\n    <text variable=\"volume\" prefix=\" \"/>\n  </macro>\n  <macro name=\"collection\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\" givenname-disambiguation-rule=\"primary-name\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text macro=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\" \">\n      <text macro=\"author\" suffix=\" \"/>\n      <date variable=\"issued\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"book\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" suffix=\".\"/>\n            <text macro=\"collection\" prefix=\"&#8211; \" suffix=\", \"/>\n          </group>\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <text variable=\"number-of-pages\"/>\n              <text term=\"page\" plural=\"true\" form=\"short\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group delimiter=\" \" prefix=\" In: \">\n            <text macro=\"editor\"/>\n            <text macro=\"container-volume\" suffix=\".\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <group>\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \" prefix=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"bill graphic legal_case legislation manuscript motion_picture report song thesis\" match=\"any\">\n          <text macro=\"legal_case\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" suffix=\".\"/>\n          </group>\n          <group delimiter=\", \" prefix=\" \">\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <text variable=\"page\"/>\n              <text term=\"page\" plural=\"true\" form=\"short\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\" \" delimiter=\" \" suffix=\".\">\n            <text macro=\"title\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <group prefix=\" - \">\n            <text variable=\"container-title\"/>\n            <group prefix=\" \">\n              <text variable=\"volume\"/>\n              <choose>\n                <if match=\"none\" variable=\"issue\">\n                  <text variable=\"issue\" form=\"short\"/>\n                </if>\n              </choose>\n            </group>\n            <text variable=\"page\" prefix=\": \"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"