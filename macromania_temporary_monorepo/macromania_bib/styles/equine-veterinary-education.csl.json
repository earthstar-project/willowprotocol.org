"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" delimiter-precedes-last=\"never\" default-locale=\"en-GB\">\n  <info>\n    <title>Equine Veterinary Education</title>\n    <title-short>EVE</title-short>\n    <id>http://www.zotero.org/styles/equine-veterinary-education</id>\n    <link href=\"http://www.zotero.org/styles/equine-veterinary-education\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/bmj\" rel=\"template\"/>\n    <link href=\"https://beva.onlinelibrary.wiley.com/hub/journal/20423292/homepage/forauthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Charles Parnot</name>\n      <email>charles.parnot@gmail.com</email>\n      <uri>http://twitter.com/cparnot</uri>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"medicine\"/>\n    <issn>0957-7734</issn>\n    <eissn>2042-3292</eissn>\n    <updated>2020-05-10T14:27:20+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\".\" name-as-sort-order=\"all\">\n        <name-part name=\"family\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" text-case=\"capitalize-first\" suffix=\": \"/>\n      <name initialize-with=\".\" sort-separator=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"URL\"/>\n          <group>\n            <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"volume page\" match=\"none\">\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book report\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"point-locators\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n        </if>\n      </choose>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"date-reference\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key variable=\"issued\" sort=\"ascending\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\":\">\n        <group delimiter=\" \">\n          <text macro=\"contributors-short\"/>\n          <text macro=\"date-reference\"/>\n        </group>\n        <text macro=\"point-locators\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography and=\"text\" et-al-min=\"12\" et-al-use-first=\"6\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \" suffix=\".\">\n        <text macro=\"author\"/>\n        <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n        <text macro=\"title\" suffix=\".\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text macro=\"edition\" prefix=\" \" suffix=\",\"/>\n            <text macro=\"publisher\" prefix=\" \" suffix=\".\"/>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </group>\n                <text macro=\"edition\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"publisher\" prefix=\" \"/>\n              </group>\n              <group delimiter=\" \">\n                <label strip-periods=\"true\" variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"false\" font-style=\"italic\" suffix=\" \"/>\n              <group delimiter=\", \">\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"