s>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\" form=\"long\"/>\n        <text variable=\"publisher\" form=\"long\"/>\n      </group>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"margin\" entry-spacing=\"0\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\" \"/>\n      <text macro=\"author\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" form=\"long\" suffix=\". \"/>\n          <text term=\"in\" form=\"long\" plural=\"false\" text-case=\"capitalize-first\" suffix=\": \"/>\n          <text macro=\"editor\" suffix=\". \"/>\n          <text variable=\"container-title\" form=\"short\" text-case=\"title\" suffix=\". \"/>\n          <text macro=\"edition\" suffix=\", \"/>\n          <text macro=\"publisher\"/>\n          <group delimiter=\" \" prefix=\": \">\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text macro=\"year-date\"/>\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text macro=\"year-date\"/>\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group>\n            <text variable=\"title\" suffix=\". \"/>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" suffix=\" \"/>\n            <text macro=\"year-date\" suffix=\"; \"/>\n            <text variable=\"volume\" suffix=\":\"/>\n            <text variable=\"page\" form=\"short\" prefix=\" \"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"