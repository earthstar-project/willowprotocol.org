"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-US\">\n  <info>\n    <title>Pest Management Science</title>\n    <title-short>PMS</title-short>\n    <id>http://www.zotero.org/styles/pest-management-science</id>\n    <link href=\"http://www.zotero.org/styles/pest-management-science\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/spie-proceedings\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1002/(ISSN)1526-4998/homepage/ForAuthors.html#preparing\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>1526-498X</issn>\n    <eissn>1526-4998</eissn>\n    <updated>2019-03-15T12:25:36+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group prefix=\", \" delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"verb-short\" suffix=\" \"/>\n      <name and=\"text\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <group>\n      <names variable=\"author\">\n        <name and=\"text\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n        <et-al font-style=\"italic\"/>\n        <label form=\"short\" prefix=\", \"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text macro=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report\"/>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\"/>\n        <text macro=\"edition\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"false\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"day\" suffix=\" \"/>\n      <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"issued-year\">\n    <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"locators\">\n    <text variable=\"volume\" prefix=\" \" font-weight=\"bold\"/>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if variable=\"container-title\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n        <text macro=\"locators\"/>\n        <text macro=\"edition\"/>\n      </if>\n      <else-if type=\"paper-conference\">\n        <group delimiter=\" \">\n          <text term=\"presented at\"/>\n          <text variable=\"event\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n      <group prefix=\"(\" suffix=\")\">\n        <label variable=\"locator\" form=\"short\" strip-periods=\"true\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"webpage post-weblog\" match=\"any\">\n          <group delimiter=\". \">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"container\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\"/>\n            <text macro=\"publisher\"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n          <text macro=\"issued-year\"/>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"publisher\"/>\n            <text variable=\"page\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n          <text macro=\"access\"/>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <choose>\n            <if variable=\"container-title\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <text macro=\"editor\"/>\n                <text variable=\"page\"/>\n                <text macro=\"publisher\"/>\n              </group>\n              <text macro=\"issued-year\"/>\n            </if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <text macro=\"issued\"/>\n                <text variable=\"event-place\"/>\n                <text variable=\"page\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </else>\n          </choose>\n          <text macro=\"access\"/>\n        </else-if>\n        <else-if type=\"interview\">\n          <group delimiter=\", \">\n            <text variable=\"note\"/>\n            <text variable=\"medium\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n        </else-if>\n        <else-if type=\"article article-journal article-newspaper\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"editor\"/>\n            <group delimiter=\":\">\n              <text macro=\"container\"/>\n              <text variable=\"page\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n          <text macro=\"access\"/>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"container\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"page\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text variable=\"number\"/>\n          <text variable=\"genre\"/>\n          <text macro=\"issued-year\"/>\n          <text macro=\"access\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"