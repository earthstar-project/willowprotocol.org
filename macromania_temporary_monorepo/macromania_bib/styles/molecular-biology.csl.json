"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\" default-locale=\"en-US\">\n  <info>\n    <title>Molecular Biology</title>\n    <id>http://www.zotero.org/styles/molecular-biology</id>\n    <link href=\"http://www.zotero.org/styles/molecular-biology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/canadian-journal-of-physics\" rel=\"template\"/>\n    <link href=\"http://www.molecbio.ru/?view=for_authors\" rel=\"documentation\"/>\n    <link href=\"http://www.molecbio.ru/suppl/guid.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>0026-8933</issn>\n    <eissn>1608-3245</eissn>\n    <updated>2020-11-13T11:59:17+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\" match=\"any\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"capitalize-first\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" text-case=\"capitalize-first\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n      <name prefix=\" \" delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" suffix=\".\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\".\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label strip-periods=\"false\" suffix=\" \" variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" strip-periods=\"false\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" et-al-min=\"10\" et-al-use-first=\"1\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"volume\"/>\n              <text macro=\"editor\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\" suffix=\". \"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \" prefix=\" \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <group delimiter=\", \">\n                  <text variable=\"container-title\" suffix=\".\"/>\n                  <text macro=\"volume\"/>\n                </group>\n              </group>\n              <text variable=\"collection-title\"/>\n              <text macro=\"edition\"/>\n              <text variable=\"event\"/>\n              <text macro=\"editor\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group prefix=\" \" delimiter=\" \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post-weblog post\" match=\"any\">\n            <text variable=\"URL\"/>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\" form=\"short\"/>\n              <group delimiter=\", \">\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"