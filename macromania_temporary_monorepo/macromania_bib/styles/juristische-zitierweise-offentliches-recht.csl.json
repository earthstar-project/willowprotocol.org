          </group>\n                  </else>\n                </choose>\n              </if>\n              <else>\n                <group delimiter=\",\">\n                  <text variable=\"container-title\"/>\n                  <text variable=\"volume\" prefix=\" \"/>\n                </group>\n              </else>\n            </choose>\n            <text macro=\"firstpage-locator\"/>\n            <text variable=\"title-short\" prefix=\" \"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"author-note\"/>\n            <choose>\n              <if match=\"any\" position=\"ibid\">\n                <text macro=\"Ebenda\"/>\n              </if>\n              <else-if match=\"any\" position=\"near-note\">\n                <text value=\"Fn.\" prefix=\" [\"/>\n                <text variable=\"first-reference-note-number\" suffix=\"]\"/>\n              </else-if>\n              <else>\n                <group>\n                  <text variable=\"title\"/>\n                  <text variable=\"URL\" prefix=\", abrufbar unter \"/>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author-note\"/>\n            <text variable=\"title\"/>\n            <text macro=\"locator-with-label\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"article-journal\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\" suffix=\" \"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n            </group>\n            <text variable=\"page\"/>\n            <text macro=\"URL\" font-style=\"italic\"/>\n          </group>\n        </if>\n        <else-if type=\"article-magazine\" match=\"all\" variable=\"volume\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <text variable=\"volume\"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\"/>\n            <text macro=\"URL\"/>\n          </group>\n        </else-if>\n        <else-if type=\"book\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <group delimiter=\" \">\n              <choose>\n                <if match=\"any\" variable=\"edition\">\n                  <group delimiter=\" \">\n                    <group delimiter=\" \">\n                      <number variable=\"edition\" form=\"ordinal\"/>\n                      <text term=\"edition\" suffix=\",\"/>\n                    </group>\n                    <text variable=\"publisher-place\"/>\n                    <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n                  </group>\n                </if>\n                <else>\n                  <text variable=\"publisher-place\" suffix=\" \"/>\n                  <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n                </else>\n              </choose>\n            </group>\n            <text macro=\"URL\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\"/>\n              <group delimiter=\", \">\n                <names variable=\"editor\" font-style=\"italic\">\n                  <name delimiter=\" / \" name-as-sort-order=\"all\"/>\n                  <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n                </names>\n                <text variable=\"container-title\"/>\n                <text variable=\"collection-number\"/>\n              </group>\n            </group>\n            <text variable=\"edition\"/>\n            <group delimiter=\" \">\n              <text variable=\"publisher-place\"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <group delimiter=\" \">\n              <text variable=\"publisher-place\"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n            </group>\n            <text macro=\"URL\"/>\n          </group>\n        </else-if>\n        <else-if type=\"entry-encyclopedia\">\n          <text macro=\"author\"/>\n          <group>\n            <text term=\"in\" prefix=\", \" suffix=\": \"/>\n            <group>\n              <names variable=\"editor\" font-style=\"normal\">\n                <name delimiter=\" / \" name-as-sort-order=\"all\"/>\n                <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n              </names>\n              <text variable=\"container-title\" prefix=\", \" suffix=\", \"/>\n              <text variable=\"collection-number\"/>\n              <group delimiter=\" \">\n                <number variable=\"edition\" form=\"ordinal\"/>\n                <text term=\"edition\" suffix=\" \"/>\n                <text variable=\"publisher-place\" suffix=\" \"/>\n                <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n              </group>\n              <text variable=\"title\" prefix=\", \"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"URL\"/>\n              <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                <text term=\"accessed\"/>\n                <date form=\"numeric\" variable=\"accessed\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <choose>\n            <if type=\"legal_case\" match=\"none\">\n              <text macro=\"author\"/>\n              <text variable=\"title\" prefix=\", \" suffix=\", \"/>\n              <text variable=\"container-title\" suffix=\" \"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\" suffix=\", \"/>\n              <text variable=\"page\"/>\n            </if>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"