"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"text\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Advertising Research</title>\n    <title-short>JAR</title-short>\n    <id>http://www.zotero.org/styles/journal-of-advertising-research</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-advertising-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/byzantine-and-modern-greek-studies\" rel=\"template\"/>\n    <link href=\"http://www.journalofadvertisingresearch.com/content/submit-manuscript\" rel=\"documentation\"/>\n    <link href=\"http://www.journalofadvertisingresearch.com/sites/default/files/Additional_assets/JAR%20Guidelines.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <issn>0021-8499</issn>\n    <eissn>1740-1909</eissn>\n    <updated>2023-09-28T20:51:48+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"editortranslator\" form=\"short\">ed. and trans.</term>\n      <term name=\"chapter\" form=\"short\">c.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"all\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter-precedes-last=\"always\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <names variable=\"editor\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-translator\">\n    <choose>\n      <if variable=\"editor translator\" match=\"any\">\n        <names variable=\"editor translator\" delimiter=\", \">\n          <name and=\"text\" initialize-with=\". \"/>\n          <label form=\"short\" strip-periods=\"false\" prefix=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"paper-conference\">\n        <group delimiter=\" \">\n          <text term=\"presented at\"/>\n          <text variable=\"event\"/>\n        </group>\n        <text variable=\"event-place\" prefix=\", \"/>\n      </if>\n      <else>\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-volumes\"/>\n      <text term=\"volume\" form=\"short\" plural=\"true\" strip-periods=\"true\"/>\n    </group>\n  </macro>\n  <macro name=\"pageno\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <group>\n          <text variable=\"page\" prefix=\": \"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pageref\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"edition-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\" strip-periods=\"false\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volref\">\n    <group delimiter=\", \">\n      <text variable=\"volume\"/>\n      <text variable=\"issue\"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\" match=\"any\">\n        <choose>\n          <if variable=\"URL\" match=\"any\">\n            <text variable=\"URL\" prefix=\" \" suffix=\", \"/>\n            <text term=\"accessed\" suffix=\" \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"month\" form=\"numeric\" suffix=\"/\"/>\n              <date-part name=\"day\" suffix=\"/\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"date\"/>\n        <text macro=\"pageref\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"2\">\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\". \"/>\n      <choose>\n        <if type=\"report thesis\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"collection-title\"/>\n          </group>\n          <text variable=\"number\" prefix=\" \"/>\n          <group delimiter=\", \">\n            <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n              <text macro=\"date\"/>\n            </group>\n            <text macro=\"pageref\"/>\n          </group>\n        </if>\n        <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n          <group delimiter=\". \">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"edition-note\"/>\n              <text macro=\"volumes\"/>\n              <text macro=\"volref\"/>\n            </group>\n            <text macro=\"editor-translator\"/>\n            <text macro=\"container\"/>\n          </group>\n          <group delimiter=\" \" prefix=\" \">\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"date\"/>\n            </group>\n            <text macro=\"pageref\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <text macro=\"container\"/>\n              </group>\n              <names variable=\"editor\">\n                <name and=\"text\" initialize-with=\". \"/>\n                <label form=\"short\" strip-periods=\"false\" prefix=\", \"/>\n                <substitute>\n                  <names variable=\"editor\"/>\n                  <names variable=\"translator\"/>\n                  <names variable=\"container-author\"/>\n                  <text macro=\"title\"/>\n                </substitute>\n              </names>\n            </group>\n            <text macro=\"volref\"/>\n            <text macro=\"volumes\"/>\n            <group delimiter=\" \">\n              <group delimiter=\", \" prefix=\" \">\n                <text macro=\"publisher\"/>\n                <text macro=\"date\"/>\n              </group>\n              <text macro=\"pageref\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <text macro=\"container\"/>\n              <text macro=\"collection\"/>\n              <text macro=\"volref\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n          <group>\n            <text macro=\"date\" prefix=\" (\" suffix=\")\"/>\n            <text macro=\"volumes\"/>\n            <text macro=\"pageno\"/>\n            <text macro=\"pageref\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"