"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"never\">\n  <!-- Polyglot; journal publishes in Spanish and English -->\n  <info>\n    <title>Ulúa. Revista de Historia, Sociedad y Cultura</title>\n    <id>http://www.zotero.org/styles/ulua-revista-de-historia-sociedad-y-cultura</id>\n    <link href=\"http://www.zotero.org/styles/ulua-revista-de-historia-sociedad-y-cultura\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/associacao-brasileira-de-normas-tecnicas\" rel=\"template\"/>\n    <link href=\"https://ulua.uv.mx/index.php/ulua/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"humanities\"/>\n    <issn>1665-8973</issn>\n    <updated>2021-06-01T12:58:56+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter\">\n        <names variable=\"container-author\" delimiter=\", \">\n          <name and=\"text\" name-as-sort-order=\"first\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"collection-editor\"/>\n          </substitute>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter\" match=\"none\">\n        <names variable=\"editor\" delimiter=\"; \" prefix=\" (\" suffix=\")\">\n          <name initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\" suffix=\".\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\" delimiter=\"; \">\n      <name delimiter=\"; \" sort-separator=\" \" delimiter-precedes-last=\"always\" prefix=\"Tradução: \">\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"first\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\".)\" text-case=\"uppercase\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"book\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <text variable=\"URL\" prefix=\"Disponível em: &lt;\" suffix=\"&gt;\"/>\n    <date variable=\"accessed\" prefix=\". Acesso em: \">\n      <date-part name=\"day\" suffix=\" \"/>\n      <date-part name=\"month\" form=\"short\" suffix=\". \" text-case=\"lowercase\"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"chapter bill\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </if>\n      <else-if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"article-newspaper article-magazine article-journal\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <text variable=\"container-title\" suffix=\". \"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\" font-weight=\"normal\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <choose>\n          <if variable=\"genre\" match=\"none\">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <text variable=\"event\" text-case=\"uppercase\"/>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              <text term=\"presented at\"/>\n              <text variable=\"event\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\" match=\"any\">\n        <group>\n          <choose>\n            <if type=\"book chapter\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part name=\"day\" suffix=\" \"/>\n                <date-part name=\"month\" form=\"short\" suffix=\" \"/>\n              </date>\n            </if>\n          </choose>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <text value=\"[s.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text value=\"[s.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"numeric\" suffix=\".\"/>\n              <text term=\"edition\" form=\"short\" suffix=\".\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\" ed.\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"bill\">\n        <group prefix=\". \" delimiter=\", \">\n          <date variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part prefix=\" \" name=\"month\" form=\"short\"/>\n            <date-part prefix=\" \" name=\"year\"/>\n          </date>\n          <text variable=\"section\" prefix=\"Sec. \"/>\n          <text variable=\"page\" prefix=\"p. \" suffix=\".\"/>\n        </group>\n      </if>\n      <else-if match=\"any\" type=\"article-journal article-magazine article-newspaper\">\n        <group delimiter=\", \">\n          <date variable=\"issued\" font-style=\"italic\">\n            <date-part name=\"month\"/>\n          </date>\n          <group delimiter=\" \">\n            <label variable=\"volume\" form=\"short\"/>\n            <text variable=\"volume\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"issue\" form=\"short\"/>\n            <text variable=\"issue\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if match=\"any\" type=\"book chapter\">\n        <group delimiter=\", \">\n          <group>\n            <text variable=\"volume\" prefix=\"v. \"/>\n            <text variable=\"page\" prefix=\"p. \"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"collection-title\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"genre\">\n    <text variable=\"genre\"/>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"place\">\n    <choose>\n      <if match=\"any\" variable=\"publisher-place\">\n        <text variable=\"publisher-place\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group>\n      <text variable=\"archive\" prefix=\" \"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" collapse=\"year\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout delimiter=\"; \">\n      <group>\n        <text suffix=\", \" macro=\"author-short\"/>\n        <text macro=\"issued-year\"/>\n        <text prefix=\", \" macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\" et-al-min=\"11\" et-al-use-first=\"10\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\" display=\"block\"/>\n      <group delimiter=\" \">\n        <text macro=\"issued-year\"/>\n        <choose>\n          <if type=\"bill\">\n            <group>\n              <text variable=\"number\" suffix=\". \"/>\n              <text macro=\"title\" suffix=\". \"/>\n              <text variable=\"references\" font-weight=\"bold\"/>\n              <text variable=\"note\"/>\n              <text macro=\"locators\" suffix=\". \"/>\n            </group>\n          </if>\n          <else-if type=\"map\">\n            <group>\n              <text macro=\"title\" suffix=\", \"/>\n              <text variable=\"note\" suffix=\". \"/>\n            </group>\n          </else-if>\n          <else-if type=\"book\">\n            <group>\n              <text macro=\"title\" suffix=\". \"/>\n              <text macro=\"translator\" suffix=\". \"/>\n              <text macro=\"edition\" suffix=\". \"/>\n              <text macro=\"publisher\" suffix=\". \"/>\n              <text macro=\"locators\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"secondary-contributors\"/>\n              <group delimiter=\" \">\n                <text term=\"in\"/>\n                <text macro=\"container-contributors\"/>\n              </group>\n              <text macro=\"container-title\"/>\n              <text variable=\"collection-title\"/>\n              <text macro=\"translator\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"locators\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-newspaper article-magazine article-journal\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"container-title\"/>\n              <text variable=\"collection-title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"locators\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group>\n              <text macro=\"title\" suffix=\". \"/>\n              <text macro=\"genre\" suffix=\"&#8212;\"/>\n              <text macro=\"publisher\" suffix=\".\"/>\n            </group>\n          </else-if>\n          <else-if type=\"manuscript\">\n            <group>\n              <text macro=\"title\" suffix=\". \"/>\n              <text macro=\"edition\" suffix=\". \"/>\n              <text macro=\"place\" suffix=\", \"/>\n              <text macro=\"access\" suffix=\". \"/>\n              <text macro=\"archive\" suffix=\". \"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group>\n              <text macro=\"title\" suffix=\". \"/>\n              <text macro=\"genre\" suffix=\". \"/>\n              <text macro=\"access\" suffix=\". \"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\">\n            <group>\n              <text macro=\"title\"/>\n              <text macro=\"container-contributors\"/>\n              <text macro=\"secondary-contributors\"/>\n              <text macro=\"container-title\"/>\n              <text variable=\"collection-title\" prefix=\": \"/>\n              <text macro=\"locators\"/>\n              <text macro=\"event\"/>\n              <text macro=\"publisher\" prefix=\". \" suffix=\". \"/>\n              <text macro=\"access\" suffix=\".\"/>\n            </group>\n          </else-if>\n          <else-if type=\"entry-dictionary\">\n            <group>\n              <text macro=\"title\"/>\n              <text macro=\"container-contributors\"/>\n              <text macro=\"secondary-contributors\"/>\n              <text macro=\"container-title\"/>\n              <text variable=\"collection-title\" prefix=\": \" suffix=\". \"/>\n              <text macro=\"locators\"/>\n              <text macro=\"event\"/>\n              <text macro=\"publisher\" prefix=\". \" suffix=\". \"/>\n              <text macro=\"collection-title\" prefix=\"(Texto para discussao, n. \" suffix=\").\"/>\n              <text macro=\"access\"/>\n            </group>\n          </else-if>\n          <else-if type=\"entry-encyclopedia\">\n            <group>\n              <text macro=\"title\"/>\n              <text variable=\"publisher-place\" prefix=\". \" suffix=\": \"/>\n              <text variable=\"publisher\" suffix=\", \"/>\n              <text macro=\"issued\" prefix=\", \" suffix=\". (Nota técnica).\"/>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"container-contributors\"/>\n            <text macro=\"secondary-contributors\"/>\n            <text macro=\"container-title\"/>\n            <text variable=\"collection-title\" prefix=\": \" suffix=\".\"/>\n            <text macro=\"locators\"/>\n            <group delimiter=\". \" prefix=\". \" suffix=\". \">\n              <text macro=\"event\"/>\n            </group>\n            <text variable=\"publisher-place\" suffix=\": \"/>\n            <text variable=\"publisher\" suffix=\", \"/>\n            <text macro=\"access\"/>\n          </else-if>\n          <else>\n            <text macro=\"title\"/>\n            <text macro=\"container-contributors\"/>\n            <text macro=\"secondary-contributors\"/>\n            <text macro=\"container-title\"/>\n            <text variable=\"collection-title\" prefix=\": \" suffix=\".\"/>\n            <text macro=\"locators\"/>\n            <group delimiter=\". \" prefix=\". \" suffix=\". \">\n              <text macro=\"event\"/>\n            </group>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\" suffix=\", \"/>\n            <text macro=\"access\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"