"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\">\n  <info>\n    <title>Proceedings of the Royal Society B</title>\n    <id>http://www.zotero.org/styles/proceedings-of-the-royal-society-b</id>\n    <link href=\"http://www.zotero.org/styles/proceedings-of-the-royal-society-b\" rel=\"self\"/>\n    <link href=\"https://royalsociety.org/journals/authors/author-guidelines/\" rel=\"documentation\"/>\n    <author>\n      <name>Josefa Bleu</name>\n      <email>josefa.bleu@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Michael Berkowitz</name>\n      <email>mberkowi@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Sean Takats</name>\n      <email>stakats@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <contributor>\n      <name>Michael Doube</name>\n      <email>mdoube@rvc.ac.uk</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>0962-8452</issn>\n    <eissn>1471-2954</eissn>\n    <updated>2017-03-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"accessed\">accessed on</term>\n      <term name=\"edition\" form=\"short\">edn</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"never\" et-al-min=\"10\" et-al-use-first=\"1\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" prefix=\"(\" suffix=\")\">\n      <label form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n      <name sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \" suffix=\". \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group suffix=\" \">\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <group delimiter=\", \" suffix=\". \">\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n          <date variable=\"issued\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\"/>\n          </date>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"book\">\n          <text variable=\"title\" font-style=\"italic\"/>\n        </if>\n        <else>\n          <text variable=\"title\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" suffix=\".\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" suffix=\" \"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"in press\" suffix=\". \" text-case=\"capitalize-first\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"(doi:\" suffix=\")\"/>\n      </if>\n      <else-if variable=\"URL\">\n        <group suffix=\".\">\n          <choose>\n            <if type=\"article-newspaper article-magazine dataset book webpage thesis\" match=\"any\">\n              <text variable=\"URL\" prefix=\"See \"/>\n            </if>\n          </choose>\n          <choose>\n            <if type=\"webpage\">\n              <group prefix=\" (\" delimiter=\" \" suffix=\")\">\n                <text term=\"accessed\"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"day\" suffix=\" \"/>\n                  <date-part name=\"month\" suffix=\" \"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" et-al-min=\"11\" et-al-use-first=\"10\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\"/>\n      <text macro=\"issued\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"book\">\n          <text macro=\"edition\" suffix=\" \"/>\n          <text macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \" suffix=\",\">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <group prefix=\" \" delimiter=\" \" suffix=\".\">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n          <text macro=\"publisher\" prefix=\" \"/>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \" suffix=\".\">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group suffix=\".\">\n            <text variable=\"volume\" font-weight=\"bold\"/>\n            <text variable=\"page\" prefix=\", \"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"