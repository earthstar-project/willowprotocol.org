"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>BrewingScience</title>\n    <id>http://www.zotero.org/styles/brewingscience</id>\n    <link href=\"http://www.zotero.org/styles/brewingscience\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/stavebni-obzor\" rel=\"template\"/>\n    <link href=\"https://www.brewingscience.de/index.php?tpl=authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <category field=\"engineering\"/>\n    <category field=\"science\"/>\n    <issn>1866-5195</issn>\n    <eissn>1613-2041</eissn>\n    <updated>2018-06-05T14:44:58+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\"; \" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" \" text-case=\"lowercase\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\" \">\n      <label variable=\"issue\" form=\"short\"/>\n      <text variable=\"issue\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <number variable=\"edition\" form=\"ordinal\"/>\n      <label variable=\"edition\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\" after-collapse-delimiter=\"; \">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n      <group delimiter=\" \">\n        <label variable=\"locator\" form=\"short\" prefix=\" \"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography and=\"text\" second-field-align=\"flush\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <group delimiter=\" - \" suffix=\": \">\n        <text macro=\"author\"/>\n      </group>\n      <choose>\n        <if type=\"book\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </if>\n        <else-if type=\"paper-conference chapter\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"container-title\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"event\" prefix=\"konference \" quotes=\"true\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"container-title\"/>\n            <group delimiter=\" \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text macro=\"issue\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"container-title\"/>\n            <text variable=\"URL\"/>\n            <group delimiter=\" \">\n              <text term=\"accessed\"/>\n              <date form=\"text\" variable=\"accessed\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text variable=\"title\"/>\n          <text variable=\"container-title\"/>\n          <text macro=\"year-date\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"