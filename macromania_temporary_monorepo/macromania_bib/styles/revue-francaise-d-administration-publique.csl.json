"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"fr-FR\" page-range-format=\"expanded\">\n  <info>\n    <title>Revue française d'administration publique (Français)</title>\n    <title-short>RFAP</title-short>\n    <id>http://www.zotero.org/styles/revue-francaise-d-administration-publique</id>\n    <link href=\"http://www.zotero.org/styles/revue-francaise-d-administration-publique\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/revue-francaise-de-gestion\" rel=\"template\"/>\n    <link href=\"https://www.ena.fr/La-recherche-a-l-ENA/Publications/rfap\" rel=\"documentation\"/>\n    <author>\n      <name>Romain Pierronnet</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <category field=\"humanities\"/>\n    <category field=\"political_science\"/>\n    <issn>0152-7401</issn>\n    <eissn>1965-0620</eissn>\n    <summary>Le style utilisé en 2019 par la Revue française d'administration publique.</summary>\n    <updated>2018-10-04T23:58:26+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>dir.</single>\n        <multiple>dirs.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\" \">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"always\" initialize-with=\".\" font-variant=\"small-caps\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"lowercase\"/>\n      <substitute>\n        <names variable=\"editorial-director\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-last\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\" ; \" delimiter-precedes-last=\"always\" font-variant=\"normal\" font-weight=\"normal\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"lowercase\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"editorial-director\"/>\n      </substitute>\n    </names>\n  </macro>\n  <!-- Same as author-last except that name is not initialized to sort on first name too -->\n  <macro name=\"author-sort\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\" ; \" delimiter-precedes-last=\"always\" font-variant=\"normal\" font-weight=\"normal\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"lowercase\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"editorial-director\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <text term=\"translator\" form=\"verb\" suffix=\" \"/>\n    <names variable=\"translator\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"always\" initialize-with=\".\" font-variant=\"small-caps\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher-collection\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n    <text variable=\"collection-title\" prefix=\" (\" suffix=\")\"/>\n  </macro>\n  <macro name=\"year-date-short\">\n    <choose>\n      <if variable=\"original-date\">\n        <date variable=\"original-date\" prefix=\"[\" suffix=\"] \">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"full-date\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"day\"/>\n      <date-part name=\"month\"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"page\">\n    <group>\n      <label variable=\"page\" suffix=\".&#160;\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <group>\n      <text variable=\"number-of-pages\" suffix=\"&#160;p\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author-last\"/>\n      <key macro=\"year-date-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"&#160;; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date-short\"/>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author-sort\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author-last\" suffix=\" \"/>\n      <text macro=\"year-date-short\" prefix=\" (\" suffix=\"), \"/>\n      <choose>\n        <if type=\"report\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text macro=\"translator\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"collection-title\" font-style=\"italic\"/>\n            <text variable=\"number\"/>\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"book\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"translator\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher-collection\"/>\n            <text variable=\"medium\"/>\n            <group delimiter=\" \">\n              <text term=\"version\"/>\n              <text variable=\"version\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"translator\"/>\n            <choose>\n              <if variable=\"genre\">\n                <text variable=\"genre\"/>\n              </if>\n              <else>\n                <text value=\"Thèse de doctorat\"/>\n              </else>\n            </choose>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher-collection\"/>\n            <text macro=\"number-of-pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <text variable=\"title\" quotes=\"true\"/>\n          <group prefix=\", \">\n            <text value=\"in\" suffix=\" \"/>\n            <text macro=\"editor\" suffix=\", \"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"translator\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher-collection\"/>\n              <text macro=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"event\"/>\n            <text variable=\"event-place\"/>\n            <text macro=\"full-date\"/>\n            <text macro=\"translator\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine interview broadcast\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n            <text macro=\"full-date\"/>\n            <text macro=\"translator\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \" suffix=\",\">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <group prefix=\" \" delimiter=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"volume\" prefix=\" vol. \"/>\n            <text variable=\"issue\" prefix=\" n° \"/>\n            <text macro=\"translator\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"