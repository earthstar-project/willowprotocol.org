"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Baishideng Publishing Group</title>\n    <id>http://www.zotero.org/styles/baishideng-publishing-group</id>\n    <link href=\"http://www.zotero.org/styles/baishideng-publishing-group\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/hypertension-research\" rel=\"template\"/>\n    <link href=\"https://www.wjgnet.com/1948-5182/\" rel=\"documentation\"/>\n    <link href=\"https://bsdwebstorage.blob.core.windows.net/bpggerinfo/Guidelines_for_Manuscript_Preparation_and_Submission-Basic_Study.pdf\" rel=\"documentation\"/>\n    <link href=\"https://www.wjgnet.com/bpg/GerInfo/218\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"generic-base\"/>\n    <updated>2020-12-21T08:23:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label strip-periods=\"true\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"none\">\n        <choose>\n          <if variable=\"page\" match=\"none\">\n            <choose>\n              <if variable=\"DOI\">\n                <text variable=\"DOI\" prefix=\" DOI:\"/>\n              </if>\n              <else-if variable=\"URL\">\n                <group delimiter=\": \">\n                  <text term=\"available at\" text-case=\"capitalize-first\"/>\n                  <text variable=\"URL\"/>\n                </group>\n              </else-if>\n            </choose>\n          </if>\n        </choose>\n      </if>\n      <else-if variable=\"page volume\" match=\"any\">\n        <choose>\n          <if variable=\"DOI\">\n            <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n              <text variable=\"PMID\" prefix=\"PMID: \"/>\n              <text variable=\"DOI\" prefix=\"DOI: \"/>\n            </group>\n          </if>\n          <else-if variable=\"URL\">\n            <text variable=\"URL\"/>\n            <group prefix=\". \" delimiter=\" \">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date variable=\"accessed\" form=\"text\"/>\n            </group>\n          </else-if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal\">\n        <choose>\n          <if variable=\"volume page\" match=\"none\">\n            <choose>\n              <if variable=\"DOI URL\" match=\"any\">\n                <group prefix=\"(e-pub ahead of print \" suffix=\")\" delimiter=\"; \">\n                  <date variable=\"issued\" form=\"text\"/>\n                  <text variable=\"DOI\" prefix=\"doi:\"/>\n                </group>\n              </if>\n              <else>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"100\" et-al-use-first=\"99\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" font-weight=\"bold\" suffix=\" \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\": \" prefix=\" \" suffix=\".\">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <group delimiter=\". \">\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n            </group>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"publisher\" prefix=\" \"/>\n            <group delimiter=\": \">\n              <text macro=\"date\"/>\n              <text variable=\"page\" suffix=\".\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group suffix=\".\">\n            <text macro=\"date\" prefix=\" \"/>\n            <group prefix=\"; \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"page\" prefix=\": \"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"