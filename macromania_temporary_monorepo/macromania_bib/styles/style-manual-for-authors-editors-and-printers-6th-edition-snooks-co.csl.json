"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Style manual for authors, editors and printers 6th edition (Snooks &amp; Co.) (author-date)</title>\n    <id>http://www.zotero.org/styles/style-manual-for-authors-editors-and-printers-6th-edition-snooks-co</id>\n    <link href=\"http://www.zotero.org/styles/style-manual-for-authors-editors-and-printers-6th-edition-snooks-co\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-cite-them-right\" rel=\"template\"/>\n    <link href=\"https://catalogue.nla.gov.au/Record/45358\" rel=\"documentation\"/>\n    <link href=\"https://libguides.library.usyd.edu.au/c.php?g=508212&amp;p=3476130\" rel=\"documentation\"/>\n    <author>\n      <name>jstedman</name>\n      <email>jessica.stedman@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Snooks style manual Australia Harvard author-date style</summary>\n    <updated>2017-10-12T00:16:23+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"edition\" form=\"short\">edn</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"symbol\" initialize-with=\"\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\"\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text value=\"viewed\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n          <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis webpage\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\" et-al-min=\"4\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <text macro=\"year-date\" prefix=\" \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\" prefix=\", \"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\",\">\n            <text macro=\"title\"/>\n            <text term=\"in\" prefix=\", \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n            <text variable=\"collection-title\" prefix=\", \"/>\n            <text variable=\"event\" prefix=\", \"/>\n            <group>\n              <text macro=\"publisher\"/>\n            </group>\n            <group prefix=\", \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\", \">\n            <text macro=\"title\"/>\n          </group>\n          <group prefix=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group prefix=\", \">\n              <text variable=\"volume\" prefix=\"vol. \"/>\n              <text variable=\"issue\" prefix=\", no. \"/>\n            </group>\n            <group prefix=\", \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\" prefix=\". \"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\", \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"