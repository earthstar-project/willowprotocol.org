"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" page-range-format=\"chicago\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>China Information</title>\n    <id>http://www.zotero.org/styles/china-information</id>\n    <link href=\"http://www.zotero.org/styles/china-information\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/twentieth-century-music\" rel=\"template\"/>\n    <link href=\"https://uk.sagepub.com/en-gb/eur/journal/china-information#JournalStyle\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>citationstyler@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <issn>0920-203X</issn>\n    <eissn>1741-590X</eissn>\n    <updated>2019-08-08T14:08:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"presented at\">presented at the conference</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <group delimiter=\" \">\n      <text term=\"in\"/>\n      <group delimiter=\", \">\n        <choose>\n          <if variable=\"author\">\n            <names variable=\"editor\" delimiter=\", \">\n              <name delimiter=\", \"/>\n              <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n            </names>\n            <choose>\n              <if variable=\"container-author\">\n                <group>\n                  <text term=\"in\" text-case=\"lowercase\" suffix=\" \"/>\n                  <names variable=\"container-author\">\n                    <name delimiter=\", \"/>\n                  </names>\n                </group>\n              </if>\n            </choose>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"author editor\" match=\"any\">\n            <names variable=\"translator\" delimiter=\", \">\n              <label form=\"short\" text-case=\"lowercase\" strip-periods=\"false\"/>\n              <name prefix=\" \"/>\n            </names>\n          </if>\n        </choose>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <text macro=\"editor-translator\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text macro=\"editor-translator\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor-note\">\n    <names variable=\"editor\">\n      <name delimiter=\", \"/>\n      <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"translator-note\">\n    <names variable=\"translator\">\n      <name delimiter=\", \"/>\n      <label form=\"short\" text-case=\"lowercase\" strip-periods=\"false\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"recipient-note\">\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"lowercase\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-note\">\n    <names variable=\"author\">\n      <name and=\"text\"/>\n      <substitute>\n        <text macro=\"editor-note\"/>\n        <text macro=\"translator-note\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient-note\"/>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name delimiter=\", \"/>\n      <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\" (\" suffix=\") \"/>\n    </names>\n  </macro>\n  <macro name=\"recipient\">\n    <choose>\n      <if type=\"personal_communication\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n          <else>\n            <text term=\"letter\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n    <text macro=\"recipient-note\" prefix=\" \"/>\n  </macro>\n  <macro name=\"contributors\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"first\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"translator\" prefix=\"(\" suffix=\")\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient\" prefix=\". \"/>\n  </macro>\n  <macro name=\"recipient-short\">\n    <names variable=\"recipient\">\n      <name form=\"short\"/>\n      <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\" (\" suffix=\") \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" initialize-with=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient-short\"/>\n  </macro>\n  <macro name=\"contributors-sort\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"interviewer-note\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <name delimiter=\", \"/>\n      <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\" (\" suffix=\") \"/>\n    </names>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title-note\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <text variable=\"genre\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"false\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"personal_communication\" match=\"none\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"false\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"interview\">\n            <text term=\"interview\" text-case=\"lowercase\"/>\n          </if>\n          <else-if type=\"manuscript speech\" match=\"any\">\n            <text variable=\"genre\" form=\"short\"/>\n          </else-if>\n          <else-if type=\"personal_communication\">\n            <text macro=\"issued\"/>\n          </else-if>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"title\" form=\"short\" quotes=\"false\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"description-note\">\n    <group delimiter=\", \">\n      <text macro=\"interviewer-note\"/>\n      <text variable=\"medium\"/>\n      <choose>\n        <if variable=\"title\" match=\"none\"/>\n        <else-if type=\"thesis speech\" match=\"any\"/>\n        <else>\n          <text variable=\"genre\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"description\">\n    <group delimiter=\", \">\n      <group delimiter=\". \">\n        <text macro=\"interviewer\"/>\n        <text variable=\"medium\" text-case=\"capitalize-first\"/>\n      </group>\n      <choose>\n        <if variable=\"title\" match=\"none\"/>\n        <else-if type=\"thesis speech\" match=\"any\"/>\n        <else>\n          <text variable=\"genre\" text-case=\"capitalize-first\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"legal_case\" match=\"none\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection-title\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <group prefix=\" \">\n          <text variable=\"volume\"/>\n          <number prefix=\"(\" suffix=\")\" variable=\"issue\"/>\n        </group>\n      </if>\n      <else-if type=\"legal_case\">\n        <text variable=\"volume\" prefix=\", \"/>\n        <text variable=\"container-title\" prefix=\" \"/>\n        <text variable=\"page\" prefix=\" \"/>\n        <text variable=\"locator\" prefix=\", \"/>\n      </else-if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group prefix=\", \" delimiter=\", \">\n          <group>\n            <text term=\"volume\" form=\"short\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"roman\"/>\n          </group>\n          <choose>\n            <if variable=\"locator\" match=\"none\">\n              <group>\n                <number variable=\"number-of-volumes\" form=\"numeric\"/>\n                <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locators-newspaper\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"edition\"/>\n            <text term=\"edition\"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\" \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <group>\n      <text term=\"presented at\" suffix=\" \"/>\n      <text variable=\"event\" font-style=\"italic\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"article-newspaper\" match=\"any\">\n        <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n          <text variable=\"publisher\"/>\n          <text macro=\"issued\"/>\n        </group>\n      </if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher-place\"/>\n          <text macro=\"issued\"/>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <group delimiter=\": \">\n            <choose>\n              <if type=\"thesis\" match=\"none\">\n                <text variable=\"publisher-place\"/>\n              </if>\n            </choose>\n            <text variable=\"publisher\"/>\n          </group>\n          <text macro=\"issued\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"graphic report\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"day\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if type=\"legal_case\">\n        <text variable=\"authority\" suffix=\" \"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else-if type=\"article-journal\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"month\"/>\n          <date-part name=\"year\" prefix=\" \"/>\n        </date>\n      </else-if>\n      <else>\n        <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"point-locators-subsequent\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <choose>\n                <if variable=\"volume\">\n                  <group>\n                    <text term=\"volume\" form=\"short\" text-case=\"lowercase\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <label variable=\"locator\" form=\"short\" prefix=\", \" suffix=\" \"/>\n                  </group>\n                </if>\n                <else>\n                  <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n                </else>\n              </choose>\n            </if>\n          </choose>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <number variable=\"volume\" form=\"numeric\" suffix=\":\"/>\n        </else-if>\n      </choose>\n      <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"point-locators\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <text macro=\"pages\"/>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"locator\" prefix=\": \"/>\n      </else-if>\n      <else-if type=\"legal_case\"/>\n      <else>\n        <group prefix=\", \">\n          <choose>\n            <if locator=\"page\" match=\"none\">\n              <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n            </if>\n          </choose>\n          <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"chapter\" match=\"any\">\n        <text variable=\"page\" prefix=\", \"/>\n      </if>\n      <else>\n        <text variable=\"page\" prefix=\": \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators-chapter\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <choose>\n          <if variable=\"page\">\n            <text variable=\"page\" prefix=\". \"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-journal\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"page\" prefix=\", \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issue-note\">\n    <choose>\n      <if type=\"article-journal legal_case\" match=\"any\">\n        <text macro=\"issued\" prefix=\", \"/>\n      </if>\n      <else-if variable=\"publisher-place publisher\" match=\"any\">\n        <group delimiter=\", \" prefix=\", \">\n          <group delimiter=\", \">\n            <choose>\n              <if variable=\"title\" match=\"none\"/>\n              <else-if type=\"thesis speech\" match=\"any\">\n                <text variable=\"genre\"/>\n              </else-if>\n            </choose>\n            <text macro=\"event\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"publisher\"/>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"issued\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue\">\n    <choose>\n      <if type=\"article-journal legal_case\" match=\"any\">\n        <text macro=\"issued\" prefix=\" \"/>\n      </if>\n      <else-if variable=\"publisher-place publisher\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <group delimiter=\", \">\n            <choose>\n              <if variable=\"title\" match=\"none\"/>\n              <else-if type=\"thesis speech\" match=\"any\">\n                <text variable=\"genre\"/>\n              </else-if>\n            </choose>\n            <text macro=\"event\"/>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"publisher\"/>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"issued\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog\">\n        <group delimiter=\" \" prefix=\" \">\n          <text variable=\"URL\"/>\n          <group prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\" text-case=\"lowercase\" suffix=\" \"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group>\n      <number variable=\"edition\" form=\"ordinal\"/>\n      <label variable=\"edition\" form=\"short\"/>\n    </group>\n  </macro>\n  <citation and=\"text\" et-al-min=\"4\" et-al-use-first=\"1\">\n    <layout delimiter=\"; \" suffix=\".\">\n      <choose>\n        <if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"contributors-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"point-locators-subsequent\" suffix=\".\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"contributors-note\"/>\n            <text macro=\"title-note\"/>\n            <text macro=\"description-note\"/>\n            <text macro=\"container-contributors\"/>\n            <text macro=\"container-title\"/>\n            <text macro=\"secondary-contributors\"/>\n          </group>\n          <text macro=\"locators\"/>\n          <text macro=\"collection-title\" prefix=\", \"/>\n          <text macro=\"issue-note\"/>\n          <text macro=\"locators-newspaper\" prefix=\", \"/>\n          <group delimiter=\". \">\n            <text macro=\"point-locators\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography and=\"text\" et-al-min=\"11\" et-al-use-first=\"7\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"contributors-sort\"/>\n      <key variable=\"title\"/>\n      <key variable=\"genre\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"contributors\"/>\n        <text macro=\"title\"/>\n        <text macro=\"description\"/>\n        <text macro=\"secondary-contributors\"/>\n        <group delimiter=\", \">\n          <text macro=\"container-contributors\"/>\n          <text macro=\"container-title\"/>\n        </group>\n      </group>\n      <text macro=\"locators\"/>\n      <text macro=\"collection-title\" prefix=\". \"/>\n      <text macro=\"issue\"/>\n      <text macro=\"locators-newspaper\" prefix=\", \"/>\n      <text macro=\"locators-journal\"/>\n      <text macro=\"locators-chapter\"/>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"