"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>Qeios</title>\n    <id>http://www.zotero.org/styles/qeios</id>\n    <link href=\"http://www.zotero.org/styles/qeios\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/vancouver\" rel=\"template\"/>\n    <link href=\"https://www.qeios.com/posting-policy#references\" rel=\"documentation\"/>\n    <author>\n      <name>Alberto Marinello</name>\n      <email>am@qeios.com</email>\n    </author>\n    <contributor>\n      <name>Gabriele Marinello</name>\n    </contributor>\n    <contributor>\n      <name>Marcello Fuschi</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <issn>2632-3834</issn>\n    <updated>2021-01-24T12:23:44+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <date form=\"text\" delimiter=\" \">\n      <date-part name=\"day\"/>\n      <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"collection-editor\" form=\"long\">\n        <single>editor</single>\n        <multiple>editors</multiple>\n      </term>\n      <term name=\"presented at\">presented at</term>\n      <term name=\"retrieved\">available from</term>\n      <term name=\"section\" form=\"short\">sect.</term>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>pp.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\", \" delimiter-precedes-et-al=\"always\" et-al-min=\"6\" et-al-use-first=\"5\" initialize=\"false\" initialize-with=\". \"/>\n      <label form=\"long\"/>\n      <et-al term=\"et-al\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\".\">\n      <name delimiter=\", \" delimiter-precedes-et-al=\"always\" et-al-min=\"6\" et-al-use-first=\"5\" initialize=\"false\" initialize-with=\". \"/>\n      <label form=\"long\"/>\n      <et-al term=\"et-al\"/>\n    </names>\n  </macro>\n  <macro name=\"chapter-marker\">\n    <choose>\n      <if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper book paper-conference review review-book thesis\" match=\"any\">\n        <choose>\n          <if variable=\"issued\" match=\"all\">\n            <date variable=\"issued\">\n              <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n            </date>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"report\">\n        <date variable=\"issued\" delimiter=\" \" prefix=\"(\" suffix=\")\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n        </date>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"extended-date\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper book paper-conference review review-book thesis\" match=\"any\">\n        <choose>\n          <if variable=\"issued\" match=\"none\">\n            <text macro=\"accessed-date\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <group delimiter=\", \">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"patent\">\n        <group suffix=\".\">\n          <group delimiter=\", \">\n            <text variable=\"number\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"speech\">\n        <group delimiter=\"; \">\n          <group delimiter=\" \">\n            <date variable=\"issued\" delimiter=\" \">\n              <date-part name=\"year\"/>\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"day\"/>\n            </date>\n          </group>\n          <text variable=\"event-place\"/>\n        </group>\n      </else-if>\n      <else-if type=\"post-weblog webpage\" match=\"any\">\n        <date variable=\"issued\" form=\"text\"/>\n        <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n          <text term=\"cited\" text-case=\"lowercase\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n        </group>\n      </else-if>\n      <else>\n        <group suffix=\".\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <text macro=\"edition\" prefix=\". \"/>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"song motion_picture\" match=\"any\">\n        <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal article-magazine chapter paper-conference article-newspaper post-weblog webpage post report\" match=\"any\">\n        <group suffix=\".\" delimiter=\" \">\n          <choose>\n            <if type=\"article-journal report\" match=\"any\">\n              <text variable=\"container-title\" font-style=\"italic\" form=\"short\" strip-periods=\"true\"/>\n            </if>\n            <else-if type=\"post-weblog webpage post\" match=\"any\">\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\" font-style=\"italic\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <choose>\n                  <if variable=\"URL\">\n                    <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n                  </if>\n                </choose>\n              </group>\n            </else-if>\n            <else>\n              <text variable=\"container-title\" font-style=\"italic\" strip-periods=\"true\"/>\n            </else>\n          </choose>\n        </group>\n        <text macro=\"edition\" prefix=\" \"/>\n      </if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\". \">\n            <text variable=\"container-title\"/>\n            <group delimiter=\" \">\n              <text term=\"section\" form=\"short\" text-case=\"capitalize-first\"/>\n              <text variable=\"section\"/>\n            </group>\n          </group>\n          <text variable=\"number\"/>\n        </group>\n      </else-if>\n      <else-if type=\"speech\">\n        <group delimiter=\": \" suffix=\";\">\n          <group delimiter=\" \">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            <text term=\"presented at\"/>\n          </group>\n          <text variable=\"event\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine review review-book\" match=\"any\">\n        <text variable=\"volume\" font-weight=\"bold\"/>\n        <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper review review-book\" match=\"any\">\n        <text variable=\"page\" prefix=\":\"/>\n      </if>\n      <else-if type=\"book\" match=\"any\">\n        <text variable=\"number-of-pages\" prefix=\" \"/>\n        <choose>\n          <if is-numeric=\"number-of-pages\">\n            <label variable=\"number-of-pages\" form=\"short\" prefix=\" \"/>\n          </if>\n        </choose>\n      </else-if>\n      <else>\n        <group prefix=\" \" delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <group delimiter=\", \" suffix=\".\">\n          <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          <text variable=\"publisher\" font-style=\"italic\"/>\n        </group>\n      </if>\n      <else>\n        <choose>\n          <if type=\"article-newspaper article-journal article-magazine\" match=\"none\">\n            <group delimiter=\": \">\n              <choose>\n                <if type=\"speech\" match=\"any\"/>\n                <else>\n                  <text variable=\"publisher-place\"/>\n                </else>\n              </choose>\n              <text variable=\"publisher\" font-style=\"italic\"/>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection-details\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper review review-book\" match=\"none\">\n        <choose>\n          <if variable=\"collection-title\">\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <names variable=\"collection-editor\" suffix=\".\">\n                <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n                <label form=\"long\" prefix=\", \"/>\n              </names>\n              <group delimiter=\"; \">\n                <text variable=\"collection-title\"/>\n                <group delimiter=\" \">\n                  <label variable=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"report-details\">\n    <choose>\n      <if type=\"report\">\n        <text variable=\"number\" prefix=\"Report No.: \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\". \">\n      <choose>\n        <if variable=\"DOI\">\n          <text variable=\"DOI\" prefix=\"doi:\"/>\n        </if>\n        <else-if variable=\"URL\">\n          <group delimiter=\". \">\n            <group delimiter=\": \">\n              <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n              <text variable=\"URL\"/>\n            </group>\n            <choose>\n              <if type=\"article-newspaper article-magazine\" match=\"any\">\n                <group delimiter=\" \" suffix=\".\">\n                  <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                  <date variable=\"accessed\" form=\"text\"/>\n                </group>\n              </if>\n            </choose>\n          </group>\n        </else-if>\n      </choose>\n      <choose>\n        <if type=\"book chapter review-book\" match=\"any\">\n          <text variable=\"ISBN\" prefix=\"ISBN \"/>\n        </if>\n      </choose>\n    </group>\n    <group delimiter=\"; \">\n      <text variable=\"PMID\" prefix=\"PubMed PMID: \"/>\n      <text variable=\"PMCID\" prefix=\"PubMed Central PMCID: \"/>\n    </group>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n          <text term=\"cited\" text-case=\"lowercase\"/>\n          <date variable=\"accessed\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"{m/\" suffix=\"/}\" delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"6\" et-al-use-first=\"5\" entry-spacing=\"1\" line-spacing=\"1\">\n    <layout>\n      <text variable=\"citation-number\" prefix=\"{r/\" suffix=\"/ \"/>\n      <group suffix=\" /}\">\n        <group delimiter=\". \" suffix=\". \">\n          <text macro=\"author\"/>\n          <text macro=\"date\"/>\n          <text macro=\"title\"/>\n        </group>\n        <group delimiter=\" \" suffix=\". \">\n          <group delimiter=\": \">\n            <text macro=\"chapter-marker\"/>\n            <group delimiter=\" \">\n              <text macro=\"editor\"/>\n              <text macro=\"container-title\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n          <group>\n            <text macro=\"extended-date\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </group>\n        <text macro=\"collection-details\" suffix=\". \"/>\n        <text macro=\"report-details\" suffix=\". \"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"