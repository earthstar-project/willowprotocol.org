"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\">\n  <info>\n    <title>Health Physics</title>\n    <title-short>HPS</title-short>\n    <id>http://www.zotero.org/styles/health-physics</id>\n    <link href=\"http://www.zotero.org/styles/health-physics\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/mary-ann-liebert-harvard\" rel=\"template\"/>\n    <link href=\"https://www.editorialmanager.com/hpj/default1.aspx\" rel=\"documentation\"/>\n    <link href=\"https://hps.org/documents/HPS_publications_style_guide.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <issn>0017-9078</issn>\n    <eissn>1538-5159</eissn>\n    <updated>2022-04-27T09:02:03+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\"\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\" \">\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            </if>\n          </choose>\n          <text macro=\"editors\"/>\n          <text variable=\"container-title\" suffix=\". \"/>\n          <text macro=\"edition\"/>\n          <group delimiter=\":\">\n            <group delimiter=\"; \">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n              <choose>\n                <if type=\"report\" match=\"any\">\n                  <group delimiter=\" \">\n                    <text variable=\"genre\"/>\n                    <label text-case=\"capitalize-first\" variable=\"issue\" form=\"short\"/>\n                    <text variable=\"number\"/>\n                  </group>\n                </if>\n              </choose>\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n            <choose>\n              <if type=\"chapter paper-conference\" match=\"any\">\n                <text variable=\"page\"/>\n              </if>\n            </choose>\n          </group>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\"; \">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n            <group delimiter=\":\">\n              <text variable=\"volume\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editors\">\n    <names variable=\"editor translator\" delimiter=\", \" suffix=\".\">\n      <name and=\"text\" sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" prefix=\" \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <label variable=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\" \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" cite-group-delimiter=\", \" collapse=\"year\" et-al-min=\"3\" et-al-use-first=\"1\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n    </layout>\n  </citation>\n  <bibliography line-spacing=\"2\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <text macro=\"container\"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"