"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"symbol\" default-locale=\"en-GB\" demote-non-dropping-particle=\"never\">\n  <info>\n    <title>Acta Zoologica Academiae Scientiarum Hungaricae</title>\n    <id>http://www.zotero.org/styles/acta-zoologica-academiae-scientiarum-hungaricae</id>\n    <link href=\"http://www.zotero.org/styles/acta-zoologica-academiae-scientiarum-hungaricae\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/veterinary-medicine-austria\" rel=\"template\"/>\n    <link href=\"http://actazool.nhmus.hu/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>citationstyler@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"zoology\"/>\n    <category field=\"biology\"/>\n    <issn>1217-8837</issn>\n    <eissn>2064-2474</eissn>\n    <updated>2019-07-15T15:12:28+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name delimiter=\", \" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\", \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter-precedes-last=\"never\" initialize-with=\". \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\",\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"chapter\">\n        <group delimiter=\" \">\n          <label text-case=\"capitalize-first\" variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <text variable=\"number-of-pages\"/>\n          <label variable=\"number-of-pages\" form=\"short\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if type=\"book chapter\" match=\"any\">\n        <group delimiter=\" \">\n          <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n          <text variable=\"volume\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <group delimiter=\": \">\n          <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n          <text macro=\"title\"/>\n        </group>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". &amp;#8211; \" suffix=\".\">\n              <group delimiter=\". \" prefix=\" \">\n                <text macro=\"volume\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"publisher\" prefix=\" \"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"chapter\" match=\"any\">\n            <group delimiter=\". \" prefix=\" \">\n              <text macro=\"pages\"/>\n              <group delimiter=\": \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n                  <text macro=\"editor\"/>\n                </group>\n                <text variable=\"container-title\"/>\n              </group>\n              <text macro=\"edition\"/>\n              <text macro=\"volume\"/>\n              <text variable=\"collection-title\"/>\n              <group suffix=\".\" delimiter=\", \">\n                <text macro=\"publisher\" prefix=\" \"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\" match=\"any\">\n            <group delimiter=\" \" prefix=\" \">\n              <text macro=\"editor\"/>\n              <text suffix=\",\" variable=\"container-title\"/>\n              <text macro=\"edition\"/>\n              <text suffix=\".\" variable=\"collection-title\"/>\n              <group delimiter=\", \" suffix=\".\">\n                <text macro=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group prefix=\" \" suffix=\".\" delimiter=\". \">\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". &amp;#8211; \">\n              <group>\n                <text macro=\"editor\" prefix=\" \"/>\n              </group>\n              <group delimiter=\" \" suffix=\".\">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <group delimiter=\": \">\n                  <group>\n                    <text variable=\"volume\" font-weight=\"bold\"/>\n                    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n            <text macro=\"access\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"