"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" default-locale=\"en-US\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Microbial Cell</title>\n    <title-short>MIC</title-short>\n    <id>http://www.zotero.org/styles/microbial-cell</id>\n    <link href=\"http://www.zotero.org/styles/microbial-cell\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/springer-basic-brackets-no-et-al\" rel=\"template\"/>\n    <link href=\"http://microbialcell.com/for-authors-2/manuscript-preparation/#format_guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <eissn>2311-2638</eissn>\n    <updated>2017-10-10T14:49:48+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\", \" suffix=\". \">\n          <text variable=\"title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\" suffix=\". \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-parenth\">\n    <date variable=\"issued\" prefix=\"(\" suffix=\").\">\n      <date-part name=\"year\" font-weight=\"bold\"/>\n    </date>\n  </macro>\n  <macro name=\"doi-pmid\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi: \"/>\n      </if>\n      <else-if match=\"any\" variable=\"PMID\">\n        <text variable=\"PMID\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"30\" et-al-use-first=\"1\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"year-parenth\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \" suffix=\". \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <names variable=\"editor\">\n              <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n              <label strip-periods=\"true\" prefix=\", \"/>\n            </names>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <text macro=\"edition\"/>\n            </group>\n          </group>\n          <group delimiter=\"; \">\n            <group font-weight=\"bold\" delimiter=\", \">\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group font-weight=\"bold\" delimiter=\", \">\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group>\n            <text variable=\"container-title\" form=\"short\" suffix=\". \"/>\n            <group delimiter=\" \">\n              <text term=\"available at\" text-case=\"capitalize-first\"/>\n              <text variable=\"URL\" suffix=\" \"/>\n            </group>\n            <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date form=\"numeric\" variable=\"accessed\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-weight=\"bold\"/>\n            <group delimiter=\": \">\n              <group>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n            <text macro=\"doi-pmid\"/>\n          </group>\n        </else-if>\n        <else>\n          <group>\n            <text variable=\"container-title\" form=\"short\" suffix=\" \"/>\n            <text variable=\"volume\" suffix=\":\"/>\n            <text variable=\"page\" suffix=\".\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"