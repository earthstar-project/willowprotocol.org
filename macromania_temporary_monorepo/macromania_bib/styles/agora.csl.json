"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" page-range-format=\"expanded\" demote-non-dropping-particle=\"sort-only\">\n  <!-- Polyglot; journal publishes in Scandinavian languages -->\n  <info>\n    <title>Agora</title>\n    <id>http://www.zotero.org/styles/agora</id>\n    <link href=\"http://www.zotero.org/styles/agora\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/annales\" rel=\"template\"/>\n    <link href=\"https://www.idunn.no/agora?languageId=2#/authors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"philosophy\"/>\n    <issn>0800-7136</issn>\n    <eissn>1500-1571</eissn>\n    <updated>2020-07-24T09:14:05+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"open-quote\">«</term>\n      <term name=\"close-quote\">»</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name form=\"long\" and=\"text\" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"long\" and=\"text\" initialize-with=\". \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name form=\"long\" and=\"text\" delimiter-precedes-last=\"never\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name form=\"long\" and=\"text\" delimiter-precedes-last=\"never\" sort-separator=\" \">\n        <name-part name=\"family\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"article-journal article-newspaper article-magazine\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\"/>\n          <group delimiter=\" \">\n            <text term=\"in\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"thesis\" match=\"any\">\n        <group>\n          <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\"/>\n          <text variable=\"genre\" suffix=\", \" prefix=\" \"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else-if>\n      <else-if type=\"manuscript\" match=\"any\">\n        <group delimiter=\",\">\n          <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\"/>\n          <text variable=\"genre\" prefix=\" \"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group>\n          <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\" suffix=\",\"/>\n          <text term=\"in\" prefix=\" \" suffix=\" \"/>\n          <text macro=\"editor\" suffix=\", \"/>\n          <text variable=\"container-title\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage\" match=\"any\">\n        <group>\n          <text variable=\"title\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\", \"/>\n          <text variable=\"URL\" text-decoration=\"underline\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"yearpage\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture paper-conference manuscript report song thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <group delimiter=\" \">\n            <text term=\"volume\" form=\"short\"/>\n            <text variable=\"volume\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"chapter entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\", \">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <group>\n            <text term=\"volume\" form=\"short\" suffix=\".\"/>\n            <text variable=\"number-of-volumes\" prefix=\". \" suffix=\"/\"/>\n            <text variable=\"volume\" suffix=\",\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <text variable=\"page\"/>\n      </else-if>\n      <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n        <group delimiter=\" \">\n          <date variable=\"issued\" form=\"numeric\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n            <date-part name=\"year\"/>\n          </date>\n          <group delimiter=\" \">\n            <choose>\n              <if variable=\"locator\" match=\"any\">\n                <group delimiter=\"&#8239;\">\n                  <label variable=\"locator\" form=\"short\"/>\n                  <text variable=\"locator\"/>\n                </group>\n              </if>\n              <else-if variable=\"locator\" match=\"none\">\n                <label variable=\"page\" form=\"short\"/>\n              </else-if>\n            </choose>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <date variable=\"accessed\" form=\"text\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song chapter paper-conference\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" text-case=\"capitalize-first\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\":\">\n          <text variable=\"volume\"/>\n          <text variable=\"issue\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <text variable=\"locator\"/>\n  </macro>\n  <citation>\n    <layout suffix=\".\" delimiter=\" ; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\".\"/>\n            <group delimiter=\"&#8239;\">\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\"/>\n            </group>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <choose>\n              <if type=\"bill book graphic legal_case motion_picture report song thesis\" match=\"any\">\n                <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n              </if>\n              <else>\n                <text variable=\"title\" text-case=\"capitalize-first\" form=\"short\" quotes=\"true\"/>\n              </else>\n            </choose>\n            <choose>\n              <if type=\"article-journal article-magazine article-newspaper chapter paper-conference\" match=\"any\">\n                <text value=\"art. cit\"/>\n              </if>\n              <else>\n                <text value=\"op. cit\" font-style=\"italic\"/>\n              </else>\n            </choose>\n            <group delimiter=\"&#8239;\">\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <group delimiter=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"locators\"/>\n            </group>\n            <text macro=\"translator\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <text macro=\"yearpage\"/>\n            </group>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"