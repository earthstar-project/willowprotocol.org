"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Strategic Design Research Journal</title>\n    <title-short>SDRJ</title-short>\n    <id>http://www.zotero.org/styles/strategic-design-research-journal</id>\n    <link href=\"http://www.zotero.org/styles/strategic-design-research-journal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/water-sa\" rel=\"template\"/>\n    <link href=\"http://revistas.unisinos.br/index.php/sdrj/about/submissions#onlineSubmissions\" rel=\"documentation\"/>\n    <author>\n      <name>Massimo Menichinelli</name>\n      <email>massimo.menichinelli@aalto.fi</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"communications\"/>\n    <issn>1984-2988</issn>\n    <summary>Based on Water SA</summary>\n    <updated>2018-07-10T08:20:58+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"edition\" form=\"short\">\n        <single>edn</single>\n        <multiple>edn</multiple>\n      </term>\n      <term name=\"ordinal\">ᵗʰ</term>\n      <term name=\"ordinal-01\">ˢᵗ</term>\n      <term name=\"ordinal-02\">ⁿᵈ</term>\n      <term name=\"ordinal-03\">ʳᵈ</term>\n      <term name=\"ordinal-11\">ᵗʰ</term>\n      <term name=\"ordinal-12\">ᵗʰ</term>\n      <term name=\"ordinal-13\">ᵗʰ</term>\n      <term name=\"and others\">and co-authors</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <names variable=\"author\" vertical-align=\"baseline\">\n          <name delimiter=\"; \" suffix=\".\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n          </name>\n          <et-al term=\"and others\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text term=\"anonymous\" text-case=\"uppercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"translator2\">\n    <text term=\"translator\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n    <names variable=\"translator\">\n      <name and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"illustrator\">\n    <text term=\"translator\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n    <names variable=\"illustrator\">\n      <name and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"responsability\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <text macro=\"author\"/>\n      </if>\n      <else>\n        <choose>\n          <if type=\"webpage post-weblog\" match=\"any\">\n            <text variable=\"container-title\" text-case=\"uppercase\"/>\n          </if>\n          <else>\n            <text term=\"anonymous\" text-case=\"uppercase\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"secondary-responsability\">\n    <choose>\n      <if variable=\"author\">\n        <choose>\n          <if variable=\"translator\">\n            <text macro=\"translator2\"/>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"illustrator\">\n            <text macro=\"illustrator\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author-citation\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <names variable=\"author\">\n          <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <choose>\n          <if type=\"webpage post-weblog\" match=\"any\">\n            <text variable=\"container-title\"/>\n          </if>\n          <else>\n            <text term=\"anonymous\" form=\"short\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name and=\"text\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-responsability\">\n    <choose>\n      <if variable=\"container-author editor\" match=\"any\">\n        <text macro=\"container-author\"/>\n      </if>\n      <else>\n        <text term=\"anonymous\" text-case=\"uppercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if is-uncertain-date=\"issued\">\n            <text term=\"circa\" form=\"short\" suffix=\"&#160;\"/>\n          </if>\n        </choose>\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <choose>\n          <if type=\"article-journal article-magazine book chapter\" match=\"any\">\n            <text value=\"in press\"/>\n          </if>\n          <else-if type=\"paper-conference\" match=\"none\">\n            <text term=\"no date\"/>\n          </else-if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book map motion_picture song manuscript broadcast\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <choose>\n            <if is-numeric=\"volume\">\n              <group delimiter=\"&#160;\">\n                <label variable=\"volume\" form=\"long\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n                <number variable=\"volume\" font-style=\"italic\"/>\n              </group>\n            </if>\n            <else>\n              <text variable=\"volume\" form=\"long\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n            </else>\n          </choose>\n        </group>\n      </if>\n      <else-if type=\"paper-conference speech chapter post\" match=\"any\">\n        <text variable=\"title\" suffix=\". \"/>\n        <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\":&#160;\"/>\n        <choose>\n          <if variable=\"container-author editor translator\" match=\"any\">\n            <text macro=\"container-responsability\"/>\n            <choose>\n              <if variable=\"container-title event\" match=\"any\">\n                <text value=\" \"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"container-title  event\" match=\"all\">\n            <text variable=\"container-title\" text-case=\"title\" font-style=\"normal\"/>\n          </if>\n          <else-if variable=\"container-title\" match=\"none\">\n            <text variable=\"event\" font-style=\"italic\"/>\n          </else-if>\n          <else>\n            <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\"/>\n          <text variable=\"container-title\" font-style=\"italic\" suffix=\",\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\"/>\n            </if>\n            <else>\n              <text variable=\"title\"/>\n            </else>\n          </choose>\n        </group>\n      </else-if>\n      <else-if type=\"patent interview thesis report\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <group>\n          <text variable=\"title\" suffix=\". \"/>\n          <text variable=\"publisher-place\" suffix=\", \"/>\n          <text variable=\"container-title\" suffix=\". \"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <text variable=\"number\"/>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\"/>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"map\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n          <else>\n            <text value=\"map\" prefix=\"[\" suffix=\"]\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"genre\" text-case=\"capitalize-first\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event-date\">\n    <choose>\n      <if variable=\"event-date\">\n        <date variable=\"event-date\">\n          <date-part name=\"day\" suffix=\"&#160;\"/>\n          <date-part name=\"month\" suffix=\"&#160;\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"date-full\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\"&#160;\"/>\n          <date-part name=\"month\" suffix=\"&#160;\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"scale\">\n    <group delimiter=\"&#160;\">\n      <text term=\"scale\" text-case=\"capitalize-first\"/>\n      <text variable=\"scale\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <number text-case=\"capitalize-first\" variable=\"edition\" form=\"long-ordinal\"/>\n        <label prefix=\"&#160;\" variable=\"edition\"/>\n      </if>\n      <else>\n        <text variable=\"edition\" form=\"long\" prefix=\"(\" suffix=\")\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if type=\"patent manuscript article-newspaper broadcast motion_picture song\" match=\"any\">\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\"/>\n          </if>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\"/>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue\">\n    <group>\n      <text variable=\"volume\" strip-periods=\"false\" font-weight=\"bold\"/>\n      <text variable=\"issue\" prefix=\"(\" suffix=\"):\"/>\n      <text macro=\"collection\" prefix=\"(\" suffix=\")\"/>\n      <text variable=\"page\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"broadcast motion_picture song report\" match=\"any\">\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\"/>\n          </if>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\"/>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-info\">\n    <choose>\n      <if variable=\"publisher\">\n        <group delimiter=\", \">\n          <text macro=\"publisher-place\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </if>\n      <else>\n        <text macro=\"publisher-place\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL DOI\" match=\"any\">\n        <group>\n          <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" on: \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"day\" suffix=\", \"/>\n            <date-part name=\"year\" prefix=\" \"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <choose>\n      <if type=\"report\">\n        <text variable=\"collection-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"collection-title\"/>\n          <text variable=\"collection-number\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"book thesis manuscript\" match=\"any\">\n        <group delimiter=\"&#160;\">\n          <text variable=\"number-of-pages\"/>\n          <label plural=\"never\" variable=\"number-of-pages\" form=\"short\"/>\n        </group>\n      </if>\n      <else-if type=\"chapter paper-conference article-newspaper\" match=\"any\">\n        <text variable=\"page\"/>\n      </else-if>\n      <else-if type=\"report patent\" match=\"any\">\n        <text variable=\"page\" suffix=\" \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else-if variable=\"URL\">\n        <text variable=\"URL\" prefix=\"Available at: \" suffix=\". \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year-suffix\" givenname-disambiguation-rule=\"primary-name\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-citation\"/>\n        <text macro=\"year-date\"/>\n        <group delimiter=\"&#160;\">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\" delimiter-precedes-last=\"never\" entry-spacing=\"0\" line-spacing=\"2\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"responsability\"/>\n      <key macro=\"year-date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout text-decoration=\"none\">\n      <group font-variant=\"normal\" delimiter=\" \" suffix=\" \">\n        <text macro=\"responsability\"/>\n        <text macro=\"year-date\" suffix=\".\"/>\n      </group>\n      <choose>\n        <if type=\"book\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"genre\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"secondary-responsability\"/>\n              <text macro=\"publisher-info\"/>\n              <text macro=\"page\"/>\n            </group>\n          </group>\n          <text macro=\"url\"/>\n        </if>\n        <else-if type=\"map\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"scale\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"secondary-responsability\"/>\n            <text macro=\"publisher-info\"/>\n            <group delimiter=\" \">\n              <text macro=\"collection\"/>\n              <text macro=\"accessed\"/>\n            </group>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"post post-weblog\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"article-journal article-magazine\" match=\"any\">\n          <group delimiter=\" \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\" \">\n              <group delimiter=\", \">\n                <text macro=\"publisher-info\"/>\n                <text macro=\"issue\"/>\n              </group>\n            </group>\n          </group>\n          <text macro=\"url\" suffix=\".\"/>\n        </else-if>\n        <else-if type=\"chapter entry entry-dictionary entry-encyclopedia\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher-info\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"speech\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"publisher-place\"/>\n            <group delimiter=\" \">\n              <text macro=\"date-full\"/>\n              <text macro=\"accessed\"/>\n            </group>\n            <text macro=\"page\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <group delimiter=\". \" suffix=\". \">\n            <group delimiter=\" \">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"event-date\"/>\n                <text variable=\"event-place\"/>\n              </group>\n            </group>\n            <text macro=\"collection\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\", \">\n              <text macro=\"genre\"/>\n              <text macro=\"publisher-info\"/>\n            </group>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text macro=\"url\"/>\n              <text macro=\"accessed\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"motion_picture\" match=\"any\">\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"medium\" prefix=\"[\" suffix=\"]. \"/>\n          <text macro=\"year-date\" suffix=\". \"/>\n          <text macro=\"responsability\" suffix=\". \"/>\n          <text macro=\"publisher-info\"/>\n        </else-if>\n        <else-if type=\"song\" match=\"any\">\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"medium\" suffix=\". \"/>\n          <text macro=\"publisher-info\"/>\n          <text macro=\"date\" suffix=\". \"/>\n          <text macro=\"accessed\" suffix=\". \"/>\n          <text macro=\"collection\" suffix=\". \"/>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"broadcast\" match=\"any\">\n          <text variable=\"container-title\" suffix=\". \"/>\n          <text macro=\"number\" suffix=\". \"/>\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"publisher\" suffix=\". \"/>\n          <text macro=\"accessed\" suffix=\". \"/>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\". \" suffix=\". \">\n            <group delimiter=\": \">\n              <text macro=\"collection\"/>\n              <text macro=\"title\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <group delimiter=\" \">\n              <text macro=\"genre\"/>\n              <text variable=\"number\"/>\n            </group>\n            <group delimiter=\" \">\n              <text macro=\"publisher-info\"/>\n            </group>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"manuscript\">\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"genre\" suffix=\". \"/>\n          <text macro=\"edition\" suffix=\". \"/>\n          <text macro=\"publisher-place\" suffix=\", \"/>\n          <text macro=\"accessed\" suffix=\". \"/>\n          <text macro=\"collection\" suffix=\". \"/>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"patent\">\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"publisher-place\" suffix=\", \"/>\n          <text macro=\"number\" suffix=\". \"/>\n          <text macro=\"accessed\" suffix=\". \"/>\n          <text macro=\"collection\" suffix=\". \"/>\n          <text macro=\"date\" suffix=\". \"/>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"personal_communication\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"date\"/>\n            <text macro=\"url\"/>\n            <text value=\"Osobní komunikace.\"/>\n          </group>\n        </else-if>\n        <else-if type=\"interview\">\n          <group delimiter=\", \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"date-full\"/>\n          </group>\n          <text variable=\"event-place\" suffix=\".\"/>\n        </else-if>\n        <else-if type=\"article-newspaper\" match=\"any\">\n          <group>\n            <text macro=\"title\"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"url\"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"medium\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"date\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher-info\"/>\n            <text macro=\"url\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"page\"/>\n            <text variable=\"ISBN\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"