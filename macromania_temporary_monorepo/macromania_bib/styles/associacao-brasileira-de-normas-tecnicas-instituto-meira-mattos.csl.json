"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"never\" default-locale=\"pt-BR\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Escola de Comando e Estado-Maior do Exército - Instituto Meira Mattos - ABNT (Português - Brasil)</title>\n    <title-short>ABNT-ECEME/IMM</title-short>\n    <id>http://www.zotero.org/styles/associacao-brasileira-de-normas-tecnicas-instituto-meira-mattos</id>\n    <link href=\"http://www.zotero.org/styles/associacao-brasileira-de-normas-tecnicas-instituto-meira-mattos\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/associacao-brasileira-de-normas-tecnicas-eceme\" rel=\"template\"/>\n    <link href=\"http://www.eceme.eb.mil.br/instituto-meira-mattos-imm\" rel=\"documentation\"/>\n    <author>\n      <name>Marcelo A. O. Malagutti</name>\n      <email>marcelomalagutti@yahoo.com.br</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Brazilian Standard Style - ECEME/IMM</summary>\n    <updated>2018-05-22T20:19:56+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"pt-BR\">\n    <terms>\n      <term name=\"month-01\" form=\"short\">Jan</term>\n      <term name=\"month-02\" form=\"short\">Fev</term>\n      <term name=\"month-03\" form=\"short\">Mar</term>\n      <term name=\"month-04\" form=\"short\">Abr</term>\n      <term name=\"month-05\" form=\"short\">Mai</term>\n      <term name=\"month-06\" form=\"short\">Jun</term>\n      <term name=\"month-07\" form=\"short\">Jul</term>\n      <term name=\"month-08\" form=\"short\">Ago</term>\n      <term name=\"month-09\" form=\"short\">Set</term>\n      <term name=\"month-10\" form=\"short\">Out</term>\n      <term name=\"month-11\" form=\"short\">Nov</term>\n      <term name=\"month-12\" form=\"short\">Dez</term>\n      <term name=\"et-al\"> e colab.</term>\n      <term name=\"editor\" form=\"short\">ed</term>\n      <term name=\"editor\" form=\"short\">org</term>\n      <term name=\"collection-editor\" form=\"short\">org</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter\">\n        <names variable=\"container-author\" delimiter=\", \" prefix=\"In: \">\n          <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" delimiter=\"; \" delimiter-precedes-last=\"always\">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n            <name-part name=\"given\" text-case=\"uppercase\"/>\n          </name>\n          <label form=\"short\" text-case=\"capitalize-first\" prefix=\" (\" suffix=\".)\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"collection-editor\"/>\n          </substitute>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter\" match=\"none\">\n        <names variable=\"editor\" delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <name and=\"symbol\" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\" suffix=\".\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"translator\">\n    <text value=\"Tradução \"/>\n    <names variable=\"translator\" delimiter=\", \">\n      <name delimiter=\"; \" sort-separator=\" \" delimiter-precedes-last=\"always\">\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n      </name>\n      <et-al/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <names variable=\"author\">\n          <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\" e \" delimiter-precedes-last=\"always\">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n            <name-part name=\"given\" text-case=\"capitalize-first\"/>\n          </name>\n          <et-al/>\n          <label form=\"short\" prefix=\" (\" suffix=\".)\" text-case=\"capitalize-first\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"title\"/>\n          </substitute>\n        </names>\n      </if>\n      <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <names variable=\"author\">\n          <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\" e \" delimiter-precedes-last=\"always\" prefix=\" In: \">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n            <name-part name=\"given\" text-case=\"capitalize-first\"/>\n          </name>\n          <et-al/>\n          <label form=\"short\" prefix=\" (\" suffix=\".)\" text-case=\"capitalize-first\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"title\"/>\n          </substitute>\n        </names>\n      </else-if>\n      <else>\n        <names variable=\"author\">\n          <name delimiter=\"; \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n            <name-part name=\"given\" text-case=\"capitalize-first\"/>\n          </name>\n          <et-al/>\n          <label form=\"short\" prefix=\" (\" suffix=\".)\" text-case=\"capitalize-first\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"title\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\"; \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\" text-case=\"capitalize-all\"/>\n      </name>\n      <et-al/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"book\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-magazine article-journal\" match=\"any\">\n        <text variable=\"URL\" prefix=\". Disponível em: &lt;\" suffix=\"&gt;\"/>\n        <date variable=\"accessed\" prefix=\". Acesso em: \">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" form=\"short\" suffix=\" \" text-case=\"lowercase\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text variable=\"URL\" prefix=\"Disponível em: &lt;\" suffix=\"&gt;\"/>\n        <date variable=\"accessed\" prefix=\". Acesso em: \">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" form=\"short\" suffix=\" \" text-case=\"lowercase\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"chapter bill paper-conference\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else-if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" font-weight=\"normal\"/>\n      </else-if>\n      <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <text variable=\"title\" text-case=\"uppercase\" font-weight=\"bold\"/>\n      </else-if>\n      <else-if type=\"article-newspaper article-magazine article-journal\" match=\"any\">\n        <text variable=\"title\" font-style=\"normal\" font-variant=\"normal\" font-weight=\"normal\" text-decoration=\"none\" display=\"right-inline\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\" font-weight=\"normal\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <text variable=\"event-place\" suffix=\". \"/>\n        <text value=\"Anais\"/>\n        <text value=\"... \"/>\n      </if>\n      <else-if type=\"chapter\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if match=\"any\" variable=\"publisher-place publisher\">\n        <group delimiter=\": \">\n          <choose>\n            <if variable=\"publisher-place\">\n              <text variable=\"publisher-place\"/>\n            </if>\n            <else>\n              <text value=\"[S.l.]\"/>\n            </else>\n          </choose>\n          <choose>\n            <if variable=\"publisher\">\n              <text variable=\"publisher\"/>\n            </if>\n            <else>\n              <text value=\"[s.n.]\"/>\n            </else>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text value=\"[S.l: s.n.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <text variable=\"event\" text-case=\"uppercase\" prefix=\"In: \"/>\n      </if>\n      <else>\n        <text variable=\"event\"/>\n        <text variable=\"event-place\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\" match=\"any\">\n        <group>\n          <choose>\n            <if type=\"book chapter\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part name=\"day\" suffix=\" \"/>\n                <date-part name=\"month\" form=\"short\" suffix=\" \"/>\n              </date>\n            </if>\n          </choose>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <text value=\"[S.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text value=\"[S.d.]\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"book chapter entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"numeric\" suffix=\".\"/>\n              <text term=\"edition\" form=\"short\" suffix=\".\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\" ed.\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"bill\">\n        <group prefix=\", \" delimiter=\", \">\n          <date variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part prefix=\" \" name=\"month\" form=\"short\"/>\n            <date-part prefix=\" \" name=\"year\"/>\n          </date>\n          <text variable=\"section\" prefix=\"Sec. \"/>\n          <text variable=\"page\" prefix=\"p. \" suffix=\".\"/>\n        </group>\n      </if>\n      <else-if match=\"any\" type=\"article-journal article-magazine article-newspaper\">\n        <group delimiter=\", \">\n          <group delimiter=\", \">\n            <text variable=\"volume\" prefix=\"v. \"/>\n            <text variable=\"issue\" prefix=\"n. \"/>\n          </group>\n          <text variable=\"page\" prefix=\"p. \"/>\n        </group>\n      </else-if>\n      <else-if match=\"any\" type=\"book chapter\">\n        <group delimiter=\", \">\n          <group>\n            <text variable=\"volume\" prefix=\"v. \" suffix=\". \"/>\n            <text variable=\"page\" prefix=\"p. \"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"genre\">\n    <text variable=\"genre\"/>\n  </macro>\n  <macro name=\"abstract\">\n    <text variable=\"abstract\"/>\n  </macro>\n  <macro name=\"place\">\n    <choose>\n      <if match=\"any\" variable=\"publisher-place\">\n        <text variable=\"publisher-place\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group>\n      <text variable=\"archive\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"number\">\n    <group>\n      <text variable=\"number\" suffix=\". \"/>\n    </group>\n  </macro>\n  <macro name=\"sort-key1\">\n    <choose>\n      <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text macro=\"author\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"sort-key2\">\n    <choose>\n      <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <text macro=\"author\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year-suffix\" givenname-disambiguation-rule=\"primary-name\" year-suffix-delimiter=\", \">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-year\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group>\n        <text suffix=\", \" macro=\"author-short\"/>\n        <text macro=\"issued-year\"/>\n        <text prefix=\", \" macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\" et-al-min=\"4\" et-al-use-first=\"1\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"sort-key1\"/>\n      <key macro=\"sort-key2\"/>\n      <key macro=\"issued\"/>\n    </sort>\n    <layout>\n      <choose>\n        <if type=\"bill\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"number\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \" font-weight=\"bold\"/>\n            <text variable=\"references\" suffix=\", \" font-weight=\"bold\"/>\n            <text variable=\"note\"/>\n            <text macro=\"locators\" suffix=\". \"/>\n            <text macro=\"access\" suffix=\". \"/>\n          </group>\n        </if>\n        <else-if type=\"map\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\", \"/>\n            <text macro=\"issued\" suffix=\". \"/>\n            <text variable=\"note\" suffix=\". \"/>\n            <text macro=\"access\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"book\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"translator\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher\" suffix=\", \"/>\n            <text macro=\"issued-year\" suffix=\". \"/>\n            <text macro=\"locators\"/>\n            <text macro=\"access\" suffix=\". \"/>\n            <group prefix=\"(\" suffix=\").\">\n              <text variable=\"collection-title\"/>\n              <text variable=\"collection-number\" prefix=\", \"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"container-contributors\" suffix=\". \"/>\n            <text macro=\"secondary-contributors\" suffix=\". \"/>\n            <text macro=\"container-title\" suffix=\". \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text macro=\"translator\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <group suffix=\". \">\n              <text macro=\"publisher\" suffix=\", \"/>\n              <text macro=\"issued\" suffix=\". \"/>\n              <text macro=\"locators\"/>\n            </group>\n            <text macro=\"access\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\" match=\"any\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"translator\" suffix=\". \"/>\n            <text macro=\"container-title\" suffix=\", \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text variable=\"note\" suffix=\", \"/>\n            <text macro=\"place\" suffix=\", \"/>\n            <text macro=\"edition\" suffix=\", \"/>\n            <text macro=\"issued\" suffix=\". \"/>\n            <text variable=\"section\"/>\n            <group suffix=\".\">\n              <text macro=\"locators\" prefix=\", \"/>\n              <text macro=\"access\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-magazine article-journal\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"container-title\" suffix=\", \"/>\n            <text variable=\"collection-title\" suffix=\". \"/>\n            <text variable=\"note\" suffix=\", \"/>\n            <text macro=\"edition\" suffix=\", \"/>\n            <text macro=\"locators\" suffix=\", \"/>\n            <text macro=\"issued\"/>\n            <text macro=\"translator\" suffix=\". \"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"issued-year\" suffix=\". \"/>\n            <text variable=\"number-of-pages\" suffix=\" f. \"/>\n            <text variable=\"genre\" suffix=\" &#8211; \"/>\n            <text variable=\"publisher\" suffix=\", \"/>\n            <text variable=\"publisher-place\" suffix=\", \"/>\n            <text macro=\"issued-year\" suffix=\". \"/>\n            <text macro=\"access\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"manuscript\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"abstract\" suffix=\". \"/>\n            <text macro=\"place\" suffix=\", \"/>\n            <text macro=\"issued\" suffix=\". \"/>\n            <text macro=\"access\" suffix=\". \"/>\n            <text macro=\"archive\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group>\n            <text macro=\"author\" suffix=\". \"/>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"access\" suffix=\". \"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <text macro=\"author\" suffix=\". \"/>\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"event\" suffix=\", \"/>\n          <text variable=\"collection-title\" suffix=\", \"/>\n          <text macro=\"issued\" suffix=\", \"/>\n          <text macro=\"locators\"/>\n          <text macro=\"container-title\"/>\n          <group delimiter=\". \" suffix=\". \">\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <text variable=\"page\" prefix=\"p. \"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <text macro=\"author\" suffix=\". \"/>\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"container-contributors\"/>\n          <text macro=\"secondary-contributors\"/>\n          <text macro=\"container-title\"/>\n          <text variable=\"collection-title\" prefix=\", \" suffix=\".\"/>\n          <text variable=\"genre\"/>\n          <text variable=\"number\" prefix=\", nº \" suffix=\".\"/>\n          <text macro=\"locators\"/>\n          <group delimiter=\". \" prefix=\". \">\n            <text macro=\"publisher\"/>\n          </group>\n          <text macro=\"issued\" prefix=\", \" suffix=\".\"/>\n          <text macro=\"access\" prefix=\" \" suffix=\".\"/>\n        </else-if>\n        <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"author\" suffix=\". \"/>\n          <text macro=\"container-contributors\"/>\n          <text macro=\"secondary-contributors\"/>\n          <text macro=\"container-title\" suffix=\". \"/>\n          <text macro=\"edition\" suffix=\" \"/>\n          <text variable=\"collection-title\" prefix=\", \" suffix=\".\"/>\n          <text macro=\"locators\"/>\n          <group delimiter=\". \">\n            <text macro=\"publisher\"/>\n          </group>\n          <group delimiter=\". \" prefix=\", \" suffix=\".\">\n            <text macro=\"issued\"/>\n            <text variable=\"volume\" prefix=\"v. \"/>\n            <text variable=\"page\" prefix=\"p. \"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"author\" suffix=\". \"/>\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"container-contributors\"/>\n          <text macro=\"secondary-contributors\"/>\n          <text macro=\"container-title\"/>\n          <text variable=\"collection-title\" prefix=\", \" suffix=\".\"/>\n          <text macro=\"locators\"/>\n          <group delimiter=\". \" prefix=\". \" suffix=\". \">\n            <text macro=\"publisher\"/>\n            <text macro=\"access\"/>\n          </group>\n          <text macro=\"issued\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"