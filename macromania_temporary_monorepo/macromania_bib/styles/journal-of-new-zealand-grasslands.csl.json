"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Journal of New Zealand Grasslands</title>\n    <title-short>JNZG</title-short>\n    <id>http://www.zotero.org/styles/journal-of-new-zealand-grasslands</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-new-zealand-grasslands\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/new-zealand-plant-protection\" rel=\"template\"/>\n    <link href=\"https://www.nzgajournal.org.nz/index.php/JoNZG/about/submissions\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>2463-2872</issn>\n    <eissn>2463-2880</eissn>\n    <updated>2020-03-23T13:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"translator\" form=\"verb-short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\"\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <choose>\n          <if type=\"report\" match=\"any\">\n            <text variable=\"number\" prefix=\" [Report \" suffix=\"]\"/>\n          </if>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"number-of-pages\" suffix=\" p.\"/>\n      </if>\n      <else-if type=\"chapter\">\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\": \">\n      <group delimiter=\" \">\n        <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n        <date form=\"text\" variable=\"accessed\"/>\n        <text term=\"from\"/>\n      </group>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <citation collapse=\"year-suffix\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor-translator\"/>\n              </group>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </group>\n          </if>\n          <else-if type=\"paper-conference chapter\" match=\"any\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </else-if>\n          <else-if type=\"article-journal article\" match=\"any\">\n            <group delimiter=\": \">\n              <group delimiter=\" \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text variable=\"volume\" font-style=\"italic\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n        </choose>\n        <group delimiter=\", \">\n          <text macro=\"publisher\"/>\n          <text macro=\"page\"/>\n        </group>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"