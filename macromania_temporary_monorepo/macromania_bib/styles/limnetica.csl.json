"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <!-- Polyglot; journal publishes in English and Spanish -->\n  <info>\n    <title>Limnetica</title>\n    <id>http://www.zotero.org/styles/limnetica</id>\n    <link href=\"http://www.zotero.org/styles/limnetica\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/arachnology\" rel=\"template\"/>\n    <link href=\"http://www.limnetica.com/\" rel=\"documentation\"/>\n    <link href=\"http://www.limnetica.com/pdf/autores.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0213-8409</issn>\n    <eissn>1989-1806</eissn>\n    <summary>Citation Style for the journal Limnetica to publish in English or Spanish.</summary>\n    <updated>2017-02-24T10:15:04+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" font-variant=\"normal\" vertical-align=\"baseline\">\n      <name font-variant=\"normal\" and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"first\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\" match=\"any\">\n        <choose>\n          <if variable=\"URL\">\n            <group delimiter=\" \">\n              <text variable=\"URL\" prefix=\"Available from: \"/>\n              <group prefix=\"(\" suffix=\")\">\n                <date variable=\"accessed\" form=\"text\"/>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"false\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"legal_case\">\n    <group prefix=\" \" delimiter=\" \">\n      <text variable=\"volume\"/>\n      <text variable=\"container-title\"/>\n    </group>\n    <text variable=\"authority\" prefix=\" (\" suffix=\")\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\" match=\"none\">\n        <group delimiter=\". \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n        <text variable=\"genre\" prefix=\". \"/>\n      </if>\n      <else>\n        <group delimiter=\". \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <group>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\" suffix=\")\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"issue\" suffix=\":\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if locator=\"page\">\n        <text variable=\"locator\" prefix=\": \"/>\n      </if>\n      <else>\n        <group delimiter=\" \" suffix=\")\">\n          <label suffix=\")\" variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" suffix=\")\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation and=\"text\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" givenname-disambiguation-rule=\"primary-name\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group>\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text macro=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"99\" et-al-use-first=\"98\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\" \">\n      <text macro=\"author\" text-case=\"uppercase\" strip-periods=\"false\" font-variant=\"normal\" suffix=\" \"/>\n      <date variable=\"issued\" suffix=\".\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"book\" match=\"any\">\n          <text macro=\"legal_case\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" suffix=\".\"/>\n          </group>\n          <group prefix=\" \" suffix=\".\" delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text variable=\"page\" prefix=\" \" suffix=\" pp\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group delimiter=\" \" prefix=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text macro=\"editor\" suffix=\":\"/>\n            <group delimiter=\" \" suffix=\".\">\n              <text variable=\"page\" suffix=\".\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"bill graphic legal_case legislation manuscript motion_picture report song thesis\" match=\"any\">\n          <text macro=\"legal_case\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" suffix=\".\"/>\n          </group>\n          <group prefix=\" \" delimiter=\", \">\n            <text macro=\"publisher\"/>\n            <text variable=\"page\" prefix=\" \" suffix=\"pp.\"/>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\" \" delimiter=\" \" suffix=\".\">\n            <text macro=\"title\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group delimiter=\" \" prefix=\" \">\n              <text variable=\"volume\" font-weight=\"normal\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\" prefix=\": \" suffix=\".\"/>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"