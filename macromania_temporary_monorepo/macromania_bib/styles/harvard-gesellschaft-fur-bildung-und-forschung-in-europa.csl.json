"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"de-DE\">\n  <info>\n    <title>Gesellschaft für Bildung und Forschung in Europa - Harvard (Deutsch)</title>\n    <title-short>GBFE</title-short>\n    <id>http://www.zotero.org/styles/harvard-gesellschaft-fur-bildung-und-forschung-in-europa</id>\n    <link href=\"http://www.zotero.org/styles/harvard-gesellschaft-fur-bildung-und-forschung-in-europa\" rel=\"self\"/>\n    <link href=\"http://gbfe.org/\" rel=\"documentation\"/>\n    <link href=\"http://www.acf.de/uploads/media/GBFE_Studienbrief_5_Form_bewahren_01.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Cornelius Lipponer</name>\n      <email>clipponer at gmail dot com</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"theology\"/>\n    <summary>The Harvard author-date style with German Unisa / GBFE changes</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <!-- SPRACHINFOS -->\n  <locale xmlns=\"http://purl.org/net/xbiblio/csl\" xml:lang=\"de-DE\">\n    <terms>\n      <term name=\"page\">\n        <!--  SEITE -->\n        <single>Seite</single>\n        <multiple>Seiten</multiple>\n      </term>\n      <term name=\"page\" form=\"short\">S.</term>\n      <term name=\"editor\">Herausgeber</term>\n      <term name=\"editor\" form=\"short\">Hg.</term>\n      <term name=\"edition\">\n        <!--  AUFLAGE -->\n        <single>Auflage</single>\n        <multiple>Auflagen</multiple>\n      </term>\n      <term name=\"edition\" form=\"short\">\n        <!--  AUFLAGE KURZ -->\n        <single>Auflage</single>\n        <multiple>Auflagen</multiple>\n      </term>\n      <term name=\"ordinal-01\">.</term>\n      <!--  1. 2. 3. -->\n      <term name=\"ordinal-02\">.</term>\n      <term name=\"ordinal-03\">.</term>\n      <term name=\"ordinal-04\">.</term>\n      <term name=\"accessed\">Stand</term>\n      <!--  ZUGRIFFSDATUM -->\n    </terms>\n    <date form=\"text\">\n      <date-part name=\"month\" suffix=\" \"/>\n      <date-part name=\"day\" suffix=\", \"/>\n      <date-part name=\"year\"/>\n    </date>\n    <date form=\"numeric\">\n      <date-part name=\"year\"/>\n      <date-part name=\"month\" form=\"numeric\" prefix=\"-\" range-delimiter=\"/\"/>\n      <date-part name=\"day\" prefix=\"-\" range-delimiter=\"/\"/>\n    </date>\n    <style-options punctuation-in-quote=\"true\"/>\n  </locale>\n  <!-- MACROS -->\n  <macro name=\"author\">\n    <!--  AUTOR -->\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <!--  HERAUSGEBER -->\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"lowercase\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <!--  ANONYM -->\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author-short\">\n    <!--  KURZAUTOR -->\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <!--  Internetquelle -->\n    <choose>\n      <if variable=\"URL\">\n        <!-- <text value=\"Online im Internet: URL:\" suffix=\" \"/> -->\n        <text variable=\"URL\"/>\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"year\" suffix=\"-\"/>\n            <date-part name=\"month\" suffix=\"-\" form=\"numeric-leading-zeros\"/>\n            <date-part name=\"day\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <!--  TITEL -->\n  <macro name=\"title\">\n    <choose>\n      <!--  BUCH O.Ä. -->\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis webpage\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\".\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <!--  VERLAG -->\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <!--  ERSCHEINUNGSJAHR -->\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <!--  AUFLAGE -->\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <!-- IN TEXT ZITAT -->\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group>\n          <text macro=\"author-short\" suffix=\" \"/>\n          <text macro=\"year-date\"/>\n          <text variable=\"locator\" prefix=\":\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <!-- BIBLIOGRAPHIE -->\n  <bibliography hanging-indent=\"true\" et-al-min=\"4\" et-al-use-first=\"1\">\n    <!-- SORTIERUNG -->\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <!-- LAYOUT DER BIBLIOGRAPHIE -->\n    <layout>\n      <text macro=\"author\"/>\n      <!--  AUTOR -->\n      <date variable=\"issued\" prefix=\" \" suffix=\".\">\n        <!--  ERSCHEINUNG -->\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <!--  BUCH O.Ä. -->\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"title\"/>\n            <text variable=\"volume\" prefix=\"Bd. \" suffix=\", \"/>\n            <text variable=\"issue\" suffix=\", \"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n        </if>\n        <!-- Kapitel, Konferenz -->\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text variable=\"event\" suffix=\".\"/>\n            <group suffix=\".\" delimiter=\", \">\n              <text macro=\"publisher\" prefix=\" \"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <!--  THESE -->\n        <else-if type=\"thesis\">\n          <group prefix=\" \" suffix=\".\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text variable=\"genre\" suffix=\". \"/>\n            <text variable=\"publisher\" suffix=\", \"/>\n            <text variable=\"publisher-place\"/>\n          </group>\n        </else-if>\n        <!--  Webseite -->\n        <else-if type=\"webpage post-weblog post\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n          </group>\n        </else-if>\n        <else-if type=\"entry-encyclopedia entry-dictionary\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <!--  ERSCHEINUNGSORT -->\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"edition\" vertical-align=\"sup\"/>\n            <group prefix=\" \">\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group prefix=\",\">\n              <!--  SEITEN IN BUCH -->\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-magazine article-newspaper article-journal\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <!--  ERSCHEINUNGSORT -->\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\" \"/>\n            <text variable=\"volume\" suffix=\", \"/>\n            <text variable=\"issue\" suffix=\", \"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else>\n          <!--  Sonstiges -->\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <!--  ERSCHEINUNGSORT -->\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group prefix=\", \">\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group prefix=\", \">\n              <!--  SEITEN IN BUCH -->\n              <!-- <label variable=\"page\" form=\"short\"/> -->\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <!--  Zugriff am -->\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"