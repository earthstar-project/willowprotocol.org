"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"chicago\" default-locale=\"en-US\">\n  <info>\n    <title>Society of Biblical Literature 2nd edition (full note)</title>\n    <title-short>SBL (2nd edition)</title-short>\n    <id>http://www.zotero.org/styles/society-of-biblical-literature-fullnote-bibliography</id>\n    <link href=\"http://www.zotero.org/styles/society-of-biblical-literature-fullnote-bibliography\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/chicago-fullnote-bibliography\" rel=\"template\"/>\n    <link href=\"http://www.sbl-site.org/publications/SBLHandbookofStyle.aspx\" rel=\"documentation\"/>\n    <author>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Simon Kornblith</name>\n      <email>simon@simonster.com</email>\n    </contributor>\n    <contributor>\n      <name>Elena Razlogova</name>\n      <email>elena.razlogova@gmail.com</email>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <contributor>\n      <name>Tyler Mykkanen</name>\n    </contributor>\n    <contributor>\n      <name>J. David Stark</name>\n      <email>david@jdavidstark.com</email>\n      <uri>https://www.jdavidstark.com/</uri>\n    </contributor>\n    <category citation-format=\"note\"/>\n    <category field=\"theology\"/>\n    <summary>Society of Biblical Literature format with full notes and bibliography</summary>\n    <updated>2022-07-02T02:53:27+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-US\">\n    <terms>\n      <term name=\"article-journal\" form=\"short\">art.</term>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"page\" form=\"short\">\n        <single>p. </single>\n        <multiple>pp. </multiple>\n      </term>\n      <term name=\"paragraph\" form=\"short\">\n        <single>¶</single>\n        <multiple>¶¶</multiple>\n      </term>\n      <term name=\"section\" form=\"short\">\n        <single>§</single>\n        <multiple>§§</multiple>\n      </term>\n      <term name=\"translator\" form=\"verb-short\">trans.</term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"editortranslator\" form=\"verb-short\">ed. and trans.</term>\n      <term name=\"editortranslator\" form=\"verb\">edited and translated by</term>\n      <term name=\"collection-editor\" form=\"verb\">edited by</term>\n      <term name=\"collection-editor\" form=\"verb-short\">ed.</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator-verb-short-comma\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <label form=\"verb-short\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"editor-translator-verb-period\">\n    <names variable=\"editor translator\" delimiter=\". \">\n      <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"editor-translator-verb-comma\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"container-author-reviewed-author-and-editor-translator\">\n    <group delimiter=\", \">\n      <choose>\n        <if variable=\"container-author reviewed-author\" match=\"any\">\n          <names variable=\"container-author reviewed-author\" delimiter=\", \">\n            <label form=\"verb-short\" suffix=\" \"/>\n            <name and=\"text\" delimiter=\", \"/>\n          </names>\n        </if>\n      </choose>\n      <text macro=\"editor-translator-verb-short-comma\"/>\n    </group>\n  </macro>\n  <macro name=\"container-author-reviewed-author-and-editor-translator-short\">\n    <group delimiter=\", \">\n      <choose>\n        <if variable=\"container-author reviewed-author\" match=\"any\">\n          <names variable=\"container-author reviewed-author\" delimiter=\", \">\n            <label form=\"verb-short\" suffix=\" \"/>\n            <name form=\"short\" and=\"text\" delimiter=\", \"/>\n          </names>\n        </if>\n      </choose>\n      <text macro=\"editor-translator-verb-short-comma\"/>\n    </group>\n  </macro>\n  <macro name=\"secondary-contributors-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <text macro=\"container-author-reviewed-author-and-editor-translator\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors-short\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <text macro=\"container-author-reviewed-author-and-editor-translator-short\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\", \">\n          <names variable=\"container-author\">\n            <label form=\"verb-short\" suffix=\" \"/>\n            <name and=\"text\" delimiter=\", \"/>\n          </names>\n          <text macro=\"editor-translator-verb-short-comma\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <group delimiter=\". \" prefix=\" \">\n          <text macro=\"editor-translator-verb-period\"/>\n          <names variable=\"container-author\">\n            <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <name and=\"text\" delimiter=\", \"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \" prefix=\" \">\n          <text macro=\"editor-translator-verb-comma\"/>\n          <names variable=\"container-author\">\n            <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <name and=\"text\" delimiter=\", \"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor-note\">\n    <names variable=\"editor\">\n      <name and=\"text\" sort-separator=\", \" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"collection-editor-note\">\n    <names variable=\"collection-editor\">\n      <label form=\"verb-short\" suffix=\" \"/>\n      <name and=\"text\" sort-separator=\", \" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"collection-editor\">\n    <group delimiter=\", \" prefix=\". \">\n      <names variable=\"collection-editor\" delimiter=\", \">\n        <label form=\"verb\" prefix=\" \" text-case=\"capitalize-first\" suffix=\" \"/>\n        <name and=\"text\" delimiter=\", \"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"translator-note\">\n    <names variable=\"translator\">\n      <name and=\"text\" sort-separator=\", \" delimiter=\", \"/>\n      <label form=\"verb-short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"recipient-note\">\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-note\">\n    <names variable=\"author\">\n      <name and=\"text\" sort-separator=\", \" delimiter=\", \"/>\n      <substitute>\n        <text macro=\"editor-note\"/>\n        <text macro=\"translator-note\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient-note\"/>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"verb-short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"recipient\">\n    <choose>\n      <if type=\"personal_communication\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n          <else>\n            <text term=\"letter\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n    <text macro=\"recipient-note\" prefix=\" \"/>\n  </macro>\n  <macro name=\"contributors\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient\" prefix=\". \"/>\n  </macro>\n  <macro name=\"recipient-short\">\n    <names variable=\"recipient\">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient-short\"/>\n  </macro>\n  <macro name=\"container-contributors-short\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\"; \">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"editor\" delimiter=\", \">\n                <label form=\"verb-short\" suffix=\" \"/>\n                <name form=\"short\" and=\"text\" delimiter=\", \"/>\n              </names>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\" delimiter=\", \">\n                <label form=\"verb-short\" suffix=\" \"/>\n                <name form=\"short\" and=\"text\" delimiter=\", \"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"contributors-sort\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"interviewer-note\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title-note\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <text variable=\"genre\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </else-if>\n      <else-if variable=\"reviewed-author\">\n        <choose>\n          <if variable=\"reviewed-title\" match=\"any\">\n            <group delimiter=\", review of \">\n              <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n              <text variable=\"reviewed-title\" font-style=\"italic\" text-case=\"title\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\" prefix=\"review of \" text-case=\"title\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"personal_communication\" match=\"none\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </else-if>\n      <else-if variable=\"reviewed-author\">\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"reviewed-title\" match=\"any\">\n              <group delimiter=\", review of \">\n                <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n                <text variable=\"reviewed-title\" font-style=\"italic\" text-case=\"title\"/>\n              </group>\n            </if>\n            <else>\n              <text variable=\"title\" font-style=\"italic\" prefix=\"Review of \" text-case=\"title\"/>\n            </else>\n          </choose>\n          <names variable=\"reviewed-author\">\n            <label form=\"verb-short\" text-case=\"lowercase\" suffix=\" \"/>\n            <name and=\"text\" delimiter=\", \"/>\n          </names>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"interview\">\n            <text term=\"interview\"/>\n          </if>\n          <else-if type=\"manuscript speech\" match=\"any\">\n            <text variable=\"genre\" form=\"short\"/>\n          </else-if>\n          <else-if type=\"personal_communication\">\n            <text macro=\"issued\"/>\n          </else-if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" form=\"short\" font-style=\"italic\" text-case=\"title\"/>\n      </else-if>\n      <else-if variable=\"reviewed-author\">\n        <choose>\n          <if variable=\"reviewed-title\" match=\"any\">\n            <group delimiter=\", review of \">\n              <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n              <text variable=\"reviewed-title\" font-style=\"italic\" text-case=\"title\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\" prefix=\"review of \" text-case=\"title\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" form=\"short\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"disambiguate-chapter\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <choose>\n          <if disambiguate=\"true\">\n            <text macro=\"container-contributors-short\" prefix=\"(\" suffix=\")\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"description-note\">\n    <group delimiter=\", \">\n      <text macro=\"interviewer-note\"/>\n      <text variable=\"medium\"/>\n      <choose>\n        <if variable=\"title\" match=\"none\"/>\n        <else-if type=\"thesis speech\" match=\"any\"/>\n        <else>\n          <text variable=\"genre\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"description\">\n    <group delimiter=\", \">\n      <group delimiter=\". \">\n        <text macro=\"interviewer\"/>\n        <text variable=\"medium\" text-case=\"capitalize-first\"/>\n      </group>\n      <choose>\n        <if variable=\"title\" match=\"none\"/>\n        <else-if type=\"thesis speech\" match=\"any\"/>\n        <else>\n          <text variable=\"genre\" text-case=\"capitalize-first\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"container-title-note\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" suffix=\" \"/>\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"container-title\" font-style=\"italic\" form=\"short\" text-case=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection-title\">\n    <choose>\n      <if variable=\"volume number-of-volumes\" match=\"any\">\n        <!-- Presence of Volume info indicates multivolume work rather than series -->\n        <choose>\n          <!--This section required here rather than in title-note because the group delimiter inserts an unneeded comma otherwise-->\n          <if type=\"chapter\" variable=\"container-title\" match=\"all\">\n            <!--Filter SBL 6.2.23-->\n          </if>\n          <else-if type=\"chapter\" variable=\"collection-title\" match=\"all\">\n            <!--Filter SBL 6.2.22-->\n            <group>\n              <text term=\"volume\" form=\"short\" suffix=\" \"/>\n              <number variable=\"volume\" form=\"numeric\"/>\n              <text value=\" of \"/>\n            </group>\n          </else-if>\n        </choose>\n        <text variable=\"collection-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </if>\n      <else>\n        <text variable=\"collection-title\" form=\"short\" text-case=\"title\"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection-title-note\">\n    <choose>\n      <if variable=\"volume number-of-volumes\" match=\"any\">\n        <!-- Presence of Volume info indicates multivolume work rather than series -->\n        <choose>\n          <!-- This section required here rather than in title-note because the group delimiter inserts an unneeded comma otherwise-->\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <choose>\n              <if variable=\"container-title\" match=\"none\">\n                <text term=\"in\" suffix=\" \"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n        <text variable=\"collection-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </if>\n      <else>\n        <text variable=\"collection-title\" form=\"short\" text-case=\"title\"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <!-- Added dictionary and encyclopedia to get page and volume information-->\n      <if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\" \" suffix=\" \">\n          <label variable=\"page\" form=\"long\" text-case=\"capitalize-first\"/>\n          <text variable=\"page\"/>\n          <text term=\"in\"/>\n          <choose>\n            <!-- Added dictionary and encyclopedia to get volume information-->\n            <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n              <choose>\n                <if match=\"any\" variable=\"volume\">\n                  <label suffix=\" \" variable=\"volume\" form=\"short\"/>\n                  <number variable=\"volume\"/>\n                  <text value=\"of\" suffix=\" \"/>\n                </if>\n              </choose>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" text-case=\"capitalize-first\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\", \">\n          <!--Repositioned Edition to proceed volume-->\n          <text macro=\"edition-note\"/>\n          <choose>\n            <if variable=\"volume\">\n              <!--If volume-->\n              <choose>\n                <if type=\"book\" variable=\"collection-title\" match=\"all\">\n                  <!--SBL 6.2.21-->\n                  <group>\n                    <text term=\"volume\" form=\"short\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <text value=\" of\"/>\n                  </group>\n                </if>\n                <else-if type=\"chapter\" variable=\"collection-title\" match=\"all\">\n                  <!--SBL 6.2.23-->\n                  <group>\n                    <text term=\"volume\" form=\"short\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <text value=\" of\"/>\n                  </group>\n                </else-if>\n                <else-if variable=\"locator page\" match=\"none\">\n                  <group>\n                    <text term=\"volume\" form=\"short\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                  </group>\n                </else-if>\n              </choose>\n            </if>\n            <else>\n              <group>\n                <number variable=\"number-of-volumes\" form=\"numeric\"/>\n                <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <group prefix=\" \" delimiter=\".\">\n          <text variable=\"volume\"/>\n          <number variable=\"issue\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group prefix=\". \" delimiter=\". \">\n          <!--Repositioned Edition to proceed volume-->\n          <text macro=\"edition\"/>\n          <choose>\n            <if variable=\"volume\">\n              <!--If volume-->\n              <choose>\n                <if type=\"book\" variable=\"collection-title\" match=\"all\">\n                  <!--SBL 6.2.21-->\n                  <group>\n                    <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <text value=\" of\"/>\n                  </group>\n                </if>\n                <else-if type=\"chapter\" variable=\"collection-title\" match=\"all\">\n                  <!--SBL 6.2.23-->\n                  <group>\n                    <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <text value=\" of\"/>\n                  </group>\n                </else-if>\n                <else-if variable=\"locator page\" match=\"none\">\n                  <group>\n                    <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                  </group>\n                </else-if>\n              </choose>\n            </if>\n            <else>\n              <group>\n                <number variable=\"number-of-volumes\" form=\"numeric\"/>\n                <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </else-if>\n      <!-- Added dictionary and encyclopedia to properly display volume information-->\n      <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group prefix=\". \">\n          <number variable=\"number-of-volumes\" form=\"numeric\"/>\n          <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locators-newspaper\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"edition\"/>\n            <text term=\"edition\"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\" \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event-note\">\n    <!-- Support citation of unpublished conference presentations per SBLHS2 §6.3.8. Corrects for dropping of event-title information. -->\n    <choose>\n      <if type=\"speech\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"genre\"/>\n          <text term=\"presented at\" suffix=\" \"/>\n          <text macro=\"event-title\" suffix=\", \"/>\n        </group>\n      </if>\n    </choose>\n    <text variable=\"event\" suffix=\",\"/>\n  </macro>\n  <macro name=\"event\">\n    <group>\n      <text term=\"presented at\" suffix=\" \"/>\n      <text macro=\"event-title\"/>\n    </group>\n  </macro>\n  <macro name=\"event-title\">\n    <choose>\n      <!-- TODO: We expect \"event-title\" to be used,\n           but processors and applications may not be updated yet.\n           This macro ensures that either \"event\" or \"event-title\" can be accpeted.\n           Remove if procesor logic and application adoption can handle this. -->\n      <if variable=\"event-title\">\n        <text variable=\"event-title\"/>\n      </if>\n      <else>\n        <text variable=\"event\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"originally-published\">\n    <group delimiter=\", \">\n      <group delimiter=\": \">\n        <text variable=\"original-publisher-place\"/>\n        <text variable=\"original-publisher\"/>\n      </group>\n      <choose>\n        <if is-uncertain-date=\"original-date\">\n          <date variable=\"original-date\" form=\"numeric\" date-parts=\"year\" prefix=\"[\" suffix=\"?]\"/>\n        </if>\n        <else>\n          <date variable=\"original-date\" form=\"numeric\" date-parts=\"year\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"reprint-note\">\n    <!--needs localization-->\n    <choose>\n      <if variable=\"original-date issued\" match=\"all\">\n        <choose>\n          <!--for whatever reason in notes, when we have both original and new publishers, reprint doesn't appear-->\n          <if variable=\"original-publisher original-publisher-place\" match=\"none\">\n            <text value=\"repr.\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"reprint\">\n    <!--needs localization-->\n    <choose>\n      <if variable=\"original-date issued\" match=\"all\">\n        <text value=\"repr.\" text-case=\"capitalize-first\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"graphic report\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else-if type=\"legal_case\">\n            <text variable=\"authority\" suffix=\" \"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis article-journal\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"point-locators-subsequent\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <text term=\"volume\" form=\"short\"/>\n                  <number variable=\"volume\" form=\"numeric\"/>\n                </group>\n                <!-- When section-based citations are used, SBL prefers no space following the section symbol(s). E.g., https://sblhs2.com/2017/04/20/citing-reference-works-7-greek-language-tools/ -->\n                <choose>\n                  <if locator=\"section\">\n                    <group>\n                      <label variable=\"locator\" form=\"short\"/>\n                      <text variable=\"locator\"/>\n                    </group>\n                  </if>\n                  <else>\n                    <group delimiter=\" \">\n                      <label variable=\"locator\" form=\"short\"/>\n                      <!-- Adds quotes around the locator when the type is sub-verbo per https://sblhs2.com/2017/03/30/citing-reference-works-2-lexica/ -->\n                      <choose>\n                        <if locator=\"sub-verbo\">\n                          <text variable=\"locator\" quotes=\"true\"/>\n                        </if>\n                        <else>\n                          <text variable=\"locator\"/>\n                        </else>\n                      </choose>\n                    </group>\n                  </else>\n                </choose>\n              </group>\n            </if>\n            <else>\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\"/>\n            </else>\n          </choose>\n        </if>\n        <else-if variable=\"volume\">\n          <choose>\n            <if type=\"book\" variable=\"collection-title\" match=\"all\">\n              <!--Filter SBL 6.2.21-->\n              <text variable=\"locator\"/>\n            </if>\n            <else-if type=\"chapter\" variable=\"container-title\" match=\"all\">\n              <!--Filter SBL 6.2.23-->\n              <text variable=\"locator\"/>\n            </else-if>\n            <else-if type=\"chapter\" variable=\"collection-title\" position=\"subsequent\" match=\"all\">\n              <!--Filter SBL 6.2.22-->\n              <text variable=\"locator\"/>\n            </else-if>\n            <else-if type=\"article-journal\">\n              <!--Filter SBL 6.3.1-->\n              <text variable=\"locator\"/>\n            </else-if>\n            <else>\n              <group delimiter=\":\">\n                <number variable=\"volume\" form=\"numeric\"/>\n                <text variable=\"locator\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else>\n          <text variable=\"locator\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"point-locators\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <text macro=\"pages\"/>\n      </if>\n      <else-if type=\"article-journal\">\n        <!-- Add support for individually paginated, electronic journal articles per https://sblhs2.com/2018/05/03/electronic-journals-with-individually-paginated-articles/ -->\n        <choose>\n          <if variable=\"number\">\n            <group delimiter=\" \" prefix=\": \">\n              <text term=\"article-journal\" form=\"short\"/>\n              <text variable=\"number\"/>\n            </group>\n            <group prefix=\", \">\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"locator\" prefix=\": \"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if position=\"first\" match=\"all\">\n            <text macro=\"point-locators-subsequent\" prefix=\" \"/>\n            <!-- Per SBLHS2 §6.3.6, the first reference will, by definition have the dictionary title or abbreviation preceding the locators, volume number included. No comma should appear after the title or abbreviation and before the locators. -->\n          </if>\n          <else>\n            <text macro=\"point-locators-subsequent\" prefix=\", \"/>\n            <!--SBL 6.3.6 prefers comma in subsequent entries before vol info.-->\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text macro=\"point-locators-subsequent\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"article-journal\">\n        <!-- Add support for individually paginated, electronic journal articles per https://sblhs2.com/2018/05/03/electronic-journals-with-individually-paginated-articles/ -->\n        <choose>\n          <if variable=\"number\">\n            <text term=\"article-journal\" form=\"short\" prefix=\": \"/>\n            <text variable=\"number\" prefix=\" \" suffix=\", \"/>\n            <choose>\n              <if variable=\"locator\">\n                <label variable=\"locator\" form=\"short\"/>\n                <text variable=\"locator\"/>\n              </if>\n              <else-if variable=\"page\">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </else-if>\n              <else>\n                <!-- Is there not a term for \"no page\"? See SBLHS2 §8.1.3 and CMS17 §10.42 where the same abbreviation applies to \"no place,\" \"no publisher,\" and \"no page.\" -->\n                <text value=\"n.p.\"/>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <choose>\n              <if variable=\"locator\">\n                <text variable=\"locator\" prefix=\": \"/>\n              </if>\n              <else>\n                <text variable=\"page\" prefix=\": \"/>\n              </else>\n            </choose>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if variable=\"volume\">\n            <choose>\n              <if type=\"chapter\" variable=\"container-title\" match=\"all\">\n                <!--Filter SBL 6.2.23-->\n                <text variable=\"page\" prefix=\", \"/>\n              </if>\n              <else-if type=\"chapter\" variable=\"collection-title\" position=\"subsequent\" match=\"all\">\n                <!--Filter SBL 6.2.22-->\n                <text variable=\"page\" prefix=\", \"/>\n              </else-if>\n              <else>\n                <number variable=\"volume\" prefix=\" \" suffix=\":\"/>\n                <text variable=\"page\"/>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <text variable=\"page\" prefix=\", \"/>\n          </else>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locators-journal\">\n    <choose>\n      <if type=\"article-journal\">\n        <!-- Add support for individually paginated, electronic journal articles per https://sblhs2.com/2018/05/03/electronic-journals-with-individually-paginated-articles/ -->\n        <choose>\n          <if variable=\"number\">\n            <text term=\"article-journal\" form=\"short\" prefix=\": \"/>\n            <text variable=\"number\" prefix=\" \" suffix=\", \"/>\n            <choose>\n              <if variable=\"page\">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </if>\n              <else>\n                <!-- Is there not a term for \"no page\"? See SBLHS2 §8.1.3 and CMS17 §10.42 where the same abbreviation applies to \"no place,\" \"no publisher,\" and \"no page.\" -->\n                <text value=\"n.p.\"/>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <text variable=\"page\" prefix=\": \"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive-note\">\n    <group delimiter=\", \">\n      <text variable=\"archive_location\"/>\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\". \">\n      <text variable=\"archive_location\" text-case=\"capitalize-first\"/>\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"issue-note\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"volume\" prefix=\" \"/>\n        <text variable=\"issue\" prefix=\".\"/>\n        <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n      </if>\n      <else-if variable=\"publisher-place publisher\" match=\"any\">\n        <group prefix=\" (\" suffix=\")\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text macro=\"event-note\"/>\n              <group delimiter=\"\">\n                <text macro=\"originally-published\" suffix=\"; \"/>\n                <group delimiter=\", \">\n                  <text macro=\"reprint-note\"/>\n                  <text macro=\"publisher\"/>\n                  <choose>\n                    <if variable=\"title\" match=\"none\"/>\n                    <else-if type=\"thesis\" match=\"any\">\n                      <text variable=\"genre\"/>\n                    </else-if>\n                  </choose>\n                </group>\n                <text macro=\"issued\" prefix=\", \"/>\n              </group>\n            </group>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"issued\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue\">\n    <choose>\n      <if type=\"article-journal\">\n        <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n      </if>\n      <else-if type=\"speech\">\n        <choose>\n          <if variable=\"title\" match=\"none\"/>\n          <else>\n            <text variable=\"genre\" text-case=\"capitalize-first\" prefix=\". \"/>\n          </else>\n        </choose>\n        <text macro=\"event\" prefix=\" \"/>\n        <text variable=\"event-place\" prefix=\". \"/>\n        <text macro=\"issued\" prefix=\", \"/>\n      </else-if>\n      <else-if variable=\"publisher-place publisher\" match=\"any\">\n        <group prefix=\". \" delimiter=\", \">\n          <choose>\n            <if type=\"thesis\">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n          <group delimiter=\". \">\n            <text macro=\"originally-published\"/>\n            <group delimiter=\", \">\n              <text macro=\"reprint\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </group>\n          <text macro=\"issued\"/>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"issued\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access-note\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"graphic report\" match=\"any\">\n          <text macro=\"archive-note\"/>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"none\">\n          <text macro=\"archive-note\"/>\n        </else-if>\n      </choose>\n      <!-- SBL style uses DOIs and URLs when the resource is electronic. SBLHS2 §§ 6.1.6, 6.2.25, 6.3.10, 6.4.13&#8211;14. If a resource has physical dimensions, however, it is not electronic. So, DOI or URL output should be bypassed. -->\n      <choose>\n        <if variable=\"dimensions\"/>\n        <else>\n          <!-- Per SBLHS2 §6.3.10, allows either DOI or URL or both to be included. -->\n          <group delimiter=\", \">\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\". \">\n      <choose>\n        <if type=\"graphic report\" match=\"any\">\n          <text macro=\"archive\"/>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"none\">\n          <text macro=\"archive\"/>\n        </else-if>\n      </choose>\n      <!-- SBL style uses DOIs and URLs when the resource is electronic. SBLHS2 §§6.1.6, 6.2.25, 6.3.10, 6.4.13&#8211;14. If a resource has physical dimensions, however, it is not electronic. So, DOI or URL output should be bypassed. -->\n      <choose>\n        <if variable=\"dimensions\"/>\n        <else>\n          <!-- Per SBLHS2 §6.3.10, allows either DOI or URL or both to be included. -->\n          <group delimiter=\", \">\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"unsigned-dictionary-note\">\n    <group delimiter=\", \">\n      <group>\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"uppercase\"/>\n        <number variable=\"volume\" prefix=\" \"/>\n        <text variable=\"collection-title\" text-case=\"uppercase\"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </group>\n      <group delimiter=\" \">\n        <text term=\"sub-verbo\" form=\"short\"/>\n        <text variable=\"title\" text-case=\"title\" quotes=\"true\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"signed-dictionary-note\">\n    <choose>\n      <if position=\"subsequent\">\n        <group delimiter=\", \">\n          <text macro=\"contributors-short\"/>\n          <text macro=\"title-short\"/>\n          <text macro=\"point-locators-subsequent\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\"\">\n          <group delimiter=\", \">\n            <text macro=\"contributors-note\"/>\n            <text macro=\"title-note\"/>\n            <text macro=\"container-title-note\"/>\n          </group>\n          <text macro=\"point-locators\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"signed-dictionary-bib\">\n    <group delimiter=\"\">\n      <group delimiter=\". \">\n        <text macro=\"contributors\"/>\n        <text macro=\"title-note\"/>\n        <text macro=\"container-title-note\"/>\n      </group>\n      <text macro=\"point-locators\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <!-- Support custom citations via the annote variable. For discussion, see https://forums.zotero.org/discussion/comment/317370 -->\n        <if variable=\"annote\">\n          <choose>\n            <!-- For situations like citation of lexica per https://sblhs2.com/2017/03/30/citing-reference-works-2-lexica/. -->\n            <if locator=\"sub-verbo\">\n              <group delimiter=\", \">\n                <text variable=\"annote\"/>\n                <text macro=\"point-locators-subsequent\"/>\n              </group>\n            </if>\n            <!-- For situations like citation of collections like (M)OTP per https://sblhs2.com/2017/06/08/citing-text-collections-3-otp/ and https://sblhs2.com/2017/06/22/citing-text-collections-3-motp/. -->\n            <else>\n              <group delimiter=\" \">\n                <text variable=\"annote\"/>\n                <text macro=\"point-locators-subsequent\"/>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <!-- Lexicon/Dictionary/Encyclopedia -->\n        <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n          <choose>\n            <if match=\"none\" variable=\"author\">\n              <!-- Unsigned Lexicon/Dictionary/Encyclopedia -->\n              <text macro=\"unsigned-dictionary-note\"/>\n            </if>\n            <else>\n              <!-- Signed Lexicon/Dictionary/Encyclopedia -->\n              <text macro=\"signed-dictionary-note\"/>\n            </else>\n          </choose>\n        </else-if>\n        <!-- Not Lexicon/Dictionary/Encyclopedia -->\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"contributors-short\"/>\n            <group>\n              <text macro=\"title-short\"/>\n              <text macro=\"secondary-contributors-short\" prefix=\" (\" suffix=\")\"/>\n            </group>\n            <text macro=\"disambiguate-chapter\"/>\n            <text macro=\"point-locators-subsequent\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"contributors-note\"/>\n            <text macro=\"title-note\"/>\n            <text macro=\"description-note\"/>\n            <text macro=\"secondary-contributors-note\"/>\n            <text macro=\"container-title-note\"/>\n            <text macro=\"container-contributors-note\"/>\n            <choose>\n              <if variable=\"volume collection-title\" match=\"all\">\n                <!--If Volume # and collection-title -->\n                <group delimiter=\" \">\n                  <!--Added group to prevent comma between vol # of...and collection-title-->\n                  <text macro=\"locators-note\"/>\n                  <text macro=\"collection-title-note\"/>\n                </group>\n              </if>\n              <else>\n                <text macro=\"locators-note\"/>\n                <text macro=\"collection-title-note\"/>\n              </else>\n            </choose>\n            <text macro=\"collection-editor-note\"/>\n          </group>\n          <text macro=\"issue-note\"/>\n          <text macro=\"locators-newspaper\" prefix=\", \"/>\n          <text macro=\"point-locators\"/>\n          <text macro=\"access-note\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"10\" et-al-use-first=\"9\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"contributors-sort\"/>\n      <key macro=\"title\"/>\n      <key variable=\"genre\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <!-- Dictionary -->\n        <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n          <choose>\n            <if match=\"none\" variable=\"author\">\n              <!-- Unsigned Not Listed -->\n            </if>\n            <else>\n              <!-- Signed -->\n              <text macro=\"signed-dictionary-bib\"/>\n            </else>\n          </choose>\n        </if>\n        <!-- Not Dictionary -->\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"contributors\"/>\n            <text macro=\"title\"/>\n            <text macro=\"description\"/>\n            <text macro=\"secondary-contributors\"/>\n            <text macro=\"container-title\"/>\n            <text macro=\"container-contributors\"/>\n          </group>\n          <choose>\n            <if variable=\"volume collection-title\" match=\"all\">\n              <!--If Volume # and collection-title -->\n              <text macro=\"locators\"/>\n              <text macro=\"collection-title\" text-case=\"title\" prefix=\" \"/>\n            </if>\n            <else>\n              <text macro=\"locators\"/>\n              <text macro=\"collection-title\" text-case=\"title\" prefix=\". \"/>\n            </else>\n          </choose>\n          <text macro=\"collection-editor\"/>\n          <text macro=\"issue\"/>\n          <text macro=\"locators-newspaper\" prefix=\", \"/>\n          <text macro=\"locators-journal\"/>\n          <text macro=\"access\" prefix=\". \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"