"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"fr-FR\">\n  <info>\n    <title>Groupe d'anthropologie et d'archéologie funéraire (Français)</title>\n    <title-short>Gaaf</title-short>\n    <id>http://www.zotero.org/styles/groupe-danthropologie-et-darcheologie-funeraire</id>\n    <link href=\"http://www.zotero.org/styles/groupe-danthropologie-et-darcheologie-funeraire\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/documents-d-archeologie-francaise\" rel=\"template\"/>\n    <link href=\"https://www.gaaf-asso.fr/wp-content/uploads/Collection-Publication-du-Gaaf_Recommandations_auteurs_DEF-V3.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Francois Capron</name>\n      <email>f.capron37@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"history\"/>\n    <category field=\"anthropology\"/>\n    <updated>2020-05-08T05:18:48+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"collection-editor\" form=\"short\">dir.</term>\n      <term name=\"editor\" form=\"short\">dir.</term>\n      <term name=\"container-author\" form=\"short\">dir.</term>\n      <term name=\"no date\">s.d.</term>\n      <term name=\"in\">in&#160;:</term>\n      <term name=\"online\">En&#160;ligne</term>\n      <term name=\"accessed\">consulté le</term>\n      <term name=\"and others\">et alii</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" initialize-with=\". \">\n        <name-part name=\"family\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if variable=\"author\">\n        <names variable=\"author\">\n          <name form=\"short\" font-weight=\"bold\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n          <label form=\"short\" prefix=\", \" suffix=\". \" strip-periods=\"true\"/>\n          <et-al font-weight=\"bold\" term=\"and others\" font-style=\"italic\"/>\n        </names>\n      </if>\n      <else-if variable=\"editor\">\n        <names variable=\"editor\">\n          <name form=\"short\" font-weight=\"bold\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n          <et-al font-weight=\"bold\" term=\"and others\" font-style=\"italic\"/>\n        </names>\n      </else-if>\n      <else-if variable=\"collection-editor\">\n        <names variable=\"collection-editor\">\n          <name form=\"short\" font-weight=\"bold\" et-al-min=\"3\" et-al-use-first=\"1\"/>\n          <et-al font-weight=\"bold\" term=\"and others\" font-style=\"italic\"/>\n        </names>\n      </else-if>\n      <else-if type=\"book\">\n        <text variable=\"title-short\" font-weight=\"bold\"/>\n      </else-if>\n      <else-if type=\"map\">\n        <text variable=\"title-short\" font-weight=\"bold\"/>\n      </else-if>\n      <else-if type=\"article-journal\">\n        <text variable=\"title-short\" font-weight=\"bold\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" initialize-with=\". \">\n        <name-part name=\"family\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\", \" form=\"short\"/>\n    </names>\n  </macro>\n  <macro name=\"collection-editor\">\n    <group>\n      <names variable=\"collection-editor\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" initialize-with=\". \">\n          <name-part name=\"family\"/>\n          <name-part name=\"given\"/>\n        </name>\n      </names>\n      <text term=\"collection-editor\" form=\"short\" prefix=\", \"/>\n    </group>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" initialize-with=\". \">\n        <name-part name=\"family\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\"\" form=\"short\" suffix=\"\"/>\n    </names>\n  </macro>\n  <macro name=\"responsability\">\n    <choose>\n      <if variable=\"version\">\n        <group delimiter=\" \">\n          <text macro=\"author\"/>\n          <text term=\"editor\" form=\"short\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n      <else-if variable=\"author editor translator collection-editor \" match=\"any\">\n        <choose>\n          <if variable=\"author\">\n            <text macro=\"author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n          <else-if variable=\"collection-editor\">\n            <text macro=\"collection-editor\"/>\n          </else-if>\n          <else>\n            <text macro=\"translator\"/>\n          </else>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"author-citation\">\n    <choose>\n      <if variable=\"author editor translator collection-editor\" match=\"any\">\n        <names variable=\"author\">\n          <name form=\"short\"/>\n          <et-al term=\"and others\" font-style=\"italic\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <names variable=\"collection-editor\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text variable=\"title\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"container-responsability\">\n    <choose>\n      <if variable=\"container-author editor translator collection-editor\" match=\"any\">\n        <choose>\n          <if variable=\"container-author\">\n            <text macro=\"container-author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n          <else-if variable=\"collection-editor\">\n            <text macro=\"collection-editor\"/>\n          </else-if>\n          <else>\n            <text macro=\"translator\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis map motion_picture song manuscript\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"chapter paper-conference broadcast entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group>\n          <text variable=\"title\" prefix=\"«&#160;\" suffix=\"&#160;»\"/>\n          <choose>\n            <if variable=\"container-author editor translator collection-editor\" match=\"any\">\n              <group delimiter=\" \">\n                <text term=\"in\" prefix=\", \"/>\n                <text macro=\"container-responsability\" suffix=\",\"/>\n              </group>\n            </if>\n            <else>\n              <text term=\"in\" prefix=\", \"/>\n            </else>\n          </choose>\n          <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n        </group>\n      </else-if>\n      <else-if type=\"report\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text variable=\"number\" prefix=\", \"/>\n      </else-if>\n      <else-if type=\"article-journal speech article-magazine article-newspaper post-weblog post\" match=\"any\">\n        <group>\n          <text variable=\"title\" prefix=\"«&#160;\" suffix=\"&#160;»\"/>\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\" prefix=\", \"/>\n          <text macro=\"issue\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\"/>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"map\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n          <else>\n            <text value=\"carte\" prefix=\"[\" suffix=\"]\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"genre\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" suffix=\".\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if variable=\"publisher-place\">\n        <text variable=\"publisher-place\"/>\n      </if>\n      <else>\n        <text value=\"s.l.\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"publi_place\">\n    <group delimiter=\", \">\n      <text macro=\"publisher\"/>\n      <text macro=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"issue\">\n    <text variable=\"volume\" prefix=\" \"/>\n    <group delimiter=\", \">\n      <text variable=\"issue\" prefix=\" (\" suffix=\")\"/>\n      <group delimiter=\"&#160;\">\n        <text variable=\"page\" prefix=\", \"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"book thesis manuscript\" match=\"any\">\n        <text variable=\"number-of-pages\" suffix=\"&#160;p\"/>\n      </if>\n      <else-if type=\"chapter paper-conference article-newspaper entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\"&#160;\">\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"report patent\" match=\"any\">\n        <text variable=\"page\" suffix=\"&#160;p\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text variable=\"URL\" prefix=\"&lt;&#160;\" suffix=\"&#160;&gt;\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"URLaccess\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \">\n          <text term=\"online\" suffix=\". \"/>\n          <text macro=\"url\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text term=\"accessed\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" prefix=\"&#160;\"/>\n            <date-part name=\"month\" prefix=\"&#160;\"/>\n            <date-part name=\"year\" prefix=\"&#160;\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" year-suffix-delimiter=\", \" after-collapse-delimiter=\"&#160;; \" name-delimiter=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\" name-as-sort-order=\"all\" sort-separator=\" \">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author-short\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"&#160;; \">\n      <group delimiter=\",&#160;\">\n        <group delimiter=\"&#160;\">\n          <text macro=\"author-citation\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year-date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <group suffix=\"&#160;:&#160;\" delimiter=\" \" font-weight=\"bold\">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\" font-weight=\"bold\"/>\n      </group>\n      <group suffix=\".&#9;\">\n        <choose>\n          <if type=\"book\">\n            <text macro=\"responsability\"/>\n            <text macro=\"year-date\" prefix=\" (\" suffix=\") ‒ \"/>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text variable=\"note\"/>\n              <text variable=\"volume\"/>\n              <choose>\n                <if variable=\"version\" match=\"none\">\n                  <text macro=\"collection\"/>\n                </if>\n              </choose>\n              <text variable=\"archive\"/>\n              <text macro=\"URLaccess\"/>\n              <text macro=\"publi_place\"/>\n              <text macro=\"edition\"/>\n            </group>\n          </if>\n          <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n            <text macro=\"responsability\"/>\n            <text macro=\"year-date\" prefix=\" (\" suffix=\") ‒ \"/>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"URLaccess\"/>\n              <text macro=\"accessed\"/>\n              <text macro=\"edition\"/>\n              <text variable=\"archive\"/>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference chapter entry entry-dictionary entry-encyclopedia\" match=\"any\">\n            <text macro=\"responsability\"/>\n            <text macro=\"year-date\" prefix=\" (\" suffix=\") ‒ \"/>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text variable=\"note\"/>\n              <text macro=\"publi_place\"/>\n              <text macro=\"page\"/>\n              <text variable=\"archive\"/>\n              <text macro=\"URLaccess\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <text macro=\"responsability\"/>\n            <text macro=\"year-date\" prefix=\" (\" suffix=\") ‒ \"/>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <text macro=\"genre\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"publisher-place\"/>\n                <text variable=\"note\"/>\n              </group>\n              <text macro=\"URLaccess\"/>\n              <text variable=\"archive\"/>\n            </group>\n          </else-if>\n          <else-if type=\"post-weblog post webpage\" match=\"any\">\n            <text macro=\"responsability\"/>\n            <text macro=\"year-date\" prefix=\" (\" suffix=\") ‒ \"/>\n            <group delimiter=\", \">\n              <text variable=\"title\" quotes=\"true\"/>\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n              <text term=\"online\"/>\n              <text macro=\"URLaccess\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\">\n            <text macro=\"responsability\"/>\n            <text macro=\"year-date\" prefix=\" (\" suffix=\") ‒ \"/>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"genre\"/>\n              <text macro=\"edition\" suffix=\". \"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text variable=\"archive\"/>\n                <text variable=\"note\"/>\n              </group>\n              <text macro=\"URLaccess\"/>\n            </group>\n          </else-if>\n          <else>\n            <text macro=\"responsability\"/>\n            <text macro=\"year-date\" prefix=\" (\" suffix=\") ‒ \"/>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"medium\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publi_place\"/>\n              <text macro=\"collection\"/>\n              <text macro=\"page\"/>\n              <text macro=\"URLaccess\"/>\n              <text variable=\"archive\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"