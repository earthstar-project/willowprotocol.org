"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>University of Lincoln - Harvard</title>\n    <id>http://www.zotero.org/styles/university-of-lincoln-harvard</id>\n    <link href=\"http://www.zotero.org/styles/university-of-lincoln-harvard\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-newcastle-university\" rel=\"template\"/>\n    <link href=\"http://guides.library.lincoln.ac.uk/learn/referencing\" rel=\"documentation\"/>\n    <link href=\"http://guides.library.lincoln.ac.uk/ld.php?content_id=26426164\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>The Harvard author-date style, 2nd edition, as outlined by the library of the University of Lincoln.</summary>\n    <updated>2019-11-13T14:03:41+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\".\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\".)\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" initialize-with=\". \" sort-separator=\",\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"false\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"bill webpage article-journal article-newspaper\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"available at\" text-case=\"capitalize-first\"/>\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book webpage graphic\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"false\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number vertical-align=\"baseline\" variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-newspaper paper-conference broadcast\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" prefix=\" \"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group>\n      <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\". \"/>\n      <number variable=\"volume\" form=\"numeric\"/>\n    </group>\n    <group>\n      <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\", \"/>\n      <text term=\"volume\" form=\"short\" prefix=\" \" suffix=\".\" plural=\"true\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\" text-case=\"capitalize-first\"/>\n        <text macro=\"year-date\"/>\n        <group>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"bill\" match=\"none\">\n          <group delimiter=\" \">\n            <text macro=\"author\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"thesis\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text macro=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"genre\" suffix=\" thesis\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"bill\">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <text variable=\"authority\" prefix=\" [\" suffix=\"]\"/>\n          <text variable=\"number\" prefix=\" Bill \" suffix=\", \"/>\n          <text variable=\"chapter-number\" suffix=\". \"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group prefix=\" \" delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal broadcast personal_communication thesis webpage\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <choose>\n            <if variable=\"author\" match=\"any\">\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\",\"/>\n            </if>\n          </choose>\n          <group prefix=\" \" suffix=\".\">\n            <group>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group prefix=\" \">\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <choose>\n            <if variable=\"author\" match=\"any\">\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\",\"/>\n            </if>\n          </choose>\n          <group delimiter=\", \" prefix=\" \">\n            <text macro=\"issued\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"book graphic report\" match=\"any\">\n          <group prefix=\" \" delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <text variable=\"collection-number\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"publisher\" suffix=\",\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <text variable=\"container-title\" prefix=\" \"/>\n          <text macro=\"access\" prefix=\" \" suffix=\". \"/>\n          <group prefix=\" \" suffix=\".\">\n            <group prefix=\" \">\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\" (\" suffix=\")\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group prefix=\" \">\n              <label variable=\"locator\" suffix=\".\" form=\"short\" strip-periods=\"true\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"