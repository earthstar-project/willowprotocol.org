ame and=\"text\" delimiter=\" \" name-as-sort-order=\"all\" sort-separator=\", \" form=\"short\"/>\n        </names>\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <text macro=\"year\"/>\n      <text macro=\"title\"/>\n      <choose>\n        <if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"page\" prefix=\"p \"/>\n          </group>\n        </if>\n        <else-if type=\"book\">\n          <text variable=\"publisher\" suffix=\", \"/>\n          <text variable=\"publisher-place\" suffix=\" \"/>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <text variable=\"event\" prefix=\" \" strip-periods=\"true\" suffix=\" \"/>\n          <text variable=\"volume\" suffix=\":\"/>\n          <text variable=\"page\"/>\n        </else-if>\n        <else-if type=\"chapter\">\n          <text macro=\"editor\"/>\n          <text variable=\"container-title\" prefix=\" \" suffix=\". \"/>\n          <text variable=\"publisher\" suffix=\", \"/>\n          <text variable=\"publisher-place\" suffix=\", \"/>\n          <text variable=\"page\" prefix=\"pp \" suffix=\" \"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <text variable=\"URL\" suffix=\" \"/>\n        </else-if>\n        <else>\n          <group suffix=\" \">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" suffix=\" \"/>\n            <text variable=\"volume\" suffix=\":\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"