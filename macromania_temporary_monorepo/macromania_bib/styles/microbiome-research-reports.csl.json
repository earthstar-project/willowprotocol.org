"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" delimiter-precedes-et-al=\"always\" delimiter-precedes-last=\"never\" page-range-format=\"minimal\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Microbiome Research Reports</title>\n    <title-short>MRR</title-short>\n    <id>http://www.zotero.org/styles/microbiome-research-reports</id>\n    <link href=\"http://www.zotero.org/styles/microbiome-research-reports\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-surgery-and-medicine\" rel=\"template\"/>\n    <link href=\"https://www.oaepublish.com/mrr/pages/view/author_instructions\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>2771-5965</issn>\n    <updated>2023-01-13T09:16:31+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <date form=\"text\" delimiter=\" \">\n      <date-part name=\"day\"/>\n      <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"collection-editor\" form=\"long\">\n        <single>Series editor</single>\n        <multiple>Series editors</multiple>\n      </term>\n      <term name=\"available at\">available from</term>\n      <term name=\"accessed\">last accessed</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-et-al=\"always\" delimiter-precedes-last=\"never\" et-al-min=\"7\" et-al-use-first=\"6\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\".\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\"; \">\n      <group delimiter=\": \">\n        <choose>\n          <if type=\"thesis\" match=\"none\">\n            <text variable=\"publisher-place\"/>\n          </if>\n        </choose>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" suffix=\".\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\">\n        <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n          <text variable=\"PMID\" prefix=\"PMID: \"/>\n          <text variable=\"DOI\" prefix=\"DOI: \"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\". \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\" form=\"text\"/>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group>\n        <group delimiter=\". \" suffix=\".\">\n          <text macro=\"author\"/>\n          <text variable=\"title\"/>\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n              <group delimiter=\". \" prefix=\" \">\n                <text macro=\"edition\"/>\n                <text variable=\"genre\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\". \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                  <group delimiter=\". \">\n                    <text macro=\"editor\"/>\n                    <text macro=\"container-title\"/>\n                  </group>\n                </group>\n                <text macro=\"edition\"/>\n                <text macro=\"publisher\"/>\n                <group delimiter=\" \">\n                  <label variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\" \">\n                <text macro=\"container-title\"/>\n                <group delimiter=\";\">\n                  <text macro=\"year-date\"/>\n                  <group>\n                    <text variable=\"volume\"/>\n                    <choose>\n                      <if is-numeric=\"issue\"/>\n                      <else>\n                        <text variable=\"issue\" prefix=\" \"/>\n                      </else>\n                    </choose>\n                    <text variable=\"page\" prefix=\":\"/>\n                  </group>\n                </group>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"