"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Neural Plasticity</title>\n    <title-short>Neural Plast</title-short>\n    <id>http://www.zotero.org/styles/neural-plasticity</id>\n    <link href=\"http://www.zotero.org/styles/neural-plasticity\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/biomed-research-international\" rel=\"template\"/>\n    <link href=\"https://www.hindawi.com/journals/np/guidelines/\" rel=\"documentation\"/>\n    <author>\n      <name>Lucio Marinelli</name>\n      <email>lucio.marinelli@unige.it</email>\n      <uri>http://www.luciomarinelli.com</uri>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>2090-5904</issn>\n    <eissn>1687-5443</eissn>\n    <updated>2017-05-08T07:23:40+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <group>\n      <names variable=\"author\">\n        <name and=\"text\" initialize-with=\".\"/>\n        <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" initialize-with=\".\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" font-style=\"italic\"/>\n    <text variable=\"publisher-place\" prefix=\", \"/>\n  </macro>\n  <macro name=\"page\">\n    <group>\n      <label plural=\"always\" suffix=\" \" variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"7\" et-al-use-first=\"3\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text macro=\"title\" suffix=\".\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\"/>\n              <text variable=\"volume\" suffix=\",\"/>\n            </group>\n            <group delimiter=\" \">\n              <text term=\"issue\" form=\"short\"/>\n              <number suffix=\",\" variable=\"issue\"/>\n            </group>\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\" form=\"short\" suffix=\",\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <choose>\n              <if variable=\"container-title\">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n                <text macro=\"editor\" suffix=\" \"/>\n                <group delimiter=\". \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <text macro=\"page\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n              </if>\n              <else>\n                <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <text variable=\"event\"/>\n                <text variable=\"event-place\" prefix=\", \"/>\n                <date variable=\"issued\" prefix=\" \">\n                  <date-part name=\"month\" suffix=\" \"/>\n                  <date-part name=\"day\" suffix=\" \"/>\n                </date>\n              </else>\n            </choose>\n            <date variable=\"issued\">\n              <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <group delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"event\" suffix=\".\"/>\n            <text variable=\"event-place\" prefix=\", \"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n            </date>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text variable=\"URL\"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n            </date>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"