"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Applied Spectroscopy Reviews</title>\n    <id>http://www.zotero.org/styles/applied-spectroscopy-reviews</id>\n    <link href=\"http://www.zotero.org/styles/applied-spectroscopy-reviews\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/molecular-and-cellular-proteomics\" rel=\"template\"/>\n    <link href=\"http://www.tandfonline.com/action/authorSubmission?journalCode=laps20&amp;page=instructions\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <issn>0570-4928</issn>\n    <eissn>1520-569X</eissn>\n    <updated>2017-04-25T13:30:05+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" prefix=\" \" delimiter=\", \">\n      <name delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\". \" and=\"text\" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else-if type=\"report\" match=\"any\">\n        <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n          <text variable=\"genre\"/>\n          <number variable=\"number\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\".\"/>\n      <choose>\n        <if match=\"any\" variable=\"issued\">\n          <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n            <date-part name=\"year\"/>\n          </date>\n        </if>\n      </choose>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text variable=\"title\" font-style=\"italic\" suffix=\".\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter\">\n          <text variable=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"editor\"/>\n            </group>\n          </group>\n          <group delimiter=\", \" prefix=\", \" suffix=\".\">\n            <text macro=\"publisher\"/>\n            <group>\n              <label variable=\"page\" suffix=\" \" form=\"short\" strip-periods=\"true\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n          <text macro=\"edition\"/>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <text variable=\"title\" prefix=\" \" suffix=\". \"/>\n          <group delimiter=\", \" suffix=\".\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"event\" font-style=\"italic\"/>\n            <text macro=\"publisher\"/>\n            <date variable=\"issued\">\n              <date-part name=\"month\"/>\n              <date-part name=\"day\" prefix=\" \"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog post speech\" match=\"any\">\n          <text variable=\"title\" prefix=\" \" suffix=\".\"/>\n          <text macro=\"access\" prefix=\" \"/>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \" prefix=\" \">\n            <group delimiter=\" \">\n              <text variable=\"title\" prefix=\" \" font-style=\"italic\"/>\n              <text variable=\"genre\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n          </group>\n        </else-if>\n        <else-if type=\"manuscript report\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <text variable=\"title\" font-style=\"italic\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"publisher-place\"/>\n            <date form=\"text\" variable=\"issued\"/>\n          </group>\n        </else-if>\n        <else>\n          <text variable=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" delimiter=\": \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"