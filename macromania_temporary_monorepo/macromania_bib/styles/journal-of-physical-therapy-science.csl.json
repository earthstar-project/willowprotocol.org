"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Physical Therapy Science</title>\n    <id>http://www.zotero.org/styles/journal-of-physical-therapy-science</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-physical-therapy-science\" rel=\"self\"/>\n    <link href=\"https://www.jstage.jst.go.jp/jstage/pub/jpts/pdf/regulation_en.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Libor Ansorge</name>\n      <email>libor.ansorge@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0915-5287</issn>\n    <eissn>2187-5626</eissn>\n    <updated>2015-06-05T17:31:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"page\" form=\"short\">p</term>\n      <term name=\"number-of-pages\" form=\"short\">\n        <single>p</single>\n        <multiple>pp</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"accessed\">\n        <text term=\"accessed\" suffix=\" \" text-case=\"capitalize-first\"/>\n        <date variable=\"accessed\">\n          <date-part name=\"month\" form=\"short\" suffix=\". \"/>\n          <date-part name=\"day\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"chapter paper-conference report\" match=\"any\">\n        <label variable=\"page\" form=\"short\" suffix=\" \"/>\n      </if>\n    </choose>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"numpage\">\n    <label variable=\"number-of-pages\" form=\"short\" suffix=\" \"/>\n    <text variable=\"number-of-pages\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\": \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\" et-al-min=\"4\" et-al-use-first=\"3\" delimiter-precedes-et-al=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\" suffix=\", \"/>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <group delimiter=\" \">\n          <text variable=\"container-title\"/>\n          <text variable=\"event\"/>\n        </group>\n      </if>\n      <else-if type=\"article article-magazine article-newspaper article-journal\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \" suffix=\", \">\n          <text variable=\"title\"/>\n          <group delimiter=\" \">\n            <text variable=\"collection-title\"/>\n            <text variable=\"collection-number\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"thesis\">\n        <group delimiter=\" \" suffix=\", \">\n          <text variable=\"title\"/>\n          <text variable=\"genre\" suffix=\")\" prefix=\"(\"/>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \" suffix=\". \">\n          <text variable=\"title\"/>\n          <group delimiter=\" \">\n            <text variable=\"collection-title\"/>\n            <text variable=\"collection-number\"/>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\" suffix=\".\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout suffix=\")\" delimiter=\", \" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" delimiter-precedes-et-al=\"always\" second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout>\n      <group vertical-align=\"sup\">\n        <text variable=\"citation-number\" suffix=\") \"/>\n      </group>\n      <choose>\n        <if type=\"webpage\">\n          <text macro=\"title\"/>\n          <text variable=\"URL\"/>\n          <text macro=\"accessed\" prefix=\" (\" suffix=\")\"/>\n        </if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"author\"/>\n            <text macro=\"title\"/>\n            <choose>\n              <if type=\"bill graphic legal_case legislation motion_picture report song\" match=\"any\">\n                <group delimiter=\" \">\n                  <text macro=\"edition\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n                <text macro=\"container-title\" suffix=\", \"/>\n                <group delimiter=\", \">\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                  <group delimiter=\": \">\n                    <text variable=\"volume\"/>\n                    <text macro=\"page\"/>\n                  </group>\n                </group>\n              </if>\n              <else-if type=\"book\">\n                <group delimiter=\" \">\n                  <text macro=\"edition\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n                <text macro=\"container-title\" suffix=\", \"/>\n                <group delimiter=\", \">\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                  <text macro=\"numpage\"/>\n                </group>\n              </else-if>\n              <else-if type=\"chapter paper-conference\" match=\"any\">\n                <text macro=\"editor\"/>\n                <text macro=\"container-title\" suffix=\". \"/>\n                <group delimiter=\" \">\n                  <text macro=\"edition\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n                <group delimiter=\", \">\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                  <text macro=\"page\"/>\n                </group>\n              </else-if>\n              <else-if type=\"thesis\">\n                <text macro=\"publisher\"/>\n                <group delimiter=\", \">\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                  <text macro=\"numpage\"/>\n                </group>\n              </else-if>\n              <else>\n                <text macro=\"container-title\" suffix=\", \"/>\n                <text macro=\"publisher\"/>\n                <group delimiter=\", \">\n                  <date variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                  <group delimiter=\": \">\n                    <text variable=\"volume\"/>\n                    <text macro=\"page\"/>\n                  </group>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"