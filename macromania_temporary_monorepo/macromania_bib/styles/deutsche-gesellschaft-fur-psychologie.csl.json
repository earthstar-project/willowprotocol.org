"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"de-DE\">\n  <info>\n    <title>Deutsche Gesellschaft f√ºr Psychologie 5. Auflage (Deutsch)</title>\n    <title-short>DGP 5. Auflage</title-short>\n    <id>http://www.zotero.org/styles/deutsche-gesellschaft-fur-psychologie</id>\n    <link href=\"http://www.zotero.org/styles/deutsche-gesellschaft-fur-psychologie\" rel=\"self\"/>\n    <link href=\"https://github.com/citation-style-language/styles/pull/432\" rel=\"documentation\"/>\n    <link href=\"https://doi.org/10.1026/02954-000\" rel=\"documentation\"/>\n    <author>\n      <name>Daniel Hirsbrunner</name>\n      <email>dhirsbrunner@gmx.ch</email>\n    </author>\n    <contributor>\n      <name>Michael Zaugg</name>\n    </contributor>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <contributor>\n      <name>Tobias Stocker</name>\n      <email>tobistocker@gmail.com</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"psychology\"/>\n    <summary>Style for the German society for psychology, based on the 5th style manual (2019).</summary>\n    <updated>2024-03-08T08:13:34+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"retrieved\">Zugriff am</term>\n      <term name=\"original-work-published\">Original erschienen</term>\n      <term name=\"no date\" form=\"short\">n. d.</term>\n    </terms>\n    <date form=\"text\">\n      <date-part name=\"year\"/>\n      <date-part name=\"month\" prefix=\", \"/>\n      <date-part name=\"day\" prefix=\" \"/>\n    </date>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <names variable=\"editor translator\" delimiter=\", \" suffix=\", \">\n          <name and=\"symbol\" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" prefix=\" (\" text-case=\"capitalize-first\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <name and=\"symbol\" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"director\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"director\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-newspaper\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text value=\"https://doi.org/\"/>\n            <text variable=\"DOI\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <choose>\n              <if type=\"article-journal\" match=\"none\">\n                <group suffix=\". \">\n                  <text term=\"retrieved\" suffix=\" \"/>\n                  <date variable=\"accessed\">\n                    <date-part name=\"day\" suffix=\".\"/>\n                    <date-part name=\"month\" form=\"numeric\" suffix=\".\"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n                <group delimiter=\": \">\n                  <text term=\"available at\" text-case=\"capitalize-first\"/>\n                  <text variable=\"URL\"/>\n                </group>\n              </if>\n            </choose>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\".\"/>\n        <group prefix=\" \" suffix=\".\" delimiter=\" \">\n          <text variable=\"genre\"/>\n          <group delimiter=\" \">\n            <text term=\"issue\" form=\"short\"/>\n            <text variable=\"number\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legislation report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text variable=\"collection-title\" prefix=\" (\" suffix=\")\"/>\n      </else-if>\n      <else-if type=\"motion_picture\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text variable=\"medium\" prefix=\" [\" suffix=\"]\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n        <text variable=\"collection-title\" prefix=\" (\" suffix=\")\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"article-newspaper\" match=\"none\">\n        <choose>\n          <if type=\"report thesis\" match=\"any\">\n            <group delimiter=\": \">\n              <text variable=\"publisher-place\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </if>\n          <else>\n            <choose>\n              <if variable=\"event\" match=\"none\">\n                <text variable=\"genre\" suffix=\", \"/>\n              </if>\n            </choose>\n            <group delimiter=\": \">\n              <text variable=\"publisher-place\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <choose>\n          <if variable=\"genre\" match=\"none\">\n            <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <text variable=\"event\"/>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              <text term=\"presented at\"/>\n              <text variable=\"event\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <group prefix=\" (\" suffix=\").\">\n      <choose>\n        <if variable=\"issued\">\n          <group delimiter=\", \">\n            <group>\n              <date variable=\"issued\" form=\"text\" date-parts=\"year\"/>\n              <text variable=\"year-suffix\"/>\n            </group>\n            <choose>\n              <if type=\"bill book graphic legal_case motion_picture report song article-journal chapter paper-conference\" match=\"none\">\n                <date variable=\"issued\">\n                  <date-part name=\"month\"/>\n                  <date-part name=\"day\" form=\"numeric\" prefix=\" \"/>\n                </date>\n              </if>\n            </choose>\n          </group>\n        </if>\n        <else>\n          <group delimiter=\"-\">\n            <text term=\"no date\" form=\"short\"/>\n            <text variable=\"year-suffix\"/>\n          </group>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <group>\n          <date variable=\"issued\" form=\"text\" date-parts=\"year\"/>\n          <text variable=\"year-suffix\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\"-\">\n          <text term=\"no date\" form=\"short\"/>\n          <text variable=\"year-suffix\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group prefix=\", \" delimiter=\", \">\n          <group>\n            <text variable=\"volume\" font-style=\"italic\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture report song chapter paper-conference\" match=\"any\">\n        <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n          <text macro=\"edition\"/>\n          <group>\n            <text term=\"volume\" form=\"long\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1-\"/>\n          </group>\n          <group>\n            <text term=\"volume\" form=\"long\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n          <group>\n            <label variable=\"page\" form=\"short\" suffix=\" \"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <group delimiter=\" \" prefix=\", \">\n          <label text-case=\"capitalize-first\" variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"issued-citation\">\n    <choose>\n      <if variable=\"issued\">\n        <group delimiter=\"/\">\n          <choose>\n            <if variable=\"original-date\">\n              <date variable=\"original-date\">\n                <date-part name=\"year\"/>\n              </date>\n            </if>\n          </choose>\n          <text macro=\"issued-year\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\"-\">\n          <text term=\"no date\" form=\"short\"/>\n          <text variable=\"year-suffix\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"original-date\">\n    <date variable=\"original-date\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publication-history\">\n    <group prefix=\"(\" suffix=\")\">\n      <choose>\n        <if variable=\"references\">\n          <text variable=\"references\"/>\n        </if>\n        <else>\n          <choose>\n            <if variable=\"original-date\">\n              <group delimiter=\":\">\n                <group delimiter=\" \">\n                  <text term=\"original-work-published\"/>\n                  <text macro=\"original-date\" prefix=\" \"/>\n                </group>\n                <choose>\n                  <if variable=\"translator original-title\" match=\"all\">\n                    <text variable=\"original-title\" prefix=\" \"/>\n                  </if>\n                </choose>\n              </group>\n            </if>\n          </choose>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"director\">\n    <names variable=\"director\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <citation et-al-min=\"6\" et-al-use-first=\"1\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-year\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued-citation\"/>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography delimiter-precedes-et-al=\"never\" et-al-min=\"7\" et-al-use-first=\"6\" entry-spacing=\"0\" line-spacing=\"2\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-year\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group suffix=\".\">\n        <text macro=\"author\" suffix=\".\"/>\n        <text macro=\"issued\" suffix=\" \"/>\n        <group delimiter=\". \">\n          <text macro=\"title\"/>\n          <group>\n            <text macro=\"container-contributors\"/>\n            <text macro=\"secondary-contributors\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </group>\n          </group>\n        </group>\n        <text macro=\"locators\"/>\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n      <text macro=\"publication-history\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"