"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Acta Ornithologica</title>\n    <id>http://www.zotero.org/styles/acta-ornithologica</id>\n    <link href=\"http://www.zotero.org/styles/acta-ornithologica\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/biologia\" rel=\"template\"/>\n    <link href=\"https://miiz.waw.pl/periodicals/acta-ornithologica/acta_ornithologica.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"zoology\"/>\n    <issn>0001-6454</issn>\n    <eissn>1734-8471</eissn>\n    <updated>2020-08-18T08:09:50+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\"(\" suffix=\")\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\". \" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if match=\"any\" variable=\"original-title\">\n        <text variable=\"original-title\" prefix=\"[\" suffix=\"]\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n      <number variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" collapse=\"year\" et-al-min=\"3\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"year\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\" \">\n        <names variable=\"author\" suffix=\", \">\n          <name and=\"symbol\" delimiter=\" \" name-as-sort-order=\"all\" sort-separator=\", \" form=\"short\"/>\n        </names>\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year\" sort=\"ascending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"year\"/>\n        <choose>\n          <if type=\"thesis\">\n            <text macro=\"title\" font-style=\"italic\"/>\n            <text variable=\"publisher\" suffix=\", \"/>\n            <text variable=\"publisher-place\"/>\n          </if>\n          <else-if type=\"book\">\n            <group delimiter=\". \">\n              <text macro=\"title\" font-style=\"italic\"/>\n              <text macro=\"volume\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"container-title\"/>\n              <group delimiter=\", \">\n                <text variable=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\">\n            <text macro=\"title\"/>\n            <text variable=\"event\" prefix=\" \" suffix=\", \" font-style=\"italic\"/>\n            <text variable=\"volume\" suffix=\", \"/>\n            <text variable=\"page\" suffix=\".\"/>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"container-title\"/>\n              <group delimiter=\": \">\n                <text term=\"available at\" text-case=\"capitalize-first\"/>\n                <text variable=\"URL\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text variable=\"container-title\" form=\"short\"/>\n                <group delimiter=\": \">\n                  <text variable=\"volume\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"