"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-GB\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Oral Diseases</title>\n    <id>http://www.zotero.org/styles/oral-diseases</id>\n    <link href=\"http://www.zotero.org/styles/oral-diseases\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/heredity\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/%28ISSN%291601-0825/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <issn>1354-523X</issn>\n    <eissn>1601-0825</eissn>\n    <summary>style for \"Oral Diseases\" - details based on published articles.</summary>\n    <updated>2012-09-14T21:22:32+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"et-al\">et al</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\"\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" text-case=\"capitalize-first\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"year\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\" suffix=\" \"/>\n      <date variable=\"issued\" prefix=\"(\" suffix=\").\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"edition\" prefix=\". \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"thesis\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n            <text variable=\"genre\" prefix=\" \"/>\n            <text prefix=\" \" variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\".\">\n            <group prefix=\" In:\">\n              <names variable=\"editor translator\">\n                <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\" prefix=\" \"/>\n                <label form=\"short\" prefix=\", \"/>\n              </names>\n              <group delimiter=\". \" prefix=\" \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text macro=\"edition\"/>\n                <group suffix=\".\">\n                  <text macro=\"publisher\" prefix=\" \"/>\n                  <label variable=\"page\" form=\"short\" prefix=\", \" suffix=\" \"/>\n                  <text variable=\"page\" suffix=\".\"/>\n                </group>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\" strip-periods=\"true\"/>\n            <text variable=\"volume\" font-weight=\"bold\" prefix=\" \"/>\n            <text variable=\"page\" prefix=\": \"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"