"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"fr-FR\">\n  <info>\n    <title>Cahiers d'ethnomusicologie (Français)</title>\n    <id>http://www.zotero.org/styles/cahiers-d-ethnomusicologie</id>\n    <link href=\"http://www.zotero.org/styles/cahiers-d-ethnomusicologie\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/iso690-author-date-en\" rel=\"template\"/>\n    <link href=\"http://www.adem-geneve.com/fr/cahiers-d-ethnomusicologie-contribution\" rel=\"documentation\"/>\n    <author>\n      <name>Victor A. Stoichita</name>\n      <email>victor.stoichita@cnrs.fr</email>\n      <uri>http://svictor.net</uri>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <issn>1662-372X</issn>\n    <eissn>2235-7688</eissn>\n    <summary>Style based on ISO 690:2010(F), V2 adapted from a template by L.\n    Mellifluo, R. Grolimund, M. Hardegger and P.-A. Giraud.</summary>\n    <updated>2016-09-24T23:32:13+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"anonymous\">Anon.</term>\n      <term name=\"no date\">[sans date]</term>\n      <term name=\"in\">in</term>\n      <term name=\"online\">en ligne</term>\n      <term name=\"accessed\">consulté le</term>\n      <term name=\"retrieved\">disponible</term>\n      <term name=\"from\">à l'adresse</term>\n      <term name=\"editor\" form=\"short\">\n        <single>dir</single>\n        <multiple>dirs.</multiple>\n      </term>\n      <term name=\"page\" form=\"short\">p.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"first\" initialize=\"false\" sort-separator=\" \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" name-as-sort-order=\"all\" form=\"short\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"responsibility\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <choose>\n          <if variable=\"author\">\n            <text macro=\"author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n          <else>\n            <text macro=\"translator\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"anonymous\" text-case=\"uppercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-citation\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <names variable=\"author\">\n          <name form=\"short\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text term=\"anonymous\" text-case=\"uppercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"container-responsibility\">\n    <choose>\n      <if variable=\"container-author editor translator\" match=\"any\">\n        <choose>\n          <if variable=\"container-author\">\n            <text macro=\"container-author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n          <else>\n            <text macro=\"translator\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"anonymous\" text-case=\"uppercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis map motion_picture song manuscript\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"paper-conference speech chapter article-journal article-magazine article-newspaper post-weblog post webpage broadcast\" match=\"any\">\n        <text variable=\"title\" prefix=\"« \" suffix=\" ». \"/>\n        <choose>\n          <if variable=\"container-author editor translator\" match=\"any\">\n            <text macro=\"container-responsibility\"/>\n            <choose>\n              <if variable=\"container-title event\" match=\"any\">\n                <text value=\", \"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"event\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"report\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"patent\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <text variable=\"number\"/>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\"/>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"map\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n          <else>\n            <text value=\"carte\" prefix=\"[\" suffix=\"]\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"genre\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <text variable=\"edition\" form=\"long\"/>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if type=\"patent manuscript article-newspaper broadcast motion_picture song\" match=\"any\">\n        <text variable=\"publisher-place\"/>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\"/>\n          </if>\n          <else>\n            <text value=\"s.l.\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue\">\n    <group>\n      <text variable=\"volume\"/>\n      <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n      <text variable=\"page\" prefix=\"&#160;:&#160;\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"broadcast motion_picture song report\" match=\"any\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\"/>\n          </if>\n          <else>\n            <text value=\"s.n.\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" prefix=\"&#160;\"/>\n            <date-part name=\"month\" prefix=\"&#160;\"/>\n            <date-part name=\"year\" prefix=\"&#160;\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\", \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <text variable=\"page\" prefix=\"&#160;:&#160;\"/>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"URL\">\n        <group>\n          <text term=\"retrieved\" suffix=\" \" text-case=\"capitalize-first\"/>\n          <text term=\"from\" suffix=\"&#160;: \"/>\n          <text variable=\"URL\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\":&#160;\">\n      <text variable=\"archive\"/>\n      <text macro=\"archive_location\"/>\n    </group>\n  </macro>\n  <macro name=\"archive_location\">\n    <choose>\n      <if variable=\"archive_location\">\n        <text variable=\"archive_location\"/>\n      </if>\n      <else>\n        <text variable=\"call-number\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"note\">\n    <text variable=\"note\"/>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\" year-suffix-delimiter=\", \" after-collapse-delimiter=\"&#160;; \">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"&#160;; \">\n      <group delimiter=\"&#160;\">\n        <text macro=\"author-citation\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <group prefix=\"&#160;: \">\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"\">\n    <sort>\n      <key macro=\"responsibility\"/>\n      <key macro=\"year-date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <group display=\"block\">\n        <text macro=\"responsibility\"/>\n      </group>\n      <group display=\"left-margin\">\n        <text macro=\"year-date\"/>\n      </group>\n      <group display=\"right-inline\">\n        <choose>\n          <if type=\"book map\" match=\"any\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-place\" suffix=\"&#160;: \"/>\n            <text macro=\"publisher\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n          </if>\n          <else-if type=\"article-journal article-magazine\" match=\"any\">\n            <text macro=\"title\" suffix=\" \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"issue\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else-if type=\"article-newspaper\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-place\" suffix=\", \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else-if type=\"chapter entry entry-dictionary entry-encyclopedia\" match=\"any\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-place\" suffix=\"&#160;: \"/>\n            <text macro=\"publisher\" suffix=\"\"/>\n            <text macro=\"collection\" suffix=\" \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else-if type=\"speech\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"publisher-place\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else-if type=\"paper-conference\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"publisher-place\" suffix=\"&#160;: \"/>\n            <text macro=\"publisher\" suffix=\"\"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else-if type=\"thesis\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"publisher-place\" suffix=\"&#160;: \"/>\n            <text macro=\"publisher\" suffix=\"\"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else-if type=\"post-weblog post webpage\" match=\"any\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else-if type=\"broadcast motion_picture song\" match=\"any\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"medium\" suffix=\". \"/>\n            <text macro=\"publisher-place\" suffix=\"&#160;: \"/>\n            <text macro=\"publisher\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else-if type=\"report\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text variable=\"number\" suffix=\" : \"/>\n            <text macro=\"publisher-place\" suffix=\". \"/>\n            <text macro=\"publisher\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else-if type=\"manuscript\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-place\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else-if type=\"patent\">\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"number\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"publisher-place\" suffix=\". \"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else-if>\n          <else>\n            <text macro=\"title\" suffix=\". \"/>\n            <text macro=\"medium\" suffix=\". \"/>\n            <text macro=\"genre\" suffix=\". \"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text macro=\"edition\" suffix=\". \"/>\n            <text macro=\"publisher-place\" suffix=\"&#160;: \"/>\n            <text macro=\"publisher\" suffix=\"\"/>\n            <text macro=\"accessed\" suffix=\". \"/>\n            <text macro=\"collection\" suffix=\". \"/>\n            <text macro=\"page\" suffix=\". \"/>\n            <text macro=\"url\" suffix=\". \"/>\n          </else>\n        </choose>\n        <text macro=\"archive\"/>\n        <text macro=\"archive_location\" suffix=\". \"/>\n        <text macro=\"note\" suffix=\". \"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"