"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Lithic Studies</title>\n    <id>http://www.zotero.org/styles/journal-of-lithic-studies</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-lithic-studies\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/permafrost-and-periglacial-processes\" rel=\"template\"/>\n    <link href=\"http://journals.ed.ac.uk/lithicstudies/article/view/1403/1951\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geology\"/>\n    <issn>2055-0472</issn>\n    <updated>2022-05-31T13:46:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </if>\n      <else-if type=\"webpage post-weblog post\" match=\"any\">\n        <group delimiter=\": \">\n          <text variable=\"URL\" prefix=\"URL: \"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label plural=\"never\" variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography delimiter-precedes-last=\"never\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author\"/>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n        <choose>\n          <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n              <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <text variable=\"number-of-pages\"/>\n                <label plural=\"never\" variable=\"number-of-pages\" form=\"short\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\", \" prefix=\" \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </group>\n                <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"collection-title\" suffix=\".\"/>\n              <text variable=\"event\" suffix=\".\"/>\n              <group delimiter=\": \" suffix=\".\">\n                <text macro=\"publisher\" prefix=\" \"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\", \" prefix=\" \" suffix=\".\">\n              <text macro=\"title\"/>\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\", \" prefix=\" \" suffix=\".\">\n              <group>\n                <text macro=\"title\" prefix=\" \"/>\n                <text macro=\"editor\" prefix=\" \"/>\n              </group>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group delimiter=\": \" prefix=\" \">\n                <group>\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"