"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\" page-range-format=\"expanded\">\n  <info>\n    <title>King's College London - Harvard</title>\n    <id>http://www.zotero.org/styles/harvard-kings-college-london</id>\n    <link href=\"http://www.zotero.org/styles/harvard-kings-college-london\" rel=\"self\"/>\n    <link href=\"http://www.kcl.ac.uk/library/help/plagiarism/citing/index.aspx\" rel=\"documentation\"/>\n    <author>\n      <name>William J Giddings</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <summary>Harvard author-date style created for King's College London</summary>\n    <updated>2018-02-15T16:23:12+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <!-- format EDITOR entry -->\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"symbol\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <!-- format ANON entry -->\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <!-- format AUTHOR entry -->\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <!-- set text-case=\"capitalize-first\" to \"uppercase\" if names required  in CAPS -->\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\".)\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <!-- format AUTHOR SHORT entry -->\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <!-- format NET ACCESS entry -->\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text term=\"online\" prefix=\"[\" suffix=\"].\"/>\n        <text value=\" Available from: \"/>\n        <text variable=\"URL\"/>\n        <group prefix=\" (\" delimiter=\" \" suffix=\")\">\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <!-- format TITLE entry -->\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book webpage graphic\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <!-- format PUBLISHER entry -->\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\" suffix=\".\"/>\n  </macro>\n  <!-- format DATE entry -->\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <!-- format EDITION entry -->\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <!-- format PAGE entry -->\n  <macro name=\"pages\">\n    <group>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <!-- format ONLINE entry -->\n  <macro name=\"online\">\n    <choose>\n      <if variable=\"DOI\">\n        <text value=\"[Online]\"/>\n      </if>\n    </choose>\n  </macro>\n  <!-- format ISSUE entry -->\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-newspaper paper-conference broadcast\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" prefix=\" \"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <!-- format VOLUME entry -->\n  <macro name=\"volume\">\n    <group>\n      <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\". \"/>\n      <number variable=\"volume\" form=\"numeric\"/>\n    </group>\n    <group>\n      <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\", \"/>\n      <text term=\"volume\" form=\"short\" prefix=\" \" suffix=\".\" plural=\"true\"/>\n    </group>\n  </macro>\n  <!-- format CITATION entry -->\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <!-- format BIBLIOGRAPHY entry -->\n  <bibliography hanging-indent=\"true\" et-al-min=\"3\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n      <choose>\n        <!-- format differint types of source -->\n        <if type=\"thesis\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text macro=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"genre\" suffix=\" thesis\"/>\n            <text macro=\"online\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"webpage\">\n          <group prefix=\" \" delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"online\"/>\n          </group>\n          <!--\n\t      <group prefix=\" \" delimiter=\". \" suffix=\".\">\n\t      <date variable=\"issued\">\n\t      <date-part name=\"day\" suffix=\" \"/>\n\t      <date-part name=\"month\" suffix=\" \"/>\n\t      <date-part name=\"year\"/>\n\t      </date>\n\t      <text variable=\"container-title\"/>\n\t      </group>\n\t  -->\n        </else-if>\n        <else-if type=\"article-journal article-newspaper broadcast personal_communication thesis webpage\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <choose>\n            <if variable=\"author\" match=\"any\">\n              <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\".\"/>\n            </if>\n          </choose>\n          <text macro=\"online\" prefix=\" \"/>\n          <group prefix=\" \" suffix=\".\">\n            <group>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\" (\" suffix=\"), \"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"book graphic\" match=\"any\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text variable=\"note\"/>\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <text variable=\"collection-number\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"online\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"title\" quotes=\"true\" suffix=\",\"/>\n            <text term=\"in\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text macro=\"edition\" prefix=\" \"/>\n            <text macro=\"online\" suffix=\".\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"publisher\" suffix=\".\"/>\n            <label variable=\"page\" form=\"short\"/>\n            <text macro=\"pages\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\". \" prefix=\" \" delimiter=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text variable=\"container-title\" suffix=\" \"/>\n          <group suffix=\".\" delimiter=\" \">\n            <group>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\" (\" suffix=\")\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group>\n              <label variable=\"locator\" form=\"short\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"