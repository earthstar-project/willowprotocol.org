"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"es-ES\">\n  <info>\n    <title>IICA-CATIE (Español)</title>\n    <id>http://www.zotero.org/styles/iica-catie</id>\n    <link href=\"http://www.zotero.org/styles/iica-catie\" rel=\"self\"/>\n    <link href=\"http://repiica.iica.int/docs/B4013e/B4013e.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Marcelo Wall</name>\n      <email>wa.marc@yahoo.com</email>\n    </author>\n    <contributor>\n      <name>Daniela Rodriguez</name>\n    </contributor>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <summary>Style for the Inter-American Institute for Cooperation on Agriculture (IICA) and the Tropical Agriculture Research and Higher Education Center (CATIE), 5th edition (2016).</summary>\n    <updated>2017-11-13T20:58:16+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"es\">\n    <terms>\n      <term name=\"accessed\">consultado</term>\n      <term name=\"in\">in</term>\n    </terms>\n  </locale>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n          <text variable=\"title\" text-case=\"capitalize-first\"/>\n        </if>\n        <else>\n          <text variable=\"title\"/>\n        </else>\n      </choose>\n      <choose>\n        <if match=\"any\" variable=\"URL\">\n          <choose>\n            <if type=\"webpage post-weblog\" match=\"none\">\n              <text term=\"online\" prefix=\"(\" suffix=\")\"/>\n            </if>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter=\"; \" delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"collection-title\">\n    <text variable=\"collection-title\"/>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <text variable=\"container-title\"/>\n      </if>\n      <else>\n        <text variable=\"genre\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name sort-separator=\", \" initialize-with=\"\" delimiter=\"; \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"editor translator\">\n      <name sort-separator=\", \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\"; \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"volume\"/>\n        <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture report song chapter paper-conference article\" match=\"any\">\n        <group delimiter=\". \">\n          <group suffix=\".\" delimiter=\", \">\n            <text macro=\"publisher-place\"/>\n            <text macro=\"publisher\"/>\n            <choose>\n              <if variable=\"volume\">\n                <group>\n                  <label variable=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n              </if>\n              <else>\n                <text variable=\"number-of-volumes\" suffix=\" v.\"/>\n              </else>\n            </choose>\n            <group delimiter=\", \" prefix=\"(\" suffix=\").\">\n              <text variable=\"collection-title\"/>\n              <text variable=\"collection-number\" prefix=\"no. \"/>\n            </group>\n          </group>\n          <choose>\n            <if variable=\"page\">\n              <group delimiter=\" \">\n                <label plural=\"never\" variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\" \">\n                <text variable=\"number-of-pages\"/>\n                <label variable=\"page\" form=\"short\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-pages\"/>\n      <label plural=\"never\" variable=\"page\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if variable=\"publisher\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <text value=\"s.e.\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if variable=\"publisher-place\">\n        <text variable=\"publisher-place\" text-case=\"capitalize-first\"/>\n      </if>\n      <else>\n        <text value=\"s.l.\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\" DOI: https://doi.org/\"/>\n      </if>\n      <else>\n        <choose>\n          <if type=\"webpage post-weblog\" match=\"any\">\n            <text value=\"en línea, sitio web\" prefix=\" (\" suffix=\")\"/>\n          </if>\n        </choose>\n        <group delimiter=\". \" prefix=\". \">\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\"/>\n              <date-part name=\"month\" form=\"short\" prefix=\" \"/>\n              <date-part name=\"year\" prefix=\" \"/>\n            </date>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\"/>\n          <label variable=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name sort-separator=\", \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\"; \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\". \"/>\n      <substitute>\n        <names variable=\"editor\" suffix=\" ed.\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-citation\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <choose>\n            <if variable=\"author\">\n              <text macro=\"author-citation\"/>\n            </if>\n            <else>\n              <text macro=\"title\"/>\n            </else>\n          </choose>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"_____\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year-date\" sort=\"ascending\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"bill book article\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"author-short\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\"; \">\n              <text macro=\"editor\"/>\n              <text macro=\"translator\"/>\n            </group>\n            <text macro=\"locators\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter\">\n          <group delimiter=\". \">\n            <text macro=\"author-short\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n            <group font-style=\"normal\" delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n              <text macro=\"container-author\" font-style=\"italic\"/>\n            </group>\n            <text macro=\"container-title\"/>\n            <text macro=\"translator\"/>\n            <text macro=\"locators\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"author-short\" suffix=\". \"/>\n          <text macro=\"year-date\" suffix=\". \"/>\n          <text macro=\"title\" suffix=\". \"/>\n          <text variable=\"genre\" suffix=\". \"/>\n          <text variable=\"page\" prefix=\"Págs. \" suffix=\". \"/>\n          <text macro=\"publisher-place\" suffix=\", \"/>\n          <text macro=\"publisher\" suffix=\". \"/>\n          <text macro=\"number-of-pages\"/>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <group delimiter=\". \">\n            <text macro=\"author-short\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n            <group>\n              <group delimiter=\", \">\n                <text macro=\"container-title\"/>\n                <text macro=\"publisher-place\"/>\n                <text macro=\"publisher\"/>\n              </group>\n              <date variable=\"issued\" prefix=\"; \">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\" form=\"short\" prefix=\" \"/>\n              </date>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"speech paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"author-short\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n              <text macro=\"container-title\"/>\n            </group>\n            <text macro=\"editor\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher-place\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <group delimiter=\" \">\n              <label plural=\"never\" variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-magazine\" match=\"any\">\n          <text macro=\"author-short\" suffix=\". \"/>\n          <text macro=\"year-date\" suffix=\". \"/>\n          <group delimiter=\" \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"note\" prefix=\"(En \" suffix=\")\" text-case=\"lowercase\"/>\n          </group>\n          <group suffix=\".\">\n            <text variable=\"container-title\" suffix=\" \"/>\n            <text macro=\"collection-title\" prefix=\"(Serie \" suffix=\") \"/>\n            <text macro=\"locators\"/>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else-if>\n        <else-if type=\"map\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"author-short\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"page\" prefix=\"Págs. \"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher-place\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <text variable=\"scale\"/>\n            <text macro=\"number-of-pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"song motion_picture broadcast musical_score graphic\" match=\"any\">\n          <text macro=\"author-short\" suffix=\". \"/>\n          <text macro=\"year-date\" suffix=\". \"/>\n          <group suffix=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"note\" prefix=\" (\" suffix=\")\"/>\n          </group>\n          <group delimiter=\". \" suffix=\".\">\n            <text macro=\"editor\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\", \">\n              <text variable=\"publisher-place\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <text variable=\"medium\" text-case=\"capitalize-first\"/>\n            <text macro=\"collection-title\" prefix=\"(\" suffix=\")\"/>\n            <text macro=\"container-title\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <text macro=\"author-short\" suffix=\". \"/>\n          <choose>\n            <if variable=\"issued\">\n              <date variable=\"issued\" suffix=\". \">\n                <date-part name=\"year\"/>\n              </date>\n            </if>\n            <else>\n              <date variable=\"accessed\" suffix=\". \">\n                <date-part name=\"year\"/>\n              </date>\n            </else>\n          </choose>\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"container-title\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article report\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"author-short\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            <text macro=\"title\"/>\n            <text variable=\"volume\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher-place\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <group delimiter=\" \">\n              <text variable=\"page\"/>\n              <label plural=\"never\" variable=\"page\" form=\"short\"/>\n            </group>\n            <text variable=\"genre\" prefix=\"(\" suffix=\")\"/>\n            <group delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <text term=\"number\" form=\"short\"/>\n              <number variable=\"collection-number\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"author-short\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\". \"/>\n            <text macro=\"editor\" suffix=\". \"/>\n            <group delimiter=\", \" prefix=\" \">\n              <text macro=\"publisher-place\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <group delimiter=\" \">\n              <text variable=\"page\" suffix=\". \"/>\n              <label plural=\"never\" variable=\"page\" form=\"short\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"