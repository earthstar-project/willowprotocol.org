"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Oil Palm Research</title>\n    <title-short>JOPR</title-short>\n    <id>http://www.zotero.org/styles/journal-of-oil-palm-research</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-oil-palm-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/mary-ann-liebert-harvard\" rel=\"template\"/>\n    <link href=\"http://jopr.mpob.gov.my/guide-t/\" rel=\"documentation\"/>\n    <link href=\"http://jopr.mpob.gov.my/wp-content/uploads/2021/03/JOPR-Vol-331-March-2021-dragged-16.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1511-2780</issn>\n    <updated>2021-11-03T09:33:07+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\">accessed on</term>\n    </terms>\n  </locale>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\"; \" and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\" \" name-as-sort-order=\"all\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\" \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"pages\">\n    <group delimiter=\" \">\n      <label plural=\"never\" variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author-short\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author\" names-min=\"3\" names-use-first=\"1\"/>\n      <key macro=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\">\n    <sort>\n      <key macro=\"author-short\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"author\" names-min=\"3\" names-use-first=\"1\"/>\n      <key macro=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"issued\" prefix=\"(\" suffix=\").\"/>\n        <choose>\n          <if type=\"article article-magazine article-newspaper article-journal review\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\" suffix=\".\"/>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n                <group delimiter=\": \">\n                  <group font-style=\"italic\">\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"chapter\" match=\"any\">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" prefix=\"In \" suffix=\",\" text-case=\"title\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\" suffix=\",\"/>\n            <text macro=\"pages\"/>\n          </else-if>\n          <else-if type=\"thesis\">\n            <text variable=\"title\" suffix=\".\"/>\n            <group delimiter=\" \">\n              <text variable=\"genre\" suffix=\".\"/>\n              <text variable=\"number\"/>\n            </group>\n            <text variable=\"publisher\"/>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text variable=\"genre\"/>\n              <group delimiter=\", \">\n                <text variable=\"URL\"/>\n                <group>\n                  <text term=\"accessed\"/>\n                  <date form=\"text\" variable=\"accessed\"/>\n                </group>\n              </group>\n              <group delimiter=\" \">\n                <text variable=\"number-of-pages\"/>\n                <label variable=\"number-of-pages\" form=\"short\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\"/>\n              <group delimiter=\", \">\n                <text variable=\"URL\"/>\n                <group delimiter=\" \">\n                  <text term=\"accessed\"/>\n                  <date form=\"text\" variable=\"accessed\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <text variable=\"title\"/>\n            <text macro=\"publisher\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"