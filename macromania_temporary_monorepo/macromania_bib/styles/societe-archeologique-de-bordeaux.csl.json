"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"note\" demote-non-dropping-particle=\"never\" default-locale=\"fr-FR\" page-range-format=\"expanded\">\n  <info>\n    <title>Société Archéologique de Bordeaux (Français)</title>\n    <title-short>SAB</title-short>\n    <id>http://www.zotero.org/styles/societe-archeologique-de-bordeaux</id>\n    <link href=\"http://www.zotero.org/styles/societe-archeologique-de-bordeaux\" rel=\"self\"/>\n    <link href=\"http://www.societe-archeologique-bordeaux.fr/publications/recommandations-aux-auteurs.html\" rel=\"documentation\"/>\n    <author>\n      <name>Hélène Réveillas</name>\n      <email>hreveillas@bordeaux-metropole.fr</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <summary>Style reprenant les normes diffusées par la SAB</summary>\n    <updated>2016-11-09T08:31:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"editor\" form=\"short\">dir.</term>\n      <term name=\"in\">dans</term>\n      <term name=\"anonymous\">anonyme</term>\n      <term name=\"anonymous\" form=\"short\">anon.</term>\n      <term name=\"accessed\">consulté le</term>\n      <term name=\"no date\">sans date</term>\n      <term name=\"no date\" form=\"short\">s.&#160;d.</term>\n      <term name=\"sub-verbo\">s.u.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\", \">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <substitute>\n        <text macro=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\" delimiter=\", \">\n      <name form=\"short\" delimiter=\", \" and=\"text\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"editor-short\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <name form=\"short\" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"editor-short-bib\">\n    <names variable=\"editor\">\n      <name form=\"long\" name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text term=\"accessed\" suffix=\" \"/>\n      <date variable=\"accessed\">\n        <date-part name=\"day\" suffix=\" \"/>\n        <date-part name=\"month\" suffix=\" \"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\" \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" suffix=\"., \" text-case=\"lowercase\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout delimiter=\"&#160;; \">\n      <text macro=\"author-short\" suffix=\"&#160;\"/>\n      <text macro=\"year-date\"/>\n      <choose>\n        <if locator=\"page\">\n          <text variable=\"locator\" prefix=\",&#160;\"/>\n        </if>\n        <else-if locator=\"paragraph\">\n          <text variable=\"locator\" prefix=\",&#160;§\"/>\n        </else-if>\n        <else>\n          <group prefix=\",&#160;\">\n            <label variable=\"locator\" form=\"short\" suffix=\".&#160;\"/>\n            <text variable=\"locator\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&#8212;\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"title\"/>\n      <key variable=\"page-first\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group>\n        <choose>\n          <if type=\"thesis\">\n            <group>\n              <text macro=\"author\" suffix=\". \"/>\n            </group>\n            <group delimiter=\", \" suffix=\".\">\n              <text macro=\"title\" font-style=\"italic\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </if>\n          <else-if type=\"webpage\">\n            <group>\n              <text macro=\"author\" suffix=\". \"/>\n            </group>\n            <group delimiter=\" \">\n              <text macro=\"title\" font-style=\"italic\"/>\n              <text variable=\"URL\" prefix=\", (\" suffix=\")\"/>\n              <text prefix=\"[\" macro=\"access\" suffix=\"]\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n            <group>\n              <text macro=\"author\" suffix=\". \"/>\n            </group>\n            <group delimiter=\", \" suffix=\".\">\n              <text macro=\"title\" prefix=\"« \"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"year-date\"/>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\"/>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"book\" match=\"any\">\n            <group>\n              <text macro=\"author\" suffix=\". \"/>\n            </group>\n            <group suffix=\".\">\n              <text macro=\"title\" suffix=\", \"/>\n              <text macro=\"edition\"/>\n              <text macro=\"collection\" suffix=\", \"/>\n              <text variable=\"publisher-place\" suffix=\", \"/>\n              <text variable=\"publisher\"/>\n              <text macro=\"year-date\" prefix=\", \"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter entry-dictionary\" match=\"any\">\n            <group>\n              <text macro=\"author\" suffix=\". \"/>\n            </group>\n            <text macro=\"title\" quotes=\"true\" suffix=\" , \"/>\n            <group>\n              <text value=\"in\" font-style=\"italic\" suffix=\"&#160;: \"/>\n              <choose>\n                <if variable=\"editor\" match=\"any\">\n                  <text macro=\"editor-short-bib\"/>\n                  <text variable=\"container-title\" font-style=\"italic\" prefix=\", \" suffix=\"&#160;\"/>\n                </if>\n                <else>\n                  <text variable=\"container-title\" font-style=\"italic\" prefix=\", \" suffix=\"&#160;\"/>\n                </else>\n              </choose>\n              <text macro=\"year-date\" prefix=\", \"/>\n              <text variable=\"page\" prefix=\", p. \" suffix=\".\"/>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\" match=\"any\">\n            <group>\n              <text macro=\"author\" suffix=\". \"/>\n            </group>\n            <text macro=\"title\" quotes=\"true\" suffix=\" , \"/>\n            <group>\n              <text value=\"in\" font-style=\"italic\" suffix=\"&#160;: \"/>\n              <choose>\n                <if variable=\"editor\" match=\"any\">\n                  <text macro=\"editor-short-bib\"/>\n                  <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n                </if>\n                <else>\n                  <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n                </else>\n              </choose>\n              <text variable=\"publisher-place\" prefix=\", \" suffix=\", \"/>\n              <text variable=\"publisher\"/>\n              <text macro=\"year-date\" prefix=\", \"/>\n              <text variable=\"page\" prefix=\", p. \" suffix=\".\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group>\n              <text macro=\"author\" suffix=\". \"/>\n            </group>\n            <text macro=\"title\" font-style=\"italic\"/>\n            <group>\n              <text variable=\"genre\" prefix=\", \" font-style=\"italic\"/>\n              <text variable=\"publisher-place\" prefix=\", \" suffix=\", \"/>\n              <text variable=\"publisher\"/>\n              <text macro=\"year-date\" prefix=\", \"/>\n              <text variable=\"page\" prefix=\",  \" suffix=\" p.\"/>\n            </group>\n          </else-if>\n          <else-if type=\"entry-encyclopedia\" match=\"any\">\n            <group>\n              <text macro=\"author\"/>\n              <text macro=\"year-date\" prefix=\"&#160;(\" suffix=\")&#160;: \"/>\n            </group>\n            <group delimiter=\", \">\n              <choose>\n                <if variable=\"title-short\" match=\"any\">\n                  <text variable=\"title-short\" font-style=\"italic\"/>\n                </if>\n                <else>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </else>\n              </choose>\n              <text variable=\"volume\"/>\n              <group delimiter=\" \">\n                <text term=\"sub-verbo\" font-style=\"italic\"/>\n                <text macro=\"title\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"