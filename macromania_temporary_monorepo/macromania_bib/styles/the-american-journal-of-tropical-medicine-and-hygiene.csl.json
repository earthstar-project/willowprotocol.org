"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>The American Journal of Tropical Medicine and Hygiene</title>\n    <id>http://www.zotero.org/styles/the-american-journal-of-tropical-medicine-and-hygiene</id>\n    <link href=\"http://www.zotero.org/styles/the-american-journal-of-tropical-medicine-and-hygiene\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-heart-association\" rel=\"template\"/>\n    <link href=\"http://www.ajtmh.org/site/misc/ifora.xhtml\" rel=\"documentation\"/>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0002-9637</issn>\n    <eissn>1476-1645</eissn>\n    <published>2012-01-19T03:31:02+00:00</published>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" prefix=\" \" strip-periods=\"true\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text macro=\"anon\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\". \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <text term=\"accessed\" text-case=\"capitalize-first\"/>\n          <date variable=\"issued\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"margin\" entry-spacing=\"0\" et-al-min=\"16\" et-al-use-first=\"15\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\". \">\n        <group delimiter=\", \">\n          <text macro=\"author\"/>\n          <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n        </group>\n        <text macro=\"title\"/>\n        <group>\n          <choose>\n            <if type=\"post-weblog webpage\" match=\"any\">\n              <text macro=\"access\"/>\n            </if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\", \">\n                <group delimiter=\". \">\n                  <text macro=\"editor\"/>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <text macro=\"edition\" prefix=\" \"/>\n                  <text macro=\"publisher\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </else-if>\n            <else-if type=\"article-journal\">\n              <group delimiter=\" \">\n                <text variable=\"container-title\" font-style=\"italic\" form=\"short\" strip-periods=\"true\"/>\n                <text variable=\"volume\" font-style=\"italic\"/>\n              </group>\n              <text variable=\"page\" prefix=\": \"/>\n            </else-if>\n            <else>\n              <group suffix=\".\" delimiter=\" \">\n                <text macro=\"edition\"/>\n                <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text prefix=\" \" macro=\"publisher\"/>\n            </else>\n          </choose>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"