"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"ja-JP\">\n  <info>\n    <title>SIST02 (日本語)</title>\n    <id>http://www.zotero.org/styles/sist02</id>\n    <link href=\"http://www.zotero.org/styles/sist02\" rel=\"self\"/>\n    <link href=\"http://sti.jst.go.jp/sist/handbook/sist02_2007/main.htm\" rel=\"documentation\"/>\n    <author>\n      <name>Shoji Takahashi</name>\n      <email>s.takahashi@elsevier.com</email>\n      <uri>http://www.mendeley.com/profiles/shoji-takahashi3/</uri>\n    </author>\n    <contributor>\n      <name>Eriko Amano</name>\n      <uri>http://www.mendeley.com/profiles/eriko-amano/</uri>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <updated>2015-12-13T06:04:50+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"ja\">\n    <terms>\n      <term name=\"open-quote\">“</term>\n      <term name=\"close-quote\">”</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name initialize=\"false\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n    </names>\n  </macro>\n  <macro name=\"event\">\n    <group delimiter=\", \">\n      <text variable=\"event-place\"/>\n      <date variable=\"event-date\">\n        <date-part name=\"year\" suffix=\"-\"/>\n        <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n        <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"chapter report paper-conference article webpage\" match=\"any\">\n        <choose>\n          <if match=\"any\" variable=\"container-title\" type=\"webpage\">\n            <text variable=\"title\" quotes=\"true\"/>\n          </if>\n          <else>\n            <text variable=\"title\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize=\"false\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" plural=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name initialize=\"false\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" plural=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"locators\">\n    <group delimiter=\", \">\n      <group>\n        <text term=\"volume\" form=\"short\" suffix=\" \"/>\n        <text variable=\"volume\"/>\n      </group>\n      <group>\n        <text term=\"issue\" form=\"short\" suffix=\" \"/>\n        <text variable=\"issue\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\", \">\n      <text variable=\"URL\" prefix=\" \"/>\n      <group>\n        <text term=\"accessed\" text-case=\"lowercase\" prefix=\"(\" suffix=\" \"/>\n        <date variable=\"accessed\" suffix=\")\">\n          <date-part name=\"year\" range-delimiter=\"-\" suffix=\"-\"/>\n          <date-part name=\"month\" form=\"numeric-leading-zeros\" range-delimiter=\"-\" strip-periods=\"true\" suffix=\"-\"/>\n          <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"article-journal chapter\" match=\"any\">\n        <label plural=\"never\" suffix=\". \" variable=\"page\" form=\"short\"/>\n        <text variable=\"page\"/>\n      </if>\n      <else-if type=\"book thesis\" match=\"any\">\n        <text variable=\"number-of-pages\"/>\n        <label plural=\"never\" variable=\"page\" form=\"short\"/>\n      </else-if>\n      <else-if type=\"report\" match=\"any\">\n        <choose>\n          <if match=\"any\" variable=\"container-title\">\n            <label plural=\"never\" suffix=\". \" variable=\"page\" form=\"short\"/>\n            <text variable=\"number-of-pages\"/>\n          </if>\n          <else>\n            <text variable=\"number-of-pages\"/>\n            <label plural=\"never\" variable=\"page\" form=\"short\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <choose>\n          <if match=\"any\" variable=\"container-title\">\n            <label plural=\"never\" suffix=\". \" variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </if>\n          <else>\n            <text variable=\"page\"/>\n            <label plural=\"never\" variable=\"page\" form=\"short\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"(\" suffix=\")\"/>\n      <text macro=\"author\"/>\n      <choose>\n        <if type=\"article-journal\">\n          <group delimiter=\". \">\n            <group delimiter=\": \">\n              <text variable=\"collection-title\"/>\n              <text macro=\"title\"/>\n            </group>\n            <text variable=\"container-title\"/>\n            <group delimiter=\", \">\n              <text macro=\"issued\"/>\n              <text macro=\"locators\"/>\n              <text macro=\"page\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"book\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"translator\"/>\n            <group delimiter=\", \">\n              <text variable=\"edition\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"page\"/>\n              <text variable=\"collection-title\" prefix=\"(\" suffix=\")\"/>\n              <text variable=\"ISBN\" prefix=\"ISBN\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\"/>\n            <text macro=\"editor\"/>\n            <group delimiter=\", \">\n              <text variable=\"edition\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"page\"/>\n              <text variable=\"collection-title\" prefix=\"(\" suffix=\")\"/>\n              <text variable=\"ISBN\" prefix=\"ISBN\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n              <text variable=\"number\"/>\n              <text macro=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"page\"/>\n              <text variable=\"genre\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"event\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"page\"/>\n              <text variable=\"collection-title\" prefix=\"(\" suffix=\")\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"number\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\"/>\n            <group delimiter=\", \">\n              <text variable=\"number\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"