"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"never\" page-range-format=\"expanded\" default-locale=\"fr-FR\">\n  <info>\n    <title>Revue archéologique de Narbonnaise (French)</title>\n    <title-short>RAN</title-short>\n    <id>http://www.zotero.org/styles/revue-archeologique-de-narbonnaise</id>\n    <link href=\"http://www.zotero.org/styles/revue-archeologique-de-narbonnaise\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/societe-francaise-detude-de-la-ceramique-antique-en-gaule\" rel=\"template\"/>\n    <link href=\"https://www.asm.cnrs.fr/images/Site/Editions/RAN/RAN-auteurs.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"history\"/>\n    <issn>2117-5683</issn>\n    <updated>2021-10-27T11:10:36+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"editor\" form=\"short\">dir.</term>\n      <term name=\"anonymous\">anonyme</term>\n      <term name=\"page-range-delimiter\">-</term>\n      <term name=\"anonymous\" form=\"short\">anon.</term>\n      <term name=\"no date\">sans date</term>\n      <term name=\"no date\" form=\"short\">s.d.</term>\n      <term name=\"et-al\">et alii</term>\n      <term name=\"in\">in</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\", \">\n      <name initialize-with=\". \" name-as-sort-order=\"all\" sort-separator=\" \">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\" prefix=\"(\" suffix=\")\"/>\n      </name>\n      <et-al font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\" prefix=\"(\" suffix=\")\"/>\n      </name>\n      <et-al font-variant=\"normal\" font-style=\"italic\"/>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" \"/>\n      <substitute>\n        <text macro=\"container-author\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\" delimiter=\", \">\n      <name sort-separator=\", \" initialize-with=\".\" delimiter=\", \"/>\n      <et-al font-style=\"italic\" font-variant=\"normal\"/>\n    </names>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\", \"/>\n      <et-al font-variant=\"normal\" font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title-short\" font-style=\"italic\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"collection\">\n    <group prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if locator=\"paragraph\">\n        <text variable=\"locator\" prefix=\"§ \"/>\n      </if>\n      <else>\n        <group>\n          <choose>\n            <if locator=\"page\">\n              <text variable=\"locator\"/>\n            </if>\n            <else>\n              <group delimiter=\" \">\n                <label variable=\"locator\" form=\"short\"/>\n                <text variable=\"locator\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"webpage post post-weblog\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text macro=\"edition\"/>\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <text variable=\"number-of-pages\" suffix=\" p.\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"lowercase\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"URL\">\n    <text variable=\"URL\" prefix=\" &lt;\" suffix=\"&gt;\"/>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text macro=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"page-first\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" : \">\n        <group font-weight=\"bold\">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"author\">\n                <name form=\"short\" delimiter=\", \" et-al-min=\"4\" et-al-use-first=\"1\"/>\n                <et-al font-variant=\"normal\" font-style=\"italic\"/>\n              </names>\n            </if>\n            <else-if variable=\"editor\">\n              <names variable=\"editor\">\n                <name form=\"short\" delimiter=\", \" et-al-min=\"4\" et-al-use-first=\"1\"/>\n                <substitute>\n                  <text macro=\"container-author\"/>\n                </substitute>\n              </names>\n            </else-if>\n            <else>\n              <text variable=\"title-short\"/>\n            </else>\n          </choose>\n          <text macro=\"year-date\" prefix=\" \"/>\n        </group>\n        <group delimiter=\" – \">\n          <choose>\n            <if variable=\"author\">\n              <text macro=\"author\"/>\n            </if>\n            <else-if variable=\"editor\">\n              <text macro=\"editor\"/>\n            </else-if>\n          </choose>\n          <choose>\n            <if type=\"thesis\">\n              <group delimiter=\", \">\n                <text macro=\"title\" font-style=\"italic\"/>\n                <text variable=\"genre\"/>\n                <text variable=\"publisher\"/>\n                <text macro=\"year-date\"/>\n                <text macro=\"number-of-pages\"/>\n              </group>\n            </if>\n            <else-if type=\"webpage post post-weblog\" match=\"any\">\n              <group delimiter=\" \">\n                <group suffix=\",\" delimiter=\", \">\n                  <choose>\n                    <if variable=\"container-title\" match=\"none\">\n                      <text macro=\"title\" font-style=\"italic\"/>\n                    </if>\n                    <else>\n                      <text macro=\"title\"/>\n                      <text variable=\"container-title\" font-style=\"italic\"/>\n                    </else>\n                  </choose>\n                </group>\n                <text macro=\"year-date\"/>\n                <text macro=\"URL\"/>\n              </group>\n            </else-if>\n            <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\"/>\n                <text macro=\"year-date\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </else-if>\n            <else-if type=\"book graphic\" match=\"any\">\n              <group delimiter=\". \">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"publisher\"/>\n                  <text macro=\"year-date\"/>\n                  <text macro=\"number-of-pages\"/>\n                </group>\n                <text macro=\"collection\"/>\n              </group>\n            </else-if>\n            <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <choose>\n                  <if variable=\"title-short\" match=\"any\">\n                    <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n                  </if>\n                  <else>\n                    <text variable=\"container-title\" font-style=\"italic\"/>\n                  </else>\n                </choose>\n                <group delimiter=\" \">\n                  <label variable=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n                <text macro=\"year-date\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </else-if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\". \">\n                <text macro=\"title\"/>\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n                  <text macro=\"editor\"/>\n                </group>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <group delimiter=\" \">\n                  <group delimiter=\", \">\n                    <text macro=\"publisher\"/>\n                    <text macro=\"year-date\"/>\n                    <text macro=\"pages\"/>\n                  </group>\n                  <text macro=\"collection\" prefix=\"(\" suffix=\")\"/>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"collection\"/>\n                <text macro=\"year-date\"/>\n                <text macro=\"number-of-pages\"/>\n                <text macro=\"URL\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"