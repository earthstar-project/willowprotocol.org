"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"text\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Transactions of the Materials Research Society of Japan</title>\n    <title-short>Trans. Mater. Res. Soc. Japan</title-short>\n    <id>http://www.zotero.org/styles/transactions-of-the-materials-research-society-of-japan</id>\n    <link href=\"http://www.zotero.org/styles/transactions-of-the-materials-research-society-of-japan\" rel=\"self\"/>\n    <link href=\"http://mc.manuscriptcentral.com/mrsj\" rel=\"documentation\"/>\n    <link href=\"http://www.mrs-j.org/en/pub/index.php\" rel=\"documentation\"/>\n    <contributor>\n      <name>Takaaki AOKI</name>\n      <email>aoki.takaaki@gmail.com</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <issn>1382-3469</issn>\n    <updated>2014-03-12T03:08:43+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-US\">\n    <style-options punctuation-in-quote=\"false\"/>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize-with=\".\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"verb-short\" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name initialize-with=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" suffix=\", \"/>\n    <text variable=\"publisher-place\" suffix=\" \"/>\n    <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"20\" et-al-use-first=\"19\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\"/>\n            <group>\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" suffix=\", \"/>\n          <text term=\"in\" suffix=\": \"/>\n          <text macro=\"editor\" suffix=\", \"/>\n          <text variable=\"container-title\" form=\"short\" text-case=\"title\" suffix=\", \"/>\n          <text macro=\"edition\" suffix=\", \"/>\n          <text macro=\"publisher\"/>\n          <group delimiter=\" \">\n            <label prefix=\", \" variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"URL\" suffix=\" \"/>\n          </group>\n        </else-if>\n        <else>\n          <group>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" strip-periods=\"false\" font-style=\"italic\" suffix=\", \"/>\n            <text variable=\"volume\" font-weight=\"bold\" suffix=\", \"/>\n            <text variable=\"page\" form=\"short\" suffix=\" \"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"