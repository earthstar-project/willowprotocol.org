"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>World Mycotoxin Journal</title>\n    <id>http://www.zotero.org/styles/world-mycotoxin-journal</id>\n    <link href=\"http://www.zotero.org/styles/world-mycotoxin-journal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/elsevier-harvard\" rel=\"template\"/>\n    <link href=\"http://www.wageningenacademic.com/default.asp?pageid=92\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <issn>1875-0710</issn>\n    <eissn>1875-0796</eissn>\n    <updated>2012-10-25T21:04:09+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" and=\"text\" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <names variable=\"editor translator\" delimiter=\", \">\n          <name initialize-with=\".\" and=\"text\" delimiter=\", \" sort-separator=\", \" name-as-sort-order=\"all\" delimiter-precedes-last=\"never\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\", doi:\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text term=\"available at\" text-case=\"capitalize-first\"/>\n                <text variable=\"URL\"/>\n              </group>\n              <group delimiter=\" \">\n                <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                <date variable=\"accessed\" form=\"text\"/>\n              </group>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\" suffix=\".\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper article-magazine\" match=\"none\">\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if variable=\"accessed\">\n        <date variable=\"accessed\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"thecontainer\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"container-title\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\": \">\n          <text variable=\"volume\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <group delimiter=\", \">\n          <date variable=\"issued\" form=\"text\"/>\n        </group>\n      </else-if>\n      <else-if type=\"book thesis report\" match=\"any\">\n        <text variable=\"number-of-pages\" suffix=\" pp\"/>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"page\" prefix=\", pp. \"/>\n      </else-if>\n      <else>\n        <text variable=\"page\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <text variable=\"locator\" prefix=\": \"/>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\" suffix=\",\"/>\n        <text macro=\"year-date\" suffix=\".\"/>\n        <text macro=\"title\"/>\n        <text macro=\"container-contributors\"/>\n        <text macro=\"thecontainer\"/>\n        <group delimiter=\", \">\n          <text macro=\"publisher\"/>\n          <text macro=\"locators\"/>\n        </group>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"