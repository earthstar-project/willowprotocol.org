"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" name-delimiter=\"; \" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"always\" default-locale=\"de-DE\">\n  <info>\n    <title>Metropol Verlag (Deutsch)</title>\n    <id>http://www.zotero.org/styles/metropol-verlag</id>\n    <link href=\"http://www.zotero.org/styles/metropol-verlag\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/infoclio-de\" rel=\"template\"/>\n    <link href=\"http://metropol-verlag.de/wp-content/uploads/2016/06/Manuskriptgestaltung.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <category field=\"social_science\"/>\n    <summary>Citation style for manuscripts for the Metropol Publishing House Berlin.</summary>\n    <updated>2018-12-27T18:36:18+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"interviewer\" form=\"verb\">Interview gef√ºhrt von</term>\n      <term name=\"letter\">Schreiben</term>\n      <term name=\"number-of-volumes\" form=\"short\">Bd.</term>\n      <term name=\"no date\" form=\"short\">o. D.</term>\n      <term name=\"ibid\" form=\"long\">ebenda</term>\n    </terms>\n  </locale>\n  <macro name=\"subsequent-reference\">\n    <choose>\n      <if match=\"any\" type=\"article\">\n        <group delimiter=\", \">\n          <text macro=\"archive-location\"/>\n          <text macro=\"locator\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <group delimiter=\", \">\n            <text macro=\"creator-for-subsequent\"/>\n            <text macro=\"identifier-for-subsequent\"/>\n          </group>\n          <text macro=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"creator\">\n    <names variable=\"author\">\n      <name delimiter=\"/\" et-al-min=\"4\" et-al-use-first=\"3\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"composer\"/>\n        <names variable=\"director\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"creator-bib\">\n    <names variable=\"author\">\n      <name delimiter=\"/\" et-al-min=\"4\" et-al-use-first=\"3\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"composer\"/>\n        <names variable=\"director\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"creator-for-subsequent\">\n    <names variable=\"author\">\n      <name form=\"short\" et-al-min=\"4\" et-al-use-first=\"1\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"composer\"/>\n        <names variable=\"director\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"identifier-for-subsequent\">\n    <choose>\n      <if variable=\"title title-short\" match=\"any\">\n        <text variable=\"title\" form=\"short\"/>\n      </if>\n      <else-if type=\"personal_communication\">\n        <group delimiter=\" \">\n          <text term=\"letter\"/>\n          <names variable=\"recipient\">\n            <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n            <name et-al-min=\"2\" et-al-use-first=\"1\"/>\n          </names>\n        </group>\n      </else-if>\n      <else-if type=\"interview\">\n        <names variable=\"interviewer\" delimiter=\", \">\n          <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n          <name et-al-min=\"2\" et-al-use-first=\"1\"/>\n        </names>\n      </else-if>\n      <else-if type=\"report song broadcast motion_picture webpage post post-weblog\" match=\"any\">\n        <text variable=\"collection-title\"/>\n        <text variable=\"container-title\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"in\">\n    <choose>\n      <if type=\"chapter paper-conference entry-encyclopedia entry-dictionary article-magazine article-newspaper article-journal\" match=\"any\">\n        <text term=\"in\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-creator\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor\">\n          <name delimiter=\"/\" et-al-min=\"4\" et-al-use-first=\"3\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n          <substitute>\n            <names variable=\"container-author\"/>\n            <names variable=\"collection-editor\"/>\n          </substitute>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-information\">\n    <choose>\n      <if type=\"chapter paper-conference entry-encyclopedia entry-dictionary article-newspaper article-magazine article-journal\" match=\"any\">\n        <text variable=\"container-title\"/>\n      </if>\n      <else-if type=\"report song broadcast motion_picture webpage post post-weblog\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"collection-title\"/>\n          <text variable=\"container-title\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"journal-volume\">\n    <choose>\n      <if type=\"article-journal\">\n        <group delimiter=\" \">\n          <number variable=\"volume\"/>\n          <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n          <number variable=\"issue\"/>\n        </group>\n      </if>\n      <else-if type=\"report song broadcast\" match=\"any\">\n        <number variable=\"number\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"volumes\">\n    <choose>\n      <if type=\"book chapter entry-encyclopedia entry-dictionary song motion_picture\" match=\"any\">\n        <group delimiter=\" / \">\n          <group delimiter=\"/\">\n            <group delimiter=\" \">\n              <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n              <number text-case=\"capitalize-first\" variable=\"volume\"/>\n            </group>\n            <number variable=\"number-of-volumes\"/>\n          </group>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"type-description\">\n    <choose>\n      <if type=\"manuscript thesis speech\" match=\"any\">\n        <text variable=\"genre\"/>\n      </if>\n      <else-if type=\"personal_communication\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text term=\"letter\"/>\n            <names variable=\"recipient\">\n              <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n              <name and=\"text\" delimiter-precedes-last=\"never\"/>\n            </names>\n          </group>\n          <text variable=\"genre\"/>\n        </group>\n      </else-if>\n      <else-if type=\"interview\">\n        <names variable=\"interviewer\" delimiter=\", \">\n          <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n          <name and=\"text\" delimiter-precedes-last=\"never\"/>\n        </names>\n      </else-if>\n      <else-if type=\"motion_picture song broadcast\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"medium\"/>\n          <text variable=\"dimensions\"/>\n        </group>\n      </else-if>\n      <else-if type=\"book\">\n        <choose>\n          <if variable=\"version medium\" match=\"any\">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"version\"/>\n                <text variable=\"version\"/>\n              </group>\n              <text variable=\"medium\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"alt-publisher\">\n    <choose>\n      <if type=\"book chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"none\">\n        <text variable=\"publisher\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"place\">\n    <choose>\n      <if type=\"speech\">\n        <group delimiter=\", \">\n          <text variable=\"event\"/>\n          <text variable=\"event-place\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"publisher-place\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal\" match=\"none\">\n        <choose>\n          <if type=\"book chapter paper-conference thesis\" match=\"any\">\n            <choose>\n              <if variable=\"issued\">\n                <date variable=\"issued\" form=\"numeric\" date-parts=\"year\"/>\n              </if>\n              <else>\n                <text term=\"no date\" form=\"short\"/>\n              </else>\n            </choose>\n          </if>\n          <else-if type=\"article-newspaper article-magazine graphic entry-encyclopedia entry-dictionary report speech interview manuscript personal_communication\" match=\"any\">\n            <choose>\n              <if variable=\"issued\">\n                <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n                  <date-part name=\"year\"/>\n                </date>\n              </if>\n              <else>\n                <text term=\"no date\" form=\"short\"/>\n              </else>\n            </choose>\n          </else-if>\n          <else-if type=\"webpage post-weblog\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"day\" suffix=\".&#160;\"/>\n              <date-part name=\"month\" form=\"numeric\" suffix=\".&#160;\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if variable=\"locator\">\n        <text macro=\"locator\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"book-series\">\n    <choose>\n      <if type=\"book chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n          <text variable=\"collection-title\"/>\n          <group delimiter=\" \">\n            <label text-case=\"capitalize-first\" variable=\"collection-number\" form=\"short\"/>\n            <text variable=\"collection-number\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"artwork-description\">\n    <choose>\n      <if type=\"graphic\">\n        <group delimiter=\", \">\n          <text variable=\"medium\"/>\n          <text variable=\"dimensions\"/>\n          <text variable=\"genre\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive-location\">\n    <group delimiter=\", \">\n      <text variable=\"archive\"/>\n      <text variable=\"archive_location\"/>\n      <text variable=\"call-number\"/>\n    </group>\n  </macro>\n  <macro name=\"url-web-documents-only\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <text macro=\"url\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"url\">\n    <group delimiter=\" \">\n      <text variable=\"URL\"/>\n      <date variable=\"accessed\" prefix=\"[\" suffix=\"]\">\n        <date-part name=\"day\" suffix=\".&#160;\"/>\n        <date-part name=\"month\" form=\"numeric\" suffix=\".&#160;\"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <number variable=\"edition\" form=\"ordinal\"/>\n      <label text-case=\"capitalize-first\" strip-periods=\"false\" variable=\"edition\" form=\"short\"/>\n    </group>\n  </macro>\n  <citation>\n    <layout delimiter=\"; \" suffix=\".\">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\"/>\n            <text macro=\"locator\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <text macro=\"subsequent-reference\"/>\n        </else-if>\n        <else>\n          <choose>\n            <if type=\"article\" match=\"any\">\n              <text macro=\"archive-location\"/>\n            </if>\n            <else>\n              <group delimiter=\", \">\n                <group delimiter=\", \">\n                  <text macro=\"creator\"/>\n                  <group delimiter=\", \">\n                    <text macro=\"title\"/>\n                    <group delimiter=\", \">\n                      <group delimiter=\": \">\n                        <text macro=\"in\"/>\n                        <text macro=\"container-creator\"/>\n                      </group>\n                      <group delimiter=\", \">\n                        <group delimiter=\" \">\n                          <text macro=\"container-information\"/>\n                          <text macro=\"journal-volume\"/>\n                        </group>\n                        <text macro=\"volumes\"/>\n                      </group>\n                    </group>\n                    <text macro=\"type-description\"/>\n                  </group>\n                </group>\n                <text macro=\"alt-publisher\"/>\n                <group delimiter=\" \">\n                  <text macro=\"place\"/>\n                  <text macro=\"date\"/>\n                  <date variable=\"original-date\" form=\"text\" prefix=\"[\" suffix=\"]\"/>\n                  <text macro=\"book-series\"/>\n                </group>\n                <text macro=\"artwork-description\"/>\n                <text macro=\"pages\"/>\n                <text macro=\"url-web-documents-only\"/>\n              </group>\n            </else>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"creator\" names-min=\"3\" names-use-first=\"3\"/>\n      <key variable=\"issued\" sort=\"descending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\", \">\n        <group delimiter=\", \">\n          <text macro=\"creator-bib\"/>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <group delimiter=\": \">\n              <text macro=\"in\"/>\n              <text macro=\"container-creator\"/>\n            </group>\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text macro=\"container-information\"/>\n                <text macro=\"journal-volume\"/>\n              </group>\n              <text macro=\"volumes\"/>\n            </group>\n            <text macro=\"type-description\"/>\n          </group>\n        </group>\n        <text macro=\"alt-publisher\"/>\n        <group delimiter=\" \">\n          <text macro=\"place\"/>\n          <text macro=\"date\"/>\n          <date variable=\"original-date\" form=\"text\" prefix=\"[\" suffix=\"]\"/>\n          <text macro=\"book-series\"/>\n        </group>\n        <text macro=\"artwork-description\"/>\n        <text macro=\"archive-location\"/>\n        <text macro=\"pages\"/>\n        <text macro=\"url-web-documents-only\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"