"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" page-range-format=\"chicago\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Early Music History</title>\n    <title-short>EMH</title-short>\n    <id>http://www.zotero.org/styles/early-music-history</id>\n    <link href=\"http://www.zotero.org/styles/early-music-history\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/cambridge-university-press-note\" rel=\"template\"/>\n    <link href=\"https://www.cambridge.org/core/journals/early-music-history/information/instructions-contributors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"humanities\"/>\n    <issn>0261-1279</issn>\n    <eissn>1474-0559</eissn>\n    <updated>2022-12-01T08:32:54+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize-with=\". \"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" text-case=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" form=\"short\" prefix=\"‘\" suffix=\"’\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n          <group delimiter=\": \">\n            <text variable=\"publisher-place\"/>\n          </group>\n          <text macro=\"date\"/>\n        </group>\n      </if>\n      <else-if type=\"book\" match=\"any\">\n        <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n          <text variable=\"publisher-place\"/>\n          <text macro=\"date\"/>\n        </group>\n      </else-if>\n      <else-if type=\"thesis\" match=\"any\">\n        <text variable=\"publisher\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label variable=\"volume\" form=\"short\"/>\n      <number variable=\"volume\" form=\"roman\"/>\n    </group>\n  </macro>\n  <macro name=\"pageno\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"pageref\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"volref\">\n    <group delimiter=\" \">\n      <group delimiter=\"/\">\n        <text variable=\"volume\"/>\n        <text variable=\"issue\"/>\n      </group>\n      <text macro=\"date\" prefix=\"(\" suffix=\")\"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text term=\"in\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\">\n      <name and=\"text\" initialize-with=\". \"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <number variable=\"number-of-volumes\"/>\n      <label variable=\"number-of-volumes\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <text variable=\"edition\"/>\n      <label variable=\"edition\" form=\"short\"/>\n    </group>\n  </macro>\n  <citation>\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if match=\"any\" position=\"ibid ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\"/>\n            <text macro=\"pageref\"/>\n          </group>\n        </if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"pageref\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <group>\n              <group delimiter=\", \">\n                <text macro=\"author\"/>\n                <text macro=\"title\"/>\n              </group>\n              <choose>\n                <if type=\"article-journal\" match=\"any\">\n                  <group delimiter=\", \" prefix=\", \">\n                    <text macro=\"container\"/>\n                    <text macro=\"volref\"/>\n                    <text macro=\"pageno\"/>\n                  </group>\n                </if>\n                <else-if type=\"chapter paper-conference\" match=\"any\">\n                  <group delimiter=\", \" prefix=\", \">\n                    <group delimiter=\" \">\n                      <text macro=\"container\"/>\n                      <text macro=\"publisher\"/>\n                    </group>\n                    <text macro=\"pageno\"/>\n                  </group>\n                </else-if>\n                <else-if type=\"book\" match=\"any\">\n                  <group delimiter=\" \" prefix=\" \">\n                    <group delimiter=\", \">\n                      <text macro=\"volumes\"/>\n                      <text macro=\"edition\"/>\n                    </group>\n                    <group delimiter=\", \">\n                      <text macro=\"publisher\"/>\n                      <text macro=\"volume\"/>\n                    </group>\n                  </group>\n                </else-if>\n                <else-if type=\"thesis\" match=\"any\">\n                  <group delimiter=\", \" prefix=\", \">\n                    <text variable=\"container-title\"/>\n                    <text variable=\"genre\"/>\n                    <group delimiter=\" \">\n                      <text macro=\"publisher\"/>\n                      <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n                    </group>\n                  </group>\n                </else-if>\n                <else>\n                  <text macro=\"publisher\" prefix=\", \"/>\n                  <text macro=\"date\" prefix=\"(\" suffix=\")\"/>\n                </else>\n              </choose>\n            </group>\n            <text macro=\"pageref\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"