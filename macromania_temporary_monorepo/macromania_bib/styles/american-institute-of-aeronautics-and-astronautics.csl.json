"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>American Institute of Aeronautics and Astronautics</title>\n    <title-short>AIAA</title-short>\n    <id>http://www.zotero.org/styles/american-institute-of-aeronautics-and-astronautics</id>\n    <link href=\"http://www.zotero.org/styles/american-institute-of-aeronautics-and-astronautics\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/ieee-with-url\" rel=\"template\"/>\n    <link href=\"https://www.aiaa.org/publications/journals/reference-style-and-format\" rel=\"documentation\"/>\n    <author>\n      <name>Garrett Foster</name>\n      <email>garrett.d.foster@gmail.com</email>\n      <uri>http://sdoresearch.com</uri>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <summary>A style for AIAA</summary>\n    <updated>2023-09-26T07:49:11+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference song thesis\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else-if type=\"report\" match=\"any\">\n        <date date-parts=\"year-month\" form=\"text\" variable=\"issued\"/>\n      </else-if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n          <date-part name=\"day\" form=\"numeric-leading-zeros\" prefix=\" \" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" delimiter=\", \" and=\"text\" name-as-sort-order=\"all\" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"verb\" suffix=\" \"/>\n      <name initialize-with=\". \" delimiter=\", \" and=\"text\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\" text-case=\"title\" quotes=\"true\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else-if type=\"webpage post post-weblog article-journal\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\" \">\n            <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n          <text variable=\"URL\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"] \"/>\n      <group delimiter=\", \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <choose>\n                <if variable=\"volume\">\n                  <group delimiter=\", \">\n                    <group delimiter=\" \">\n                      <text term=\"in\" text-case=\"capitalize-first\"/>\n                      <text variable=\"event\" font-style=\"italic\"/>\n                    </group>\n                    <text macro=\"volume\"/>\n                    <text macro=\"editor\"/>\n                    <text variable=\"event-place\"/>\n                    <text macro=\"issued\"/>\n                    <text variable=\"page\" prefix=\"pp. \"/>\n                  </group>\n                </if>\n                <else>\n                  <group delimiter=\", \">\n                    <group delimiter=\" \">\n                      <text term=\"presented at\"/>\n                      <text variable=\"event\"/>\n                    </group>\n                    <text variable=\"event-place\"/>\n                    <text macro=\"issued\"/>\n                  </group>\n                </else>\n              </choose>\n            </group>\n          </if>\n          <else-if type=\"book thesis\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"version\" text-case=\"title\"/>\n              <text variable=\"genre\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"issued\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"chapter\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"editor\"/>\n              <text macro=\"volume\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-journal article-newspaper\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\" form=\"long\"/>\n              <group delimiter=\" \">\n                <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <group delimiter=\" \">\n                <label text-case=\"capitalize-first\" variable=\"issue\" form=\"short\"/>\n                <text variable=\"issue\"/>\n              </group>\n              <text macro=\"issued\"/>\n              <text macro=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text variable=\"genre\"/>\n                <text variable=\"number\"/>\n              </group>\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"patent\">\n            <text variable=\"number\"/>\n            <text macro=\"issued\"/>\n          </else-if>\n          <else-if type=\"article\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"editor\"/>\n              <text variable=\"issue\"/>\n              <group delimiter=\", \">\n                <text variable=\"volume\" prefix=\"Volume \"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"legislation\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text variable=\"number\"/>\n              <text variable=\"section\"/>\n              <text macro=\"issued\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text macro=\"publisher\"/>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"page\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"