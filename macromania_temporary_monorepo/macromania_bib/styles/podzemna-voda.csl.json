"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"sk-SK\">\n  <info>\n    <title>Podzemná voda (Slovenčina)</title>\n    <id>http://www.zotero.org/styles/podzemna-voda</id>\n    <link href=\"http://www.zotero.org/styles/podzemna-voda\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/iso690-author-date-cs\" rel=\"template\"/>\n    <link href=\"http://sah-podzemnavoda.sk/cms/page.php?8\" rel=\"documentation\"/>\n    <author>\n      <name>Jaromír Švasta</name>\n      <email>jaromir.svasta@geology.sk</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geology\"/>\n    <issn>1335-1052</issn>\n    <summary>Style based on STN ISO 690:2012</summary>\n    <updated>2014-07-11T08:38:55+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"from\">Dostupné na</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"first\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"first\">\n        <name-part name=\"given\"/>\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label prefix=\", \" form=\"short\" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"editor2\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"given\"/>\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label prefix=\", \" form=\"short\" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n      <label prefix=\" (\" form=\"short\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"translator2\">\n    <text term=\"translator\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n    <names variable=\"translator\">\n      <name and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"illustrator\">\n    <text term=\"translator\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n    <names variable=\"illustrator\">\n      <name and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"interviewer\">\n    <text term=\"interviewer\" form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n    <names variable=\"interviewer\">\n      <name and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n        <name-part name=\"given\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"responsability\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <choose>\n          <if variable=\"author\">\n            <text macro=\"author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor\"/>\n          </else-if>\n          <else>\n            <text macro=\"translator\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"anonymous\" form=\"short\" text-case=\"uppercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"secondary-responsability\">\n    <choose>\n      <if variable=\"author\">\n        <choose>\n          <if variable=\"translator\">\n            <text macro=\"translator2\"/>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"illustrator\">\n            <text macro=\"illustrator\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author-citation\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"any\">\n        <names variable=\"author\">\n          <name form=\"short\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text term=\"anonymous\" form=\"short\" text-case=\"uppercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\">\n        <name-part name=\"given\"/>\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"container-responsability\">\n    <choose>\n      <if variable=\"container-author editor translator\" match=\"any\">\n        <choose>\n          <if variable=\"container-author\">\n            <text macro=\"container-author\"/>\n          </if>\n          <else-if variable=\"editor\">\n            <text macro=\"editor2\"/>\n          </else-if>\n          <else>\n            <text macro=\"translator\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"anonymous\" text-case=\"uppercase\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if is-uncertain-date=\"issued\">\n            <text term=\"circa\" form=\"short\" suffix=\" \"/>\n          </if>\n        </choose>\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis map motion_picture song manuscript broadcast report\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"paper-conference speech chapter post\" match=\"any\">\n        <text variable=\"title\" suffix=\". \"/>\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <choose>\n          <if variable=\"container-author editor translator\" match=\"any\">\n            <text macro=\"container-responsability\"/>\n            <choose>\n              <if variable=\"container-title event\" match=\"any\">\n                <text value=\" \"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"container-title\">\n            <choose>\n              <if variable=\"event\">\n                <text variable=\"event\" font-style=\"italic\" suffix=\": \"/>\n              </if>\n            </choose>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"event\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\"/>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </if>\n            <else>\n              <text variable=\"title\" font-style=\"italic\"/>\n            </else>\n          </choose>\n        </group>\n      </else-if>\n      <else-if type=\"patent interview\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n    <choose>\n      <if variable=\"DOI URL\" match=\"any\">\n        <text term=\"online\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <text variable=\"number\"/>\n  </macro>\n  <macro name=\"medium\">\n    <text variable=\"medium\"/>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"map\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n          <else>\n            <text value=\"map\" prefix=\"[\" suffix=\"]\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text variable=\"genre\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\". \"/>\n          <date-part name=\"month\" suffix=\" \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"date-day-month\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\".\"/>\n          <date-part name=\"month\" form=\"numeric\" suffix=\".\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"scale\">\n    <group delimiter=\" \">\n      <text term=\"scale\" text-case=\"capitalize-first\"/>\n      <text variable=\"scale\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <number variable=\"edition\" form=\"ordinal\"/>\n        <label variable=\"edition\" form=\"short\" prefix=\" \"/>\n      </if>\n      <else>\n        <text variable=\"edition\" form=\"long\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if type=\"patent manuscript article-newspaper broadcast motion_picture song\" match=\"any\">\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\"/>\n          </if>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\"/>\n          </if>\n          <else>\n            <text value=\"b.m.\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <label suffix=\".\" variable=\"volume\" form=\"short\"/>\n        <text variable=\"volume\"/>\n      </group>\n      <group delimiter=\" \">\n        <label suffix=\".\" variable=\"issue\" form=\"short\"/>\n        <text variable=\"issue\"/>\n      </group>\n      <text macro=\"collection\"/>\n      <group delimiter=\" \">\n        <label suffix=\".\" variable=\"page\" form=\"short\"/>\n        <text variable=\"page\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"broadcast motion_picture song report\" match=\"any\">\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\"/>\n          </if>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\"/>\n          </if>\n          <else>\n            <text value=\"b.n.\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher-info\">\n    <choose>\n      <if variable=\"publisher\">\n        <group delimiter=\": \">\n          <text macro=\"publisher-place\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"accessed\">\n    <choose>\n      <if variable=\"URL DOI\" match=\"any\">\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\" form=\"short\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" prefix=\". \"/>\n            <date-part name=\"month\" prefix=\". \"/>\n            <date-part name=\"year\" prefix=\" \"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <choose>\n      <if type=\"report\">\n        <text variable=\"collection-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"collection-title\"/>\n          <text variable=\"collection-number\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"book thesis manuscript\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"number-of-pages\"/>\n          <text term=\"page\" form=\"short\"/>\n        </group>\n      </if>\n      <else-if type=\"chapter paper-conference article-newspaper\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"report patent\" match=\"any\">\n        <text variable=\"page\" suffix=\" \"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"isbn\">\n    <text variable=\"ISBN\" prefix=\"ISBN \"/>\n  </macro>\n  <macro name=\"issn\">\n    <text variable=\"ISSN\" prefix=\"ISSN \"/>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if variable=\"DOI\">\n        <group>\n          <text term=\"from\" suffix=\": doi:\"/>\n          <text variable=\"DOI\"/>\n        </group>\n      </if>\n      <else-if variable=\"URL\">\n        <group>\n          <text term=\"from\" suffix=\": \"/>\n          <text variable=\"URL\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" givenname-disambiguation-rule=\"primary-name\" collapse=\"year\" year-suffix-delimiter=\", \" after-collapse-delimiter=\"; \" et-al-min=\"3\" et-al-use-first=\"1\" and=\"text\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-citation\" suffix=\",\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"1\" line-spacing=\"1\">\n    <sort>\n      <key macro=\"responsability\"/>\n      <key macro=\"year-date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\" \" suffix=\". \">\n        <text macro=\"responsability\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <choose>\n        <if type=\"book\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <group delimiter=\" \">\n              <group delimiter=\". \">\n                <text macro=\"title\"/>\n                <text macro=\"genre\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"secondary-responsability\"/>\n                <text macro=\"publisher-info\"/>\n                <text macro=\"collection\"/>\n              </group>\n              <text macro=\"accessed\"/>\n            </group>\n            <text macro=\"isbn\"/>\n          </group>\n          <text macro=\"url\"/>\n        </if>\n        <else-if type=\"map\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"scale\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"secondary-responsability\"/>\n            <text macro=\"publisher-info\"/>\n            <group delimiter=\" \">\n              <text macro=\"collection\"/>\n              <text macro=\"accessed\"/>\n            </group>\n            <text macro=\"isbn\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"post post-weblog\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"isbn\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\" \">\n              <group delimiter=\", \">\n                <text macro=\"publisher-info\"/>\n                <text macro=\"date-day-month\"/>\n                <text macro=\"issue\"/>\n              </group>\n              <text macro=\"accessed\"/>\n            </group>\n            <text macro=\"issn\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"chapter entry entry-dictionary entry-encyclopedia\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\" \">\n              <group delimiter=\", \">\n                <text macro=\"publisher-info\"/>\n                <text macro=\"collection\"/>\n                <text macro=\"page\"/>\n              </group>\n              <text macro=\"accessed\"/>\n            </group>\n            <text macro=\"isbn\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"speech\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"publisher-place\"/>\n            <group delimiter=\" \">\n              <text macro=\"date-day-month\"/>\n              <text macro=\"accessed\"/>\n            </group>\n            <text macro=\"page\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"genre\"/>\n            <group delimiter=\" \">\n              <group delimiter=\", \">\n                <text macro=\"publisher-info\"/>\n                <text macro=\"page\"/>\n              </group>\n              <text macro=\"accessed\"/>\n            </group>\n            <text macro=\"collection\"/>\n            <text macro=\"isbn\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text macro=\"publisher-place\"/>\n              <text macro=\"accessed\"/>\n            </group>\n            <text macro=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\". \" suffix=\". \">\n            <group delimiter=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"accessed\"/>\n            </group>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"motion_picture\" match=\"any\">\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"medium\" prefix=\"[\" suffix=\"]. \"/>\n          <text macro=\"year-date\" suffix=\". \"/>\n          <text macro=\"responsability\" suffix=\". \"/>\n          <text macro=\"publisher-place\" suffix=\": \"/>\n          <text macro=\"publisher\" suffix=\". \"/>\n        </else-if>\n        <else-if type=\"song\" match=\"any\">\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"medium\" suffix=\". \"/>\n          <text macro=\"publisher-place\" suffix=\": \"/>\n          <text macro=\"publisher\" suffix=\". \"/>\n          <text macro=\"date\" suffix=\". \"/>\n          <text macro=\"accessed\" suffix=\". \"/>\n          <text macro=\"collection\" suffix=\". \"/>\n          <text macro=\"isbn\" suffix=\". \"/>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"broadcast\" match=\"any\">\n          <text variable=\"container-title\" suffix=\". \"/>\n          <text macro=\"number\" suffix=\". \"/>\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"publisher\" suffix=\". \"/>\n          <text macro=\"accessed\" suffix=\". \"/>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\". \" suffix=\". \">\n            <group delimiter=\": \">\n              <text macro=\"collection\"/>\n              <text macro=\"title\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <group delimiter=\" \">\n              <text macro=\"genre\"/>\n              <text variable=\"number\"/>\n            </group>\n            <group delimiter=\" \">\n              <text macro=\"publisher-info\"/>\n              <text macro=\"accessed\"/>\n            </group>\n          </group>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"manuscript\">\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"genre\" suffix=\". \"/>\n          <text macro=\"edition\" suffix=\". \"/>\n          <text macro=\"publisher-place\" suffix=\", \"/>\n          <text macro=\"accessed\" suffix=\". \"/>\n          <text macro=\"collection\" suffix=\". \"/>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"patent\">\n          <text macro=\"title\" suffix=\". \"/>\n          <text macro=\"publisher-place\" suffix=\", \"/>\n          <text macro=\"number\" suffix=\". \"/>\n          <text macro=\"accessed\" suffix=\". \"/>\n          <text macro=\"collection\" suffix=\". \"/>\n          <text macro=\"date\" suffix=\". \"/>\n          <text macro=\"url\"/>\n        </else-if>\n        <else-if type=\"personal_communication\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"date\"/>\n            <text macro=\"url\"/>\n            <text value=\"Osobní komunikace.\"/>\n          </group>\n        </else-if>\n        <else-if type=\"interview\">\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"interviewer\"/>\n            <text macro=\"date-day-month\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"medium\"/>\n            <text macro=\"genre\"/>\n            <text macro=\"date\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher-info\"/>\n            <text macro=\"accessed\"/>\n            <text macro=\"collection\"/>\n            <text macro=\"page\"/>\n            <text macro=\"isbn\"/>\n          </group>\n          <text macro=\"url\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"