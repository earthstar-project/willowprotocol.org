"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Integrated OMICS</title>\n    <title-short>JIOMICS</title-short>\n    <id>http://www.zotero.org/styles/journal-of-integrated-omics</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-integrated-omics\" rel=\"self\"/>\n    <link href=\"http://www.jiomics.com/ojs/index.php/jio/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Jens Allmer</name>\n      <email>jens@allmer.de</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"generic-base\"/>\n    <issn>2182-0287</issn>\n    <updated>2012-09-20T14:08:35+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"page\">\n        <single>p.</single>\n        <multiple>pp.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <!-- Macros -->\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" text-case=\"capitalize-first\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\" suffix=\", \"/>\n    <text variable=\"publisher-place\" suffix=\", \"/>\n    <text macro=\"year-date\"/>\n  </macro>\n  <macro name=\"edition\">\n    <!--TODO: CSL should have low numeric be text (e.g. '3'->'third')-->\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"long-ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \" suffix=\".\">\n            <text variable=\"title\" text-case=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group suffix=\".\">\n            <text term=\"in\" suffix=\": \"/>\n            <text macro=\"editor\" suffix=\", \"/>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" suffix=\", \"/>\n            <!--text macro=\"edition\"/-->\n            <text macro=\"publisher\" suffix=\", \"/>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\" form=\"long\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \" suffix=\".\">\n            <text variable=\"title\" text-case=\"title\"/>\n            <text variable=\"number\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \" suffix=\".\">\n            <text variable=\"title\" text-case=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \" suffix=\".\">\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\"/>\n            <text variable=\"volume\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            <text variable=\"page\" form=\"long\"/>\n          </group>\n        </else>\n      </choose>\n      <choose>\n        <if variable=\"DOI\">\n          <text variable=\"DOI\" prefix=\" DOI: \"/>\n        </if>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"