"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" page-range-format=\"expanded\" demote-non-dropping-particle=\"never\" default-locale=\"es-ES\">\n  <info>\n    <title>Biblio 3W (Español)</title>\n    <id>http://www.zotero.org/styles/biblio-3w</id>\n    <link href=\"http://www.zotero.org/styles/biblio-3w\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/societe-archeologique-de-bordeaux\" rel=\"template\"/>\n    <link href=\"http://www.ub.edu/geocrit/nrm-b3w.htm\" rel=\"documentation\"/>\n    <link href=\"http://www.ub.edu/geocrit/b-normes.htm\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"geography\"/>\n    <category field=\"social_science\"/>\n    <issn>1138-9796</issn>\n    <summary>Estilo por Biblio 3W. Revista Bibliográfica de Geografía y Ciencias Sociales.</summary>\n    <updated>2019-01-04T11:29:40+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"es\">\n    <terms>\n      <term name=\"in\">in</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" delimiter=\", \">\n      <name delimiter=\"; \" delimiter-precedes-last=\"never\" name-as-sort-order=\"all\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <substitute>\n        <text macro=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\" delimiter=\", \">\n      <name form=\"short\" delimiter=\", \" and=\"text\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"editor-short\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <names variable=\"editor\">\n      <name form=\"short\" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"editor-short-bib\">\n    <names variable=\"editor\">\n      <name and=\"text\" name-as-sort-order=\"all\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <date variable=\"accessed\">\n        <date-part name=\"day\" suffix=\" de \"/>\n        <date-part name=\"month\" suffix=\" de \"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\" \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"lowercase\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"number-of-pages\"/>\n          <label plural=\"never\" variable=\"number-of-pages\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\" \">\n      <text term=\"issue\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n      <text variable=\"issue\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label variable=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <choose>\n          <if locator=\"paragraph\">\n            <text variable=\"locator\" prefix=\"§\"/>\n          </if>\n          <else>\n            <group>\n              <label variable=\"locator\" form=\"short\" suffix=\". \"/>\n              <text variable=\"locator\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"title\"/>\n      <key variable=\"page-first\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <group delimiter=\". \">\n          <text macro=\"author\"/>\n          <text macro=\"title\"/>\n        </group>\n        <choose>\n          <if type=\"thesis\">\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n              <text macro=\"year-date\"/>\n            </group>\n          </if>\n          <else-if type=\"webpage post-weblog\">\n            <group delimiter=\". \">\n              <group delimiter=\" \">\n                <text term=\"available at\"/>\n                <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n              </group>\n              <text prefix=\"[\" macro=\"access\" suffix=\"]\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"year-date\"/>\n              <text macro=\"volume\"/>\n              <text macro=\"issue\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"book\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"edition\"/>\n              <text macro=\"collection\" suffix=\", \"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"year-date\"/>\n              </group>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter entry-dictionary\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor-short-bib\"/>\n              </group>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"year-date\"/>\n                <text macro=\"pages\" suffix=\".\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\" match=\"any\">\n            <group>\n              <choose>\n                <if variable=\"editor\" match=\"any\">\n                  <text macro=\"editor-short-bib\"/>\n                  <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n                </if>\n                <else>\n                  <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n                </else>\n              </choose>\n              <text variable=\"publisher-place\" prefix=\", \" suffix=\", \"/>\n              <text variable=\"publisher\"/>\n              <text macro=\"year-date\" prefix=\", \"/>\n              <text variable=\"page\" suffix=\".\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group>\n              <text variable=\"genre\" prefix=\", \" font-style=\"italic\"/>\n              <text variable=\"publisher-place\" prefix=\", \" suffix=\", \"/>\n              <text variable=\"publisher\"/>\n              <text macro=\"year-date\" prefix=\", \"/>\n              <text variable=\"page\" prefix=\", \"/>\n            </group>\n          </else-if>\n          <else-if type=\"entry-encyclopedia\" match=\"any\">\n            <group>\n              <text macro=\"year-date\" prefix=\" (\" suffix=\") : \"/>\n            </group>\n            <group delimiter=\", \">\n              <choose>\n                <if variable=\"title-short\" match=\"any\">\n                  <text variable=\"title-short\" font-style=\"italic\"/>\n                </if>\n                <else>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </else>\n              </choose>\n              <text variable=\"volume\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"