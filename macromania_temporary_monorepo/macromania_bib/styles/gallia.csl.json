"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\" default-locale=\"fr-FR\">\n  <info>\n    <title>Gallia (Français)</title>\n    <id>http://www.zotero.org/styles/gallia</id>\n    <link href=\"http://www.zotero.org/styles/gallia\" rel=\"self\"/>\n    <link href=\"https://journals.openedition.org/gallia/1305#tocto1n5\" rel=\"documentation\"/>\n    <author>\n      <name>Nicolas Monteix</name>\n      <email>nicolas.monteix@univ-rouen.fr</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"history\"/>\n    <category field=\"anthropology\"/>\n    <issn>0016-4119</issn>\n    <eissn>2109-9588</eissn>\n    <summary>Author-date style for Gallia - Archéologie des Gaules. Extra fields needed for exhibition catalogues: genre, event place, event date; place is to be empty replaced by publisher place in extra field. Ancient sources need to be inserted manually</summary>\n    <updated>2021-09-06T04:59:23+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr-FR\">\n    <terms>\n      <term name=\"editor\" form=\"short\">dir.</term>\n      <term name=\"in\">in</term>\n      <term name=\"accessed\">consulté le</term>\n      <term name=\"paragraph\" form=\"short\">§</term>\n      <term name=\"no date\">sans date</term>\n      <term name=\"no date\" form=\"short\">s.&#160;d.</term>\n      <term name=\"translator\" form=\"verb-short\">éd. et trad. par</term>\n    </terms>\n  </locale>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"book chapter\" variable=\"genre\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"event-place\"/>\n          <date variable=\"event-date\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor-encyclopedia\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\".&#160;\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <choose>\n      <if variable=\"container-author\">\n        <text macro=\"container-author\"/>\n      </if>\n      <else>\n        <names variable=\"editor\">\n          <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\".&#160;\" delimiter=\", \"/>\n          <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\" delimiter=\", \">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\".&#160;\" delimiter=\", \"/>\n      <et-al font-style=\"italic\" font-variant=\"normal\"/>\n    </names>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <text variable=\"page\" prefix=\"p.&#160;\"/>\n    </group>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\", \"/>\n      <et-al font-variant=\"normal\" font-style=\"italic\"/>\n      <substitute>\n        <text macro=\"editor-short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <choose>\n      <if variable=\"editor\">\n        <names variable=\"editor\">\n          <name form=\"short\" delimiter=\", \"/>\n          <label form=\"short\" text-case=\"lowercase\" prefix=\" \"/>\n          <et-al font-variant=\"normal\" font-style=\"italic\"/>\n        </names>\n      </if>\n      <else>\n        <text value=\"Collectif\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short-call\">\n    <names variable=\"author\" delimiter=\", \">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\".&#160;\" delimiter=\", \"/>\n      <substitute>\n        <text macro=\"editor-short-call\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-short-call\">\n    <choose>\n      <if variable=\"editor\">\n        <names variable=\"editor\">\n          <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\".&#160;\" delimiter=\", \"/>\n          <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n      <else>\n        <text value=\"Collectif\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text term=\"accessed\" suffix=\" \"/>\n      <date variable=\"accessed\">\n        <date-part name=\"day\" suffix=\" \"/>\n        <date-part name=\"month\" suffix=\" \"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"collection\">\n    <group prefix=\" (coll. \" suffix=\")\" delimiter=\", \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if variable=\"container-title\">\n        <group delimiter=\", \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <text macro=\"genre\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <text macro=\"genre\"/>\n        </group>\n      </if>\n      <else-if type=\"webpage\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"lowercase\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"&#160;; \">\n      <text macro=\"author-short\" suffix=\" \"/>\n      <text macro=\"year-date\"/>\n      <group prefix=\", \">\n        <choose>\n          <if locator=\"paragraph issue\" match=\"any\">\n            <label variable=\"locator\" form=\"short\" suffix=\"&#160;\"/>\n            <text variable=\"locator\"/>\n          </if>\n          <else>\n            <label variable=\"locator\" form=\"short\" suffix=\".&#160;\"/>\n            <text variable=\"locator\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author-short-call\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"page-first\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \" font-weight=\"bold\" suffix=\"&#160;\">\n        <text macro=\"author-short-call\"/>\n        <text macro=\"year-date\"/>\n        <group font-weight=\"normal\">\n          <group prefix=\"(\" suffix=\")\" delimiter=\" \">\n            <choose>\n              <if variable=\"original-date\" match=\"all\">\n                <group>\n                  <text value=\"1\"/>\n                  <text value=\"ère\" vertical-align=\"sup\"/>\n                </group>\n                <text term=\"edition\" form=\"short\"/>\n                <date variable=\"original-date\">\n                  <date-part name=\"year\"/>\n                </date>\n              </if>\n              <else-if is-numeric=\"edition\">\n                <text macro=\"edition\"/>\n              </else-if>\n            </choose>\n          </group>\n        </group>\n      </group>\n      <group prefix=\": \">\n        <choose>\n          <if type=\"thesis report\">\n            <group delimiter=\", \">\n              <text macro=\"title\" font-style=\"italic\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n              <choose>\n                <if variable=\"number-of-pages\" match=\"any\">\n                  <text variable=\"number-of-pages\" suffix=\" p.\"/>\n                </if>\n                <else>\n                  <text variable=\"page\" suffix=\" p.\"/>\n                </else>\n              </choose>\n            </group>\n          </if>\n          <else-if type=\"webpage\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"container-title\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-journal article-magazine article-newspaper broadcast personal_communication\" match=\"any\">\n            <group delimiter=\", \" suffix=\".\">\n              <text macro=\"title\"/>\n              <text macro=\"container-title\"/>\n              <group delimiter=\"&#8209;\">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\"/>\n              </group>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"book graphic\" match=\"any\">\n            <group suffix=\".\">\n              <text macro=\"title\" suffix=\", \"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"collection\"/>\n              <group>\n                <text variable=\"number-of-pages\" prefix=\", \" suffix=\" p.\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"container-title\"/>\n              <text macro=\"editor-encyclopedia\" prefix=\"éd. \"/>\n              <group delimiter=\" \">\n                <text macro=\"publisher\"/>\n                <text macro=\"collection\"/>\n              </group>\n              <group delimiter=\" \">\n                <text term=\"sub-verbo\" form=\"short\" font-style=\"italic\"/>\n                <text macro=\"title\"/>\n              </group>\n              <text macro=\"pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text macro=\"title\" suffix=\", \"/>\n            <group>\n              <choose>\n                <if variable=\"container-author editor\" match=\"none\">\n                  <text term=\"in\" suffix=\" \"/>\n                </if>\n                <else>\n                  <text term=\"in\" suffix=\" \" font-style=\"italic\"/>\n                  <text macro=\"editor\" suffix=\", \"/>\n                </else>\n              </choose>\n              <text macro=\"container-title\"/>\n              <text macro=\"publisher\" prefix=\", \"/>\n              <text macro=\"collection\"/>\n              <text macro=\"pages\" prefix=\", \" suffix=\".\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"collection\"/>\n              <group>\n                <text variable=\"number-of-pages\" suffix=\" p.\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <group prefix=\" [\" suffix=\"]\" delimiter=\", \">\n        <text variable=\"URL\" prefix=\"URL&#160;: \"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"