"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Folia Morphologia</title>\n    <id>http://www.zotero.org/styles/folia-morphologia</id>\n    <link href=\"http://www.zotero.org/styles/folia-morphologia\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/the-korean-journal-of-internal-medicine\" rel=\"template\"/>\n    <link href=\"https://journals.viamedica.pl/folia_morphologica/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0015-5659</issn>\n    <eissn>1644-3284</eissn>\n    <updated>2020-07-31T14:22:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"container-title\" form=\"short\"/>\n              <text macro=\"edition\"/>\n              <group delimiter=\" \">\n                <text macro=\"publisher\"/>\n                <group delimiter=\":\">\n                  <text macro=\"year-date\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"article-journal\">\n            <group delimiter=\"; \">\n              <group delimiter=\". \">\n                <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n                <text macro=\"year-date\"/>\n              </group>\n              <group delimiter=\": \">\n                <group>\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"edition\"/>\n              <group delimiter=\" \">\n                <text macro=\"publisher\"/>\n                <text macro=\"year-date\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"container-title\" suffix=\". \"/>\n              <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n                <text term=\"cited\"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"year\" suffix=\" \"/>\n                  <date-part name=\"month\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n                  <date-part name=\"day\"/>\n                </date>\n              </group>\n              <group delimiter=\": \">\n                <text term=\"available at\" text-case=\"capitalize-first\"/>\n                <text variable=\"URL\" suffix=\". \"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\". \">\n              <text macro=\"year-date\"/>\n              <group delimiter=\", \">\n                <text variable=\"genre\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group>\n              <group delimiter=\". \">\n                <text variable=\"container-title\" form=\"short\" suffix=\" \"/>\n                <text macro=\"year-date\"/>\n                <text macro=\"publisher\"/>\n              </group>\n              <group prefix=\", \" delimiter=\": \">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text variable=\"DOI\" prefix=\", doi: \"/>\n    </layout>\n  </bibliography>\n</style>\n"