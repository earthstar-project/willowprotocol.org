"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>MIS Quarterly</title>\n    <title-short>MISQ</title-short>\n    <id>http://www.zotero.org/styles/mis-quarterly</id>\n    <link href=\"http://www.zotero.org/styles/mis-quarterly\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/apa\" rel=\"template\"/>\n    <link href=\"http://www.misq.org/manuscript-guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Mikko Rönkkö</name>\n      <email>mikko.ronkko@tkk.fi</email>\n    </author>\n    <author>\n      <name>Simon Kornblith</name>\n      <email>simon@simonster.com</email>\n    </author>\n    <author>\n      <name>Brenton M. Wiernik</name>\n      <email>zotero@wiernik.org</email>\n    </author>\n    <contributor>\n      <name>Bruce D'Arcus</name>\n    </contributor>\n    <contributor>\n      <name>Curtis M. Humphrey</name>\n    </contributor>\n    <contributor>\n      <name>Richard Karnesky</name>\n      <email>karnesky+zotero@gmail.com</email>\n      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <contributor>\n      <name>James Howison</name>\n      <email>james@howison.name</email>\n      <uri>http://james.howison.name</uri>\n    </contributor>\n    <contributor>\n      <name>Gerit Wagner</name>\n    </contributor>\n    <contributor>\n      <name>Dennis Riehle</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <issn>0276-7783</issn>\n    <updated>2014-12-10T00:32:45+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en-US\">\n    <terms>\n      <term name=\"ordinal\">ᵗʰ</term>\n      <term name=\"ordinal-01\">ˢᵗ</term>\n      <term name=\"ordinal-02\">ⁿᵈ</term>\n      <term name=\"ordinal-03\">ʳᵈ</term>\n      <term name=\"ordinal-04\">ᵗʰ</term>\n      <term name=\"ordinal-11\">ᵗʰ</term>\n      <term name=\"ordinal-12\">ᵗʰ</term>\n      <term name=\"ordinal-13\">ᵗʰ</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if variable=\"container-title\" type=\"post-weblog webpage post report thesis\" match=\"none\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\" text-case=\"title\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\" text-case=\"title\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text prefix=\"(\" term=\"no date\" suffix=\")\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\", \">\n      <choose>\n        <if variable=\"container-title\" type=\"post-weblog webpage post report thesis\" match=\"none\">\n          <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n        </if>\n        <else>\n          <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n        </else>\n      </choose>\n      <choose>\n        <if type=\"report thesis\" match=\"any\">\n          <group delimiter=\" \">\n            <text variable=\"genre\"/>\n            <text variable=\"number\" prefix=\" No. \"/>\n          </group>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if variable=\"volume\">\n        <group>\n          <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <number variable=\"volume\" form=\"numeric\"/>\n        </group>\n      </if>\n      <else>\n        <group>\n          <text term=\"volume\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1-\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-info\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <text term=\"in\"/>\n        </if>\n      </choose>\n      <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      <choose>\n        <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n          <group prefix=\"(\" suffix=\")\" delimiter=\":\">\n            <text variable=\"volume\"/>\n            <text variable=\"issue\"/>\n          </group>\n        </if>\n        <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n          <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n            <text macro=\"edition\"/>\n            <text macro=\"volume\"/>\n          </group>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor translator\" delimiter=\", \">\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n      <else>\n        <names variable=\"editor translator\" delimiter=\", \" prefix=\"(\" suffix=\")\">\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \"/>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event-genre\">\n    <choose>\n      <if variable=\"container-title\" match=\"none\">\n        <choose>\n          <if variable=\"event\">\n            <group delimiter=\" \">\n              <text variable=\"genre\"/>\n              <text term=\"presented at\"/>\n              <text variable=\"event\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            <text variable=\"medium\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n    <choose>\n      <if type=\"speech paper-conference report post post-weblog webpage interview manuscript dataset\" match=\"any\">\n        <date variable=\"issued\" prefix=\", \" delimiter=\" \">\n          <date-part name=\"month\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if is-numeric=\"page\">\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"page\" text-case=\"capitalize-first\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else>\n        <text variable=\"URL\"/>\n        <choose>\n          <if type=\"post post-weblog webpage manuscript dataset\" match=\"any\">\n            <group prefix=\", \" delimiter=\" \">\n              <text term=\"accessed\"/>\n              <date variable=\"accessed\">\n                <date-part name=\"month\" form=\"long\"/>\n                <date-part name=\"day\" form=\"numeric\" suffix=\",\" prefix=\" \"/>\n                <date-part name=\"year\" prefix=\" \"/>\n              </date>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" givenname-disambiguation-rule=\"by-cite\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-year\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n        </group>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"1\" line-spacing=\"1\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-year\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"issued\"/>\n        <group delimiter=\", \">\n          <text macro=\"title\"/>\n          <text macro=\"container-info\"/>\n          <text variable=\"collection-title\" text-case=\"title\"/>\n          <text macro=\"container-contributors\"/>\n          <text macro=\"event-genre\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"page\"/>\n        </group>\n        <text macro=\"access\" prefix=\" (\" suffix=\")\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"