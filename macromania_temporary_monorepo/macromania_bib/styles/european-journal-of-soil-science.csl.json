"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>European Journal of Soil Science</title>\n    <id>http://www.zotero.org/styles/european-journal-of-soil-science</id>\n    <link href=\"http://www.zotero.org/styles/european-journal-of-soil-science\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/asa-cssa-sssa\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/(ISSN)1365-2389/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Ross Bricklemyer</name>\n      <email>ross.bricklemyer@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Tom Gottfried</name>\n      <email>tom.gottfried@tum.de</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1351-0754</issn>\n    <eissn>1365-2389</eissn>\n    <updated>2017-10-13T21:44:24+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\"\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\"\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"(\" suffix=\")\">\n          <text term=\"at\" text-case=\"capitalize-first\" suffix=\": \"/>\n          <text variable=\"URL\" suffix=\". \"/>\n          <group>\n            <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\"/\"/>\n              <date-part name=\"month\" form=\"numeric\" suffix=\"/\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation collapse=\"year-suffix\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key variable=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\" suffix=\",\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\" suffix=\".\"/>\n      <date variable=\"issued\" prefix=\" \" suffix=\".\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \" font-style=\"italic\"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <text macro=\"edition\" prefix=\" \"/>\n          <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n          <text macro=\"access\" prefix=\" \" suffix=\".\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\". \"/>\n          <group delimiter=\", \" suffix=\".\">\n            <group>\n              <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <names variable=\"editor\" prefix=\" (\" suffix=\")\">\n                <label form=\"short\" suffix=\" \"/>\n                <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n              </names>\n            </group>\n            <group delimiter=\" \">\n              <label variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n          <text macro=\"edition\" prefix=\" \"/>\n          <group suffix=\".\">\n            <text macro=\"publisher\" prefix=\" \" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\" font-style=\"italic\"/>\n          <group prefix=\" \" delimiter=\", \" suffix=\".\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            <text variable=\"publisher\"/>\n          </group>\n          <text macro=\"access\" prefix=\" \" suffix=\".\"/>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" delimiter=\", \" suffix=\".\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"volume\" font-weight=\"bold\"/>\n            <text variable=\"page\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"