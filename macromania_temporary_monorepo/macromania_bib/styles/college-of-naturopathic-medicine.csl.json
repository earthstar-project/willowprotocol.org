"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"in-text\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\" page-range-format=\"expanded\">\n  <info>\n    <title>College of Naturopathic Medicine</title>\n    <title-short>CNM</title-short>\n    <id>http://www.zotero.org/styles/college-of-naturopathic-medicine</id>\n    <link href=\"http://www.zotero.org/styles/college-of-naturopathic-medicine\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-durham-university-business-school\" rel=\"template\"/>\n    <link href=\"https://github.com/citation-style-language/styles/pull/3815#issuecomment-445793526\" rel=\"documentation\"/>\n    <author>\n      <name>Mike P ODonoghue</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <updated>2018-12-05T16:44:33+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\" \"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\" suffix=\", \">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL DOI\" match=\"any\">\n        <group>\n          <text value=\"Available at: \"/>\n          <choose>\n            <if variable=\"DOI\">\n              <text variable=\"DOI\" text-decoration=\"underline\" prefix=\"doi:\"/>\n            </if>\n            <else>\n              <text variable=\"URL\"/>\n            </else>\n          </choose>\n          <group prefix=\" (\" suffix=\")\">\n            <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book webpage graphic\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-case=\"capitalize-first\" prefix=\"'\" suffix=\"'\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" suffix=\".\">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <text variable=\"page\" prefix=\"pp. \"/>\n    </group>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if variable=\"DOI URL\" match=\"any\">\n        <text value=\"[Online]\" suffix=\".\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-newspaper paper-conference broadcast\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\"/>\n        </date>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" prefix=\" \"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\" et-al-min=\"4\" et-al-use-first=\"3\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <text macro=\"year-date\" prefix=\" (\" suffix=\"). \"/>\n      <choose>\n        <if type=\"thesis\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text macro=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"online\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"webpage\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"online\"/>\n          </group>\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <date variable=\"issued\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n            <text variable=\"container-title\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-newspaper broadcast personal_communication thesis\" match=\"any\">\n          <group suffix=\",\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <choose>\n            <if variable=\"author\" match=\"any\">\n              <text variable=\"container-title\" text-case=\"capitalize-all\" font-style=\"italic\" prefix=\" \" suffix=\",\"/>\n            </if>\n          </choose>\n          <group prefix=\" \" suffix=\".\">\n            <text variable=\"volume\"/>\n            <group delimiter=\", \">\n              <text variable=\"issue\" prefix=\" (\" suffix=\")\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n          <text macro=\"online\" prefix=\" \"/>\n        </else-if>\n        <else-if type=\"book graphic\" match=\"any\">\n          <group prefix=\" \" delimiter=\". \" suffix=\".\">\n            <text variable=\"note\"/>\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <text variable=\"collection-number\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"online\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\". \"/>\n          <group delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <text macro=\"editor\" suffix=\". \"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text macro=\"edition\" prefix=\" \" suffix=\".\"/>\n            <text macro=\"online\" suffix=\".\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"publisher\" suffix=\".\"/>\n            <label variable=\"page\" form=\"short\"/>\n            <text macro=\"pages\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"editor\" suffix=\".\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text macro=\"online\" suffix=\".\"/>\n            <group suffix=\".\">\n              <group>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\" (\" suffix=\")\"/>\n                <text macro=\"issued\"/>\n              </group>\n              <group prefix=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"