"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>Karstenia</title>\n    <id>http://www.zotero.org/styles/karstenia</id>\n    <link href=\"http://www.zotero.org/styles/karstenia\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/myrmecological-news\" rel=\"template\"/>\n    <link href=\"http://karstenia.fi/instructions-to-authors/#Manuscript%20preparation\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0453-3402</issn>\n    <updated>2020-07-15T14:25:19+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"symbol\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" \" suffix=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter-precedes-last=\"never\" et-al-min=\"3\" et-al-use-first=\"1\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author\"/>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n        <choose>\n          <if type=\"book thesis\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\" text-case=\"capitalize-all\"/>\n              </group>\n              <group delimiter=\", \">\n                <group delimiter=\". \">\n                  <text variable=\"container-title\"/>\n                  <text variable=\"collection-title\"/>\n                </group>\n                <text macro=\"pages\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \" prefix=\" \">\n                <text variable=\"container-title\"/>\n                <group delimiter=\": \">\n                  <text variable=\"volume\"/>\n                  <text macro=\"pages\"/>\n                </group>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"