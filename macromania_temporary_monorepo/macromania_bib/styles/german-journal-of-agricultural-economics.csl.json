"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\">\n  <info>\n    <title>German Journal of Agricultural Economics</title>\n    <title-short>GJAE</title-short>\n    <id>http://www.zotero.org/styles/german-journal-of-agricultural-economics</id>\n    <link href=\"http://www.zotero.org/styles/german-journal-of-agricultural-economics\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/veterinary-medicine-austria\" rel=\"template\"/>\n    <link href=\"https://www.gjae-online.de/instructions-for-authors/\" rel=\"documentation\"/>\n    <link href=\"https://www.gjae-online.de/wp-content/uploads/Guide_for_Authors-Dec_2020.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <issn>0002-1121</issn>\n    <eissn>2191-4028</eissn>\n    <updated>2021-11-10T15:34:02+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"and\">u.</term>\n      <term name=\"page\" form=\"short\">\n        <single>p</single>\n        <multiple>pp</multiple>\n      </term>\n      <term name=\"accessed\">letzter Zugriff</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name initialize-with=\".\" name-as-sort-order=\"first\">\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"first\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" font-variant=\"normal\" and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" suffix=\",\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\",\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"none\" variable=\"page volume\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\". \">\n      <number variable=\"volume\"/>\n      <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&#8211;\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <group delimiter=\": \">\n          <date variable=\"issued\" prefix=\" (\" suffix=\"):\">\n            <date-part name=\"year\"/>\n          </date>\n          <text macro=\"title\" suffix=\".\"/>\n        </group>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n            <group delimiter=\" \">\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n                <text variable=\"container-title\"/>\n              </group>\n              <text macro=\"edition\"/>\n              <text variable=\"collection-title\" suffix=\".\"/>\n              <text macro=\"volume\"/>\n              <group delimiter=\": \">\n                <text macro=\"publisher\" prefix=\" \"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\" match=\"any\">\n            <group delimiter=\" \">\n              <text macro=\"editor\"/>\n              <text suffix=\",\" variable=\"container-title\"/>\n              <text macro=\"edition\"/>\n              <text suffix=\".\" variable=\"collection-title\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis report\">\n            <group delimiter=\". \">\n              <group delimiter=\" \">\n                <text variable=\"genre\"/>\n                <text variable=\"number\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"URL\"/>\n              <group delimiter=\": \">\n                <text term=\"accessed\"/>\n                <date form=\"text\" variable=\"accessed\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\": \">\n              <group delimiter=\" \" prefix=\" \">\n                <group delimiter=\": \">\n                  <text term=\"in\" text-case=\"capitalize-first\"/>\n                  <text variable=\"container-title\" strip-periods=\"false\"/>\n                </group>\n                <text variable=\"volume\" font-weight=\"normal\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"