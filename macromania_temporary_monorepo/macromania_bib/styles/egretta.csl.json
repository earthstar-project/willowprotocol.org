"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"symbol\" initialize-with=\".\" demote-non-dropping-particle=\"never\" default-locale=\"de-AT\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Egretta (Deutsch - Ã–sterreich)</title>\n    <title-short>Egretta</title-short>\n    <id>http://www.zotero.org/styles/egretta</id>\n    <link href=\"http://www.zotero.org/styles/egretta\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-zoo-biology\" rel=\"template\"/>\n    <link href=\"https://www.zobodat.at/zeitschriftinfos/egretta_aut.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Georg Mayr-Duffner</name>\n      <email>g.duffner@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0013-2373</issn>\n    <updated>2017-10-28T21:35:14+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"accessed\">Zugriff am</term>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"anonymous\" form=\"long\">Ohne Verfasser</term>\n      <term name=\"anonymous\" form=\"short\">O.V.</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\">\n      <name and=\"symbol\" name-as-sort-order=\"first\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" name-as-sort-order=\"first\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book report\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <choose>\n              <if match=\"any\" variable=\"volume\">\n                <group delimiter=\" \">\n                  <text term=\"volume\" suffix=\" \"/>\n                  <text variable=\"volume\"/>\n                </group>\n              </if>\n            </choose>\n          </group>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"year-suffix\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n        <text variable=\"year-suffix\" prefix=\"-\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number suffix=\". \" variable=\"edition\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\" form=\"short\"/>\n          <group delimiter=\": \">\n            <group delimiter=\" \">\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"in\" text-case=\"capitalize-first\" font-style=\"normal\" suffix=\":\"/>\n          <text macro=\"editor-translator\"/>\n          <group delimiter=\": \">\n            <text variable=\"container-title\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n      <else-if type=\"report\" match=\"any\">\n        <text variable=\"genre\"/>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"internet-locator\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"DOI: \"/>\n      </if>\n      <else-if match=\"any\" variable=\"URL\">\n        <group delimiter=\", \">\n          <text variable=\"URL\" prefix=\"URL: \"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\"/>\n            <date form=\"numeric\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\" year-suffix-delimiter=\",\">\n    <sort>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <group prefix=\", \" delimiter=\" \">\n        <label variable=\"locator\" form=\"short\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography name-as-sort-order=\"first\" sort-separator=\" \" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author\"/>\n          <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n        </group>\n        <group delimiter=\". \">\n          <text macro=\"title\"/>\n          <text macro=\"container\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"internet-locator\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"