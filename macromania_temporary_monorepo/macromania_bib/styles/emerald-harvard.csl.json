"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Emerald - Harvard</title>\n    <id>http://www.zotero.org/styles/emerald-harvard</id>\n    <link href=\"http://www.zotero.org/styles/emerald-harvard\" rel=\"self\"/>\n    <link href=\"https://www.emeraldgrouppublishing.com/use-harvard-reference-system\" rel=\"documentation\"/>\n    <link href=\"https://www.emeraldgrouppublishing.com/journal/jmp?id=jmp#author-guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Bruce D'Arcus</name>\n    </contributor>\n    <contributor>\n      <name>Curtis M. Humphrey</name>\n    </contributor>\n    <contributor>\n      <name>Richard Karnesky</name>\n      <email>karnesky+zotero@gmail.com</email>\n      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>\n    </contributor>\n    <contributor>\n      <name>Simon Kornblith</name>\n      <email>simon@simonster.com</email>\n    </contributor>\n    <contributor>\n      <name>Brenton M. Wiernik</name>\n      <email>zotero@wiernik.org</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <updated>2022-12-12T18:17:18+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <style-options punctuation-in-quote=\"false\"/>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" suffix=\" \"/>\n        <names variable=\"editor translator\" delimiter=\",\" suffix=\", \">\n          <name and=\"text\" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\" name-as-sort-order=\"all\" sort-separator=\", \"/>\n          <label form=\"short\" prefix=\" (\" text-case=\"capitalize-first\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\",\">\n          <label form=\"verb\" suffix=\" \"/>\n          <name and=\"text\" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\" name-as-sort-order=\"all\" sort-separator=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"article-magazine article-newspaper entry entry-dictionary entry-encyclopedia\" match=\"any\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n        </choose>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"article-magazine article-newspaper entry entry-dictionary entry-encyclopedia\" match=\"any\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n        </choose>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\" text-case=\"title\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"thesis\" variable=\"archive\" match=\"all\">\n        <group delimiter=\" \">\n          <text term=\"available at\" suffix=\":\"/>\n          <text variable=\"archive\" suffix=\".\"/>\n          <text variable=\"archive_location\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"doi: \"/>\n          </if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\" \">\n              <text term=\"available at\" suffix=\":\"/>\n              <text variable=\"URL\"/>\n              <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                <text term=\"accessed\"/>\n                <date variable=\"accessed\" delimiter=\" \">\n                  <date-part name=\"day\"/>\n                  <date-part name=\"month\"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </group>\n          </else-if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n        <group prefix=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"number\" prefix=\" No. \"/>\n        </group>\n      </if>\n      <else-if type=\"manuscript\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n        <group prefix=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"number\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis report\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"event\" match=\"none\">\n            <text variable=\"genre\" suffix=\", \"/>\n          </if>\n        </choose>\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <choose>\n          <if variable=\"genre\" match=\"none\">\n            <text term=\"presented at\" suffix=\" \"/>\n            <text variable=\"event\"/>\n          </if>\n          <else>\n            <group>\n              <text variable=\"genre\"/>\n              <text term=\"presented at\" suffix=\" \"/>\n              <text variable=\"event\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <group prefix=\" (\" suffix=\"),\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <text prefix=\" (\" term=\"no date\" suffix=\").\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"periodical-date\">\n    <choose>\n      <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"day\"/>\n          <date-part name=\"month\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\", \">\n          <group>\n            <text variable=\"volume\" prefix=\"Vol. \"/>\n            <text variable=\"issue\" prefix=\" No. \"/>\n          </group>\n          <group>\n            <label variable=\"page\" form=\"short\" suffix=\" \"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"edition\"/>\n          <group>\n            <choose>\n              <if variable=\"volume\" match=\"any\">\n                <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <number variable=\"volume\" form=\"numeric\"/>\n              </if>\n              <else>\n                <text term=\"volume\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1-\"/>\n              </else>\n            </choose>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-year\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"issued-year\"/>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"8\" et-al-use-first=\"7\" entry-spacing=\"0\" line-spacing=\"2\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-year\" sort=\"ascending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group>\n        <text macro=\"author\" suffix=\".\"/>\n        <text macro=\"issued\" suffix=\" \"/>\n        <group delimiter=\", \">\n          <text macro=\"title\"/>\n          <group>\n            <text macro=\"container-contributors\"/>\n            <text macro=\"secondary-contributors\"/>\n            <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n          </group>\n          <choose>\n            <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n              <text macro=\"locators\"/>\n            </if>\n          </choose>\n        </group>\n        <group delimiter=\", \" prefix=\", \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"periodical-date\"/>\n          <choose>\n            <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n              <text macro=\"locators\"/>\n            </if>\n            <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n              <group>\n                <label variable=\"page\" form=\"short\" suffix=\" \"/>\n                <text variable=\"page\"/>\n              </group>\n            </else-if>\n          </choose>\n        </group>\n      </group>\n      <text macro=\"access\" prefix=\", \"/>\n    </layout>\n  </bibliography>\n</style>\n"