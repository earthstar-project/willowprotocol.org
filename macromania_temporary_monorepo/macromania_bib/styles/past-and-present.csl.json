"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Past &amp; Present</title>\n    <id>http://www.zotero.org/styles/past-and-present</id>\n    <link href=\"http://www.zotero.org/styles/past-and-present\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/manchester-university-press\" rel=\"template\"/>\n    <link href=\"http://www.oxfordjournals.org/our_journals/past/for_authors/style_guide.docx\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <issn>0031-2746</issn>\n    <eissn>1477-464X</eissn>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"folio\" form=\"short\">\n        <single>fo.</single>\n        <multiple>fos.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-editor\">\n    <group delimiter=\" \">\n      <text term=\"in\" suffix=\" \"/>\n      <names variable=\"editor\">\n        <name and=\"text\" delimiter=\", \"/>\n        <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n        <substitute>\n          <names variable=\"container-author\"/>\n          <names variable=\"translator\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <label form=\"short\" prefix=\" \" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"manuscript personal_communication\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\" form=\"short\"/>\n      </if>\n      <else-if type=\"manuscript personal_communication\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" form=\"short\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"thesis\">\n          <text variable=\"publisher\"/>\n          <text variable=\"genre\"/>\n        </if>\n        <else>\n          <text variable=\"publisher-place\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\", \">\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n      <text variable=\"archive_location\"/>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-volumes\"/>\n      <text term=\"volume\" form=\"short\" plural=\"true\"/>\n    </group>\n  </macro>\n  <macro name=\"pageref\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <label variable=\"locator\" suffix=\" \" form=\"short\"/>\n        </if>\n      </choose>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"volref\">\n    <number variable=\"volume\" form=\"roman\"/>\n  </macro>\n  <macro name=\"container\">\n    <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"pageref\"/>\n          </group>\n        </if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <choose>\n              <if type=\"thesis\">\n                <group delimiter=\" \">\n                  <text macro=\"title\"/>\n                  <group delimiter=\", \">\n                    <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n                      <text macro=\"publisher\"/>\n                      <text macro=\"container\"/>\n                      <text macro=\"date\"/>\n                    </group>\n                    <text macro=\"volref\"/>\n                    <text macro=\"pageref\"/>\n                  </group>\n                </group>\n              </if>\n              <else-if type=\"bill book graphic legal_case legislation manuscript motion_picture personal_communication report song\" match=\"any\">\n                <group delimiter=\" \">\n                  <group delimiter=\", \">\n                    <text macro=\"title\"/>\n                    <text macro=\"editor\"/>\n                    <text macro=\"volumes\"/>\n                  </group>\n                  <group delimiter=\", \">\n                    <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n                      <text macro=\"publisher\"/>\n                      <text macro=\"container\"/>\n                      <text macro=\"date\"/>\n                    </group>\n                    <text macro=\"archive\"/>\n                    <text macro=\"volref\"/>\n                    <text macro=\"pageref\"/>\n                  </group>\n                </group>\n              </else-if>\n              <else-if type=\"chapter paper-conference\" match=\"any\">\n                <group delimiter=\" \">\n                  <group delimiter=\", \">\n                    <text macro=\"title\"/>\n                    <text macro=\"container-editor\"/>\n                    <text macro=\"container\"/>\n                    <text macro=\"volumes\"/>\n                  </group>\n                  <group delimiter=\", \">\n                    <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n                      <text macro=\"publisher\"/>\n                      <text macro=\"date\"/>\n                    </group>\n                    <text macro=\"volref\"/>\n                    <text macro=\"pageref\"/>\n                  </group>\n                </group>\n              </else-if>\n              <else>\n                <group delimiter=\" \">\n                  <group delimiter=\", \">\n                    <text macro=\"title\"/>\n                    <text macro=\"publisher\"/>\n                    <text macro=\"container\"/>\n                    <group delimiter=\" \">\n                      <text macro=\"volref\"/>\n                      <text macro=\"date\" prefix=\"(\" suffix=\")\"/>\n                    </group>\n                    <text macro=\"volumes\"/>\n                    <text macro=\"pageref\"/>\n                  </group>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"