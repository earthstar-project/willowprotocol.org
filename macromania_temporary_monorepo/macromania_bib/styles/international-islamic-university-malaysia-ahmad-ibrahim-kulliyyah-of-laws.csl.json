"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" version=\"1.0\" class=\"note\" page-range-format=\"expanded\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\">\n  <info>\n    <title>International Islamic University Malaysia - Ahmad Ibrahim Kulliyyah of Laws</title>\n    <title-short>IIUM-AIKOL</title-short>\n    <id>http://www.zotero.org/styles/international-islamic-university-malaysia-ahmad-ibrahim-kulliyyah-of-laws</id>\n    <link href=\"http://www.zotero.org/styles/international-islamic-university-malaysia-ahmad-ibrahim-kulliyyah-of-laws\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/oxford-art-journal\" rel=\"template\"/>\n    <link href=\"https://forums.zotero.org/discussion/78653/style-request-aikol-iium-thesis-citation-style\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"law\"/>\n    <updated>2019-12-27T11:18:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"edition\" form=\"short\">edn.</term>\n      <term name=\"presented at\">Paper presented at</term>\n      <term name=\"open-quote\">\"</term>\n      <term name=\"close-quote\">\"</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-bib\">\n    <names variable=\"author\">\n      <name initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" font-style=\"normal\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-editor\">\n    <names variable=\"editor\">\n      <label form=\"verb\"/>\n      <name prefix=\" \" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-editor-bib\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if disambiguate=\"true\">\n        <text variable=\"title\" form=\"short\" quotes=\"false\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-bib\">\n    <choose>\n      <if type=\"paper-conference article-journal entry-encyclopedia entry-dictionary\" match=\"any\">\n        <text variable=\"title\" quotes=\"false\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song webpage post post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <choose>\n            <if type=\"report\" match=\"any\">\n              <text variable=\"number\" prefix=\"(Report No. \" suffix=\")\"/>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if match=\"any\" is-uncertain-date=\"issued\">\n        <text term=\"no date\" form=\"short\"/>\n      </if>\n      <else>\n        <choose>\n          <if type=\"article-newspaper article-magazine\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n              <date-part name=\"month\" prefix=\", \"/>\n              <date-part name=\"day\" prefix=\" \"/>\n            </date>\n          </if>\n          <else-if type=\"paper-conference\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"month\"/>\n              <date-part name=\"year\" prefix=\" \"/>\n            </date>\n          </else-if>\n          <else>\n            <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date-bib\">\n    <choose>\n      <if type=\"article-newspaper\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\"/>\n          <date-part name=\"month\" prefix=\" \"/>\n          <date-part name=\"year\" prefix=\", \"/>\n        </date>\n      </if>\n      <else>\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pageno\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <text variable=\"page\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pageref\">\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"journal-locator\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <label variable=\"volume\" form=\"short\"/>\n        <text variable=\"volume\"/>\n      </group>\n      <group delimiter=\" \">\n        <label variable=\"issue\" form=\"short\"/>\n        <text variable=\"issue\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"post post-weblog webpage\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"normal\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <choose>\n        <if locator=\"chapter\">\n          <label variable=\"locator\" text-case=\"capitalize-first\"/>\n        </if>\n      </choose>\n      <text variable=\"locator\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <number variable=\"edition\" form=\"ordinal\"/>\n      <label variable=\"edition\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"editor-translator\">\n    <names variable=\"translator editor\" delimiter=\", \">\n      <label form=\"verb\"/>\n      <name prefix=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n      <text term=\"accessed\"/>\n      <date variable=\"accessed\">\n        <date-part name=\"day\"/>\n        <date-part name=\"month\" prefix=\" \"/>\n        <date-part name=\"year\" prefix=\", \"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"page-bib\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"volref\">\n    <choose>\n      <if match=\"any\" is-numeric=\"volume\">\n        <group font-style=\"italic\" delimiter=\" \">\n          <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n          <text variable=\"volume\"/>\n        </group>\n      </if>\n      <else>\n        <group font-style=\"italic\" delimiter=\" \">\n          <label plural=\"always\" text-case=\"capitalize-first\" variable=\"number-of-volumes\" form=\"short\"/>\n          <text variable=\"number-of-volumes\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation and=\"text\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" givenname-disambiguation-rule=\"primary-name\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if match=\"any\" position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" text-case=\"capitalize-first\"/>\n            <text macro=\"citation-locator\"/>\n          </group>\n        </if>\n        <else-if match=\"any\" position=\"ibid\">\n          <text term=\"ibid\" text-case=\"capitalize-first\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"pageref\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <choose>\n              <if type=\"bill book graphic legal_case legislation motion_picture report song interview\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"volref\"/>\n                  <text macro=\"editor-translator\"/>\n                </group>\n                <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                  <text macro=\"publisher\"/>\n                  <text macro=\"container\"/>\n                  <text macro=\"edition\"/>\n                  <text macro=\"date\"/>\n                </group>\n                <text macro=\"pageref\"/>\n              </if>\n              <else-if type=\"chapter\">\n                <group delimiter=\", \">\n                  <group delimiter=\" \">\n                    <text macro=\"title\"/>\n                    <text term=\"in\" suffix=\" \"/>\n                    <text macro=\"container\"/>\n                    <text macro=\"volref\"/>\n                  </group>\n                  <group>\n                    <text macro=\"container-editor\"/>\n                    <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n                      <text macro=\"publisher\"/>\n                      <text macro=\"date\"/>\n                    </group>\n                  </group>\n                  <text macro=\"pageno\"/>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else-if>\n              <else-if type=\"paper-conference\">\n                <group delimiter=\", \">\n                  <group delimiter=\" \">\n                    <text macro=\"title\"/>\n                    <text term=\"in\" suffix=\" \"/>\n                    <text macro=\"container\"/>\n                  </group>\n                  <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                    <text macro=\"publisher\"/>\n                    <text macro=\"date\"/>\n                  </group>\n                  <text macro=\"pageno\"/>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else-if>\n              <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"container\"/>\n                  <text macro=\"date\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </else-if>\n              <else-if type=\"webpage post post-weblog\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"container\"/>\n                  <group delimiter=\" \">\n                    <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n                    <text macro=\"access\"/>\n                  </group>\n                </group>\n              </else-if>\n              <else-if type=\"thesis\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                    <text variable=\"genre\"/>\n                    <text variable=\"publisher\"/>\n                    <text macro=\"date\"/>\n                  </group>\n                  <text macro=\"pageref\"/>\n                </group>\n              </else-if>\n              <else-if type=\"entry-encyclopedia entry-dictionary\" match=\"any\">\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <group delimiter=\" \">\n                    <text term=\"in\"/>\n                    <text macro=\"container\"/>\n                    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                      <text variable=\"publisher\"/>\n                      <text macro=\"date\"/>\n                    </group>\n                  </group>\n                </group>\n              </else-if>\n              <else>\n                <group delimiter=\", \">\n                  <text macro=\"title\"/>\n                  <text macro=\"container\"/>\n                  <group delimiter=\": \">\n                    <group delimiter=\" \">\n                      <text macro=\"journal-locator\"/>\n                      <text macro=\"date\" prefix=\"(\" suffix=\")\"/>\n                    </group>\n                    <text macro=\"pageno\"/>\n                    <text macro=\"pageref\"/>\n                  </group>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography and=\"symbol\" hanging-indent=\"true\">\n    <sort>\n      <key variable=\"author\"/>\n      <key macro=\"date-bib\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"author-bib\"/>\n        <text macro=\"date\" prefix=\"(\" suffix=\")\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song interview\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\" \">\n                <group delimiter=\" \">\n                  <text macro=\"title-bib\"/>\n                  <text macro=\"volref\" font-style=\"italic\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text macro=\"editor-translator\"/>\n                <text macro=\"edition\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"container-editor-bib\"/>\n              </group>\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <text macro=\"container\"/>\n                  <text macro=\"page-bib\" prefix=\"(\" suffix=\")\"/>\n                  <text macro=\"volref\" prefix=\"(\" suffix=\")\"/>\n                </group>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"genre\" prefix=\"(\" suffix=\")\"/>\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title-bib\"/>\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <text term=\"presented at\"/>\n                  <text variable=\"container-title\"/>\n                </group>\n                <text variable=\"publisher-place\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"month\"/>\n                  <date-part name=\"day\" prefix=\" \"/>\n                  <date-part name=\"year\" prefix=\", \"/>\n                </date>\n              </group>\n              <group delimiter=\" \">\n                <group delimiter=\" \">\n                  <text term=\"from\"/>\n                  <text macro=\"container\"/>\n                </group>\n                <text variable=\"URL\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"entry-encyclopedia entry-dictionary\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title-bib\"/>\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"container\"/>\n                <group delimiter=\". \" prefix=\"(\" suffix=\")\">\n                  <group delimiter=\" \">\n                    <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n                    <number variable=\"volume\"/>\n                  </group>\n                  <text macro=\"page-bib\"/>\n                </group>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title-bib\"/>\n              <text macro=\"container\"/>\n              <text macro=\"page-bib\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <text macro=\"title-bib\"/>\n              <group delimiter=\" \">\n                <text macro=\"container\"/>\n                <group delimiter=\". \">\n                  <text macro=\"journal-locator\"/>\n                  <group>\n                    <label variable=\"page\" form=\"short\"/>\n                    <text variable=\"page\"/>\n                  </group>\n                </group>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"