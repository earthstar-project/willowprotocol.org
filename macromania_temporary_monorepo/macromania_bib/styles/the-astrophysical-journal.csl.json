"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>The Astrophysical Journal</title>\n    <id>http://www.zotero.org/styles/the-astrophysical-journal</id>\n    <link href=\"http://www.zotero.org/styles/the-astrophysical-journal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/nature\" rel=\"template\"/>\n    <link href=\"http://aas.org/journals/authors/common_instruct#references\" rel=\"documentation\"/>\n    <author>\n      <name>Chris Lowder</name>\n      <email>clowder@solar.physics.montana.edu</email>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <issn>0004-637X</issn>\n    <eissn>1538-4357</eissn>\n    <updated>2012-12-12T13:57:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\", \" delimiter=\", \" and=\"symbol\" initialize-with=\". \" delimiter-precedes-last=\"always\" name-as-sort-order=\"all\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" sort-separator=\", \" delimiter=\", \" and=\"symbol\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"issuance\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"editor\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor\">\n          <label form=\"short\" plural=\"never\" suffix=\" \"/>\n          <name and=\"symbol\" delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <text term=\"in\" form=\"long\" plural=\"false\"/>\n            </if>\n          </choose>\n          <text variable=\"container-title\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"volume\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n          <text variable=\"volume\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <text macro=\"edition\"/>\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n    </group>\n  </macro>\n  <citation collapse=\"year\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"6\" et-al-use-first=\"3\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issuance\"/>\n    </sort>\n    <layout>\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author\" suffix=\".\"/>\n          <text macro=\"issuance\"/>\n        </group>\n        <choose>\n          <if type=\"book report thesis\" match=\"any\">\n            <text variable=\"title\"/>\n          </if>\n        </choose>\n        <text macro=\"container-title\"/>\n        <text macro=\"editor\"/>\n        <text macro=\"volume\"/>\n      </group>\n      <text macro=\"publisher\" prefix=\" \"/>\n      <text variable=\"page-first\" prefix=\", \"/>\n      <text variable=\"URL\" prefix=\", \"/>\n    </layout>\n  </bibliography>\n</style>\n"