"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <!-- This style was edited with the Visual CSL Editor (https://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Trends in Glycoscience and Glycotechnology</title>\n    <title-short>TIGG</title-short>\n    <id>http://www.zotero.org/styles/trends-in-glycoscience-and-glycotechnology</id>\n    <link href=\"http://www.zotero.org/styles/trends-in-glycoscience-and-glycotechnology\" rel=\"self\"/>\n    <link href=\"http://www.fcca.gr.jp/TIGG/Guide.html\" rel=\"documentation\"/>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>0915-7352</issn>\n    <eissn>1883-2113</eissn>\n    <summary>TIGG Style</summary>\n    <updated>2019-07-24T05:42:30+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\", \" initialize-with=\". \" name-as-sort-order=\"all\" delimiter=\"; \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name suffix=\" \" and=\"text\" delimiter-precedes-last=\"never\" et-al-min=\"3\" et-al-use-first=\"1\" initialize-with=\".  \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \" suffix=\".  \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <text variable=\"collection-title\"/>\n  </macro>\n  <macro name=\"pages\">\n    <label suffix=\" \" form=\"short\" variable=\"page\"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"book-container\">\n    <group delimiter=\" \">\n      <text term=\"in\" text-case=\"capitalize-first\"/>\n      <text variable=\"container-title\" font-style=\"italic\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"full-issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n      <date-part name=\"day\" suffix=\", \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"baseline\" delimiter=\",\" prefix=\"(\" suffix=\")\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\"/>\n      <choose>\n        <if type=\"article-magazine\">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"full-issued\" suffix=\",\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"genre\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"volume\"/>\n            <text macro=\"pages\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\"; \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"editor\" prefix=\" \"/>\n            <text macro=\"series\"/>\n            <text macro=\"edition\"/>\n            <choose>\n              <if type=\"report\">\n                <group delimiter=\" \">\n                  <text variable=\"genre\"/>\n                  <text variable=\"number\"/>\n                </group>\n              </if>\n            </choose>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"number\"/>\n            </group>\n            <date variable=\"issued\" form=\"text\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\"; \">\n            <text macro=\"book-container\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"series\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\" \">\n            <text variable=\"title\"/>\n            <text variable=\"URL\"/>\n            <date variable=\"accessed\" prefix=\"(accessed \" suffix=\")\" delimiter=\" \">\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\" \">\n            <text macro=\"issued\" font-weight=\"normal\" prefix=\"(\" suffix=\")\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n              <text variable=\"volume\" font-style=\"normal\" font-weight=\"bold\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"issued\" font-weight=\"bold\"/>\n              <text variable=\"volume\" font-style=\"italic\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"