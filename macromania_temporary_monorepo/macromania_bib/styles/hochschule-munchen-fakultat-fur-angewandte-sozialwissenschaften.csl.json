"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"de-DE\">\n  <info>\n    <title>Hochschule München - Fakultät für Angewandte Sozialwissenschaften (Deutsch)</title>\n    <title-short>MUAS</title-short>\n    <id>http://www.zotero.org/styles/hochschule-munchen-fakultat-fur-angewandte-sozialwissenschaften</id>\n    <link href=\"http://www.zotero.org/styles/hochschule-munchen-fakultat-fur-angewandte-sozialwissenschaften\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/taylor-and-francis-harvard-x\" rel=\"template\"/>\n    <link href=\"http://www.sw.hm.edu/publikationen/lehrmaterialien/schoenberger.de.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <updated>2017-12-05T12:26:30+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"from\">vom</term>\n      <term name=\"available at\">verfügbar über</term>\n      <term name=\"accessed\">letzter Zugriff</term>\n      <term name=\"volume\" form=\"short\">Jg.</term>\n    </terms>\n  </locale>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference article-newspaper\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" font-style=\"normal\" suffix=\": \"/>\n        <group delimiter=\" \">\n          <names variable=\"editor translator\" delimiter=\", \">\n            <name delimiter=\"; \" and=\"text\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" name-as-sort-order=\"all\"/>\n            <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n          </names>\n          <text variable=\"container-title\" font-style=\"normal\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"container-title\"/>\n      </else-if>\n      <else-if type=\"broadcast\">\n        <choose>\n          <if variable=\"container-title\">\n            <group delimiter=\", \">\n              <text variable=\"number\" prefix=\"Episode \"/>\n              <text variable=\"title\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <group delimiter=\": \">\n          <text term=\"in\" text-case=\"capitalize-first\"/>\n          <text variable=\"container-title\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"motion_picture broadcast\" match=\"none\">\n        <names variable=\"author\">\n          <name delimiter=\"; \" and=\"text\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" name-as-sort-order=\"all\"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"title\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"motion_picture broadcast\" match=\"none\">\n        <names variable=\"author\">\n          <name form=\"short\" and=\"text\" delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n          <et-al font-style=\"italic\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <choose>\n              <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n                <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n              </if>\n              <else>\n                <text variable=\"title\" form=\"short\"/>\n              </else>\n            </choose>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"container-title\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <group delimiter=\" \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group delimiter=\": \" prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date form=\"numeric\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\"/>\n          <text variable=\"number\" prefix=\" No. \"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation report song speech\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\" font-style=\"normal\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else-if type=\"motion_picture broadcast\" match=\"any\"/>\n      <else>\n        <text variable=\"title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"report-details\">\n    <choose>\n      <if type=\"report\">\n        <group>\n          <text variable=\"genre\"/>\n          <text variable=\"number\" prefix=\" No. \"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n          </group>\n          <text variable=\"genre\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <group delimiter=\": \">\n            <text variable=\"publisher-place\"/>\n            <text variable=\"publisher\"/>\n          </group>\n          <text macro=\"report-details\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <text variable=\"event\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if variable=\"accessed\">\n        <choose>\n          <if type=\"webpage\">\n            <date variable=\"accessed\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <group delimiter=\" \">\n            <number variable=\"edition\" form=\"ordinal\"/>\n            <text term=\"edition\" form=\"short\"/>\n          </group>\n        </if>\n        <else>\n          <text variable=\"edition\" suffix=\".\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\" \" prefix=\": \">\n          <group delimiter=\" \">\n            <group delimiter=\" \">\n              <label variable=\"volume\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <group delimiter=\", \" prefix=\", \">\n          <group delimiter=\" \">\n            <group>\n              <label text-case=\"capitalize-first\" variable=\"number\" form=\"short\"/>\n              <text variable=\"call-number\"/>\n              <text term=\"from\"/>\n            </group>\n            <date form=\"numeric\" variable=\"issued\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"bill  book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </else-if>\n      <else-if type=\"broadcast\">\n        <group delimiter=\". \" prefix=\", \">\n          <text variable=\"publisher\"/>\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\", \" prefix=\". \">\n            <text macro=\"event\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"2\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"0\" line-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\"/>\n      <group delimiter=\" \" prefix=\" \">\n        <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n        <text macro=\"title\"/>\n        <text macro=\"container\"/>\n        <text variable=\"medium\"/>\n      </group>\n      <text macro=\"locators\"/>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"