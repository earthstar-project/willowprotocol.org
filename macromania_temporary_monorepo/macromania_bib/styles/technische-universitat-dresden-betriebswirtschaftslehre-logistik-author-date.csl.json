"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Technische Universität Dresden - Betriebswirtschaftslehre/Logistik (author-date)</title>\n    <title-short>TUD BWL/Logistik (author-date)</title-short>\n    <id>http://www.zotero.org/styles/technische-universitat-dresden-betriebswirtschaftslehre-logistik-author-date</id>\n    <link href=\"http://www.zotero.org/styles/technische-universitat-dresden-betriebswirtschaftslehre-logistik-author-date\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-cite-them-right\" rel=\"template\"/>\n    <link href=\"https://tu-dresden.de/bu/wirtschaft/bwllog/ressourcen/dateien/studium/studienarbeiten/dateien_studienarbeiten/20180619_Homepage_Hinweise-zu-Zitierungen.pdf?lang=de\" rel=\"documentation\"/>\n    <author>\n      <name>Jens Mittelbach</name>\n      <email>jens.mittelbach@slub-dresden.de</email>\n      <uri>http://www.slub-dresden.de</uri>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <summary>Zitierstil entsprechend den Zitierrichtlinien des Lehrstuhls  Betriebswirtschaftslehre, insbesondere Logistik,der Fakultät Wirtschaftswissenschaften, Technische Universität Dresden.</summary>\n    <updated>2019-11-08T13:17:31+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <style-options punctuation-in-quote=\"false\"/>\n    <terms>\n      <term name=\"open-quote\">‘</term>\n      <term name=\"close-quote\">’</term>\n      <term name=\"open-inner-quote\">“</term>\n      <term name=\"close-inner-quote\">”</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"anonymous\" form=\"short\">o.&#8201;V.</term>\n      <term name=\"accessed\" form=\"verb\">abgerufen</term>\n      <term name=\"presented at\">gehalten auf</term>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"available at\">verfügbar unter</term>\n      <term name=\"open-quote\">,</term>\n      <term name=\"close-quote\">‘</term>\n      <term name=\"open-inner-quote\">„</term>\n      <term name=\"close-inner-quote\">“</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \" prefix=\" \">\n      <name initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" strip-periods=\"false\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" font-variant=\"normal\">\n      <name delimiter=\"; \" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" [\" suffix=\"]\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\" / \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi: \"/>\n      </if>\n      <else-if variable=\"URL\">\n        <group delimiter=\": \" suffix=\" \">\n          <text term=\"available at\" text-case=\"capitalize-first\"/>\n          <text variable=\"URL\"/>\n        </group>\n        <group delimiter=\": \" prefix=\"(\" suffix=\")\">\n          <text term=\"accessed\" form=\"verb\"/>\n          <date form=\"numeric\" variable=\"accessed\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" strip-periods=\"true\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\" \">\n      <text variable=\"publisher-place\" suffix=\":\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"long\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation and=\"text\" et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\" (\" suffix=\")\">\n      <group delimiter=\", \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label plural=\"never\" suffix=\"&#160;\" variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" form=\"short\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography and=\"text\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\" \"/>\n      <group delimiter=\"; \" suffix=\":\">\n        <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <text macro=\"publisher\" prefix=\" \"/>\n        </if>\n        <else-if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\", \">\n            <text term=\"in\" suffix=\":\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\": \" suffix=\".\"/>\n            <group delimiter=\", \" suffix=\".\">\n              <text variable=\"collection-title\" prefix=\" \"/>\n              <number variable=\"collection-number\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\" prefix=\" \"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \" prefix=\" \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\", \">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group delimiter=\", \">\n            <choose>\n              <if type=\"interview\" match=\"any\">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n                <text variable=\"medium\" suffix=\", \"/>\n              </if>\n              <else-if type=\"entry-dictionary post-weblog\" match=\"any\">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n                <text variable=\"container-title\" suffix=\", \"/>\n              </else-if>\n              <else-if type=\"speech\" match=\"any\">\n                <group>\n                  <text term=\"presented at\" suffix=\": \"/>\n                  <text variable=\"event\" font-style=\"italic\"/>\n                </group>\n                <text variable=\"event-place\" font-style=\"normal\"/>\n              </else-if>\n              <else>\n                <group delimiter=\": \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </group>\n              </else>\n            </choose>\n            <group>\n              <group delimiter=\" \">\n                <text variable=\"volume\"/>\n              </group>\n              <group prefix=\"(\" suffix=\")\">\n                <text variable=\"issue\" form=\"short\"/>\n              </group>\n            </group>\n            <group>\n              <label plural=\"never\" suffix=\" \" variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"