"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>World Congress on Engineering Asset Management 2010</title>\n    <title-short>WCEAM</title-short>\n    <id>http://www.zotero.org/styles/world-congress-on-engineering-asset-management</id>\n    <link href=\"http://www.zotero.org/styles/world-congress-on-engineering-asset-management\" rel=\"self\"/>\n    <link href=\"http://wceam.com/\" rel=\"documentation\"/>\n    <author>\n      <name>Lubos</name>\n      <email>zothero@hotmail.com</email>\n    </author>\n    <contributor>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <summary>Citation style for the World Congress for Engineering Asset Management (WCEAM)</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \" suffix=\".\">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL accessed\" match=\"all\">\n        <date variable=\"accessed\" prefix=\" Retrieved \">\n          <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n          <date-part name=\"day\" form=\"numeric\" suffix=\", \"/>\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n        <text variable=\"URL\" prefix=\", from \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"genre\">\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"genre\" suffix=\" thesis.\"/>\n      </if>\n      <else>\n        <group delimiter=\" \" suffix=\".\">\n          <text variable=\"genre\"/>\n          <text variable=\"number\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\" entry-spacing=\"0\" line-spacing=\"1\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\" \"/>\n      <text macro=\"author\"/>\n      <date variable=\"issued\">\n        <date-part name=\"year\" prefix=\"(\" suffix=\")\"/>\n      </date>\n      <text variable=\"title\" prefix=\" \" suffix=\".\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <text macro=\"edition\" prefix=\" \"/>\n          <text macro=\"publisher\" prefix=\" \"/>\n          <text macro=\"genre\" prefix=\" \"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\",\" suffix=\".\">\n            <group prefix=\" \" delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\" suffix=\".\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </group>\n            <group>\n              <text macro=\"publisher\" prefix=\" \"/>\n              <label variable=\"page\" form=\"short\" prefix=\" \"/>\n              <text variable=\"page\" suffix=\".\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\" \" delimiter=\", \" suffix=\".\">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <group>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\" suffix=\".\"/>\n          </group>\n          <text macro=\"genre\" prefix=\" \"/>\n          <text macro=\"publisher\" prefix=\" \"/>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"