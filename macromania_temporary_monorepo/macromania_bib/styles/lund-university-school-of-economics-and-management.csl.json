"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-GB\">\n  <info>\n    <title>Lund University School of Economics and Management</title>\n    <title-short>LUSEM</title-short>\n    <id>http://www.zotero.org/styles/lund-university-school-of-economics-and-management</id>\n    <link href=\"http://www.zotero.org/styles/lund-university-school-of-economics-and-management\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-information-technology\" rel=\"template\"/>\n    <link href=\"http://libguides.lub.lu.se/ld.php?content_id=12012699\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <updated>2012-12-14T10:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"edition\" form=\"short\">edn.</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds</multiple>\n      </term>\n      <term name=\"available at\">available online</term>\n      <term name=\"retrieved\">available through</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <names variable=\"editor translator\" delimiter=\", \" suffix=\", \">\n          <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"article-journal chapter paper-conference\" match=\"none\">\n        <names variable=\"translator editor\" delimiter=\", \">\n          <label form=\"verb\" suffix=\" \"/>\n          <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\". \" delimiter-precedes-last=\"never\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \">\n          <group delimiter=\": \">\n            <group delimiter=\" \">\n              <choose>\n                <if variable=\"archive\">\n                  <text term=\"retrieved\" text-case=\"title\"/>\n                </if>\n                <else>\n                  <text term=\"available at\" text-case=\"title\"/>\n                </else>\n              </choose>\n            </group>\n            <group delimiter=\" \">\n              <text variable=\"archive\"/>\n              <text variable=\"URL\"/>\n            </group>\n          </group>\n          <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\" form=\"text\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\" text-case=\"title\"/>\n      <choose>\n        <if type=\"webpage\">\n          <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"report thesis\" match=\"any\">\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n      <else>\n        <choose>\n          <if type=\"article-journal article-magazine speech\" match=\"none\">\n            <group delimiter=\" \">\n              <text macro=\"epub-type\"/>\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"epub-type\">\n    <choose>\n      <if variable=\"URL\">\n        <choose>\n          <if type=\"book chapter\" match=\"any\">\n            <text value=\"e-book\" prefix=\"[\" suffix=\"]\"/>\n          </if>\n          <else-if type=\"article-journal\">\n            <text value=\"e-journal\" prefix=\"[\" suffix=\"]\"/>\n          </else-if>\n          <else-if type=\"motion_picture\">\n            <text value=\"video-online\" prefix=\"[\" suffix=\"]\"/>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if type=\"speech paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"event\" text-case=\"title\"/>\n          <text variable=\"event-place\"/>\n          <date variable=\"issued\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"description\">\n    <choose>\n      <if type=\"webpage\" match=\"none\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"number\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued-sort\">\n    <choose>\n      <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\"/>\n          <date-part name=\"day\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"year-suffix\"/>\n      </if>\n      <else-if type=\"webpage\">\n        <date variable=\"accessed\">\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"year-suffix\"/>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n        <text variable=\"year-suffix\" prefix=\"-\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group prefix=\", \" delimiter=\" \">\n          <text macro=\"epub-type\"/>\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text term=\"volume\" form=\"short\"/>\n              <text variable=\"volume\"/>\n            </group>\n            <group delimiter=\" \">\n              <text term=\"issue\" form=\"short\"/>\n              <text variable=\"issue\"/>\n            </group>\n            <text macro=\"pages\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\">\n        <group prefix=\", \" delimiter=\", \">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n          </date>\n          <text macro=\"pages\"/>\n        </group>\n      </else-if>\n      <else-if type=\"book graphic motion_picture report song chapter paper-conference speech\" match=\"any\">\n        <group prefix=\", \" delimiter=\", \">\n          <text macro=\"edition\"/>\n          <group>\n            <text term=\"volume\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1-\"/>\n          </group>\n          <group>\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"pages\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group>\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <group>\n      <choose>\n        <if type=\"chapter paper-conference entry-encyclopedia\" match=\"any\">\n          <text term=\"in\" suffix=\" \"/>\n        </if>\n      </choose>\n      <text macro=\"container-contributors\"/>\n      <text macro=\"secondary-contributors\"/>\n      <text macro=\"container-title\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"bill legal_case legislation\" match=\"none\">\n        <choose>\n          <if type=\"article-journal article-newspaper article-magazine\" match=\"any\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <choose>\n            <if variable=\"container-title\">\n              <text variable=\"volume\"/>\n              <text variable=\"container-title\"/>\n              <group delimiter=\" \">\n                <!--change to label variable=\"section\" as that becomes available -->\n                <text term=\"section\" form=\"symbol\"/>\n                <text variable=\"section\"/>\n              </group>\n              <text variable=\"page\"/>\n            </if>\n            <else>\n              <choose>\n                <if type=\"legal_case\">\n                  <text variable=\"number\" prefix=\"No. \"/>\n                </if>\n                <else>\n                  <text variable=\"number\" prefix=\"Pub. L. No. \"/>\n                  <group delimiter=\" \">\n                    <!--change to label variable=\"section\" as that becomes available -->\n                    <text term=\"section\" form=\"symbol\"/>\n                    <text variable=\"section\"/>\n                  </group>\n                </else>\n              </choose>\n            </else>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor-prefix\">\n    <choose>\n      <if variable=\"author\" match=\"none\">\n        <choose>\n          <if variable=\"editor\">\n            <text term=\"editor\" form=\"short\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-sort\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"editor-prefix\"/>\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n          <choose>\n            <if type=\"legal_case\">\n              <text macro=\"container\"/>\n            </if>\n          </choose>\n          <text macro=\"citation-locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued-sort\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"issued-year\" prefix=\"(\" suffix=\")\"/>\n      </group>\n      <group delimiter=\", \">\n        <text macro=\"title\"/>\n        <text macro=\"description\"/>\n        <text macro=\"container\"/>\n      </group>\n      <text macro=\"locators\"/>\n      <text macro=\"access\" prefix=\", \"/>\n    </layout>\n  </bibliography>\n</style>\n"