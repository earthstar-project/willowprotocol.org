"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Applied Polymer Science</title>\n    <id>http://www.zotero.org/styles/journal-of-applied-polymer-science</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-applied-polymer-science\" rel=\"self\"/>\n    <link href=\"https://onlinelibrary.wiley.com/page/journal/10974628/homepage/author-guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <issn>0021-8995</issn>\n    <eissn>1097-4628</eissn>\n    <updated>2024-02-01T10:19:21+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" text-case=\"capitalize-first\" suffix=\": \"/>\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <text variable=\"collection-title\"/>\n  </macro>\n  <macro name=\"pages\">\n    <label strip-periods=\"true\" suffix=\" \" variable=\"page\" form=\"short\"/>\n    <text variable=\"page-first\"/>\n  </macro>\n  <macro name=\"book-container\">\n    <text variable=\"container-title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"year\" font-weight=\"bold\"/>\n    </date>\n  </macro>\n  <macro name=\"full-issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n      <date-part name=\"day\" suffix=\", \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"margin\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <group delimiter=\", \">\n        <text macro=\"author\"/>\n        <choose>\n          <if type=\"article-magazine\">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" font-style=\"italic\" form=\"short\" suffix=\".\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"full-issued\" suffix=\",\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </if>\n          <else-if type=\"thesis\">\n            <group delimiter=\", \">\n              <text variable=\"genre\" font-style=\"italic\"/>\n              <group delimiter=\" \">\n                <text variable=\"publisher\"/>\n                <text macro=\"issued\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text macro=\"editor\" prefix=\" \"/>\n              <text macro=\"series\"/>\n              <text macro=\"edition\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"issued\"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"volume\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"in\"/>\n                <text macro=\"book-container\"/>\n                <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text macro=\"series\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"issued\"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"volume\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"article-journal\">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n              <group delimiter=\", \">\n                <text macro=\"issued\" font-weight=\"bold\"/>\n                <text variable=\"volume\" font-style=\"italic\"/>\n                <text variable=\"page-first\" form=\"short\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post-weblog\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\". \">\n                <text variable=\"title\"/>\n                <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n              </group>\n              <group delimiter=\" \">\n                <text variable=\"URL\"/>\n                <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                  <text term=\"accessed\"/>\n                  <date form=\"text\" variable=\"accessed\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\" \">\n              <group delimiter=\". \">\n                <text variable=\"title\"/>\n                <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n              </group>\n              <group delimiter=\", \">\n                <text macro=\"issued\" font-weight=\"bold\"/>\n                <text variable=\"volume\" font-style=\"italic\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"