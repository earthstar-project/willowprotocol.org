"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" page-range-format=\"expanded\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Ephemerides Theologicae Lovanienses</title>\n    <title-short>ETL</title-short>\n    <id>http://www.zotero.org/styles/ephemerides-theologicae-lovanienses</id>\n    <link href=\"http://www.zotero.org/styles/ephemerides-theologicae-lovanienses\" rel=\"self\"/>\n    <link href=\"http://www.peeters-leuven.be/pdf/instructionsETL.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Carl Johan Berglund</name>\n      <email>cajo@cajo.nu</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"theology\"/>\n    <issn>0013-9513</issn>\n    <eissn>1783-1423</eissn>\n    <summary>Ephemerides Theologicae Lovanienses (ETL) and Bibliotheca Ephemeridum Theologicarum Lovaniensium (BETL)</summary>\n    <updated>2017-12-14T21:48:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>pp.</multiple>\n      </term>\n      <term name=\"in\">in</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>éd.</single>\n        <multiple>éds.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>pp.</multiple>\n      </term>\n      <term name=\"in\">dans</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"editor\" form=\"short\">\n        <single>Hg.</single>\n        <multiple>Hgg.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"page\" form=\"short\">S.</term>\n      <term name=\"in\">in</term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator\">\n    <group delimiter=\", \">\n      <choose>\n        <if variable=\"author\">\n          <names variable=\"editor\" delimiter=\", \">\n            <label form=\"short\" suffix=\" \"/>\n            <name font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n          </names>\n        </if>\n      </choose>\n      <choose>\n        <if variable=\"author editor\" match=\"any\">\n          <names variable=\"translator\" delimiter=\", \">\n            <label form=\"short\" suffix=\" \"/>\n            <name font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n          </names>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"secondary-contributors-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <text macro=\"editor-translator\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\"; \">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"editor\" delimiter=\", \">\n                <name font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n                <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n              </names>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\" delimiter=\", \">\n                <name font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n                <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor-note\">\n    <names variable=\"editor\">\n      <name font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"translator-note\">\n    <names variable=\"translator\">\n      <name font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"recipient-note\">\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-note\">\n    <names variable=\"author\">\n      <name font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n      <substitute>\n        <text macro=\"editor-note\"/>\n        <text macro=\"translator-note\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient-note\"/>\n  </macro>\n  <macro name=\"recipient-short\">\n    <names variable=\"recipient\">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"short\" font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient-short\"/>\n  </macro>\n  <macro name=\"container-contributors-short\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\"; \">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"editor\" delimiter=\", \">\n                <label form=\"short\" suffix=\" \"/>\n                <name form=\"short\" font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n              </names>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\" delimiter=\", \">\n                <label form=\"short\" suffix=\" \"/>\n                <name form=\"short\" font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"interviewer-note\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name font-variant=\"small-caps\" initialize-with=\".\" delimiter=\" &#8212; \"/>\n    </names>\n  </macro>\n  <macro name=\"title-note\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <text variable=\"genre\"/>\n      </if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <text variable=\"title\" form=\"short\" font-style=\"italic\" text-case=\"title\"/>\n  </macro>\n  <macro name=\"description-note\">\n    <group delimiter=\", \">\n      <text macro=\"interviewer-note\"/>\n      <text variable=\"medium\"/>\n      <choose>\n        <if variable=\"title\" match=\"none\"/>\n        <else-if type=\"thesis speech\" match=\"any\"/>\n        <else>\n          <text variable=\"genre\" prefix=\"(\" suffix=\")\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"container-title-note\">\n    <choose>\n      <if type=\"chapter paper-conference article-journal\" match=\"any\">\n        <text term=\"in\" suffix=\" \"/>\n        <text macro=\"container-contributors-note\" suffix=\", \"/>\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else>\n        <text macro=\"container-contributors-note\" suffix=\", \"/>\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection-title\">\n    <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"edition-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \" prefix=\", \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\", \" prefix=\", \">\n          <group>\n            <text term=\"volume\" form=\"short\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n          <group>\n            <number variable=\"number-of-volumes\" form=\"numeric\"/>\n            <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n          </group>\n          <text macro=\"edition-note\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-newspaper\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group delimiter=\", \">\n          <group>\n            <text variable=\"edition\" suffix=\" \"/>\n            <text term=\"edition\" prefix=\" \"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\" \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <group>\n      <text term=\"presented at\" suffix=\" \"/>\n      <text variable=\"event\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\", \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"graphic report\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else-if type=\"legal_case\">\n            <text variable=\"authority\" suffix=\" \"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis article-journal\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"point-locators-subsequent\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <choose>\n                <if variable=\"volume\">\n                  <group>\n                    <text term=\"volume\" form=\"short\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <label variable=\"locator\" form=\"short\" prefix=\", \" suffix=\" \"/>\n                  </group>\n                </if>\n                <else>\n                  <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n                </else>\n              </choose>\n            </if>\n          </choose>\n        </if>\n        <else-if variable=\"volume\">\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <choose>\n                <if position=\"subsequent\">\n                  <group delimiter=\" \" suffix=\", \">\n                    <text term=\"in\"/>\n                    <text variable=\"container-title\" font-style=\"italic\"/>\n                    <text macro=\"container-contributors-short\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                </if>\n              </choose>\n              <number variable=\"volume\" form=\"numeric\" suffix=\":\"/>\n            </if>\n          </choose>\n        </else-if>\n      </choose>\n      <group delimiter=\" \">\n        <label variable=\"page\" form=\"short\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"point-locators\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <text macro=\"pages\"/>\n      </if>\n      <else-if type=\"article-journal chapter\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"pages\"/>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"locator\"/>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"point-locators-subsequent\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"archive-note\">\n    <group delimiter=\", \">\n      <text variable=\"archive_location\"/>\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"issue-note\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"volume\" prefix=\" \"/>\n        <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n        <text variable=\"issue\" prefix=\", no. \"/>\n      </if>\n      <else-if variable=\"publisher-place publisher\" match=\"any\">\n        <group>\n          <group delimiter=\", \">\n            <text macro=\"locators-note\"/>\n            <text macro=\"collection-title\"/>\n          </group>\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <choose>\n                <if variable=\"title\" match=\"none\"/>\n                <else-if type=\"thesis speech\" match=\"any\">\n                  <text variable=\"genre\" prefix=\" (\" suffix=\")\"/>\n                </else-if>\n              </choose>\n              <text macro=\"event\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"issued\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access-note\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"graphic report\" match=\"any\">\n          <text macro=\"archive-note\"/>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"none\">\n          <text macro=\"archive-note\"/>\n        </else-if>\n      </choose>\n      <choose>\n        <if variable=\"URL\">\n          <group delimiter=\". \">\n            <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n              <text term=\"cited\"/>\n              <date variable=\"accessed\" form=\"text\"/>\n            </group>\n            <group delimiter=\": \">\n              <text term=\"online\" text-case=\"capitalize-first\"/>\n              <text variable=\"URL\"/>\n            </group>\n          </group>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator ibid subsequent\" match=\"any\">\n          <group delimiter=\" \">\n            <text macro=\"contributors-short\" suffix=\",\"/>\n            <text macro=\"title-short\"/>\n            <text variable=\"first-reference-note-number\" prefix=\"(n. \" suffix=\"),\"/>\n            <text macro=\"point-locators-subsequent\"/>\n          </group>\n        </if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"contributors-note\"/>\n            <text macro=\"secondary-contributors-note\"/>\n            <text macro=\"title-note\"/>\n            <text macro=\"description-note\"/>\n            <text macro=\"container-title-note\"/>\n          </group>\n          <text macro=\"issue-note\"/>\n          <text macro=\"locators-newspaper\" prefix=\", \"/>\n          <text macro=\"point-locators\" prefix=\", \"/>\n          <text macro=\"access-note\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"