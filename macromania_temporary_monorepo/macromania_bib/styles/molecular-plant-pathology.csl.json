"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" default-locale=\"en-US\">\n  <info>\n    <title>Molecular Plant Pathology</title>\n    <title-short>MPP</title-short>\n    <id>http://www.zotero.org/styles/molecular-plant-pathology</id>\n    <link href=\"http://www.zotero.org/styles/molecular-plant-pathology\" rel=\"self\"/>\n    <link href=\"https://bsppjournals.onlinelibrary.wiley.com/pb-assets/hub-assets/bsppjournals/13643703/MPP%20Author%20Guidelines%202021-1.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Libor Ansorge</name>\n      <email>libor.ansorge@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>1464-6722</issn>\n    <eissn>1364-3703</eissn>\n    <summary>Molecular Plant Pathology (Wiley) style.</summary>\n    <updated>2021-03-25T09:55:07+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\"., \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\", \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" font-weight=\"normal\"/>\n      <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n        <date-part name=\"year\" font-weight=\"normal\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n            <group delimiter=\" \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"container-title\"/>\n              <text variable=\"collection-title\"/>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\" \" suffix=\".\" delimiter=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \" prefix=\" \" suffix=\".\">\n            <text macro=\"title\"/>\n            <text macro=\"editor\"/>\n          </group>\n          <group delimiter=\", \" prefix=\" \" suffix=\".\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"volume\" font-weight=\"normal\" prefix=\" \"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"