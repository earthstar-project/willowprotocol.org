"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" and=\"symbol\" page-range-format=\"minimal\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Refugee Survey Quarterly</title>\n    <id>http://www.zotero.org/styles/refugee-survey-quarterly</id>\n    <link href=\"http://www.zotero.org/styles/refugee-survey-quarterly\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/henoch\" rel=\"template\"/>\n    <link href=\"https://academic.oup.com/rsq/pages/information_for_authors\" rel=\"documentation\"/>\n    <link href=\"https://static.primary.prod.gcms.the-infra.com/static/site/rsq/document/rsq-style-sheet-for-authors.pdf?node=9f756c3cee0c555986ff&amp;version=78284:309db0246a9285b2f409\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <issn>1020-4067</issn>\n    <eissn>1471-695X</eissn>\n    <updated>2021-03-28T08:30:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <term name=\"editortranslator\" form=\"short\">ed. and trans.</term>\n      <term name=\"chapter\" form=\"short\">c.</term>\n      <term name=\"open-quote\">\"</term>\n      <term name=\"close-quote\">\"</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\", \">\n      <name initialize-with=\".\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n      <substitute>\n        <names variable=\"translator\"/>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor-translator\">\n    <choose>\n      <if variable=\"editor translator\" match=\"any\">\n        <names variable=\"editor translator\">\n          <label form=\"short\" suffix=\". \" strip-periods=\"false\"/>\n          <name and=\"text\" initialize-with=\".\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-editor\">\n    <group delimiter=\" \">\n      <text term=\"in\" suffix=\" \"/>\n      <names variable=\"editor\">\n        <name initialize-with=\".\"/>\n        <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"false\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <names variable=\"translator\"/>\n          <names variable=\"container-author\"/>\n          <text macro=\"title\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"paper-conference\">\n        <group delimiter=\" \">\n          <text term=\"presented at\"/>\n          <text variable=\"event\"/>\n        </group>\n        <text variable=\"event-place\" prefix=\", \"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-newspaper\" match=\"any\">\n        <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volumes\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-volumes\"/>\n      <text term=\"volume\" form=\"short\" plural=\"true\" strip-periods=\"true\"/>\n    </group>\n  </macro>\n  <macro name=\"pageno\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <text variable=\"page\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pageref\">\n    <choose>\n      <if variable=\"locator\">\n        <group delimiter=\" \">\n          <choose>\n            <if match=\"none\" locator=\"page\">\n              <label variable=\"locator\" form=\"short\"/>\n            </if>\n          </choose>\n          <text variable=\"locator\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pageref-subsequent\">\n    <label variable=\"locator\" form=\"short\" suffix=\" \"/>\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"volref\">\n    <text variable=\"volume\"/>\n    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog report article-newspaper\" match=\"any\">\n        <choose>\n          <if variable=\"URL\" match=\"any\">\n            <group delimiter=\" \">\n              <group delimiter=\": \">\n                <text term=\"available at\"/>\n                <text variable=\"URL\" prefix=\" \" suffix=\", \"/>\n              </group>\n              <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                <text term=\"accessed\"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"month\" form=\"numeric\" suffix=\"/\"/>\n                  <date-part name=\"day\" suffix=\"/\"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </group>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator ibid\" match=\"any\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" form=\"short\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n            <text macro=\"pageref\"/>\n          </group>\n        </if>\n        <else-if match=\"any\" position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"pageref-subsequent\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"author\"/>\n          <choose>\n            <if type=\"report thesis\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text variable=\"genre\"/>\n                <text variable=\"publisher\"/>\n                <text variable=\"number\" prefix=\" \"/>\n                <text variable=\"collection-title\"/>\n                <text macro=\"date\"/>\n                <text macro=\"pageref\"/>\n                <text macro=\"access\"/>\n              </group>\n            </if>\n            <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"editor-translator\"/>\n                <text macro=\"container\"/>\n                <text macro=\"volumes\"/>\n                <text macro=\"collection\"/>\n                <text macro=\"volref\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"date\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </else-if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"container-editor\"/>\n                <text macro=\"container\"/>\n                <text macro=\"volref\"/>\n                <text macro=\"volumes\"/>\n                <text macro=\"collection\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"date\"/>\n                <text macro=\"pageno\"/>\n                <text macro=\"pageref\"/>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"publisher\"/>\n                <text macro=\"container\"/>\n                <text macro=\"collection\"/>\n                <text macro=\"volref\"/>\n                <text macro=\"date\"/>\n                <text macro=\"volumes\"/>\n                <text macro=\"pageno\"/>\n                <text macro=\"pageref\"/>\n                <text macro=\"access\"/>\n              </group>\n            </else>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"