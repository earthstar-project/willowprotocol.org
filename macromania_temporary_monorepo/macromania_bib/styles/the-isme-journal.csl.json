"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>The ISME Journal</title>\n    <id>http://www.zotero.org/styles/the-isme-journal</id>\n    <link href=\"http://www.zotero.org/styles/the-isme-journal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-frailty-and-aging\" rel=\"template\"/>\n    <link href=\"http://www.nature.com/ismej/ismej_new_gta.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>1751-7362</issn>\n    <eissn>1751-7370</eissn>\n    <summary>The ISME Journal style, which is not the same as for Nature</summary>\n    <updated>2018-03-19T15:30:26+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\").\"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\", \" suffix=\". \">\n          <text variable=\"title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\" suffix=\". \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\" line-spacing=\"2\" entry-spacing=\"0\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if type=\"chapter\">\n          <text macro=\"editor\"/>\n          <group delimiter=\". \" suffix=\". \">\n            <group prefix=\" \" delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n            </group>\n            <text macro=\"year-date\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"paper-conference\">\n          <text macro=\"editor\"/>\n          <group delimiter=\". \" suffix=\". \">\n            <group prefix=\" \" delimiter=\", \">\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n            </group>\n            <text macro=\"year-date\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\" strip-periods=\"true\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\"; \" suffix=\". \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" suffix=\" \" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n              <text macro=\"year-date\"/>\n            </group>\n            <group delimiter=\": \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <text variable=\"container-title\" suffix=\" \" font-style=\"italic\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group suffix=\". \">\n            <text variable=\"container-title\" suffix=\". \" font-style=\"italic\"/>\n            <text variable=\"URL\" suffix=\". \"/>\n            <date variable=\"accessed\">\n              <date-part prefix=\"Accessed \" name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\". \" suffix=\". \">\n            <text variable=\"container-title\" suffix=\" \" font-style=\"italic\"/>\n            <text macro=\"year-date\"/>\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group>\n            <group delimiter=\". \" suffix=\". \">\n              <text variable=\"container-title\" form=\"short\" suffix=\" \" strip-periods=\"true\" font-style=\"italic\"/>\n              <text macro=\"year-date\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <group prefix=\", \" delimiter=\": \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"