"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"chicago\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Ageing &amp; Society</title>\n    <id>http://www.zotero.org/styles/ageing-and-society</id>\n    <link href=\"http://www.zotero.org/styles/ageing-and-society\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/emu-austral-ornithology\" rel=\"template\"/>\n    <link href=\"https://www.cambridge.org/core/journals/ageing-and-society/information/instructions-contributors\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <issn>0144-686X</issn>\n    <eissn>1469-1779</eissn>\n    <updated>2021-04-10T17:19:24+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available online at</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" initialize-with=\". \" name-as-sort-order=\"all\" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"available at\" text-case=\"capitalize-first\"/>\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\">\n    <sort>\n      <key variable=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" line-spacing=\"2\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\" \"/>\n      <date variable=\"issued\" suffix=\".\">\n        <date-part name=\"year\"/>\n      </date>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n          <group prefix=\" \" suffix=\".\">\n            <text macro=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\" prefix=\". \"/>\n            <text macro=\"editor\" prefix=\". \"/>\n          </group>\n          <text macro=\"publisher\" prefix=\" \"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\" suffix=\",\"/>\n            <text variable=\"container-title\" text-case=\"capitalize-first\" quotes=\"false\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <text variable=\"event\" suffix=\".\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"pages\" prefix=\" \"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\" \" suffix=\".\" delimiter=\" \">\n            <text macro=\"title\" suffix=\".\"/>\n            <text variable=\"genre\" text-case=\"capitalize-first\" suffix=\",\"/>\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\". \" prefix=\" \" suffix=\".\">\n            <text macro=\"title\"/>\n            <group delimiter=\" \">\n              <choose>\n                <if match=\"any\" variable=\"collection-title\">\n                  <text variable=\"collection-title\"/>\n                </if>\n                <else>\n                  <text variable=\"genre\"/>\n                </else>\n              </choose>\n              <choose>\n                <if match=\"any\" is-numeric=\"number\">\n                  <group delimiter=\" \">\n                    <text value=\"No.\"/>\n                    <text variable=\"number\"/>\n                  </group>\n                </if>\n                <else>\n                  <text variable=\"number\"/>\n                </else>\n              </choose>\n            </group>\n            <group delimiter=\", \">\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group delimiter=\", \" prefix=\" \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"volume\" font-weight=\"bold\"/>\n            <text variable=\"issue\"/>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"