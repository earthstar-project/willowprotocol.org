"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Acta Scientiae Veterinariae</title>\n    <id>http://www.zotero.org/styles/acta-scientiae-veterinariae</id>\n    <link href=\"http://www.zotero.org/styles/acta-scientiae-veterinariae\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/world-applied-sciences-journal\" rel=\"template\"/>\n    <link href=\"http://www.ufrgs.br/actavet/instrucoes.htm\" rel=\"documentation\"/>\n    <link href=\"http://www.ufrgs.br/actavet/Instructions-case-ASV-English.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <issn>1678-0345</issn>\n    <eissn>1679-9216</eissn>\n    <updated>2021-01-14T09:09:46+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"symbol\" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <date date-parts=\"year\" form=\"text\" variable=\"issued\" font-weight=\"normal\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" font-weight=\"bold\" suffix=\" \"/>\n      <group font-weight=\"bold\" delimiter=\". \">\n        <text macro=\"author\" font-weight=\"bold\"/>\n        <choose>\n          <if variable=\"issued\">\n            <text macro=\"year-date\" font-weight=\"bold\" suffix=\". \"/>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\" suffix=\". \"/>\n          </else>\n        </choose>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\". \">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n            </group>\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <group prefix=\", \">\n            <text variable=\"number-of-pages\"/>\n            <label plural=\"never\" variable=\"number-of-pages\" form=\"short\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <group>\n                <label strip-periods=\"false\" variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"number\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog post\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <group delimiter=\": \">\n              <text term=\"available at\" text-case=\"capitalize-first\"/>\n              <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n            </group>\n            <group delimiter=\" \">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date date-parts=\"year-month\" form=\"numeric\" variable=\"accessed\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\". \">\n            <text variable=\"title\" suffix=\".\"/>\n            <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n            <group delimiter=\": \">\n              <group>\n                <text variable=\"volume\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\" form=\"short\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text variable=\"title\" suffix=\".\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n              <group delimiter=\" \">\n                <group suffix=\":\">\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\" form=\"short\"/>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"