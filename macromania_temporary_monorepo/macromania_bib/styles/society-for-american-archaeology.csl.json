"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <!--\n Polyglot; journal publishes in English and Spanish \n-->\n  <info>\n    <title>Society for American Archaeology</title>\n    <title-short>SAA</title-short>\n    <id>http://www.zotero.org/styles/society-for-american-archaeology</id>\n    <link href=\"http://www.zotero.org/styles/society-for-american-archaeology\" rel=\"self\"/>\n    <link href=\"https://www.saa.org/publications\" rel=\"documentation\"/>\n    <link href=\"https://documents.saa.org/container/docs/default-source/doc-publications/style-guide/saa-style-guide_english_updated_2021_final08023c15928949dabd02faafb269fb1c.pdf?sfvrsn=c1f41c1b_2\" rel=\"documentation\"/>\n    <link href=\"https://documents.saa.org/container/docs/default-source/doc-publications/style-guide/saa_guia_de_estilo_updated_2021_final770fea7e32154c1e80ed075cb9de3333.pdf?sfvrsn=7250a084_2\" rel=\"documentation\"/>\n    <author>\n      <name>Michael Barton</name>\n      <email>michael.barton@asu.edu</email>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <contributor>\n      <name>Allison Grunwald</name>\n      <email>agrunwa1@uwyo.edu</email>\n    </contributor>\n    <contributor>\n      <name>Erik Marsh</name>\n      <email>erik.marsh@gmail.com</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"anthropology\"/>\n    <updated>2022-02-22T08:50:31+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"translator\" form=\"verb-short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\". \">\n          <label form=\"verb\" prefix=\" \" text-case=\"capitalize-first\" suffix=\" \"/>\n          <name and=\"text\" delimiter=\", \"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group prefix=\",\" delimiter=\", \">\n          <names variable=\"editor\">\n            <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n            <name and=\"text\" delimiter=\", \"/>\n          </names>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\">\n                <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n                <name and=\"text\" delimiter=\", \"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"anon\">\n    <choose>\n      <if variable=\"author editor translator\" match=\"none\">\n        <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"recipient\">\n    <choose>\n      <if type=\"personal_communication\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\"/>\n          </if>\n          <else>\n            <text term=\"letter\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"anon\"/>\n    <text macro=\"recipient\"/>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"anon\"/>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\". \">\n      <text variable=\"archive_location\" text-case=\"capitalize-first\"/>\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\". \">\n      <choose>\n        <if type=\"graphic report\" match=\"any\">\n          <text macro=\"archive\"/>\n        </if>\n        <else-if type=\"article-journal\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"DOI\" prefix=\" DOI:\"/>\n            <group delimiter=\" \">\n              <text term=\"accessed\"/>\n              <date form=\"text\" variable=\"accessed\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog post article-magazine article-newspaper\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text variable=\"URL\"/>\n            <group delimiter=\" \">\n              <text term=\"accessed\"/>\n              <date form=\"text\" variable=\"accessed\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case motion_picture report song article-journal article-magazine article-newspaper thesis chapter paper-conference\" match=\"none\">\n          <text macro=\"archive\"/>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"personal_communication\" match=\"none\">\n            <text variable=\"genre\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song chapter paper-conference\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"volume\" prefix=\" \"/>\n        <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <group prefix=\". \" delimiter=\". \">\n          <group>\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n          <group>\n            <number variable=\"number-of-volumes\" form=\"numeric\"/>\n            <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locators-chapter\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group prefix=\", \">\n          <text variable=\"volume\" suffix=\":\"/>\n          <text variable=\"page\" prefix=\"pp. \"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-article\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group prefix=\", \" delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"edition\"/>\n            <text term=\"edition\"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\" \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"page\" prefix=\":\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"point-locators\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <label text-case=\"capitalize-first\" suffix=\" \" variable=\"locator\"/>\n        </if>\n      </choose>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"container-prefix\">\n    <text term=\"in\" text-case=\"capitalize-first\"/>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text macro=\"container-prefix\" suffix=\" \"/>\n      </if>\n    </choose>\n    <text variable=\"container-title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"report thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date1\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"day-month\">\n    <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n  </macro>\n  <macro name=\"collection-title\">\n    <text variable=\"collection-title\"/>\n    <text variable=\"collection-number\" prefix=\" \"/>\n  </macro>\n  <macro name=\"event\">\n    <group>\n      <text term=\"presented at\" suffix=\" \"/>\n      <text variable=\"event\"/>\n    </group>\n  </macro>\n  <macro name=\"description\">\n    <group delimiter=\". \">\n      <text macro=\"interviewer\"/>\n      <text variable=\"medium\" text-case=\"capitalize-first\"/>\n    </group>\n  </macro>\n  <macro name=\"issue\">\n    <choose>\n      <if type=\"article-journal\"/>\n      <else-if type=\"speech\">\n        <group prefix=\" \" delimiter=\", \">\n          <text macro=\"event\"/>\n          <text macro=\"day-month\"/>\n          <text variable=\"event-place\"/>\n        </group>\n      </else-if>\n      <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n        <text macro=\"day-month\" prefix=\", \"/>\n      </else-if>\n      <else>\n        <group prefix=\". \" delimiter=\", \">\n          <choose>\n            <if type=\"thesis\">\n              <text variable=\"genre\" prefix=\"Unpublished \"/>\n            </if>\n          </choose>\n          <text macro=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"rest-of-bib\">\n    <text macro=\"title\"/>\n    <text macro=\"description\"/>\n    <text macro=\"secondary-contributors\" prefix=\". \"/>\n    <text macro=\"container-title\" prefix=\". \"/>\n    <text macro=\"container-contributors\"/>\n    <text macro=\"locators-chapter\"/>\n    <text macro=\"edition\" prefix=\". \"/>\n    <text macro=\"locators\"/>\n    <text macro=\"collection-title\" prefix=\". \"/>\n    <text macro=\"issue\"/>\n    <text macro=\"locators-article\"/>\n    <text macro=\"access\" prefix=\". \"/>\n  </macro>\n  <macro name=\"original-date\">\n    <date date-parts=\"year\" form=\"text\" variable=\"original-date\"/>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"contributors\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\":\">\n        <group delimiter=\" \">\n          <text macro=\"contributors-short\"/>\n          <text macro=\"date1\"/>\n          <text macro=\"original-date\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n        <text macro=\"point-locators\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"\" entry-spacing=\"0\" line-spacing=\"2\">\n    <sort>\n      <key macro=\"contributors\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout>\n      <group display=\"block\">\n        <text macro=\"contributors\"/>\n      </group>\n      <group display=\"left-margin\">\n        <text macro=\"date1\" prefix=\" \"/>\n        <text macro=\"original-date\" prefix=\" [\" suffix=\"]\"/>\n      </group>\n      <group display=\"right-inline\" suffix=\".\">\n        <text macro=\"rest-of-bib\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"