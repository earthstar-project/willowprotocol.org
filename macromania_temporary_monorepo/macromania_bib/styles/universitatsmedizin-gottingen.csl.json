"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"de-DE\">\n  <info>\n    <title>Universitätsmedizin Göttingen (Deutsch)</title>\n    <title-short>UMG</title-short>\n    <id>http://www.zotero.org/styles/universitatsmedizin-gottingen</id>\n    <link href=\"http://www.zotero.org/styles/universitatsmedizin-gottingen\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/plant-physiology\" rel=\"template\"/>\n    <link href=\"https://www.umg.eu/studium-lehre/promotion/downloads-promotion/\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"medicine\"/>\n    <summary>Zitierstil für die Universitätsmedizin Göttingen</summary>\n    <updated>2022-01-23T19:13:23+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"editor\" form=\"verb-short\">hrsg. v.</term>\n      <term name=\"retrieved\">abgerufen am</term>\n    </terms>\n  </locale>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"report\">\n            <text variable=\"title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"title\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-count\">\n    <names variable=\"author\">\n      <name form=\"count\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" suffix=\")\" prefix=\" (\"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\" \">\n      <group delimiter=\", \">\n        <text variable=\"publisher\"/>\n        <text variable=\"publisher-place\"/>\n      </group>\n      <choose>\n        <if type=\"report\" match=\"none\">\n          <text macro=\"year\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\"; \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \">\n            <text term=\"retrieved\"/>\n            <date variable=\"accessed\" form=\"numeric\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"sort-key\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <text value=\"2\"/>\n      </if>\n      <else>\n        <text value=\"1\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key macro=\"year\" sort=\"ascending\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"12\" et-al-use-first=\"10\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"sort-key\"/>\n      <key macro=\"author\" names-min=\"1\" names-use-first=\"1\"/>\n      <key macro=\"author-count\" names-min=\"3\" names-use-first=\"3\"/>\n      <key macro=\"year\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\": \" delimiter=\". \">\n            <text variable=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n              <text macro=\"editor\"/>\n              <group delimiter=\" \">\n                <text variable=\"container-title\"/>\n                <group prefix=\"(\" suffix=\")\" delimiter=\" \">\n                  <text variable=\"collection-title\"/>\n                  <text variable=\"collection-number\"/>\n                </group>\n              </group>\n            </group>\n            <group delimiter=\"; \">\n              <group delimiter=\" \">\n                <label variable=\"volume\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <text macro=\"edition\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"report\">\n          <group delimiter=\". \">\n            <group delimiter=\": \">\n              <text macro=\"year\" prefix=\" (\" suffix=\")\"/>\n              <text variable=\"title\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n          <group prefix=\": \" delimiter=\". \">\n            <group delimiter=\" \">\n              <text variable=\"title\"/>\n              <group prefix=\"(\" suffix=\")\" delimiter=\" \">\n                <text variable=\"collection-title\"/>\n                <text variable=\"collection-number\"/>\n              </group>\n            </group>\n            <group delimiter=\"; \">\n              <group delimiter=\" \">\n                <label variable=\"volume\"/>\n                <text variable=\"volume\"/>\n              </group>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog post\" match=\"any\">\n          <choose>\n            <if variable=\"author editor translator title\" match=\"any\">\n              <text macro=\"access\" prefix=\". \"/>\n            </if>\n            <else>\n              <text macro=\"access\"/>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\". \" prefix=\": \">\n            <text variable=\"title\"/>\n            <text variable=\"genre\"/>\n            <group delimiter=\" \">\n              <text variable=\"publisher-place\"/>\n              <text macro=\"year\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n          <group delimiter=\" \">\n            <group prefix=\" \" delimiter=\": \">\n              <text macro=\"year\" prefix=\"(\" suffix=\")\"/>\n              <text variable=\"title\" suffix=\".\"/>\n            </group>\n            <group delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text variable=\"volume\" text-decoration=\"underline\"/>\n              <date variable=\"issued\" form=\"numeric\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <group prefix=\" \" delimiter=\": \">\n              <text macro=\"year\" prefix=\"(\" suffix=\")\"/>\n              <text variable=\"title\" suffix=\".\"/>\n            </group>\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n            <group delimiter=\", \">\n              <text variable=\"volume\" text-decoration=\"underline\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"