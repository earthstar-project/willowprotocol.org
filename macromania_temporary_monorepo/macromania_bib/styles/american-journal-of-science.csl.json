"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>American Journal of Science</title>\n    <title-short>AJS</title-short>\n    <id>http://www.zotero.org/styles/american-journal-of-science</id>\n    <link href=\"http://www.zotero.org/styles/american-journal-of-science\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/us-geological-survey\" rel=\"template\"/>\n    <link href=\"http://www.ajsonline.org/site/misc/ifora.xhtml\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geology\"/>\n    <issn>0002-9599</issn>\n    <eissn>1945-452x</eissn>\n    <summary>Style for AJS based on the US Geological Survey Style.</summary>\n    <updated>2016-12-28T16:01:03+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n      <et-al term=\"and others\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <et-al term=\"and others\"/>\n      <label prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\".\"/>\n      <et-al term=\"and others\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"false\" disambiguate-add-givenname=\"false\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"15\" et-al-use-first=\"14\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key variable=\"issued\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\", \">\n        <text macro=\"author\"/>\n        <date variable=\"issued\" suffix=\",\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n      <choose>\n        <if type=\"report\">\n          <group prefix=\" \" delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"title\" suffix=\":\"/>\n              <text variable=\"publisher\"/>\n              <text variable=\"collection-title\"/>\n              <text variable=\"number\"/>\n            </group>\n            <group>\n              <text variable=\"page\" suffix=\" \"/>\n              <label variable=\"page\" form=\"short\" plural=\"never\"/>\n            </group>\n            <group prefix=\" \">\n              <text term=\"accessed\"/>\n              <group prefix=\" \" suffix=\",\">\n                <date variable=\"accessed\">\n                  <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n                  <date-part name=\"day\" form=\"numeric\" suffix=\", \"/>\n                  <date-part name=\"year\" form=\"long\"/>\n                </date>\n              </group>\n              <group prefix=\" \">\n                <text term=\"at\"/>\n                <text variable=\"container-title\" prefix=\" \"/>\n              </group>\n              <group prefix=\" \">\n                <text term=\"at\"/>\n                <text variable=\"URL\" prefix=\" \"/>\n              </group>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n          <group suffix=\":\">\n            <text variable=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\" delimiter=\", \">\n            <text variable=\"collection-title\"/>\n            <text macro=\"publisher\"/>\n            <text variable=\"number-of-pages\" suffix=\" p.\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group prefix=\" \" delimiter=\", \">\n            <text variable=\"title\"/>\n            <group delimiter=\" \">\n              <text term=\"accessed\"/>\n              <date variable=\"accessed\">\n                <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n                <date-part name=\"day\" form=\"numeric\" suffix=\", \"/>\n                <date-part name=\"year\" form=\"long\"/>\n              </date>\n            </group>\n            <group delimiter=\" \">\n              <group delimiter=\" \">\n                <text term=\"at\"/>\n                <text variable=\"container-title\"/>\n              </group>\n              <group delimiter=\" \">\n                <text term=\"at\"/>\n                <text variable=\"URL\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text variable=\"title\" prefix=\" \"/>\n          <group prefix=\", \">\n            <text term=\"in\" font-style=\"italic\"/>\n            <names variable=\"editor translator\" prefix=\" \" suffix=\",\" delimiter=\", \">\n              <name and=\"text\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n              <et-al term=\"and others\"/>\n              <label form=\"short\" prefix=\" \"/>\n            </names>\n            <text variable=\"event\" prefix=\" \" suffix=\" &#8211;\"/>\n            <text variable=\"container-title\" prefix=\" \" suffix=\":\"/>\n            <group suffix=\".\">\n              <text macro=\"publisher\" prefix=\" \"/>\n              <group prefix=\", \">\n                <label plural=\"never\" suffix=\" \" variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"map\">\n          <text variable=\"title\" prefix=\" \"/>\n          <text variable=\"collection-title\" prefix=\": \"/>\n          <text variable=\"note\" prefix=\", scale \"/>\n        </else-if>\n        <else-if type=\"speech\">\n          <text variable=\"title\" prefix=\" \" suffix=\":\"/>\n          <text variable=\"event\" prefix=\" \"/>\n          <text variable=\"event-place\" prefix=\", \"/>\n        </else-if>\n        <else>\n          <group suffix=\":\">\n            <text variable=\"title\" prefix=\" \"/>\n            <text macro=\"editor-translator\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" suffix=\".\" delimiter=\", \">\n            <text variable=\"container-title\"/>\n            <group delimiter=\", \">\n              <text variable=\"volume\" prefix=\"v. \"/>\n            </group>\n            <text variable=\"page\" prefix=\"p. \"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"