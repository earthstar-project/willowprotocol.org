"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Proceedings of the National Academy of Sciences of the United States of America</title>\n    <title-short>PNAS</title-short>\n    <id>http://www.zotero.org/styles/pnas</id>\n    <link href=\"http://www.zotero.org/styles/pnas\" rel=\"self\"/>\n    <link href=\"http://www.pnas.org/site/authors/procedures.xhtml\" rel=\"documentation\"/>\n    <author>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Rintze Zelle</name>\n      <uri>http://twitter.com/rintzezelle</uri>\n    </contributor>\n    <contributor>\n      <name>Bela Hausmann</name>\n      <uri>https://github.com/and3k</uri>\n    </contributor>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <issn>0027-8424</issn>\n    <eissn>1091-6490</eissn>\n    <summary>The Proceedings of the National Academy of Sciences Journal style as per the updated guidelines from 04/2019.</summary>\n    <updated>2024-03-27T16:45:36+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" prefix=\" \" delimiter=\", \">\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \" sort-separator=\" \"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"false\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \" sort-separator=\" \"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" type=\"webpage post post-weblog article\">\n        <group delimiter=\" \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group prefix=\" [\" suffix=\"]\">\n            <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <choose>\n          <if variable=\"volume\" match=\"none\">\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          </if>\n        </choose>\n      </else-if>\n      <else-if type=\"article\" match=\"any\">\n        <group delimiter=\" \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\" suffix=\" \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"dataset software\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          <date variable=\"issued\" prefix=\"Deposited \">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"chapter report thesis\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" text-case=\"capitalize-first\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\" et-al-min=\"6\" et-al-use-first=\"1\" delimiter-precedes-et-al=\"always\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\", \" suffix=\".\">\n        <text macro=\"author\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\" \">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"edition\"/>\n              </group>\n              <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                <text macro=\"publisher\"/>\n                <text macro=\"year-date\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\" \">\n              <text macro=\"title\"/>\n              <text term=\"in\"/>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n                <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n                <text macro=\"edition\"/>\n                <group delimiter=\" \">\n                  <text macro=\"editor\"/>\n                  <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                    <text macro=\"publisher\"/>\n                    <text macro=\"year-date\"/>\n                  </group>\n                </group>\n                <group>\n                  <label suffix=\" \" variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\" \">\n              <group delimiter=\",  \" suffix=\".\">\n                <text macro=\"title\"/>\n                <text variable=\"publisher\"/>\n                <text variable=\"publisher-place\"/>\n              </group>\n              <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article\" match=\"any\">\n            <group delimiter=\". \" suffix=\".\">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n                <text term=\"preprint\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n                <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"dataset\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n                <group delimiter=\", \">\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text variable=\"page\"/>\n                </group>\n                <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"