"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" page-range-format=\"expanded\" default-locale=\"fr-FR\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>Campus adventiste du Salève - Faculté adventiste de théologie (Français)</title>\n    <title-short>FAT France</title-short>\n    <id>http://www.zotero.org/styles/campus-adventiste-du-saleve-faculte-adventiste-de-theologie</id>\n    <link href=\"http://www.zotero.org/styles/campus-adventiste-du-saleve-faculte-adventiste-de-theologie\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/invisu\" rel=\"template\"/>\n    <link href=\"https://github.com/citation-style-language/styles/pull/668\" rel=\"documentation\"/>\n    <author>\n      <name>Nytanéliah Ranaivoson</name>\n      <email>nytaneliah@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Stéphane Eucharis</name>\n      <email>eukastef@gmail.com</email>\n    </contributor>\n    <contributor>\n      <name>Luca Marulli</name>\n      <email>luca.marulli@campusadventiste.edu</email>\n    </contributor>\n    <category citation-format=\"note\"/>\n    <category field=\"theology\"/>\n    <category field=\"humanities\"/>\n    <summary>Le style de la Faculté Adventiste de Théologie de Collonges-sous-Salève a été conçu à partir du Guide méthodologique de Gabriel MONET, ISBN 978-2-911358-18-0. Il découle du style InVisu.</summary>\n    <updated>2013-08-16T00:37:02+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"cited\"/>\n      <term name=\"page\" form=\"short\">p.</term>\n      <term name=\"editor\" form=\"short\">éd.</term>\n      <term name=\"online\">[en ligne]</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <choose>\n      <if variable=\"author\">\n        <names variable=\"author\" font-variant=\"normal\" suffix=\", \">\n          <name font-style=\"normal\" delimiter-precedes-et-al=\"always\" delimiter-precedes-last=\"always\" et-al-min=\"4\" et-al-use-first=\"3\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" initialize=\"false\" sort-separator=\" \">\n            <name-part name=\"family\" text-case=\"sentence\" font-variant=\"small-caps\"/>\n          </name>\n          <et-al font-style=\"italic\"/>\n        </names>\n      </if>\n      <else-if variable=\"editor\">\n        <names variable=\"editor\" suffix=\", \">\n          <name font-style=\"normal\" delimiter-precedes-last=\"never\" et-al-min=\"4\" et-al-use-first=\"3\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" sort-separator=\" \">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n          <label form=\"short\" prefix=\"&#160;(\" suffix=\".)\"/>\n        </names>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"author-bib\">\n    <choose>\n      <if variable=\"author\">\n        <names variable=\"author\" suffix=\", \">\n          <name font-style=\"normal\" delimiter-precedes-et-al=\"always\" delimiter-precedes-last=\"always\" et-al-min=\"4\" et-al-use-first=\"3\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" initialize=\"false\" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \">\n            <name-part name=\"family\" text-case=\"sentence\" font-variant=\"small-caps\"/>\n          </name>\n          <et-al font-style=\"italic\"/>\n        </names>\n      </if>\n      <else-if variable=\"editor\">\n        <names variable=\"editor\" suffix=\", \">\n          <name font-style=\"normal\" delimiter-precedes-last=\"never\" et-al-min=\"4\" et-al-use-first=\"3\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\" name-as-sort-order=\"all\" sort-separator=\" \">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n          <label form=\"short\" prefix=\"&#160;(\" suffix=\".)\"/>\n        </names>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name form=\"long\" and=\"text\" delimiter-precedes-last=\"never\" sort-separator=\" \" font-style=\"normal\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\"&#160;(\" suffix=\".)\"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book report\" match=\"any\">\n        <group>\n          <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"false\" font-style=\"italic\"/>\n          <text macro=\"online\"/>\n        </group>\n      </if>\n      <else-if type=\"graphic personal_communication interview\" match=\"any\">\n        <group>\n          <text variable=\"title\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n          <text macro=\"online\"/>\n        </group>\n        <text macro=\"date\"/>\n        <group delimiter=\", \" prefix=\", \" suffix=\".\">\n          <text macro=\"interviewer\"/>\n          <text variable=\"medium\"/>\n          <text variable=\"genre\"/>\n          <text variable=\"issue\"/>\n        </group>\n      </else-if>\n      <else-if type=\"motion_picture\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n          <text variable=\"medium\"/>\n          <text macro=\"online\"/>\n        </group>\n      </else-if>\n      <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\", \">\n          <group>\n            <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\" font-style=\"normal\"/>\n            <text macro=\"online\"/>\n          </group>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else-if type=\"thesis broadcast\" match=\"any\">\n        <group delimiter=\", \">\n          <group>\n            <text variable=\"title\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n            <text macro=\"online\"/>\n          </group>\n          <text variable=\"genre\"/>\n          <choose>\n            <if variable=\"publisher\">\n              <text variable=\"publisher\"/>\n            </if>\n            <else>\n              <text value=\"[s.n.]\"/>\n            </else>\n          </choose>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <text macro=\"number\"/>\n        </group>\n      </else-if>\n      <else-if type=\"manuscript\" match=\"any\">\n        <group delimiter=\"\">\n          <group suffix=\",\">\n            <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\" font-style=\"normal\"/>\n            <text macro=\"online\"/>\n          </group>\n          <text variable=\"genre\" prefix=\" \"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group>\n          <group suffix=\",\">\n            <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\"/>\n            <text macro=\"online\"/>\n          </group>\n          <text value=\"in\" prefix=\" \" suffix=\" \"/>\n          <text macro=\"editor\" suffix=\", \"/>\n          <text variable=\"container-title\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else-if type=\"entry-dictionary entry-encyclopedia paper-conference chapter\" match=\"any\">\n        <group delimiter=\"\">\n          <group suffix=\",\">\n            <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\" prefix=\"article \"/>\n            <text macro=\"online\"/>\n          </group>\n          <text value=\"in\" font-style=\"normal\" prefix=\" \" suffix=\" \"/>\n          <text macro=\"editor\" suffix=\", \"/>\n          <text variable=\"container-title\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage\" match=\"any\">\n        <group delimiter=\", \">\n          <group>\n            <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\" font-style=\"normal\"/>\n            <text macro=\"online\"/>\n          </group>\n          <text variable=\"genre\"/>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else-if type=\"map\" match=\"any\">\n        <group delimiter=\", \">\n          <group>\n            <text variable=\"title\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n            <text macro=\"online\"/>\n          </group>\n          <text variable=\"genre\"/>\n          <text variable=\"edition\"/>\n        </group>\n      </else-if>\n      <else>\n        <group>\n          <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\" font-style=\"normal\"/>\n          <text macro=\"online\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"translator-bib\">\n    <names variable=\"translator\">\n      <name prefix=\", trad. \" name-as-sort-order=\"all\" sort-separator=\" \">\n        <name-part name=\"family\" text-case=\"sentence\" font-variant=\"small-caps\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name font-style=\"normal\" prefix=\", trad. \" delimiter-precedes-last=\"never\" sort-separator=\" \">\n        <name-part name=\"family\" text-case=\"sentence\" font-variant=\"small-caps\"/>\n      </name>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"book graphic report entry-dictionary entry-encyclopedia chapter speech paper-conference\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \" prefix=\", \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" prefix=\", \"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"article-journal article-magazine\" match=\"any\">\n        <group font-style=\"normal\">\n          <choose>\n            <if variable=\"issued\">\n              <group prefix=\" (\" suffix=\")\">\n                <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n                <text macro=\"issue\" text-decoration=\"none\" prefix=\"/\"/>\n              </group>\n            </if>\n            <else>\n              <text macro=\"volume\" prefix=\", \"/>\n            </else>\n          </choose>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"newspaper-edition\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group delimiter=\", \">\n          <group>\n            <text term=\"edition\" prefix=\" \"/>\n            <text variable=\"edition\" prefix=\" \"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\". \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <choose>\n          <if variable=\"genre\" match=\"none\">\n            <group delimiter=\" \" prefix=\", \">\n              <text term=\"presented at\" suffix=\" \"/>\n              <text variable=\"event\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\" \" prefix=\", \">\n              <text variable=\"genre\"/>\n              <text term=\"presented at\"/>\n              <text variable=\"event\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"place\">\n    <choose>\n      <if type=\"article-newspaper paper-conference\" match=\"any\">\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\" prefix=\", \" suffix=\", \"/>\n          </if>\n          <else>\n            <text value=\"\" prefix=\", \" suffix=\", \"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"speech manuscript thesis broadcast\" match=\"any\">\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\" prefix=\", \"/>\n          </if>\n          <else>\n            <text value=\"\" prefix=\", \"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"article-magazine article-journal personal_communication interview graphic webpage\" match=\"any\"/>\n      <else-if type=\"book chapter motion_picture report entry-dictionary entry-encyclopedia map\" match=\"any\">\n        <choose>\n          <if variable=\"publisher-place\">\n            <text variable=\"publisher-place\" prefix=\", \" suffix=\", \"/>\n          </if>\n          <else>\n            <text value=\"\" prefix=\", \" suffix=\", \"/>\n          </else>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"motion_picture map book paper-conference entry-dictionary entry-encyclopedia chapter report\" match=\"any\">\n        <choose>\n          <if variable=\"publisher\">\n            <text variable=\"publisher\"/>\n          </if>\n          <else>\n            <text value=\"\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"thesis broadcast\" match=\"any\"/>\n      <else>\n        <text variable=\"publisher\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <choose>\n      <if type=\"motion_picture map entry-dictionary entry-encyclopedia book speech paper-conference chapter\" match=\"any\">\n        <group prefix=\", \">\n          <group delimiter=\"\">\n            <text variable=\"collection-title\"/>\n            <text variable=\"collection-number\" prefix=\"&#160;\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"collection-title\" prefix=\", \" font-style=\"italic\"/>\n      </else-if>\n      <else>\n        <text variable=\"collection-title\" suffix=\", \" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\" form=\"text\" prefix=\", \"/>\n      </if>\n      <else>\n        <text value=\"\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"yearpage\">\n    <choose>\n      <if type=\"map book webpage\" match=\"any\">\n        <group>\n          <text macro=\"date\"/>\n          <choose>\n            <if variable=\"locator\" match=\"any\">\n              <text variable=\"locator\" prefix=\", p.&#160;\"/>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else-if type=\"manuscript thesis motion_picture broadcast\" match=\"any\">\n        <group>\n          <text macro=\"date\"/>\n          <choose>\n            <if variable=\"locator\" match=\"any\">\n              <text variable=\"locator\" prefix=\"p.&#160;\"/>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n      <else-if type=\"article-newspaper speech paper-conference report entry-dictionary entry-encyclopedia chapter\" match=\"any\">\n        <group>\n          <text macro=\"newspaper-edition\"/>\n          <text macro=\"date\"/>\n          <choose>\n            <if variable=\"locator\" match=\"any\">\n              <group delimiter=\"&#160;\" prefix=\", \" suffix=\".\">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"locator\"/>\n              </group>\n            </if>\n            <else-if variable=\"locator\" match=\"none\">\n              <group delimiter=\"&#160;\" prefix=\", \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </else-if>\n          </choose>\n        </group>\n      </else-if>\n      <else-if type=\"graphic personal_communication interview\" match=\"any\"/>\n      <else-if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\" \" font-style=\"normal\" prefix=\", \" suffix=\".\">\n          <choose>\n            <if variable=\"locator\" match=\"any\">\n              <group delimiter=\"&#160;\">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"locator\"/>\n              </group>\n            </if>\n            <else-if variable=\"locator\" match=\"none\">\n              <group delimiter=\"&#160;\">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </else-if>\n          </choose>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \" font-style=\"normal\">\n          <text macro=\"date\"/>\n          <choose>\n            <if variable=\"locator\" match=\"any\">\n              <text variable=\"locator\" prefix=\"p.&#160;\"/>\n            </if>\n            <else-if variable=\"locator\" match=\"none\">\n              <text variable=\"number-of-pages\" suffix=\"&#160;p\"/>\n            </else-if>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"yearpage-bib\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <group>\n          <text macro=\"date\"/>\n        </group>\n      </if>\n      <else-if type=\"map manuscript thesis motion_picture broadcast\" match=\"any\">\n        <group>\n          <text macro=\"date\" suffix=\".\"/>\n          <text variable=\"number-of-pages\" prefix=\", \" suffix=\"&#160;p\"/>\n        </group>\n      </else-if>\n      <else-if type=\"report entry-dictionary entry-encyclopedia chapter\" match=\"any\">\n        <group>\n          <text macro=\"date\"/>\n          <group delimiter=\"&#160;\" prefix=\", \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"graphic personal_communication interview\" match=\"any\"/>\n      <else-if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\" \" font-style=\"normal\" prefix=\", \">\n          <group delimiter=\"&#160;\" suffix=\".\">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"article-newspaper speech paper-conference\" match=\"any\">\n        <group delimiter=\"\" font-style=\"normal\">\n          <text macro=\"newspaper-edition\"/>\n          <text macro=\"date\"/>\n          <group delimiter=\"&#160;\" prefix=\", \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"webpage\" match=\"any\">\n        <text macro=\"date\"/>\n      </else-if>\n      <else>\n        <group>\n          <text macro=\"date\"/>\n          <text variable=\"number-of-pages\" prefix=\", \" suffix=\"&#160;p.\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"archive\">\n    <group suffix=\".\" prefix=\". \">\n      <group delimiter=\", \">\n        <text variable=\"archive\"/>\n        <text variable=\"archive_location\"/>\n        <text variable=\"call-number\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if type=\"article-journal article-newspaper\" match=\"any\">\n        <choose>\n          <if is-numeric=\"volume\">\n            <text variable=\"volume\" prefix=\" \"/>\n          </if>\n          <else>\n            <text variable=\"number-of-volumes\" suffix=\"vol.,\"/>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if match=\"any\" variable=\"volume\">\n            <text variable=\"volume\" prefix=\", vol. \"/>\n          </if>\n          <else>\n            <text variable=\"number-of-volumes\" prefix=\", \" suffix=\" vol.\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"number\">\n    <choose>\n      <if is-numeric=\"number\">\n        <text variable=\"number\" prefix=\"n°&#160;\"/>\n      </if>\n      <else>\n        <text variable=\"number\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue\">\n    <choose>\n      <if is-numeric=\"issue\">\n        <text variable=\"issue\"/>\n      </if>\n      <else>\n        <text variable=\"issue\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"interviewer\">\n    <group>\n      <names variable=\"interviewer\">\n        <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n        <name form=\"long\" and=\"text\" delimiter=\", \">\n          <name-part name=\"family\" font-variant=\"small-caps\"/>\n        </name>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"accessed\">\n    <group delimiter=\" \">\n      <text variable=\"URL\" text-decoration=\"none\" prefix=\", disponible sur&#160;&lt;\" suffix=\"&gt;\"/>\n    </group>\n    <group delimiter=\" \" font-style=\"normal\">\n      <text value=\"(consulté le\" prefix=\", \"/>\n      <date form=\"text\" variable=\"accessed\" suffix=\")\">\n        <date-part name=\"day\"/>\n        <date-part name=\"month\"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if match=\"any\" variable=\"URL\">\n        <text term=\"online\" prefix=\" \"/>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"3\" et-al-subsequent-min=\"3\" et-al-subsequent-use-first=\"1\">\n    <layout suffix=\".\" delimiter=\"&#160;; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" text-case=\"capitalize-first\" font-style=\"italic\" suffix=\".\"/>\n            <text variable=\"locator\" prefix=\"p. \"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <text macro=\"author\"/>\n          <group delimiter=\", \">\n            <choose>\n              <if type=\"book graphic report\" match=\"any\">\n                <text macro=\"title\" font-style=\"normal\"/>\n              </if>\n              <else>\n                <text macro=\"title\" text-case=\"capitalize-first\" quotes=\"false\" font-style=\"normal\"/>\n              </else>\n            </choose>\n            <text term=\"cited\" font-style=\"italic\" suffix=\".\"/>\n            <group delimiter=\"&#160;\">\n              <label plural=\"never\" variable=\"page\" form=\"short\"/>\n              <text variable=\"locator\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"author\"/>\n          <text macro=\"title\"/>\n          <text macro=\"translator\"/>\n          <text macro=\"collection\"/>\n          <text macro=\"volume\"/>\n          <text macro=\"edition\"/>\n          <text macro=\"event\"/>\n          <text macro=\"place\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"yearpage\"/>\n          <text macro=\"accessed\"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\" sort=\"descending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author-bib\"/>\n      <text macro=\"title\"/>\n      <text macro=\"translator-bib\"/>\n      <text macro=\"collection\"/>\n      <text macro=\"volume\"/>\n      <text macro=\"edition\"/>\n      <text macro=\"event\"/>\n      <text macro=\"place\"/>\n      <text macro=\"publisher\"/>\n      <text macro=\"yearpage-bib\"/>\n      <text macro=\"archive\"/>\n      <text macro=\"accessed\"/>\n    </layout>\n  </bibliography>\n</style>\n"