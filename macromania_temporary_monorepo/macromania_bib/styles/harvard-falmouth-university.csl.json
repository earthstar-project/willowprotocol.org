"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\" page-range-format=\"minimal\">\n  <info>\n    <title>Falmouth University - Harvard</title>\n    <id>http://www.zotero.org/styles/harvard-falmouth-university</id>\n    <link href=\"http://www.zotero.org/styles/harvard-falmouth-university\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/solent-university-harvard\" rel=\"template\"/>\n    <link href=\"http://ask.fxplus.ac.uk/harvard-falmouth\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary/>\n    <updated>2013-03-16T02:03:44+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"edition\" form=\"short\">edn.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"song\" variable=\"composer\" match=\"all\">\n        <names variable=\"composer\">\n          <name name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" and=\"text\" delimiter-precedes-last=\"never\">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n          </name>\n        </names>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" and=\"text\" delimiter-precedes-last=\"never\">\n            <name-part name=\"family\" text-case=\"uppercase\"/>\n          </name>\n          <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"title\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-sort\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" and=\"text\" delimiter-precedes-last=\"never\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title-citation\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"song\" variable=\"composer\" match=\"all\">\n        <names variable=\"composer\">\n          <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n        </names>\n      </if>\n      <else>\n        <names variable=\"author\">\n          <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <text macro=\"title-citation\"/>\n          </substitute>\n        </names>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor-translator\">\n    <choose>\n      <if type=\"chapter paper-conference entry-encyclopedia entry-dictionary\" match=\"none\">\n        <names variable=\"editor translator\" delimiter=\", \">\n          <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <name delimiter-precedes-last=\"never\" and=\"text\"/>\n        </names>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"song\">\n        <choose>\n          <if variable=\"composer author\" match=\"all\">\n            <names variable=\"author\">\n              <name prefix=\"Perf.\"/>\n            </names>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\" delimiter=\" \">\n              <date-part name=\"day\"/>\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"graphic personal_communication\" match=\"any\">\n        <text variable=\"archive\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation  report song thesis\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\" \">\n            <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n            <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n            <choose>\n              <if type=\"legislation\">\n                <text variable=\"section\" prefix=\"(\" suffix=\")\"/>\n              </if>\n            </choose>\n          </group>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else-if type=\"motion_picture\">\n        <group delimiter=\" \">\n          <text variable=\"title\" font-style=\"italic\"/>\n          <choose>\n            <if variable=\"medium\">\n              <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n            </if>\n            <else>\n              <text value=\"[Film]\"/>\n            </else>\n          </choose>\n        </group>\n      </else-if>\n      <else-if type=\"interview broadcast\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"title\"/>\n          <text variable=\"number\"/>\n          <text variable=\"medium\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </else-if>\n      <else-if type=\"personal_communication\">\n        <group delimiter=\", \">\n          <text variable=\"title\"/>\n          <date variable=\"issued\" form=\"text\"/>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\" \">\n          <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n          <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-citation\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture  report song speech thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"interview broadcast personal_communication\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <group delimiter=\". \">\n          <names variable=\"editor translator\" delimiter=\", \">\n            <name sort-separator=\", \" and=\"text\" delimiter=\", \">\n              <name-part name=\"family\" text-case=\"uppercase\"/>\n            </name>\n            <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n          </names>\n          <group delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"container-title\"/>\n      </else-if>\n      <else-if type=\"broadcast\">\n        <choose>\n          <if variable=\"container-title\">\n            <group delimiter=\", \">\n              <text variable=\"number\" prefix=\"Episode \"/>\n              <text variable=\"title\"/>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n      <else-if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <text macro=\"online\"/>\n        </group>\n      </else-if>\n      <else-if type=\"post\" match=\"any\">\n        <group delimiter=\". \">\n          <date variable=\"issued\" form=\"text\" prefix=\"Posted \"/>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"container-title\" font-style=\"italic\"/>\n          <text macro=\"book-volume\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"book-volume\">\n    <choose>\n      <if type=\"book chapter entry-encyclopedia entry-dictionary\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"volume\" form=\"short\"/>\n          <text variable=\"volume\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <group delimiter=\", \">\n          <text variable=\"event\"/>\n          <text variable=\"event-place\"/>\n          <date variable=\"issued\" form=\"text\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"legislation\"/>\n      <else-if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else-if variable=\"accessed\">\n        <choose>\n          <if type=\"webpage\">\n            <date variable=\"accessed\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-bib\">\n    <choose>\n      <if type=\"legislation\"/>\n      <else-if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else-if variable=\"accessed\">\n        <choose>\n          <if type=\"webpage\">\n            <date variable=\"accessed\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <text term=\"no date\" form=\"short\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <group delimiter=\" \">\n            <number variable=\"edition\" form=\"ordinal\"/>\n            <text term=\"edition\" form=\"short\"/>\n          </group>\n        </if>\n        <else>\n          <text variable=\"edition\" suffix=\".\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"online\">\n    <choose>\n      <if variable=\"URL\">\n        <text term=\"online\" prefix=\"[\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group prefix=\" \" delimiter=\", \">\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text macro=\"online\"/>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <date variable=\"issued\" delimiter=\" \" prefix=\", \">\n          <date-part name=\"day\"/>\n          <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n        </date>\n        <text macro=\"online\" prefix=\" \"/>\n        <text variable=\"page\" prefix=\", \"/>\n      </else-if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </else-if>\n      <else-if type=\"broadcast interview\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <text variable=\"publisher\"/>\n          <date variable=\"issued\" form=\"text\"/>\n        </group>\n      </else-if>\n      <else-if type=\"paper-conference  speech\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference entry-encyclopedia entry-dictionary speech\" match=\"any\">\n        <group delimiter=\", \" prefix=\". \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"5\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author-sort\"/>\n      <key macro=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\".\"/>\n      <group delimiter=\". \" prefix=\" \">\n        <text macro=\"issued-bib\"/>\n        <text macro=\"title\"/>\n        <text macro=\"editor-translator\"/>\n        <text macro=\"container\"/>\n      </group>\n      <text macro=\"locators\"/>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"