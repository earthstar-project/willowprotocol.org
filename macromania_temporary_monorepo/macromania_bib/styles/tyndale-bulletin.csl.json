"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"chicago\" default-locale=\"en-GB\">\n  <info>\n    <title>Tyndale Bulletin</title>\n    <id>http://www.zotero.org/styles/tyndale-bulletin</id>\n    <link href=\"http://www.zotero.org/styles/tyndale-bulletin\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/society-of-biblical-literature-fullnote-bibliography\" rel=\"template\"/>\n    <link href=\"https://tyndalebulletin.org/for-authors\" rel=\"documentation\"/>\n    <author>\n      <name>J. David Stark</name>\n      <email>david@jdavidstark.com</email>\n      <uri>https://www.jdavidstark.com/</uri>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"theology\"/>\n    <issn>0082-7118</issn>\n    <eissn>2752-7042</eissn>\n    <summary>Tyndale Bulletin format with full notes and bibliography</summary>\n    <updated>2022-03-04T14:17:21+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n    <!-- Tyndale Bulletin style differs from SBLHS2 only rarely (§4.1). The departures are that Tyndale Bulletin \n      1) insists that DOIs be included where available (§§11.1, 11.3.7) and\n      2) uses British-style quotation mark conventions (§§8.1, 11.1, 11.6.6-11.3.8). -->\n  </info>\n  <locale xml:lang=\"en-GB\">\n    <!-- Punctuate outside quotations per the January 2022 update to the style guide. -->\n    <style-options punctuation-in-quote=\"false\"/>\n    <terms>\n      <term name=\"collection-editor\" form=\"verb\">edited by</term>\n      <term name=\"collection-editor\" form=\"verb-short\">ed.</term>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"editortranslator\" form=\"verb-short\">ed. and trans.</term>\n      <term name=\"editortranslator\" form=\"verb\">edited and translated by</term>\n      <!-- Use en dash in page ranges per the January 2022 update to the style guide. -->\n      <term name=\"page-range-delimiter\">&#8211;</term>\n      <term name=\"review-of\">review of</term>\n      <term name=\"section\" form=\"short\">\n        <single>§</single>\n        <multiple>§§</multiple>\n      </term>\n      <term name=\"translator\" form=\"verb-short\">trans.</term>\n      <term name=\"translator\" form=\"short\">trans.</term>\n      <!-- Use a period after the abbreviation for volume(s) per §12.3. -->\n      <term name=\"volume\" form=\"short\">\n        <single>vol.</single>\n        <multiple>vols.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"editor-translator-verb-short-comma\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <label form=\"verb-short\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"editor-translator-verb-period\">\n    <names variable=\"editor translator\" delimiter=\". \">\n      <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"editor-translator-verb-comma\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"container-author-reviewed-author-and-editor-translator\">\n    <group delimiter=\", \">\n      <choose>\n        <if variable=\"container-author reviewed-author\" match=\"any\">\n          <names variable=\"container-author reviewed-author\" delimiter=\", \">\n            <label form=\"verb-short\" suffix=\" \"/>\n            <name and=\"text\" delimiter=\", \"/>\n          </names>\n        </if>\n      </choose>\n      <text macro=\"editor-translator-verb-short-comma\"/>\n    </group>\n  </macro>\n  <macro name=\"secondary-contributors-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n        <text macro=\"container-author-reviewed-author-and-editor-translator\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"editor-translator-verb-short-comma\"/>\n          <names variable=\"container-author\">\n            <label form=\"verb-short\" suffix=\" \"/>\n            <name and=\"text\" delimiter=\", \"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <group delimiter=\". \" prefix=\" \">\n          <text macro=\"editor-translator-verb-period\"/>\n          <names variable=\"container-author\">\n            <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <name and=\"text\" delimiter=\", \"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \" prefix=\" \">\n          <text macro=\"editor-translator-verb-comma\"/>\n          <names variable=\"container-author\">\n            <label form=\"verb\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <name and=\"text\" delimiter=\", \"/>\n          </names>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor-note\">\n    <names variable=\"editor\">\n      <name and=\"text\" sort-separator=\", \" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"collection-editor-note\">\n    <names variable=\"collection-editor\">\n      <label form=\"verb-short\" suffix=\" \"/>\n      <name and=\"text\" sort-separator=\", \" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"collection-editor\">\n    <group delimiter=\", \" prefix=\". \">\n      <names variable=\"collection-editor\" delimiter=\", \">\n        <label form=\"verb\" prefix=\" \" text-case=\"capitalize-first\" suffix=\" \"/>\n        <name and=\"text\" delimiter=\", \"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"translator-note\">\n    <names variable=\"translator\">\n      <name and=\"text\" sort-separator=\", \" delimiter=\", \"/>\n      <label form=\"verb-short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"recipient-note\">\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-note\">\n    <names variable=\"author\">\n      <name and=\"text\" sort-separator=\", \" delimiter=\", \"/>\n      <substitute>\n        <text macro=\"editor-note\"/>\n        <text macro=\"translator-note\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient-note\"/>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <name name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"verb-short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"recipient\">\n    <choose>\n      <if type=\"personal_communication\">\n        <choose>\n          <if variable=\"genre\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n          <else>\n            <text term=\"letter\" text-case=\"capitalize-first\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n    <text macro=\"recipient-note\" prefix=\" \"/>\n  </macro>\n  <macro name=\"contributors\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"first\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <text macro=\"editor\"/>\n        <text macro=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient\" prefix=\". \"/>\n  </macro>\n  <macro name=\"recipient-short\">\n    <names variable=\"recipient\">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"contributors-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n    <text macro=\"recipient-short\"/>\n  </macro>\n  <macro name=\"container-contributors-short\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\"; \">\n          <choose>\n            <if variable=\"author\">\n              <names variable=\"editor\" delimiter=\", \">\n                <label form=\"verb-short\" suffix=\" \"/>\n                <name form=\"short\" and=\"text\" delimiter=\", \"/>\n              </names>\n            </if>\n          </choose>\n          <choose>\n            <if variable=\"author editor\" match=\"any\">\n              <names variable=\"translator\" delimiter=\", \">\n                <label form=\"verb-short\" suffix=\" \"/>\n                <name form=\"short\" and=\"text\" delimiter=\", \"/>\n              </names>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"contributors-sort\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"text\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"interviewer-note\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"interviewer\">\n    <names variable=\"interviewer\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"capitalize-first\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title-note\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <text variable=\"genre\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </else-if>\n      <!-- Include \"review of\" prefix, italicize the book titles in untitled book reviews per §§11.3.8-11.3.9. -->\n      <else-if variable=\"reviewed-author\">\n        <choose>\n          <!-- Include the review title, if avaialable, in quotation marks per §11.3.8. -->\n          <if variable=\"reviewed-title\">\n            <group delimiter=\" \">\n              <text variable=\"title\" quotes=\"true\" suffix=\", \" text-case=\"title\"/>\n              <text term=\"review-of\"/>\n              <text variable=\"reviewed-title\" font-style=\"italic\" text-case=\"title\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text term=\"review-of\"/>\n              <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"personal_communication\" match=\"none\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </else-if>\n      <!-- Include the reviewed author for book reviews per §§11.3.8-11.3.9. -->\n      <else-if variable=\"reviewed-author\">\n        <choose>\n          <if variable=\"reviewed-title\">\n            <group delimiter=\" \">\n              <text variable=\"title\" quotes=\"true\" suffix=\",\" text-case=\"title\"/>\n              <text term=\"review-of\"/>\n              <text variable=\"reviewed-title\" font-style=\"italic\" suffix=\",\" text-case=\"title\"/>\n              <names variable=\"reviewed-author\">\n                <label form=\"verb\" suffix=\" \"/>\n                <name and=\"text\"/>\n              </names>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text term=\"review-of\" text-case=\"capitalize-first\"/>\n              <text variable=\"title\" font-style=\"italic\" suffix=\", \" text-case=\"title\"/>\n              <names variable=\"reviewed-author\">\n                <label form=\"verb\" suffix=\" \"/>\n                <name and=\"text\"/>\n              </names>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if variable=\"title\" match=\"none\">\n        <choose>\n          <if type=\"interview\">\n            <text term=\"interview\"/>\n          </if>\n          <else-if type=\"manuscript speech\" match=\"any\">\n            <text variable=\"genre\" form=\"short\"/>\n          </else-if>\n          <else-if type=\"personal_communication\">\n            <text macro=\"issued\"/>\n          </else-if>\n        </choose>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" form=\"short\" font-style=\"italic\" text-case=\"title\"/>\n      </else-if>\n      <!-- For short citations, use the short review title if available. Otherwise, use a \"review of\" prefix, italicize the short book title per §11.3.8. -->\n      <else-if variable=\"reviewed-author\">\n        <choose>\n          <if variable=\"reviewed-title\">\n            <text variable=\"title\" quotes=\"true\" text-case=\"title\"/>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text term=\"review-of\"/>\n              <text variable=\"title\" font-style=\"italic\" form=\"short\" text-case=\"title\"/>\n            </group>\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text variable=\"title\" form=\"short\" quotes=\"true\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"disambiguate-chapter\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <choose>\n          <if disambiguate=\"true\">\n            <text macro=\"container-contributors-short\" prefix=\"(\" suffix=\")\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"description-note\">\n    <group delimiter=\", \">\n      <text macro=\"interviewer-note\"/>\n      <text variable=\"medium\"/>\n      <choose>\n        <if variable=\"title\" match=\"none\"/>\n        <else-if type=\"thesis speech\" match=\"any\"/>\n        <else>\n          <text variable=\"genre\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"description\">\n    <group delimiter=\", \">\n      <group delimiter=\". \">\n        <text macro=\"interviewer\"/>\n        <text variable=\"medium\" text-case=\"capitalize-first\"/>\n      </group>\n      <choose>\n        <if variable=\"title\" match=\"none\"/>\n        <else-if type=\"thesis speech\" match=\"any\"/>\n        <else>\n          <text variable=\"genre\" text-case=\"capitalize-first\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"container-title-note\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" suffix=\" \"/>\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"container-title\" font-style=\"italic\" form=\"short\" text-case=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection-title\">\n    <choose>\n      <if variable=\"volume number-of-volumes\" match=\"any\">\n        <!-- Presence of Volume info indicates multivolume work rather than series -->\n        <choose>\n          <!--This section required here rather than in title-note because the group delimiter inserts an unneeded comma otherwise-->\n          <if type=\"chapter\" variable=\"container-title\" match=\"all\">\n            <!--Filter SBL 6.2.23-->\n          </if>\n          <else-if type=\"chapter\" variable=\"collection-title\" match=\"all\">\n            <!--Filter SBL 6.2.22-->\n            <group>\n              <text term=\"volume\" form=\"short\" suffix=\" \"/>\n              <number variable=\"volume\" form=\"numeric\"/>\n              <text value=\" of \"/>\n            </group>\n          </else-if>\n        </choose>\n        <text variable=\"collection-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </if>\n      <else>\n        <text variable=\"collection-title\" form=\"short\" text-case=\"title\"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection-title-note\">\n    <choose>\n      <if variable=\"volume number-of-volumes\" match=\"any\">\n        <!-- Presence of Volume info indicates multivolume work rather than series -->\n        <choose>\n          <!-- This section required here rather than in title-note because the group delimiter inserts an unneeded comma otherwise-->\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <choose>\n              <if variable=\"container-title\" match=\"none\">\n                <text term=\"in\" suffix=\" \"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n        <text variable=\"collection-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </if>\n      <else>\n        <text variable=\"collection-title\" form=\"short\" text-case=\"title\"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <!-- Added dictionary and encyclopedia to get page and volume information-->\n      <if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group delimiter=\" \" suffix=\" \">\n          <label variable=\"page\" form=\"long\" text-case=\"capitalize-first\"/>\n          <text variable=\"page\"/>\n          <text term=\"in\"/>\n          <choose>\n            <!-- Added dictionary and encyclopedia to get volume information-->\n            <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n              <choose>\n                <if match=\"any\" variable=\"volume\">\n                  <label suffix=\" \" variable=\"volume\" form=\"short\"/>\n                  <number variable=\"volume\"/>\n                  <text value=\"of\" suffix=\" \"/>\n                </if>\n              </choose>\n            </if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" text-case=\"capitalize-first\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators-note\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group delimiter=\", \">\n          <!--Repositioned Edition to proceed volume-->\n          <text macro=\"edition-note\"/>\n          <choose>\n            <if variable=\"volume\">\n              <!--If volume-->\n              <choose>\n                <if type=\"book\" variable=\"collection-title\" match=\"all\">\n                  <!--SBL 6.2.21-->\n                  <group>\n                    <text term=\"volume\" form=\"short\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <text value=\" of\"/>\n                  </group>\n                </if>\n                <else-if type=\"chapter\" variable=\"collection-title\" match=\"all\">\n                  <!--SBL 6.2.23-->\n                  <group>\n                    <text term=\"volume\" form=\"short\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <text value=\" of\"/>\n                  </group>\n                </else-if>\n                <else-if variable=\"locator page\" match=\"none\">\n                  <group>\n                    <text term=\"volume\" form=\"short\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                  </group>\n                </else-if>\n              </choose>\n            </if>\n            <else>\n              <group>\n                <number variable=\"number-of-volumes\" form=\"numeric\"/>\n                <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\">\n        <group prefix=\" \" delimiter=\".\">\n          <text variable=\"volume\"/>\n          <number variable=\"issue\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <group prefix=\". \" delimiter=\". \">\n          <!--Repositioned Edition to proceed volume-->\n          <text macro=\"edition\"/>\n          <choose>\n            <if variable=\"volume\">\n              <!--If volume-->\n              <choose>\n                <if type=\"book\" variable=\"collection-title\" match=\"all\">\n                  <!--SBL 6.2.21-->\n                  <group>\n                    <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <text value=\" of\"/>\n                  </group>\n                </if>\n                <else-if type=\"chapter\" variable=\"collection-title\" match=\"all\">\n                  <!--SBL 6.2.23-->\n                  <group>\n                    <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                    <text value=\" of\"/>\n                  </group>\n                </else-if>\n                <else-if variable=\"locator page\" match=\"none\">\n                  <group>\n                    <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n                    <number variable=\"volume\" form=\"numeric\"/>\n                  </group>\n                </else-if>\n              </choose>\n            </if>\n            <else>\n              <group>\n                <number variable=\"number-of-volumes\" form=\"numeric\"/>\n                <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </else-if>\n      <!-- Added dictionary and encyclopedia to properly display volume information-->\n      <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <group prefix=\". \">\n          <number variable=\"number-of-volumes\" form=\"numeric\"/>\n          <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locators-newspaper\">\n    <choose>\n      <if type=\"article-newspaper\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"edition\"/>\n            <text term=\"edition\"/>\n          </group>\n          <group>\n            <text term=\"section\" form=\"short\" suffix=\" \"/>\n            <text variable=\"section\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"event\">\n    <group>\n      <text term=\"presented at\" suffix=\" \"/>\n      <text variable=\"event\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <group delimiter=\": \">\n          <text variable=\"publisher-place\"/>\n          <text variable=\"publisher\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"graphic report\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else-if type=\"legal_case\">\n            <text variable=\"authority\" suffix=\" \"/>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else-if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis article-journal\" match=\"any\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"point-locators-subsequent\">\n    <group>\n      <choose>\n        <if locator=\"page\" match=\"none\">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\", \">\n                <group delimiter=\" \">\n                  <text term=\"volume\" form=\"short\"/>\n                  <number variable=\"volume\" form=\"numeric\"/>\n                </group>\n                <!-- When section-based citations are used, SBL prefers no space following the section symbol(s). E.g., https://sblhs2.com/2017/04/20/citing-reference-works-7-greek-language-tools/ -->\n                <choose>\n                  <if locator=\"section\">\n                    <group>\n                      <label variable=\"locator\" form=\"short\"/>\n                      <text variable=\"locator\"/>\n                    </group>\n                  </if>\n                  <else>\n                    <group delimiter=\" \">\n                      <label variable=\"locator\" form=\"short\"/>\n                      <text variable=\"locator\"/>\n                    </group>\n                  </else>\n                </choose>\n              </group>\n            </if>\n          </choose>\n        </if>\n        <else-if variable=\"volume\">\n          <choose>\n            <if type=\"book\" variable=\"collection-title\" match=\"all\">\n              <!--Filter SBL 6.2.21-->\n              <text variable=\"locator\"/>\n            </if>\n            <else-if type=\"chapter\" variable=\"container-title\" match=\"all\">\n              <!--Filter SBL 6.2.23-->\n              <text variable=\"locator\"/>\n            </else-if>\n            <else-if type=\"chapter\" variable=\"collection-title\" position=\"subsequent\" match=\"all\">\n              <!--Filter SBL 6.2.22-->\n              <text variable=\"locator\"/>\n            </else-if>\n            <else-if type=\"article-journal\">\n              <!--Filter SBL 6.3.1-->\n              <text variable=\"locator\"/>\n            </else-if>\n            <else>\n              <group delimiter=\":\">\n                <number variable=\"volume\" form=\"numeric\"/>\n                <text variable=\"locator\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else>\n          <text variable=\"locator\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"point-locators\">\n    <choose>\n      <if variable=\"locator\" match=\"none\">\n        <text macro=\"pages\"/>\n      </if>\n      <else-if type=\"article-journal\">\n        <text variable=\"locator\" prefix=\": \"/>\n      </else-if>\n      <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if position=\"first\" variable=\"volume\" match=\"all\">\n            <text macro=\"point-locators-subsequent\" prefix=\" \"/>\n            <!--SBL 6.3.6 prefers space in first entry before vol info.-->\n          </if>\n          <else>\n            <text macro=\"point-locators-subsequent\" prefix=\", \"/>\n            <!--SBL 6.3.6 prefers comma in subsequent entries before vol info.-->\n          </else>\n        </choose>\n      </else-if>\n      <else>\n        <text macro=\"point-locators-subsequent\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"page\" prefix=\": \"/>\n      </if>\n      <else-if type=\"chapter paper-conference entry-dictionary entry-encyclopedia\" match=\"any\">\n        <choose>\n          <if variable=\"volume\">\n            <choose>\n              <if type=\"chapter\" variable=\"container-title\" match=\"all\">\n                <!--Filter SBL 6.2.23-->\n                <text variable=\"page\" prefix=\", \"/>\n              </if>\n              <else-if type=\"chapter\" variable=\"collection-title\" position=\"subsequent\" match=\"all\">\n                <!--Filter SBL 6.2.22-->\n                <text variable=\"page\" prefix=\", \"/>\n              </else-if>\n              <else>\n                <number variable=\"volume\" prefix=\" \" suffix=\":\"/>\n                <text variable=\"page\"/>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <text variable=\"page\" prefix=\", \"/>\n          </else>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"locators-journal\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"page\" prefix=\": \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"archive-note\">\n    <group delimiter=\", \">\n      <text variable=\"archive_location\"/>\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"archive\">\n    <group delimiter=\". \">\n      <text variable=\"archive_location\" text-case=\"capitalize-first\"/>\n      <text variable=\"archive\"/>\n      <text variable=\"archive-place\"/>\n    </group>\n  </macro>\n  <macro name=\"issue-note\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"volume\" prefix=\" \"/>\n        <text variable=\"issue\" prefix=\".\"/>\n        <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n      </if>\n      <else-if variable=\"publisher-place publisher\" match=\"any\">\n        <group prefix=\" (\" suffix=\")\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <choose>\n                <if variable=\"title\" match=\"none\"/>\n                <else-if type=\"thesis speech\" match=\"any\">\n                  <text variable=\"genre\"/>\n                </else-if>\n              </choose>\n              <text macro=\"event\"/>\n            </group>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"issued\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issue\">\n    <choose>\n      <if type=\"article-journal\">\n        <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n      </if>\n      <else-if type=\"speech\">\n        <choose>\n          <if variable=\"title\" match=\"none\"/>\n          <else>\n            <text variable=\"genre\" text-case=\"capitalize-first\" prefix=\". \"/>\n          </else>\n        </choose>\n        <text macro=\"event\" prefix=\" \"/>\n        <text variable=\"event-place\" prefix=\". \"/>\n        <text macro=\"issued\" prefix=\", \"/>\n      </else-if>\n      <else-if variable=\"publisher-place publisher\" match=\"any\">\n        <group prefix=\". \" delimiter=\", \">\n          <choose>\n            <if type=\"thesis\">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n          <text macro=\"publisher\"/>\n          <text macro=\"issued\"/>\n        </group>\n      </else-if>\n      <else>\n        <text macro=\"issued\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access-note\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"graphic report\" match=\"any\">\n          <text macro=\"archive-note\"/>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"none\">\n          <text macro=\"archive-note\"/>\n        </else-if>\n      </choose>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\". \">\n      <choose>\n        <if type=\"graphic report\" match=\"any\">\n          <text macro=\"archive\"/>\n        </if>\n        <else-if type=\"article-journal article-magazine article-newspaper bill book chapter graphic legal_case legislation motion_picture paper-conference report song thesis\" match=\"none\">\n          <text macro=\"archive\"/>\n        </else-if>\n      </choose>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <macro name=\"unsigned-dictionary-note\">\n    <group delimiter=\", \">\n      <group>\n        <text variable=\"container-title\" font-style=\"italic\" text-case=\"uppercase\"/>\n        <number variable=\"volume\" prefix=\" \"/>\n        <text variable=\"collection-title\" text-case=\"uppercase\"/>\n        <text variable=\"collection-number\" prefix=\" \"/>\n      </group>\n      <group delimiter=\" \">\n        <text term=\"sub-verbo\" form=\"short\"/>\n        <text variable=\"title\" text-case=\"title\" quotes=\"true\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"signed-dictionary-note\">\n    <choose>\n      <if position=\"subsequent\">\n        <group delimiter=\", \">\n          <text macro=\"contributors-short\"/>\n          <text macro=\"title-short\"/>\n          <text macro=\"point-locators-subsequent\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\"\">\n          <group delimiter=\", \">\n            <text macro=\"contributors-note\"/>\n            <text macro=\"title-note\"/>\n            <text macro=\"container-title-note\"/>\n          </group>\n          <text macro=\"point-locators\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"signed-dictionary-bib\">\n    <group delimiter=\"\">\n      <group delimiter=\". \">\n        <text macro=\"contributors\"/>\n        <text macro=\"title-note\"/>\n        <text macro=\"container-title-note\"/>\n      </group>\n      <text macro=\"point-locators\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <!-- Support custom citations via the annote variable. For discussion, see https://forums.zotero.org/discussion/comment/317370 -->\n        <if variable=\"annote\">\n          <choose>\n            <if locator=\"section\">\n              <group delimiter=\" \">\n                <text variable=\"annote\"/>\n                <text macro=\"point-locators-subsequent\"/>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\", \">\n                <text variable=\"annote\"/>\n                <text macro=\"point-locators-subsequent\"/>\n              </group>\n            </else>\n          </choose>\n        </if>\n        <!-- Lexicon/Dictionary/Encyclopedia -->\n        <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n          <choose>\n            <if match=\"none\" variable=\"author\">\n              <!-- Unsigned Lexicon/Dictionary/Encyclopedia -->\n              <text macro=\"unsigned-dictionary-note\"/>\n            </if>\n            <else>\n              <!-- Signed Lexicon/Dictionary/Encyclopedia -->\n              <text macro=\"signed-dictionary-note\"/>\n            </else>\n          </choose>\n        </else-if>\n        <!-- Not Lexicon/Dictionary/Encyclopedia -->\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"contributors-short\"/>\n            <text macro=\"title-short\"/>\n            <text macro=\"disambiguate-chapter\"/>\n            <text macro=\"point-locators-subsequent\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"contributors-note\"/>\n            <text macro=\"title-note\"/>\n            <text macro=\"description-note\"/>\n            <text macro=\"secondary-contributors-note\"/>\n            <text macro=\"container-title-note\"/>\n            <text macro=\"container-contributors-note\"/>\n            <choose>\n              <if variable=\"volume collection-title\" match=\"all\">\n                <!--If Volume # and collection-title -->\n                <group delimiter=\" \">\n                  <!--Added group to prevent comma between vol # of...and collection-title-->\n                  <text macro=\"locators-note\"/>\n                  <text macro=\"collection-title-note\"/>\n                </group>\n              </if>\n              <else>\n                <text macro=\"locators-note\"/>\n                <text macro=\"collection-title-note\"/>\n              </else>\n            </choose>\n            <text macro=\"collection-editor-note\"/>\n          </group>\n          <text macro=\"issue-note\"/>\n          <text macro=\"locators-newspaper\" prefix=\", \"/>\n          <text macro=\"point-locators\"/>\n          <text variable=\"DOI\" prefix=\", https://doi.org/\"/>\n          <text macro=\"access-note\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"10\" et-al-use-first=\"9\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"contributors-sort\"/>\n      <key variable=\"title\"/>\n      <key variable=\"genre\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <!-- Dictionary -->\n        <if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n          <choose>\n            <if match=\"none\" variable=\"author\">\n              <!-- Unsigned Not Listed -->\n            </if>\n            <else>\n              <!-- Signed -->\n              <text macro=\"signed-dictionary-bib\"/>\n            </else>\n          </choose>\n        </if>\n        <!-- Not Dictionary -->\n        <else>\n          <group delimiter=\". \">\n            <text macro=\"contributors\"/>\n            <text macro=\"title\"/>\n            <text macro=\"description\"/>\n            <text macro=\"secondary-contributors\"/>\n            <text macro=\"container-title\"/>\n            <text macro=\"container-contributors\"/>\n          </group>\n          <choose>\n            <if variable=\"volume collection-title\" match=\"all\">\n              <!--If Volume # and collection-title -->\n              <text macro=\"locators\"/>\n              <text macro=\"collection-title\" text-case=\"title\" prefix=\" \"/>\n            </if>\n            <else>\n              <text macro=\"locators\"/>\n              <text macro=\"collection-title\" text-case=\"title\" prefix=\". \"/>\n            </else>\n          </choose>\n          <text macro=\"collection-editor\"/>\n          <text macro=\"issue\"/>\n          <text macro=\"locators-newspaper\" prefix=\", \"/>\n          <text macro=\"locators-journal\"/>\n          <text variable=\"DOI\" prefix=\". https://doi.org/\"/>\n          <text macro=\"access\" prefix=\". \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"