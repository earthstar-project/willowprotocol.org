"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Hypertension Research</title>\n    <id>http://www.zotero.org/styles/hypertension-research</id>\n    <link href=\"http://www.zotero.org/styles/hypertension-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-hypertension\" rel=\"template\"/>\n    <link href=\"http://mc.manuscriptcentral.com/societyimages/htr/HR_GTA.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <category field=\"medicine\"/>\n    <issn>0916-9636</issn>\n    <eissn>1348-4214</eissn>\n    <updated>2012-09-09T21:58:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\" suffix=\": \"/>\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"none\">\n        <choose>\n          <if variable=\"page\" match=\"none\">\n            <choose>\n              <if variable=\"DOI\">\n                <text variable=\"DOI\" prefix=\" doi:\"/>\n              </if>\n              <else-if variable=\"URL\">\n                <text variable=\"URL\"/>\n                <group prefix=\". \" delimiter=\" \">\n                  <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                  <date variable=\"accessed\" form=\"text\"/>\n                </group>\n              </else-if>\n            </choose>\n          </if>\n        </choose>\n      </if>\n      <else-if variable=\"page volume\" match=\"any\">\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\" doi:\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <text variable=\"URL\"/>\n            <group prefix=\". \" delimiter=\" \">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date variable=\"accessed\" form=\"text\"/>\n            </group>\n          </else-if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal\">\n        <choose>\n          <if variable=\"volume page\" match=\"none\">\n            <choose>\n              <if variable=\"DOI URL\" match=\"any\">\n                <group prefix=\"(e-pub ahead of print \" suffix=\")\" delimiter=\"; \">\n                  <date variable=\"issued\" form=\"text\"/>\n                  <text variable=\"DOI\" prefix=\"doi:\"/>\n                </group>\n              </if>\n              <else>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"100\" et-al-use-first=\"99\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\" \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\".\" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <group delimiter=\", \">\n              <text macro=\"editor\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"edition\"/>\n            </group>\n          </group>\n          <group delimiter=\", \">\n            <text macro=\"publisher\" prefix=\" \"/>\n            <text macro=\"date\"/>\n            <text variable=\"page\" suffix=\".\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group suffix=\".\">\n            <text macro=\"date\" prefix=\" \"/>\n            <group prefix=\"; \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"page\" prefix=\": \"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"