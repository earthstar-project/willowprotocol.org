\" font-variant=\"small-caps\" delimiter=\" / \" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date-and-edition\">\n    <group>\n      <date form=\"text\" variable=\"original-date\" suffix=\" \">\n        <date-part name=\"year\"/>\n      </date>\n      <number vertical-align=\"sup\" variable=\"edition\"/>\n      <text macro=\"issued\"/>\n    </group>\n  </macro>\n  <macro name=\"collection-info\">\n    <group delimiter=\" \">\n      <text variable=\"collection-title\" form=\"short\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <layout delimiter=\"; \">\n      <choose>\n        <if match=\"any\" position=\"first\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <choose>\n              <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n                <group delimiter=\" \">\n                  <text variable=\"title\"/>\n                  <group prefix=\"(\" suffix=\")\" delimiter=\", \">\n                    <text variable=\"container-title\"/>\n                    <text variable=\"volume\"/>\n                    <text macro=\"issued\"/>\n                    <text variable=\"page\"/>\n                  </group>\n                </group>\n              </if>\n              <else-if type=\"chapter paper-conference\" match=\"any\">\n                <group delimiter=\" \">\n                  <text variable=\"title\"/>\n                  <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                    <group>\n                      <date form=\"text\" variable=\"original-date\" suffix=\"; \">\n                        <date-part name=\"year\"/>\n                      </date>\n                      <text term=\"in\" suffix=\": \"/>\n                      <group delimiter=\", \">\n                        <names variable=\"container-author\" font-variant=\"normal\" delimiter=\", \">\n                          <name font-variant=\"small-caps\" delimiter=\" / \" delimiter-precedes-last=\"always\" initialize-with=\".\"/>\n                          <label form=\"short\" text-case=\"capitalize-first\" prefix=\" [\" suffix=\"]\"/>\n                        </names>\n                        <group delimiter=\" \">\n                          <group delimiter=\", \">\n                            <text variable=\"container-title\" text-case=\"title\"/>\n                            <names variable=\"editor\">\n                              <label form=\"verb-short\" suffix=\" \"/>\n                              <name font-variant=\"small-caps\" delimiter=\" / \" initialize-with=\".\"/>\n                            </names>\n                          </group>\n                          <text macro=\"collection-info\" prefix=\"[\" suffix=\"]\"/>\n                        </group>\n                      </group>\n                    </group>\n                    <group>\n                      <number vertical-align=\"sup\" variable=\"edition\"/>\n                      <text macro=\"issued\"/>\n                    </group>\n                  </group>\n                </group>\n              </else-if>\n              <else-if type=\"entry-encyclopedia\" match=\"any\">\n                <text variable=\"title\" form=\"short\" prefix=\"Art. \"/>\n                <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                  <group delimiter=\" \">\n                    <group>\n                      <text variable=\"container-title\" form=\"short\"/>\n                      <number vertical-align=\"sup\" variable=\"edition\"/>\n                    </group>\n                    <text variable=\"volume\"/>\n                  </group>\n                  <date date-parts=\"year\" form=\"text\" variable=\"issued\">\n                    <date-part name=\"year\"/>\n                  </date>\n                  <text variable=\"page\"/>\n                </group>\n              </else-if>\n              <else>\n                <text variable=\"title\"/>\n                <text macro=\"collection-info\" prefix=\"(\" suffix=\")\"/>\n                <names variable=\"editor translator\" delimiter=\", \">\n                  <label form=\"verb-short\" suffix=\" \"/>\n                  <name font-variant=\"small-caps\" delimiter=\" / \" initialize-with=\".\"/>\n                </names>\n                <text macro=\"date-and-edition\"/>\n              </else>\n            </choose>\n            <text variable=\"locator\"/>\n          </group>\n        </if>\n        <else-if match=\"any\" position=\"ibid-with-locator\">\n          <group delimiter=\" \">\n            <text value=\"AaO\"/>\n            <text variable=\"locator\"/>\n          </group>\n        </else-if>\n        <else-if match=\"any\" position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text macro=\"author-short\"/>\n              <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                <text value=\"s. Anm.\"/>\n                <text variable=\"first-reference-note-number\"/>\n              </group>\n            </group>\n            <text variable=\"locator\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"1\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group delimiter=\", \">\n      