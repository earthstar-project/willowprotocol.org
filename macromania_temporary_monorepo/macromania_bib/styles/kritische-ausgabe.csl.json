"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"de-DE\">\n  <info>\n    <title>Kritische Ausgabe (Deutsch)</title>\n    <id>http://www.zotero.org/styles/kritische-ausgabe</id>\n    <link href=\"http://www.zotero.org/styles/kritische-ausgabe\" rel=\"self\"/>\n    <link href=\"http://kritische-ausgabe.de/\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"humanities\"/>\n    <issn>1617-1357</issn>\n    <summary>Format for the literary journal \"Kritische Ausgabe</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">Hrsg. v.</term>\n      <term name=\"translator\" form=\"verb-short\">Übers. v.</term>\n      <term name=\"editortranslator\" form=\"verb-short\">Hrsg. &amp; übers. von</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter=\"/\" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if variable=\"container-title\">\n        <choose>\n          <if type=\"article-journal article-magazine\" match=\"any\">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <text variable=\"container-title\"/>\n          </if>\n          <else>\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <text variable=\"container-title\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\" delimiter=\". \">\n      <label form=\"verb-short\"/>\n      <name delimiter=\"/\" delimiter-precedes-last=\"always\" prefix=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\" text-case=\"capitalize-first\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\" text-case=\"capitalize-first\" suffix=\".\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"volume-edition\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"volume\" prefix=\" \"/>\n        <text variable=\"issue\" prefix=\"/\"/>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"volume\">\n            <group delimiter=\" \">\n              <number variable=\"volume\" form=\"long-ordinal\" text-case=\"capitalize-first\"/>\n              <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n            </group>\n          </if>\n          <else>\n            <group>\n              <number variable=\"number-of-volumes\" form=\"numeric\"/>\n              <text term=\"volume\" form=\"short\" prefix=\" \" plural=\"true\"/>\n            </group>\n          </else>\n        </choose>\n        <text macro=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher-place\"/>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-journal\">\n        <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"point-locators-subsequent\">\n    <label variable=\"locator\" form=\"short\"/>\n    <text variable=\"locator\" prefix=\" \"/>\n  </macro>\n  <macro name=\"point-locators\">\n    <choose>\n      <if variable=\"page\">\n        <label variable=\"locator\" form=\"short\" prefix=\"hier: \"/>\n        <text variable=\"locator\" prefix=\" \"/>\n      </if>\n      <else>\n        <label variable=\"locator\" form=\"short\"/>\n        <text variable=\"locator\" prefix=\" \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" text-case=\"capitalize-first\" form=\"short\"/>\n    <text variable=\"page\" prefix=\" \"/>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\". \">\n          <text variable=\"URL\" prefix=\"Internet: \"/>\n          <date variable=\"accessed\" prefix=\"Zuletzt geprüft am: \" delimiter=\".\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\" form=\"numeric\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"3\" disambiguate-add-names=\"true\">\n    <layout delimiter=\"; \" suffix=\".\">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\"/>\n            <text macro=\"point-locators-subsequent\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <text macro=\"author\"/>\n          <text variable=\"title\" form=\"short\" prefix=\": \"/>\n          <text variable=\"first-reference-note-number\" prefix=\" [wie Anm. \" suffix=\"]\"/>\n          <text macro=\"point-locators-subsequent\" prefix=\", \"/>\n        </else-if>\n        <else>\n          <group suffix=\". \" delimiter=\": \">\n            <text macro=\"author\"/>\n            <text variable=\"title\"/>\n          </group>\n          <group delimiter=\". \">\n            <group delimiter=\" \">\n              <text macro=\"container-title\"/>\n              <text macro=\"editor\" suffix=\".\"/>\n              <text macro=\"volume-edition\"/>\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n            <group delimiter=\". \">\n              <text macro=\"access\"/>\n              <group delimiter=\"; \">\n                <text macro=\"pages\"/>\n                <text macro=\"point-locators\"/>\n              </group>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"11\" et-al-use-first=\"7\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group suffix=\". \" delimiter=\": \">\n        <text macro=\"author\"/>\n        <text variable=\"title\"/>\n      </group>\n      <group delimiter=\". \">\n        <group delimiter=\" \">\n          <text macro=\"container-title\"/>\n          <text macro=\"editor\" suffix=\".\"/>\n          <text macro=\"volume-edition\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <group delimiter=\". \">\n          <text macro=\"access\"/>\n          <text macro=\"pages\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"