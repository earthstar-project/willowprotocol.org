"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Health and Human Rights Journal</title>\n    <title-short>HHR</title-short>\n    <id>http://www.zotero.org/styles/health-and-human-rights-journal</id>\n    <link href=\"http://www.zotero.org/styles/health-and-human-rights-journal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/cultural-geographies\" rel=\"template\"/>\n    <link href=\"https://www.hhrjournal.org/submissions/author-guidelines/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"humanities\"/>\n    <issn>1079-0969</issn>\n    <eissn>2150-4113</eissn>\n    <updated>2020-12-07T09:46:20+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \">\n      <text term=\"available at\" text-case=\"capitalize-first\"/>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\" (\" suffix=\")\">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\" form=\"long\"/>\n      </group>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"citation-locator\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" quotes=\"false\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation>\n    <layout delimiter=\",\">\n      <group>\n        <choose>\n          <if match=\"any\" position=\"ibid ibid-with-locator\">\n            <group delimiter=\", \">\n              <text term=\"ibid\" text-case=\"capitalize-first\"/>\n              <text macro=\"citation-locator\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\", \" suffix=\".\">\n              <text macro=\"author\"/>\n              <text macro=\"title\"/>\n              <choose>\n                <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n                  <group>\n                    <group delimiter=\", \">\n                      <text macro=\"edition\"/>\n                    </group>\n                    <text macro=\"publisher\"/>\n                  </group>\n                </if>\n                <else-if type=\"chapter paper-conference\" match=\"any\">\n                  <group delimiter=\", \" prefix=\" \">\n                    <group delimiter=\" \">\n                      <text term=\"in\" suffix=\" \"/>\n                      <text macro=\"editor\"/>\n                    </group>\n                    <text macro=\"edition\"/>\n                    <group delimiter=\" \">\n                      <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n                      <text macro=\"publisher\"/>\n                    </group>\n                    <group delimiter=\" \">\n                      <label variable=\"page\" form=\"short\"/>\n                      <text variable=\"page\"/>\n                    </group>\n                  </group>\n                </else-if>\n                <else-if type=\"webpage post post-weblog\" match=\"any\">\n                  <group delimiter=\", \">\n                    <text variable=\"container-title\" font-style=\"italic\"/>\n                    <text macro=\"publisher\"/>\n                    <text macro=\"access\"/>\n                  </group>\n                </else-if>\n                <else-if type=\"patent\">\n                  <group delimiter=\", \">\n                    <text variable=\"number\"/>\n                    <text macro=\"year-date\"/>\n                  </group>\n                </else-if>\n                <else-if type=\"thesis\">\n                  <group delimiter=\", \">\n                    <group delimiter=\" \">\n                      <text variable=\"genre\"/>\n                      <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n                    </group>\n                  </group>\n                </else-if>\n                <else-if type=\"article article-journal\" match=\"any\">\n                  <group delimiter=\" \">\n                    <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n                    <group delimiter=\" \" suffix=\",\">\n                      <group delimiter=\"/\">\n                        <text variable=\"volume\" font-weight=\"normal\"/>\n                        <text variable=\"issue\"/>\n                      </group>\n                      <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n                    </group>\n                    <text macro=\"page\"/>\n                  </group>\n                </else-if>\n                <else-if type=\"article-newspaper\" match=\"any\">\n                  <group delimiter=\". \">\n                    <group delimiter=\" \">\n                      <text variable=\"container-title\" font-style=\"italic\"/>\n                      <date form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n                    </group>\n                    <text macro=\"access\"/>\n                  </group>\n                </else-if>\n                <else>\n                  <group delimiter=\", \">\n                    <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n                    <text variable=\"volume\" font-weight=\"normal\"/>\n                    <text macro=\"year-date\"/>\n                    <text variable=\"page\" form=\"short\"/>\n                  </group>\n                </else>\n              </choose>\n              <text macro=\"citation-locator\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </citation>\n</style>\n"