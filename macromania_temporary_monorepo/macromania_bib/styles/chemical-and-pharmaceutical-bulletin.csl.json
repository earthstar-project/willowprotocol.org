"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Chemical and Pharmaceutical Bulletin</title>\n    <title-short>Chem. Pharm. Bull.</title-short>\n    <id>http://www.zotero.org/styles/chemical-and-pharmaceutical-bulletin</id>\n    <link href=\"http://www.zotero.org/styles/chemical-and-pharmaceutical-bulletin\" rel=\"self\"/>\n    <link href=\"http://cpb.pharm.or.jp/document/inst_e.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Shoji Takahashi</name>\n      <email>s.takahashi@elsevier.com</email>\n      <uri>http://www.mendeley.com/profiles/shoji-takahashi3/</uri>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <issn>0009-2363</issn>\n    <eissn>1347-5223</eissn>\n    <updated>2015-12-12T10:21:20+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" name-as-sort-order=\"all\" sort-separator=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"chapter\" match=\"any\">\n        <text variable=\"container-title\" quotes=\"true\" font-style=\"normal\"/>\n      </else-if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <text term=\"in\" suffix=\" \"/>\n        <text variable=\"container-title\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" font-style=\"normal\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <number suffix=\" \" variable=\"edition\" form=\"ordinal\"/>\n    <label plural=\"never\" variable=\"edition\" form=\"short\"/>\n  </macro>\n  <macro name=\"event\">\n    <group delimiter=\", \">\n      <text variable=\"event-place\"/>\n      <group>\n        <text value=\"on\" suffix=\" \"/>\n        <date variable=\"event-date\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book webpage\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\" font-style=\"normal\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-journal patent\" match=\"any\">\n        <date date-parts=\"year\" form=\"numeric\" variable=\"issued\" font-weight=\"normal\" prefix=\"(\" suffix=\")\">\n          <date-part name=\"year\" font-weight=\"normal\"/>\n        </date>\n      </if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else-if type=\"webpage\" match=\"any\">\n        <date variable=\"accessed\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\", \"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else>\n        <date date-parts=\"year\" form=\"numeric\" variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"verb-short\" plural=\"never\" suffix=\" \"/>\n      <name initialize=\"false\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n    </names>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"volume\" font-style=\"normal\" font-weight=\"bold\"/>\n      </if>\n      <else-if type=\"book chapter\" match=\"any\">\n        <group delimiter=\", \">\n          <group>\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" font-style=\"normal\" suffix=\" \"/>\n            <text variable=\"volume\"/>\n          </group>\n          <group>\n            <text term=\"chapter\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <text variable=\"chapter-number\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <label variable=\"page\" form=\"short\"/>\n        <text variable=\"page\"/>\n      </if>\n      <else>\n        <text variable=\"page\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\" suffix=\")\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\")\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"article-journal\">\n          <group delimiter=\", \">\n            <text macro=\"container-title\"/>\n            <text macro=\"locators\"/>\n            <group delimiter=\" \">\n              <text macro=\"page\"/>\n              <text macro=\"issued\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"book\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"container-title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"locators\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"paper-conference\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\"/>\n            <text macro=\"container-title\"/>\n            <text macro=\"event\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\" \">\n            <text variable=\"number\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"genre\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\" match=\"any\">\n          <group delimiter=\".\">\n            <text macro=\"title\"/>\n            <group delimiter=\", \">\n              <text variable=\"URL\" prefix=\": &lt;\" suffix=\"&gt;\"/>\n              <group delimiter=\" \">\n                <text term=\"cited\" suffix=\" \"/>\n                <text macro=\"issued\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"