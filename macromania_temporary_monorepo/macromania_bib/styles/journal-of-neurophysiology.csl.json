"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Neurophysiology</title>\n    <id>http://www.zotero.org/styles/journal-of-neurophysiology</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-neurophysiology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/american-physics-society\" rel=\"template\"/>\n    <link href=\"https://www.physiology.org/author-info.manuscript-composition\" rel=\"documentation\"/>\n    <author>\n      <name>Michael Pesavento</name>\n      <email>mpesaven@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien, Ph</name>\n      <email>obrienpat86@gmail.com</email>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"medicine\"/>\n    <issn>0022-3077</issn>\n    <eissn>1522-1598</eissn>\n    <summary>The Journal of Neurophysiology style</summary>\n    <updated>2018-12-28T10:52:18+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"no date\" form=\"long\">date unknown</term>\n    </terms>\n  </locale>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author-bib\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" font-weight=\"bold\" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \" suffix=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-cit\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\" \" initialize-with=\"\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" prefix=\"edited by \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <text variable=\"publisher-place\" suffix=\": \"/>\n        <text variable=\"publisher\" suffix=\", \"/>\n        <text macro=\"date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <choose>\n          <if type=\"article-journal\" match=\"any\">\n            <choose>\n              <if match=\"none\" variable=\"volume page\">\n                <text variable=\"DOI\" prefix=\". doi:\"/>\n              </if>\n            </choose>\n          </if>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"URL\">\n            <group delimiter=\" \">\n              <text variable=\"URL\"/>\n              <date variable=\"accessed\" prefix=\"[\" suffix=\"]\">\n                <date-part name=\"day\"/>\n                <date-part name=\"month\" prefix=\" \" suffix=\" \" form=\"short\"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"title\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <choose>\n                <if variable=\"URL\">\n                  <text variable=\"title\"/>\n                </if>\n                <else>\n                  <text variable=\"title\" font-style=\"italic\"/>\n                </else>\n              </choose>\n            </if>\n            <else>\n              <text variable=\"title\"/>\n            </else>\n          </choose>\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"online\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author-bib\"/>\n      <key macro=\"date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-cit\"/>\n        <text macro=\"date\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author-bib\"/>\n      <key macro=\"date\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author-bib\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \" suffix=\".\">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <group>\n              <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"editor\" prefix=\", \"/>\n            </group>\n            <text variable=\"event\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text variable=\"page\" prefix=\", p. \" suffix=\".\"/>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" suffix=\" \" form=\"short\" font-style=\"italic\" strip-periods=\"true\"/>\n              <group delimiter=\": \">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n            <text macro=\"date\"/>\n          </group>\n        </else-if>\n        <else>\n          <text variable=\"container-title\" suffix=\" \" form=\"short\" font-style=\"italic\"/>\n          <text variable=\"volume\"/>\n          <text variable=\"publisher\"/>\n          <text variable=\"page\" prefix=\": \" suffix=\", \"/>\n          <text macro=\"date\" suffix=\".\"/>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"