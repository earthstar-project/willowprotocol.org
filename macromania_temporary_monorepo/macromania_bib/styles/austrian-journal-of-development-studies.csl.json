"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" name-delimiter=\"/\" demote-non-dropping-particle=\"never\">\n  <!-- Polyglot; journal publishes in English and German -->\n  <info>\n    <title>Austrian Journal of Development Studies (Journal für Entwicklungspolitik)</title>\n    <title-short>JEP</title-short>\n    <id>http://www.zotero.org/styles/austrian-journal-of-development-studies</id>\n    <link href=\"http://www.zotero.org/styles/austrian-journal-of-development-studies\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/zdfm-zeitschrift-fur-diversitatsforschung-und-management\" rel=\"template\"/>\n    <link href=\"https://www.mattersburgerkreis.at/site/en/articlesubmission\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <uri>citationstyler@gmail.com</uri>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <issn>0258-2384</issn>\n    <eissn>2414-3197</eissn>\n    <updated>2019-07-05T11:30:09+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"de\">\n    <terms>\n      <term name=\"accessed\">Zugriff</term>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"presented at\">Paper präsentiert bei der</term>\n      <term name=\"anonymous\" form=\"long\">Ohne Verfasser</term>\n      <term name=\"anonymous\" form=\"short\">O.V.</term>\n      <term name=\"editor\" form=\"short\">Hg.</term>\n    </terms>\n  </locale>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n        <names variable=\"editor translator\" delimiter=\", \" suffix=\": \">\n          <name delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n          <label form=\"short\" text-case=\"capitalize-first\" prefix=\" (\" suffix=\")\"/>\n        </names>\n        <group delimiter=\", \" suffix=\". \">\n          <text variable=\"container-title\" text-case=\"title\"/>\n          <text variable=\"collection-title\" text-case=\"title\"/>\n        </group>\n      </if>\n      <else-if type=\"book graphic legal_case motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"container-title\"/>\n          <text variable=\"collection-title\"/>\n        </group>\n      </else-if>\n      <else-if type=\"article-journal article-magazine article-newspaper article\" match=\"any\">\n        <group delimiter=\" \">\n          <group delimiter=\": \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text variable=\"collection-title\"/>\n            </group>\n          </group>\n          <group delimiter=\", \">\n            <group>\n              <text variable=\"volume\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <group delimiter=\" \">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\"/>\n              <text variable=\"volume\"/>\n              <group delimiter=\"/\">\n                <text variable=\"number\"/>\n                <date date-parts=\"year\" form=\"numeric\" variable=\"issued\"/>\n              </group>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n          <group delimiter=\" \">\n            <text value=\"in der Fassung\"/>\n            <text variable=\"references\"/>\n          </group>\n        </group>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"container-title\"/>\n          <text variable=\"collection-title\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize=\"false\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <group>\n          <text variable=\"title-short\"/>\n        </group>\n        <group>\n          <text term=\"anonymous\" form=\"short\"/>\n        </group>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <group>\n          <text term=\"anonymous\" form=\"short\"/>\n        </group>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"URL\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n            <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\".\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <text variable=\"title\"/>\n        <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n          <text variable=\"genre\"/>\n          <text variable=\"number\" prefix=\"Nr. \"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song speech\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\"/>\n          <text variable=\"medium\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <text variable=\"event\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </if>\n        <else>\n          <text variable=\"edition\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"pages\"/>\n        </group>\n      </else-if>\n      <else-if type=\"patent\">\n        <text variable=\"number\"/>\n      </else-if>\n      <else-if type=\"thesis\" match=\"any\">\n        <group delimiter=\". \">\n          <text macro=\"publisher\"/>\n          <text variable=\"genre\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"author-year-title\">\n    <group delimiter=\": \">\n      <group>\n        <choose>\n          <if type=\"bill\" match=\"any\">\n            <choose>\n              <if match=\"none\" variable=\"author editor\">\n                <text variable=\"title-short\"/>\n              </if>\n              <else>\n                <text macro=\"author\"/>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <text macro=\"author\"/>\n          </else>\n        </choose>\n        <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n      </group>\n      <group>\n        <text macro=\"title\"/>\n      </group>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"descending\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <choose>\n        <if match=\"any\" position=\"ibid-with-locator\">\n          <group delimiter=\": \">\n            <text term=\"ibid\" form=\"short\"/>\n            <text variable=\"locator\"/>\n          </group>\n        </if>\n        <else-if match=\"any\" position=\"ibid\">\n          <text term=\"ibid\" form=\"short\"/>\n        </else-if>\n        <else>\n          <group delimiter=\": \">\n            <group delimiter=\" \">\n              <choose>\n                <if type=\"legislation\" match=\"any\">\n                  <text variable=\"title-short\"/>\n                </if>\n                <else>\n                  <text macro=\"author-short\"/>\n                </else>\n              </choose>\n              <text macro=\"issued\"/>\n            </group>\n            <text variable=\"locator\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography initialize=\"false\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"descending\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \">\n        <text macro=\"author-year-title\"/>\n        <group suffix=\".\">\n          <text macro=\"container\"/>\n          <text macro=\"locators\"/>\n        </group>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"