"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>The Journal of the Acoustical Society of America (numeric)</title>\n    <title-short>JASA</title-short>\n    <id>http://www.zotero.org/styles/the-journal-of-the-acoustical-society-of-america-numeric</id>\n    <link href=\"http://www.zotero.org/styles/the-journal-of-the-acoustical-society-of-america-numeric\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/the-journal-of-the-acoustical-society-of-america\" rel=\"template\"/>\n    <link href=\"https://pubs.aip.org/DocumentLibrary/files/publications/jasa/jasinfcon.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"psychology\"/>\n    <category field=\"physics\"/>\n    <category field=\"engineering\"/>\n    <issn>0001-4966</issn>\n    <eissn>1520-8524</eissn>\n    <updated>2021-02-10T08:04:36+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"translator\" form=\"short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"container-contributors\">\n    <names variable=\"editor\" delimiter=\", \">\n      <label form=\"verb\"/>\n      <name prefix=\" \" and=\"text\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"none\">\n        <names variable=\"translator\" delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n          <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n          <substitute>\n            <names variable=\"editor\"/>\n          </substitute>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"thesis\">\n        <choose>\n          <if variable=\"archive\" match=\"any\">\n            <group>\n              <text term=\"retrieved\" text-case=\"capitalize-first\" suffix=\" \"/>\n              <text term=\"from\" suffix=\" \"/>\n              <text variable=\"archive\" suffix=\".\"/>\n              <text variable=\"archive_location\" prefix=\" (\" suffix=\")\"/>\n            </group>\n          </if>\n          <else>\n            <group>\n              <text term=\"retrieved\" text-case=\"capitalize-first\" suffix=\" \"/>\n              <text term=\"from\" suffix=\" \"/>\n              <text variable=\"URL\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\"doi:\"/>\n          </if>\n          <else>\n            <choose>\n              <if type=\"webpage post post-weblog\" match=\"any\">\n                <group>\n                  <text term=\"retrieved\" text-case=\"capitalize-first\" suffix=\" \"/>\n                  <date variable=\"accessed\" suffix=\", \">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                  <group>\n                    <text term=\"from\" suffix=\" \"/>\n                    <text variable=\"URL\"/>\n                  </group>\n                </group>\n              </if>\n              <else>\n                <group>\n                  <text term=\"retrieved\" text-case=\"capitalize-first\" suffix=\" \"/>\n                  <text term=\"from\" suffix=\" \"/>\n                  <text variable=\"URL\"/>\n                </group>\n              </else>\n            </choose>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <group prefix=\" (\" suffix=\"),\">\n          <text variable=\"genre\"/>\n          <text variable=\"number\" prefix=\" No. \"/>\n        </group>\n      </if>\n      <else-if type=\"bill graphic legal_case legislation manuscript motion_picture report thesis song speech\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"patent webpage book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\",\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n      <text macro=\"issued-year\"/>\n    </group>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <choose>\n          <if variable=\"genre\" match=\"none\">\n            <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <text variable=\"event\"/>\n          </if>\n          <else>\n            <group delimiter=\" \">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              <text term=\"presented at\"/>\n              <text variable=\"event\"/>\n            </group>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"year-suffix\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n        <text variable=\"year-suffix\" prefix=\"-\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <choose>\n          <if variable=\"page volume\" match=\"all\">\n            <group delimiter=\" \" prefix=\" \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <group delimiter=\", \">\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text variable=\"page\"/>\n              </group>\n              <text macro=\"issued-year\" prefix=\"(\" suffix=\")\"/>\n            </group>\n          </if>\n          <else-if variable=\"DOI\">\n            <text variable=\"DOI\" prefix=\", doi: \"/>\n          </else-if>\n          <else-if variable=\"URL\">\n            <group>\n              <text term=\"retrieved\" text-case=\"capitalize-first\" suffix=\" \"/>\n              <text term=\"from\" suffix=\" \"/>\n              <text variable=\"URL\"/>\n            </group>\n          </else-if>\n        </choose>\n      </if>\n      <else-if type=\"paper-conference chapter\" match=\"any\">\n        <group delimiter=\" \">\n          <text term=\"in\" suffix=\" \"/>\n          <group delimiter=\", \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"container-contributors\"/>\n          </group>\n          <text macro=\"publisher\"/>\n          <group delimiter=\" \">\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report thesis song\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"publisher\"/>\n          <text macro=\"edition\"/>\n          <group>\n            <text term=\"volume\" form=\"short\" plural=\"true\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"number-of-volumes\" form=\"numeric\" prefix=\"1-\"/>\n          </group>\n          <group>\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\" suffix=\" \"/>\n            <number variable=\"volume\" form=\"numeric\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"webpage post post-weblog\" match=\"any\">\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"URL\">\n              <group delimiter=\", \">\n                <text variable=\"URL\" prefix=\" Available: \"/>\n                <group prefix=\"(\" suffix=\")\">\n                  <date variable=\"accessed\" prefix=\"date last viewed: \">\n                    <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n                    <date-part name=\"month\" form=\"short\" suffix=\"-\" strip-periods=\"true\"/>\n                    <date-part name=\"year\" form=\"short\"/>\n                  </date>\n                </group>\n              </group>\n            </if>\n          </choose>\n        </group>\n      </else-if>\n      <else-if type=\"patent\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher-place\" suffix=\" patent\"/>\n          <text variable=\"number\"/>\n          <text variable=\"note\" prefix=\" (\" suffix=\")\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"8\" et-al-use-first=\"7\" entry-spacing=\"0\" line-spacing=\"2\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout>\n      <group delimiter=\", \" suffix=\".\">\n        <group>\n          <text variable=\"citation-number\" vertical-align=\"sup\"/>\n          <text macro=\"author\"/>\n        </group>\n        <text macro=\"title\"/>\n        <text macro=\"secondary-contributors\"/>\n        <text variable=\"collection-title\" suffix=\",\"/>\n        <text macro=\"locators\" suffix=\".\"/>\n      </group>\n      <text macro=\"event\" prefix=\". \"/>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"