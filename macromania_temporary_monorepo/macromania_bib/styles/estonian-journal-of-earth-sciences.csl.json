"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Estonian Journal of Earth Sciences</title>\n    <title-short>EJES</title-short>\n    <id>http://www.zotero.org/styles/estonian-journal-of-earth-sciences</id>\n    <link href=\"http://www.zotero.org/styles/estonian-journal-of-earth-sciences\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/biostatistics\" rel=\"template\"/>\n    <link href=\"https://kirj.ee/earth-guidelines-for-authors/?v=04c19fa1e772\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geology\"/>\n    <issn>1736-4728</issn>\n    <eissn>1736-7557</eissn>\n    <updated>2022-08-23T09:40:48+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\" font-variant=\"normal\">\n      <name font-variant=\"normal\" and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <choose>\n          <if type=\"book\" match=\"any\">\n            <choose>\n              <if match=\"none\" variable=\"page\">\n                <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n              </if>\n            </choose>\n          </if>\n          <else>\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if variable=\"URL\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <group prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\" suffix=\" \"/>\n            <date variable=\"accessed\">\n              <date-part name=\"year\" suffix=\"-\"/>\n              <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\"-\"/>\n              <date-part name=\"day\" form=\"numeric-leading-zeros\"/>\n            </date>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis report patent\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" year-suffix-delimiter=\",\">\n    <sort>\n      <key variable=\"issued\" sort=\"ascending\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture song thesis\" match=\"any\">\n            <group delimiter=\". \" prefix=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <choose>\n                <if type=\"thesis\" match=\"any\">\n                  <text variable=\"genre\"/>\n                </if>\n              </choose>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \" prefix=\" \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text term=\"in\" suffix=\" \" text-case=\"capitalize-first\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text macro=\"edition\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\". \" prefix=\" \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \" prefix=\" \">\n                <group delimiter=\" \" prefix=\" \">\n                  <text variable=\"collection-title\" font-style=\"italic\"/>\n                  <text variable=\"number\"/>\n                </group>\n                <text variable=\"page\" suffix=\" pp\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"patent\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <text term=\"patent\" text-case=\"capitalize-first\"/>\n                <text variable=\"number\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" form=\"long\" font-style=\"italic\"/>\n                <group>\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"