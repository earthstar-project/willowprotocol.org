"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Liver International</title>\n    <id>http://www.zotero.org/styles/liver-international</id>\n    <link href=\"http://www.zotero.org/styles/liver-international\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/hepatology\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/(ISSN)1478-3231/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>MÃ©lanie Tremblay</name>\n      <email>melanie.tremblay@hepato-neuro.ca</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>1478-3223</issn>\n    <eissn>1478-3231</eissn>\n    <summary>Liver international journal style.</summary>\n    <updated>2015-01-01T17:31:53+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\" suffix=\". \">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\"; \">\n      <group delimiter=\": \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <group suffix=\". \">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"volume\"/>\n      <else-if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </else-if>\n      <else-if variable=\"URL\">\n        <group delimiter=\". \" prefix=\" \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date variable=\"accessed\" form=\"text\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" suffix=\". \" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" suffix=\". \" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" suffix=\". \">\n            <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\"/>\n          </group>\n          <text macro=\"publisher\" prefix=\" \"/>\n          <text variable=\"page\" prefix=\" p. \" suffix=\".\"/>\n        </else-if>\n        <else>\n          <text macro=\"journal-title\"/>\n          <group delimiter=\";\" suffix=\". \">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n            <group delimiter=\":\">\n              <text variable=\"volume\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"