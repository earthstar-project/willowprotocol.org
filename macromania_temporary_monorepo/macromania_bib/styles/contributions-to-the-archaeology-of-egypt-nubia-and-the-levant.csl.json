"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" default-locale=\"en-GB\">\n  <info>\n    <title>Contributions to the Archaeology of Egypt, Nubia and the Levant</title>\n    <title-short>CAENL</title-short>\n    <id>http://www.zotero.org/styles/contributions-to-the-archaeology-of-egypt-nubia-and-the-levant</id>\n    <link href=\"http://www.zotero.org/styles/contributions-to-the-archaeology-of-egypt-nubia-and-the-levant\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/veterinary-medicine-austria\" rel=\"template\"/>\n    <link href=\"https://www.orea.oeaw.ac.at/en/publications/series/contributions-to-the-archaeology-of-egypt-nubia-and-the-levant-caenl/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"history\"/>\n    <updated>2018-07-26T15:51:03+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\".\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\" font-variant=\"normal\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" suffix=\",\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\",\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography>\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" display=\"block\"/>\n      <group>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <group display=\"block\">\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group prefix=\" \" delimiter=\" \">\n                <text macro=\"title\" suffix=\",\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <group delimiter=\", \" suffix=\".\">\n                <text macro=\"publisher\" prefix=\" \"/>\n                <text macro=\"pages\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter\" match=\"any\">\n              <group delimiter=\", \" prefix=\" \">\n                <text macro=\"title\" prefix=\" \"/>\n                <group delimiter=\": \">\n                  <text term=\"in\"/>\n                  <text macro=\"editor\"/>\n                </group>\n                <text macro=\"edition\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text variable=\"collection-title\"/>\n                <text macro=\"publisher\" prefix=\" \"/>\n                <text macro=\"pages\"/>\n              </group>\n            </else-if>\n            <else-if type=\"paper-conference\" match=\"any\">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <group delimiter=\" \" prefix=\" \">\n                <text macro=\"editor\"/>\n                <text suffix=\",\" variable=\"container-title\"/>\n                <text macro=\"edition\"/>\n                <text suffix=\".\" variable=\"collection-title\"/>\n                <group delimiter=\", \" suffix=\".\">\n                  <text macro=\"publisher\"/>\n                  <text macro=\"pages\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"thesis\">\n              <group prefix=\" \" suffix=\".\" delimiter=\". \">\n                <text macro=\"title\"/>\n                <text variable=\"genre\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </else-if>\n            <else>\n              <group suffix=\".\">\n                <text macro=\"title\" prefix=\" \"/>\n                <text macro=\"editor\" prefix=\" \"/>\n              </group>\n              <group delimiter=\", \" prefix=\" \">\n                <group delimiter=\" \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <text variable=\"volume\" font-weight=\"normal\"/>\n                  <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </else>\n          </choose>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"