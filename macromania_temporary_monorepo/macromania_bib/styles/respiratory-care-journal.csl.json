"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Respiratory Care Journal</title>\n    <title-short>RC</title-short>\n    <id>http://www.zotero.org/styles/respiratory-care-journal</id>\n    <link href=\"http://www.zotero.org/styles/respiratory-care-journal\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/haematologica\" rel=\"template\"/>\n    <link href=\"http://rc.rcjournal.com/content/author-guidelines#references\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0020-1324</issn>\n    <eissn>1943-3654</eissn>\n    <updated>2022-03-31T12:01:03+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label strip-periods=\"true\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label strip-periods=\"true\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-newspaper report\" match=\"any\">\n        <date variable=\"issued\" form=\"text\"/>\n      </if>\n      <else>\n        <date variable=\"issued\" form=\"text\" date-parts=\"year\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\"; \">\n      <group delimiter=\" \">\n        <text variable=\"genre\" text-case=\"title\"/>\n        <text variable=\"number\"/>\n      </group>\n      <group delimiter=\": \">\n        <choose>\n          <if type=\"thesis\" match=\"none\">\n            <text variable=\"publisher-place\"/>\n          </if>\n        </choose>\n        <text variable=\"publisher\"/>\n      </group>\n      <choose>\n        <if variable=\"URL\" match=\"none\">\n          <text macro=\"date\" prefix=\" \"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <choose>\n          <if variable=\"page volume\" match=\"none\">\n            <text variable=\"DOI\" text-case=\"capitalize-first\" prefix=\" doi:\"/>\n          </if>\n        </choose>\n      </if>\n      <else-if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\", \" prefix=\" (\" suffix=\")\">\n          <text variable=\"URL\"/>\n          <text macro=\"accessed-date\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"accessed-date\">\n    <group delimiter=\" \">\n      <text term=\"accessed\" text-case=\"capitalize-first\"/>\n      <date form=\"text\" variable=\"accessed\" text-case=\"capitalize-first\"/>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography delimiter-precedes-et-al=\"always\" et-al-min=\"7\" et-al-use-first=\"6\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture patent report song thesis\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"edition\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" font-style=\"normal\"/>\n                <text macro=\"edition\"/>\n              </group>\n              <group delimiter=\":\">\n                <text macro=\"publisher\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"article-newspaper article-magazine\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <text macro=\"container-title\"/>\n                <text macro=\"date\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\";\">\n                <group delimiter=\" \">\n                  <text macro=\"container-title\"/>\n                  <choose>\n                    <if variable=\"page\" match=\"any\">\n                      <text macro=\"date\"/>\n                    </if>\n                  </choose>\n                </group>\n                <group delimiter=\":\">\n                  <group>\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"