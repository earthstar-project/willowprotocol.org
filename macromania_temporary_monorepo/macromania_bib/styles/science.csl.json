"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Science</title>\n    <id>http://www.zotero.org/styles/science</id>\n    <link href=\"http://www.zotero.org/styles/science\" rel=\"self\"/>\n    <link href=\"https://www.science.org/content/page/instructions-preparing-initial-manuscript#science-citation-style\" rel=\"documentation\"/>\n    <author>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <contributor>\n      <name>Greg Barendt</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <issn>0036-8075</issn>\n    <eissn>1095-9203</eissn>\n    <summary>The Science journal style.</summary>\n    <updated>2019-07-02T08:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\", \" initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"page\" match=\"none\">\n        <text macro=\"access-value\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"access-value\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else>\n        <text variable=\"URL\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" text-case=\"title\"/>\n      </if>\n      <else-if type=\"chapter report thesis paper-conference\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"article-details\">\n    <group delimiter=\", \">\n      <group delimiter=\" \">\n        <text form=\"short\" variable=\"container-title\" font-style=\"italic\"/>\n        <text variable=\"volume\" font-weight=\"bold\"/>\n      </group>\n      <choose>\n        <if variable=\"page volume\" match=\"none\">\n          <text variable=\"DOI\" prefix=\"doi: \"/>\n        </if>\n      </choose>\n      <text variable=\"page\"/>\n    </group>\n    <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <label variable=\"page\" form=\"short\" suffix=\" \"/>\n    <text variable=\"page\" form=\"short\"/>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <text term=\"edition\" form=\"short\"/>\n          <number variable=\"edition\" form=\"numeric\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\" font-style=\"italic\"/>\n    </layout>\n  </citation>\n  <bibliography second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\", \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if type=\"thesis\">\n          <group delimiter=\" \" prefix=\", \">\n            <group delimiter=\", \">\n              <text term=\"thesis\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture song chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \" prefix=\" \">\n            <choose>\n              <if type=\"chapter paper-conference\" match=\"any\">\n                <group delimiter=\", \">\n                  <group delimiter=\" \">\n                    <text term=\"in\"/>\n                    <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n                  </group>\n                  <text macro=\"editor\"/>\n                </group>\n              </if>\n            </choose>\n            <group prefix=\"(\" suffix=\")\" delimiter=\"; \">\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"issued\"/>\n              </group>\n              <text variable=\"URL\"/>\n            </group>\n          </group>\n          <group delimiter=\" of \">\n            <group>\n              <label variable=\"volume\" form=\"short\" suffix=\" \"/>\n              <number variable=\"volume\"/>\n            </group>\n            <text variable=\"collection-title\" font-style=\"italic\"/>\n          </group>\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <text macro=\"pages\" prefix=\", \"/>\n            </if>\n          </choose>\n        </else-if>\n        <else-if type=\"article-journal\">\n          <text macro=\"article-details\" prefix=\". \"/>\n        </else-if>\n        <else-if type=\"report\">\n          <group prefix=\" (\" suffix=\")\" delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"genre\" form=\"short\"/>\n              <number variable=\"number\"/>\n            </group>\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n            <text macro=\"issued\"/>\n          </group>\n          <text macro=\"access-value\" prefix=\"; \"/>\n        </else-if>\n        <else-if type=\"article\">\n          <group delimiter=\". \" prefix=\". \">\n            <group delimiter=\" \">\n              <text variable=\"publisher\"/>\n              <text variable=\"number\"/>\n              <text term=\"article\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n              <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text macro=\"access-value\"/>\n          </group>\n        </else-if>\n        <else-if type=\"dataset software\" match=\"any\">\n          <group delimiter=\" \" prefix=\", \">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"version\"/>\n                <text variable=\"version\"/>\n              </group>\n              <text variable=\"publisher\"/>\n            </group>\n            <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text macro=\"access-value\" prefix=\"; \"/>\n        </else-if>\n        <else>\n          <group>\n            <group delimiter=\", \" prefix=\", \">\n              <text macro=\"editor\"/>\n              <group delimiter=\". \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text variable=\"volume\" font-weight=\"bold\"/>\n              </group>\n            </group>\n            <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n          </group>\n          <text macro=\"pages\"/>\n          <text macro=\"access\" prefix=\". \"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"