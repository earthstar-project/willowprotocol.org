"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>The Journal of Comparative Neurology</title>\n    <id>http://www.zotero.org/styles/the-journal-of-comparative-neurology</id>\n    <link href=\"http://www.zotero.org/styles/the-journal-of-comparative-neurology\" rel=\"self\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1002/(ISSN)1096-9861/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Rintze Zelle</name>\n      <uri>http://twitter.com/rintzezelle</uri>\n    </author>\n    <author>\n      <name>James Johnston</name>\n      <email>j.e.johnston@lse.ac.uk</email>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"science\"/>\n    <issn>0021-9967</issn>\n    <eissn>1096-9861</eissn>\n    <summary>Style for The Journal of Comparative Neurology</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \" suffix=\".\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" prefix=\" \" strip-periods=\"true\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text macro=\"anon\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\".\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group>\n      <text value=\"Available from:\" suffix=\" \"/>\n      <text variable=\"URL\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"locator\">\n    <label variable=\"locator\" form=\"short\" strip-periods=\"true\"/>\n    <text variable=\"locator\" prefix=\" \"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\" prefix=\"p \"/>\n  </macro>\n  <macro name=\"journal\">\n    <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n    <choose>\n      <if variable=\"URL\">\n        <text term=\"internet\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"year-issued\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n        </date>\n        <text macro=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"99\" et-al-use-first=\"98\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-issued\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\"/>\n      <date variable=\"issued\" prefix=\" \" suffix=\".\">\n        <date-part name=\"year\"/>\n      </date>\n      <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group prefix=\" \" suffix=\".\" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text prefix=\" \" suffix=\".\" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"container-title\" suffix=\".\"/>\n            <text variable=\"volume\" prefix=\"Vol. \" suffix=\".\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <group suffix=\".\">\n              <text macro=\"publisher\"/>\n              <group suffix=\".\" prefix=\". \" delimiter=\". \">\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\" \" suffix=\".\">\n            <text macro=\"journal\"/>\n            <text variable=\"volume\" prefix=\" \"/>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"