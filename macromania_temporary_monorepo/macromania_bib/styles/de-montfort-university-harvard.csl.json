"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"text\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>De Montfort University - Harvard</title>\n    <id>http://www.zotero.org/styles/de-montfort-university-harvard</id>\n    <link href=\"http://www.zotero.org/styles/de-montfort-university-harvard\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-cite-them-right\" rel=\"template\"/>\n    <link href=\"http://www.library.dmu.ac.uk/Images/Selfstudy/Harvard.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>ALRichards</name>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>The Harvard author-date style for De Montfort University</summary>\n    <updated>2017-12-01T17:04:35+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available from</term>\n      <term name=\"director\">directed by</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n        <name-part name=\"given\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor collection-editor\" font-variant=\"small-caps\" suffix=\"(eds.)\"/>\n        <text variable=\"title\" font-style=\"italic\" suffix=\".\"/>\n        <text macro=\"anon\" font-variant=\"small-caps\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"anon\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if type=\"article-newspaper article-magazine interview\" match=\"any\">\n        <date delimiter=\"\" variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" form=\"short\"/>\n        </date>\n      </if>\n      <else-if type=\"article-magazine article-newspaper article-journal\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"page\">\n            <group delimiter=\" \">\n              <choose>\n                <if variable=\"DOI URL\" match=\"any\">\n                  <group delimiter=\" \">\n                    <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"] \"/>\n                    <group delimiter=\": \">\n                      <text term=\"available at\" text-case=\"capitalize-first\"/>\n                      <choose>\n                        <if variable=\"DOI\">\n                          <text variable=\"DOI\" prefix=\"doi.org/\"/>\n                        </if>\n                        <else>\n                          <text variable=\"URL\"/>\n                        </else>\n                      </choose>\n                    </group>\n                    <group prefix=\" [\" suffix=\"]\" delimiter=\" \">\n                      <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                      <date variable=\"accessed\">\n                        <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\"/\"/>\n                        <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\"/\"/>\n                        <date-part name=\"year\"/>\n                      </date>\n                    </group>\n                  </group>\n                </if>\n              </choose>\n            </group>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <label variable=\"page\" form=\"short\" suffix=\" \"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <choose>\n        <if type=\"motion_picture broadcast\" match=\"any\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </if>\n        <else>\n          <group delimiter=\", \">\n            <group delimiter=\", \">\n              <text macro=\"author-short\"/>\n              <text macro=\"year-date\"/>\n            </group>\n            <group>\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"1\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <choose>\n        <if type=\"motion_picture\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <group delimiter=\" \">\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n              <text variable=\"note\" suffix=\".\"/>\n              <text variable=\"genre\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n              <text term=\"director\" text-case=\"capitalize-first\"/>\n              <text macro=\"author\"/>\n            </group>\n            <text macro=\"publisher\"/>\n          </group>\n        </if>\n        <else-if type=\"broadcast\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <group delimiter=\" \">\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n              <group delimiter=\", \">\n                <text variable=\"number\" prefix=\"Episode \"/>\n                <text variable=\"container-title\" suffix=\".\"/>\n              </group>\n              <text variable=\"medium\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"]\"/>\n              <text variable=\"publisher\"/>\n            </group>\n            <group delimiter=\", \">\n              <date form=\"text\" variable=\"issued\">\n                <date-part name=\"day\" form=\"ordinal\"/>\n              </date>\n              <text variable=\"note\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\" \">\n            <text macro=\"author\"/>\n            <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\". \">\n                <group delimiter=\". \" prefix=\" \">\n                  <text macro=\"title\"/>\n                  <text macro=\"edition\"/>\n                  <text macro=\"editor\"/>\n                </group>\n                <text macro=\"publisher\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <group prefix=\" \" delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n                <text macro=\"editor\"/>\n                <text variable=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n                <text variable=\"collection-title\" suffix=\".\"/>\n                <text variable=\"event\" suffix=\".\"/>\n                <group suffix=\".\" delimiter=\", \">\n                  <text macro=\"publisher\" prefix=\" \"/>\n                  <text macro=\"pages\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"thesis\">\n              <group prefix=\" \" suffix=\".\" delimiter=\". \">\n                <text macro=\"title\"/>\n                <text variable=\"genre\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </else-if>\n            <else-if type=\"article\" match=\"any\"/>\n            <else-if type=\"webpage post-weblog\" match=\"any\">\n              <group delimiter=\" \">\n                <group delimiter=\". \" prefix=\" \">\n                  <text variable=\"title\" font-style=\"italic\"/>\n                  <group delimiter=\" \">\n                    <text term=\"online\" text-case=\"capitalize-first\" prefix=\"[\" suffix=\"] \"/>\n                    <text variable=\"container-title\"/>\n                  </group>\n                  <group delimiter=\": \">\n                    <text term=\"available at\" text-case=\"capitalize-first\" suffix=\" \"/>\n                    <text variable=\"URL\"/>\n                  </group>\n                </group>\n                <group delimiter=\" \" prefix=\" [\" suffix=\"]\">\n                  <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                  <date variable=\"accessed\">\n                    <date-part name=\"day\" form=\"numeric-leading-zeros\" suffix=\"/\"/>\n                    <date-part name=\"month\" form=\"numeric-leading-zeros\" suffix=\"/\"/>\n                    <date-part name=\"year\" form=\"short\"/>\n                  </date>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <group suffix=\".\">\n                <text macro=\"title\" prefix=\" \"/>\n                <text macro=\"editor\" prefix=\" \"/>\n              </group>\n              <group delimiter=\", \" prefix=\" \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <group>\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text macro=\"locator\" prefix=\" \"/>\n                <group delimiter=\" \" prefix=\" \">\n                  <label variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n              <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n            </else>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"