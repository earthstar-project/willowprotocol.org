"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Taylor &amp; Francis - Council of Science Editors (numeric)</title>\n    <title-short>TF-CSE</title-short>\n    <id>http://www.zotero.org/styles/taylor-and-francis-council-of-science-editors-numeric</id>\n    <link href=\"http://www.zotero.org/styles/taylor-and-francis-council-of-science-editors-numeric\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/taylor-and-francis-council-of-science-editors-author-date\" rel=\"template\"/>\n    <link href=\"https://files.taylorandfrancis.com/tf_USCSEA.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <summary>The Council of Science Editors style for T&amp;F journals as per guidelines (2018).</summary>\n    <updated>2022-01-04T08:55:48+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\"; \">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"long\" prefix=\", \"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <names variable=\"translator\"/>\n          <text variable=\"title\" form=\"short\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </if>\n      <else>\n        <text variable=\"URL\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <group delimiter=\" \">\n      <text variable=\"title\"/>\n      <choose>\n        <if type=\"thesis\" match=\"any\">\n          <text variable=\"genre\" form=\"long\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n      </choose>\n      <choose>\n        <if variable=\"container-title\" match=\"none\">\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"internet\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"publisher-place\">\n    <choose>\n      <if variable=\"publisher-place\" match=\"none\">\n        <text value=\"[place unknown]\"/>\n      </if>\n      <else>\n        <text variable=\"publisher-place\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\"; \">\n      <group delimiter=\": \">\n        <text macro=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"cited\"/>\n    </group>\n  </macro>\n  <macro name=\"cited\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \" prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"year\"/>\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\" prefix=\" \"/>\n            <date-part name=\"day\" prefix=\" \"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <label plural=\"never\" suffix=\" \" variable=\"page\" form=\"short\"/>\n    <text variable=\"page\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"container\">\n    <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n    <choose>\n      <if variable=\"URL\">\n        <text term=\"internet\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"12\" et-al-use-first=\"10\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <group suffix=\".\" delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n        <text macro=\"title\"/>\n      </group>\n      <group suffix=\".\">\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n            <group prefix=\" \" suffix=\".\" delimiter=\" \">\n              <text macro=\"edition\"/>\n              <text macro=\"editor-translator\"/>\n            </group>\n            <text prefix=\" \" macro=\"publisher\"/>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\" prefix=\" \"/>\n            <group prefix=\" \" delimiter=\". \">\n              <text macro=\"editor-translator\"/>\n              <text macro=\"container\"/>\n              <text variable=\"volume\" prefix=\"Vol. \"/>\n              <text macro=\"edition\"/>\n              <group suffix=\".\" delimiter=\"; \">\n                <text macro=\"publisher\"/>\n                <text macro=\"pages\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <text macro=\"editor-translator\" suffix=\".\"/>\n            <group prefix=\" \" suffix=\".\">\n              <text macro=\"container\" suffix=\".\"/>\n              <group delimiter=\" \">\n                <text macro=\"cited\"/>\n                <group>\n                  <text variable=\"volume\" prefix=\" \"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n              </group>\n              <text variable=\"page\" prefix=\":\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"