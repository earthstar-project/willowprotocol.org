"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <!-- This style was edited with the Visual CSL Editor (https://editor.citationstyles.org/visualEditor/) -->\n  <info>\n    <title>La Trobe University - Harvard</title>\n    <id>http://www.zotero.org/styles/la-trobe-university-harvard</id>\n    <link href=\"http://www.zotero.org/styles/la-trobe-university-harvard\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/elsevier-harvard\" rel=\"template\"/>\n    <link href=\"http://www.lib.latrobe.edu.au/referencing-tool/harvard\" rel=\"documentation\"/>\n    <author>\n      <name>David Venema</name>\n      <email>dvenema@ltu.edu.au</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <category field=\"generic-base\"/>\n    <updated>2020-11-20T00:49:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter\" match=\"any\">\n        <text variable=\"title\" prefix=\"'\" suffix=\"'\"/>\n        <text term=\"in\" prefix=\", \" suffix=\" \"/>\n        <names variable=\"editor translator collection-editor\" suffix=\", \">\n          <name and=\"symbol\" delimiter-precedes-last=\"always\" sort-separator=\"\"/>\n          <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n        </names>\n        <group delimiter=\", \">\n          <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n          <text variable=\"collection-title\" text-case=\"title\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"container-title\"/>\n          <text variable=\"collection-title\"/>\n        </group>\n      </else-if>\n      <else-if type=\"entry-dictionary entry-encyclopedia\" match=\"any\">\n        <text variable=\"title\" prefix=\" '\" suffix=\"'\"/>\n        <text term=\"in\" prefix=\", \" suffix=\" \"/>\n        <group delimiter=\", \">\n          <text variable=\"container-title\" font-style=\"italic\" suffix=\", \"/>\n          <text variable=\"collection-title\" suffix=\", \"/>\n        </group>\n        <names variable=\"editor editorial-director\" delimiter=\" \">\n          <name prefix=\" \" initialize-with=\"\"/>\n          <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n        </names>\n        <number prefix=\"vol. \" variable=\"volume\"/>\n      </else-if>\n      <else-if type=\"article\" match=\"any\">\n        <text variable=\"publisher\"/>\n      </else-if>\n      <else-if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"paper-conference speech\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" prefix=\"'\" suffix=\"'\"/>\n          <choose>\n            <if type=\"paper-conference\" match=\"any\">\n              <text variable=\"event\" font-style=\"italic\"/>\n            </if>\n            <else-if type=\"speech\" match=\"any\">\n              <text variable=\"genre\"/>\n              <text variable=\"event\"/>\n            </else-if>\n          </choose>\n          <text variable=\"publisher\"/>\n          <text variable=\"event-place\"/>\n          <date delimiter=\" \" variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"broadcast\" match=\"any\">\n        <choose>\n          <if match=\"any\" variable=\"container-title\">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <date delimiter=\"\" variable=\"issued\" suffix=\", \">\n              <date-part name=\"year\" prefix=\" \"/>\n            </date>\n          </if>\n        </choose>\n        <group delimiter=\", \">\n          <text variable=\"medium\"/>\n          <text variable=\"publisher\"/>\n          <text variable=\"event-place\"/>\n        </group>\n      </else-if>\n      <else-if type=\"personal_communication\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"note\"/>\n          <date delimiter=\" \" variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"post-weblog\" match=\"any\">\n        <group delimiter=\", \">\n          <choose>\n            <if match=\"any\" variable=\"note\">\n              <text variable=\"container-title\" font-style=\"normal\"/>\n            </if>\n            <else>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </else>\n          </choose>\n          <text variable=\"genre\"/>\n          <date delimiter=\" \" variable=\"issued\">\n            <date-part name=\"day\"/>\n            <date-part name=\"month\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"song\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"medium\"/>\n          <text variable=\"number\"/>\n          <text variable=\"collection-title\"/>\n        </group>\n      </else-if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\" vertical-align=\"baseline\"/>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"container-title\"/>\n          <text variable=\"collection-title\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"motion_picture\" match=\"none\">\n        <names variable=\"author\">\n          <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\"\" name-as-sort-order=\"all\"/>\n          <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n          <substitute>\n            <names variable=\"collection-editor editor\"/>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"motion_picture\" match=\"none\">\n        <names variable=\"author\">\n          <name form=\"short\" and=\"symbol\" initialize-with=\". \"/>\n          <substitute>\n            <names variable=\"editor\"/>\n            <names variable=\"translator\"/>\n            <choose>\n              <if type=\"bill book graphic legal_case legislation motion_picture report\" match=\"any\">\n                <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n              </if>\n              <else-if type=\"song\" match=\"any\">\n                <text variable=\"title\"/>\n              </else-if>\n              <else>\n                <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n              </else>\n            </choose>\n          </substitute>\n        </names>\n      </if>\n      <else>\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" variable=\"URL\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <choose>\n              <if type=\"report\" match=\"any\">\n                <text value=\"accessed\"/>\n              </if>\n              <else>\n                <text value=\"viewed\"/>\n              </else>\n            </choose>\n            <date delimiter=\"\" variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n          <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n        </group>\n      </if>\n      <else-if match=\"none\" variable=\"URL\">\n        <choose>\n          <if match=\"any\" variable=\"event\">\n            <text value=\"viewed \"/>\n            <date delimiter=\" \" variable=\"accessed\">\n              <date-part name=\"day\"/>\n              <date-part name=\"month\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else-if type=\"broadcast\" match=\"any\">\n            <date delimiter=\" \" variable=\"accessed\">\n              <date-part name=\"month\"/>\n              <date-part name=\"day\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <group delimiter=\", \" prefix=\", \">\n          <text variable=\"genre\"/>\n          <text variable=\"number\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation report song\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\" \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"noauthoreditorissued\"/>\n          </group>\n          <text macro=\"edition\"/>\n          <text macro=\"editor\"/>\n          <text macro=\"translator\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </else-if>\n      <else-if type=\"chapter entry-dictionary entry-encyclopedia paper-conference speech\" match=\"any\"/>\n      <else-if type=\"article-journal article-magazine article-newspaper graphic thesis\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"title\" quotes=\"false\" prefix=\"'\" suffix=\"'\"/>\n          <text macro=\"noauthoreditorissued\"/>\n        </group>\n      </else-if>\n      <else-if type=\"broadcast article personal_communication\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" font-style=\"italic\"/>\n        </group>\n      </else-if>\n      <else-if type=\"post-weblog\" match=\"any\">\n        <choose>\n          <if match=\"any\" variable=\"note\">\n            <text variable=\"title\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\"/>\n          </else>\n        </choose>\n      </else-if>\n      <else-if type=\"motion_picture\" match=\"any\"/>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"thesis\" match=\"any\">\n          <text variable=\"genre\"/>\n        </if>\n      </choose>\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if match=\"any\" variable=\"author editor container-author composer illustrator editorial-director original-author director\">\n        <choose>\n          <if type=\"post-weblog\" match=\"any\">\n            <text variable=\"note\" prefix=\" \" suffix=\" \"/>\n          </if>\n        </choose>\n        <choose>\n          <if variable=\"issued\">\n            <date variable=\"issued\" prefix=\" \" suffix=\", \">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n          <else>\n            <choose>\n              <if type=\"song\" match=\"any\">\n                <text variable=\"note\" prefix=\" \" suffix=\", \"/>\n                <text value=\" \"/>\n              </if>\n            </choose>\n            <text value=\" \"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <number variable=\"edition\" form=\"ordinal\"/>\n        </if>\n        <else>\n          <text variable=\"edition\" suffix=\", \"/>\n        </else>\n      </choose>\n      <text value=\"edn\" strip-periods=\"true\"/>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group prefix=\" \" delimiter=\", \">\n          <group delimiter=\", \">\n            <group>\n              <choose>\n                <if match=\"any\" variable=\"volume\">\n                  <text term=\"volume\" form=\"short\"/>\n                  <text variable=\"volume\" prefix=\" \"/>\n                </if>\n              </choose>\n            </group>\n            <group>\n              <choose>\n                <if match=\"any\" variable=\"issue\">\n                  <text term=\"issue\" form=\"short\"/>\n                  <text variable=\"issue\" prefix=\" \"/>\n                </if>\n              </choose>\n            </group>\n          </group>\n          <group>\n            <choose>\n              <if type=\"article-magazine article-newspaper\" match=\"any\">\n                <date delimiter=\" \" variable=\"issued\">\n                  <date-part name=\"day\"/>\n                  <date-part name=\"month\"/>\n                </date>\n              </if>\n            </choose>\n          </group>\n          <group>\n            <label variable=\"page\" form=\"short\"/>\n            <text variable=\"page\" prefix=\" \"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation song thesis entry-dictionary entry-encyclopedia motion_picture\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"motionpictureformat\"/>\n          <text macro=\"publisher\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"publisher\"/>\n          <group>\n            <label variable=\"page\" form=\"short\" suffix=\" \"/>\n            <text variable=\"page\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"patent\">\n        <text variable=\"number\" prefix=\". \"/>\n      </else-if>\n      <else-if type=\"report\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <group>\n            <choose>\n              <if match=\"none\" variable=\"URL\">\n                <text value=\"viewed \"/>\n                <date delimiter=\" \" variable=\"accessed\">\n                  <date-part name=\"day\"/>\n                  <date-part name=\"month\"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </if>\n            </choose>\n          </group>\n          <group delimiter=\" \">\n            <choose>\n              <if match=\"any\" variable=\"archive\">\n                <text value=\"retrieved from\"/>\n                <text variable=\"archive\"/>\n              </if>\n            </choose>\n          </group>\n          <group delimiter=\", \">\n            <choose>\n              <if match=\"any\" variable=\"number\">\n                <text variable=\"event-place\"/>\n              </if>\n            </choose>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"translator\">\n    <choose>\n      <if match=\"any\" variable=\"translator\">\n        <text term=\"translator\" form=\"short\" plural=\"true\" text-case=\"lowercase\" suffix=\" \"/>\n        <names variable=\"translator\">\n          <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\"\" sort-separator=\"\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <choose>\n      <if match=\"any\" variable=\"author\">\n        <names variable=\"collection-editor editor\">\n          <name initialize-with=\"\"/>\n          <label form=\"short\" text-case=\"lowercase\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"noauthoreditorissued\">\n    <choose>\n      <if match=\"none\" variable=\"author editor\">\n        <choose>\n          <if match=\"any\" variable=\"issued\">\n            <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </if>\n        </choose>\n        <choose>\n          <if type=\"song\" match=\"any\">\n            <text variable=\"note\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"graphicmediumandmore\">\n    <choose>\n      <if type=\"graphic\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"medium\"/>\n          <text variable=\"dimensions\"/>\n          <choose>\n            <if match=\"any\" variable=\"title-short\">\n              <group>\n                <text value=\"exhibited at\" suffix=\" \"/>\n                <text variable=\"title-short\"/>\n              </group>\n            </if>\n          </choose>\n          <choose>\n            <if match=\"any\" variable=\"archive\">\n              <text variable=\"archive\"/>\n            </if>\n            <else-if match=\"any\" variable=\"source\">\n              <text variable=\"source\"/>\n            </else-if>\n          </choose>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"showNote\">\n    <choose>\n      <if type=\"motion_picture graphic\" match=\"any\">\n        <text variable=\"note\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"motionpictureformat\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"motion_picture broadcast\" match=\"any\">\n          <text variable=\"medium\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"citeissued\">\n    <date delimiter=\"\" variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n    <choose>\n      <if type=\"song\" match=\"any\">\n        <text variable=\"note\"/>\n      </if>\n      <else-if type=\"article-newspaper\" match=\"any\">\n        <choose>\n          <if match=\"none\" variable=\"URL\">\n            <text variable=\"page\" prefix=\", p. \"/>\n          </if>\n        </choose>\n      </else-if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"descending\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"citeissued\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"descending\"/>\n    </sort>\n    <layout>\n      <group suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"issued\"/>\n        <group delimiter=\", \">\n          <text macro=\"title\"/>\n          <text macro=\"container\"/>\n          <text macro=\"locators\"/>\n          <text macro=\"graphicmediumandmore\"/>\n          <text macro=\"access\"/>\n          <text macro=\"showNote\"/>\n        </group>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"