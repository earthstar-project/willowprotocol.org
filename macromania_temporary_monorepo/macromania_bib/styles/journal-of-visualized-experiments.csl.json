"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Visualized Experiments</title>\n    <title-short>JoVE</title-short>\n    <id>http://www.zotero.org/styles/journal-of-visualized-experiments</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-visualized-experiments\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/nature\" rel=\"template\"/>\n    <link href=\"https://www.jove.com/author\" rel=\"documentation\"/>\n    <author>\n      <name>Charles Parnot</name>\n      <uri>http://twitter.com/cparnot</uri>\n      <email>charles.parnot@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <eissn>1940-087X</eissn>\n    <updated>2018-04-04T10:01:20+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <et-al font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi: \"/>\n      </if>\n      <else-if variable=\"URL\">\n        <text term=\"at\"/>\n        <text variable=\"URL\" prefix=\" &lt;\" suffix=\"&gt;\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"issuance\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\". \" prefix=\". \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n          <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <date prefix=\" (\" suffix=\").\" variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"1\" second-field-align=\"flush\" entry-spacing=\"0\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <group delimiter=\". \">\n          <text macro=\"title\"/>\n          <text variable=\"container-title\" strip-periods=\"false\" font-style=\"italic\"/>\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <text variable=\"page\"/>\n            <text macro=\"access\"/>\n          </group>\n        </group>\n      </group>\n      <text macro=\"issuance\"/>\n    </layout>\n  </bibliography>\n</style>\n"