"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Transportation Research Record: Journal of the Transportation Research Board</title>\n    <title-short>TRR</title-short>\n    <id>http://www.zotero.org/styles/transportation-research-record</id>\n    <link href=\"http://www.zotero.org/styles/transportation-research-record\" rel=\"self\"/>\n    <link href=\"http://www.trb.org/GetInvolvedwithTRB/GetInvolvedSubmitaPaper.aspx\" rel=\"documentation\"/>\n    <author>\n      <name>John Coburn</name>\n      <email>jscoburn@wisc.edu</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <issn>0361-1981</issn>\n    <eissn>2169-4052</eissn>\n    <updated>2017-07-03T14:08:19+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-journal report bill book chapter graphic legal_case legislation motion_picture paper-conference song thesis\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n          <date-part name=\"day\" form=\"numeric-leading-zeros\" prefix=\" \"/>\n          <date-part name=\"year\" form=\"long\" prefix=\", \"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" delimiter=\", \" and=\"text\" name-as-sort-order=\"first\" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\". \" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book report thesis legislation\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout font-style=\"normal\" delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <text variable=\"citation-number\" font-style=\"italic\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\". \"/>\n      <choose>\n        <if type=\"paper-conference\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\"/>\n            <choose>\n              <if variable=\"volume\">\n                <group delimiter=\", \">\n                  <text variable=\"event\" prefix=\"In \" font-style=\"italic\"/>\n                  <text variable=\"volume\" prefix=\"No. \"/>\n                  <text macro=\"editor\"/>\n                  <text variable=\"event-place\"/>\n                  <text macro=\"issued\"/>\n                  <text variable=\"page\" prefix=\"pp. \"/>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\", \">\n                  <text variable=\"event\" prefix=\"Presented at the \"/>\n                  <text variable=\"event-place\"/>\n                  <text macro=\"issued\"/>\n                </group>\n              </else>\n            </choose>\n          </group>\n        </if>\n        <else-if type=\"book thesis\" match=\"any\">\n          <group delimiter=\". \">\n            <text variable=\"version\" text-case=\"title\"/>\n            <text macro=\"title\"/>\n            <text variable=\"genre\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"chapter\">\n          <group delimiter=\". \">\n            <text variable=\"title\" text-case=\"title\"/>\n            <group delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n            </group>\n          </group>\n          <text macro=\"publisher\" prefix=\", \"/>\n          <text macro=\"page\" prefix=\", \"/>\n        </else-if>\n        <else-if type=\"article-journal article-newspaper\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\" text-case=\"title\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" font-style=\"italic\" form=\"long\"/>\n              <text variable=\"volume\" prefix=\"Vol. \"/>\n              <text variable=\"issue\" prefix=\"No. \"/>\n              <text macro=\"issued\"/>\n              <text macro=\"page\"/>\n            </group>\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <group delimiter=\". \">\n            <text macro=\"title\" text-case=\"title\"/>\n            <text variable=\"number\" prefix=\"Publication \"/>\n            <group delimiter=\", \">\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n              <text macro=\"issued\"/>\n              <text macro=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <text macro=\"title\" text-case=\"title\" suffix=\", \"/>\n          <text variable=\"number\"/>\n          <text macro=\"issued\"/>\n        </else-if>\n        <else-if type=\"article webpage\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"title\" text-case=\"title\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"issue\"/>\n            <group delimiter=\", \">\n              <text variable=\"volume\" prefix=\"Volume \"/>\n              <text variable=\"page\"/>\n            </group>\n            <text variable=\"URL\"/>\n            <group delimiter=\" \">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date variable=\"accessed\">\n                <date-part name=\"month\" suffix=\" \" form=\"short\"/>\n                <date-part name=\"day\" suffix=\", \"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"legislation\">\n          <text macro=\"title\" text-case=\"title\" suffix=\". \"/>\n          <group delimiter=\", \">\n            <text variable=\"container-title\"/>\n            <text variable=\"number\"/>\n            <text variable=\"section\"/>\n            <text macro=\"issued\"/>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"title\" text-case=\"title\" suffix=\". \"/>\n          <group delimiter=\", \">\n            <text variable=\"container-title\"/>\n            <text macro=\"publisher\"/>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\"/>\n            <text macro=\"issued\"/>\n            <text macro=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"