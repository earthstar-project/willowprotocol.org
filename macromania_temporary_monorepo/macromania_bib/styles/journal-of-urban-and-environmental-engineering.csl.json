"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of Urban and Environmental Engineering</title>\n    <id>http://www.zotero.org/styles/journal-of-urban-and-environmental-engineering</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-urban-and-environmental-engineering\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-evolutionary-biology\" rel=\"template\"/>\n    <link href=\"http://periodicos.ufpb.br/ojs2/index.php/juee/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Libor Ansorge</name>\n      <email>libor.ansorge@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"engineering\"/>\n    <issn>1982-3932</issn>\n    <updated>2016-04-12T14:18:13+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" prefix=\"(ed. by \" suffix=\")\" delimiter=\", \">\n      <name and=\"symbol\" initialize-with=\". \" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" and=\"symbol\" sort-separator=\", \" initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" delimiter=\", \" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\", \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text macro=\"edition\"/>\n          </group>\n          <text macro=\"editor-translator\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\". \">\n          <text variable=\"title\"/>\n          <text variable=\"genre\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n        <text variable=\"year-suffix\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"journal-location\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\" font-style=\"italic\" form=\"short\"/>\n          <group delimiter=\", \">\n            <group>\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <choose>\n                <if variable=\"DOI\" match=\"none\">\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </if>\n              </choose>\n            </group>\n            <text variable=\"page\"/>\n            <text variable=\"DOI\" prefix=\"doi: \"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"chapter-info\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group delimiter=\", \" suffix=\".\">\n          <group delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"genre\" prefix=\"genre\" font-style=\"italic\"/>\n            <text variable=\"collection-title\" font-style=\"italic\"/>\n            <text variable=\"event\" font-style=\"italic\"/>\n            <text variable=\"collection-number\" font-style=\"italic\"/>\n            <text macro=\"editor-translator\"/>\n            <choose>\n              <if type=\"paper-conference\">\n                <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                  <text variable=\"event\"/>\n                  <text variable=\"event-place\"/>\n                  <date variable=\"event-date\">\n                    <date-part name=\"day\" suffix=\" \" range-delimiter=\"-\"/>\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"year\" range-delimiter=\"/\"/>\n                  </date>\n                </group>\n              </if>\n            </choose>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key macro=\"year-date\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n        <text macro=\"title\" suffix=\".\"/>\n        <text macro=\"journal-location\"/>\n        <text macro=\"chapter-info\"/>\n        <text macro=\"publisher\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"