"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <!-- Polyglot; used both with Catalan and Spanish\n       http://forums.zotero.org/discussion/23798/created-new-style-teologia-catalunya/?Focus=128763#Comment_128763 -->\n  <info>\n    <title>Teologia Catalunya</title>\n    <id>http://www.zotero.org/styles/teologia-catalunya</id>\n    <link href=\"http://www.zotero.org/styles/teologia-catalunya\" rel=\"self\"/>\n    <link href=\"http://www.teologia-catalunya.cat\" rel=\"documentation\"/>\n    <link href=\"http://www.worldcat.org/oclc/804144878\" rel=\"documentation\"/>\n    <author>\n      <name>Xavier Matoses</name>\n      <email>xaviermatoses@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Nicolae Turcan</name>\n      <email>nturcan@gmail.com</email>\n    </contributor>\n    <category citation-format=\"note\"/>\n    <category field=\"theology\"/>\n    <summary>Llibre d’estil per a escrits de teologia i filosofia. Facultat de Teologia de Catalunya. Facultat de Filosofia de Catalunya (URL). Associació Bíblica de Catalunya</summary>\n    <updated>2016-06-27T00:00:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"es\">\n    <terms>\n      <term name=\"accessed\">consulta</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"ca\">\n    <terms>\n      <term name=\"accessed\">consulta</term>\n      <term name=\"reference\" form=\"short\">\n        <single>ref.</single>\n        <multiple>refs.</multiple>\n      </term>\n      <!-- SHORT LOCATOR FORMS -->\n      <term name=\"page\" form=\"short\">\n        <single>p.</single>\n        <multiple>pp.</multiple>\n      </term>\n      <term name=\"verse\" form=\"short\">\n        <single>v.</single>\n        <multiple>vv.</multiple>\n      </term>\n      <term name=\"volume\" form=\"short\">\n        <single>vol.</single>\n        <multiple>vols.</multiple>\n      </term>\n      <!-- SYMBOL LOCATOR FORMS -->\n      <term name=\"paragraph\" form=\"symbol\">\n        <single>§</single>\n        <multiple>§§</multiple>\n      </term>\n      <term name=\"section\" form=\"symbol\">\n        <single>§</single>\n        <multiple>§§</multiple>\n      </term>\n      <!-- SHORT ROLE FORMS -->\n      <term name=\"author\" form=\"short\">\n        <single/>\n        <multiple/>\n      </term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"editorial-director\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"translator\" form=\"short\">\n        <single>trad.</single>\n        <multiple>trads.</multiple>\n      </term>\n      <term name=\"editortranslator\" form=\"short\">\n        <single>ed. i trad.</single>\n        <multiple>eds. i trads.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <label form=\"verb\" suffix=\" \" text-case=\"lowercase\"/>\n      <name delimiter=\", \" and=\"text\" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\" delimiter=\", \">\n      <label form=\"verb\" text-case=\"lowercase\"/>\n      <name prefix=\" \" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n    </names>\n  </macro>\n  <macro name=\"editor-short\">\n    <choose>\n      <if variable=\"author\">\n        <names variable=\"container-author editor\" delimiter=\", \">\n          <name delimiter=\" &#8211; \" delimiter-precedes-last=\"always\">\n            <name-part name=\"family\" font-variant=\"small-caps\"/>\n          </name>\n          <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n        </names>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter=\" &#8211; \" delimiter-precedes-last=\"always\" sort-separator=\", \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\" &#8211; \" delimiter-precedes-last=\"always\" sort-separator=\", \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-bib\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" form=\"long\" delimiter=\" &#8211; \" delimiter-precedes-last=\"always\" sort-separator=\", \">\n        <name-part name=\"family\" font-variant=\"small-caps\"/>\n      </name>\n      <label form=\"short\" text-case=\"lowercase\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"recipient\">\n    <names variable=\"recipient\" delimiter=\", \">\n      <label form=\"verb\" prefix=\" \" text-case=\"lowercase\" suffix=\" \"/>\n      <name and=\"text\" delimiter=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <text variable=\"URL\" prefix=\"&lt;\" suffix=\"&gt;\"/>\n        <group>\n          <text term=\"accessed\" text-case=\"capitalize-first\" prefix=\" [\" suffix=\":\"/>\n          <date variable=\"accessed\" prefix=\" \" suffix=\"]\">\n            <date-part name=\"day\" suffix=\" \"/>\n            <date-part name=\"month\" suffix=\" \" text-case=\"lowercase\"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"graphic report\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \" text-case=\"lowercase\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture report song thesis chapter paper-conference\" match=\"any\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else-if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"day\" suffix=\" \"/>\n          <date-part name=\"month\" suffix=\" \" text-case=\"lowercase\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if locator=\"page\">\n        <text variable=\"locator\" prefix=\" \"/>\n      </if>\n      <else-if locator=\"paragraph\">\n        <group>\n          <label variable=\"locator\" form=\"symbol\" strip-periods=\"true\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </else-if>\n      <else>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locators-con-pp\">\n    <choose>\n      <if locator=\"paragraph\">\n        <group>\n          <label variable=\"locator\" form=\"symbol\" strip-periods=\"true\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </if>\n      <else>\n        <group>\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" prefix=\" \"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <group>\n      <text variable=\"page\" prefix=\" \"/>\n    </group>\n  </macro>\n  <macro name=\"edition-issued\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <text variable=\"edition\" prefix=\" \" vertical-align=\"sup\"/>\n        <text macro=\"issued\"/>\n      </if>\n      <else>\n        <text variable=\"edition\" prefix=\", \"/>\n        <choose>\n          <if type=\"report\">\n            <text macro=\"issued\"/>\n          </if>\n          <else>\n            <text macro=\"issued\" prefix=\" \"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-names=\"true\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" text-case=\"capitalize-first\"/>\n            <text macro=\"locators\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\" text-case=\"capitalize-first\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <group>\n              <text macro=\"author-short\"/>\n              <text macro=\"recipient\"/>\n            </group>\n            <choose>\n              <if type=\"chapter article-journal\" match=\"any\">\n                <text macro=\"title-short\"/>\n              </if>\n              <else-if type=\"book\">\n                <group delimiter=\", \">\n                  <text macro=\"title-short\"/>\n                  <text variable=\"volume\"/>\n                </group>\n              </else-if>\n              <else>\n                <group delimiter=\", \">\n                  <text macro=\"title-short\"/>\n                  <text macro=\"volume\"/>\n                </group>\n              </else>\n            </choose>\n            <choose>\n              <if type=\"interview\">\n                <text term=\"interview\" text-case=\"lowercase\"/>\n              </if>\n              <else-if variable=\"recipient\">\n                <text macro=\"issued\"/>\n              </else-if>\n            </choose>\n            <text macro=\"locators\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\", \">\n            <text macro=\"author\"/>\n            <text macro=\"recipient\"/>\n          </group>\n          <text macro=\"title\"/>\n          <choose>\n            <if type=\"chapter paper-conference\" match=\"any\">\n              <group>\n                <text term=\"in\" text-case=\"lowercase\" prefix=\", \"/>\n                <group delimiter=\", \" prefix=\" \">\n                  <text macro=\"editor-short\"/>\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                </group>\n                <text macro=\"volume\" prefix=\", \"/>\n                <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n                  <text variable=\"collection-title\"/>\n                  <text variable=\"collection-number\"/>\n                </group>\n              </group>\n              <text macro=\"translator\" prefix=\", \"/>\n              <text macro=\"publisher\" prefix=\", \"/>\n              <text macro=\"edition-issued\" prefix=\" \"/>\n            </if>\n            <else-if type=\"article-journal article-newspaper article-magazine\" match=\"any\">\n              <group>\n                <group delimiter=\" \">\n                  <text variable=\"container-title\" form=\"short\" font-style=\"italic\" prefix=\", \"/>\n                  <group delimiter=\", \">\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\"/>\n                  </group>\n                  <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text macro=\"editor\" prefix=\", \"/>\n                <text macro=\"translator\" prefix=\", \"/>\n                <text variable=\"edition\" prefix=\", \"/>\n                <text macro=\"publisher\" prefix=\", \"/>\n              </group>\n            </else-if>\n            <else-if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n              <group>\n                <text macro=\"volume\" prefix=\", \"/>\n                <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n                  <text variable=\"collection-title\"/>\n                  <text variable=\"collection-number\"/>\n                </group>\n              </group>\n              <text macro=\"translator\" prefix=\", \"/>\n              <text macro=\"editor\" prefix=\", \"/>\n              <group prefix=\", \" delimiter=\" \">\n                <text macro=\"publisher\"/>\n                <text macro=\"edition-issued\"/>\n              </group>\n            </else-if>\n            <else>\n              <group>\n                <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n                <text variable=\"volume\" prefix=\" \"/>\n                <text variable=\"issue\" prefix=\", \"/>\n              </group>\n            </else>\n          </choose>\n          <choose>\n            <if type=\"article-journal\">\n              <text macro=\"pages\" prefix=\" \"/>\n              <text macro=\"locators-con-pp\" prefix=\", \"/>\n            </if>\n            <else-if type=\"chapter book\" match=\"any\">\n              <text macro=\"pages\" prefix=\", \"/>\n              <text macro=\"locators-con-pp\" prefix=\", \"/>\n            </else-if>\n            <else>\n              <text macro=\"locators\" prefix=\", \"/>\n            </else>\n          </choose>\n          <choose>\n            <if type=\"webpage\">\n              <text macro=\"access\" prefix=\", \"/>\n            </if>\n          </choose>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" et-al-min=\"7\" et-al-use-first=\"6\" subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"0\">\n    <sort>\n      <key macro=\"author-bib\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author-bib\" suffix=\", \"/>\n      <text macro=\"title\"/>\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <group>\n            <text term=\"in\" text-case=\"lowercase\" prefix=\", \"/>\n            <group delimiter=\", \" prefix=\" \">\n              <text macro=\"editor-short\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </group>\n            <text macro=\"volume\" prefix=\", \"/>\n            <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <text variable=\"collection-number\"/>\n            </group>\n          </group>\n          <text macro=\"translator\" prefix=\", \"/>\n          <text macro=\"publisher\" prefix=\", \"/>\n          <text macro=\"edition-issued\" prefix=\" \"/>\n          <group>\n            <text variable=\"page\" prefix=\", \"/>\n          </group>\n        </if>\n        <else-if type=\"article-journal article-newspaper article-magazine\" match=\"any\">\n          <group delimiter=\" \">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\" prefix=\", \"/>\n            <group delimiter=\", \">\n              <text variable=\"volume\"/>\n              <text variable=\"issue\"/>\n            </group>\n            <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text macro=\"editor\" prefix=\", \"/>\n          <text macro=\"translator\" prefix=\", \"/>\n          <text variable=\"edition\" prefix=\", \"/>\n          <text macro=\"publisher\" prefix=\", \"/>\n          <text macro=\"pages\" prefix=\" \"/>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n          <group>\n            <text macro=\"volume\" prefix=\", \"/>\n            <group prefix=\" (\" suffix=\")\" delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <text variable=\"collection-number\"/>\n            </group>\n          </group>\n          <text macro=\"translator\" prefix=\", \"/>\n          <text macro=\"editor\" prefix=\", \"/>\n          <group prefix=\", \" delimiter=\" \">\n            <text macro=\"publisher\"/>\n            <text macro=\"edition-issued\"/>\n          </group>\n        </else-if>\n        <else>\n          <group>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\", \"/>\n            <text macro=\"volume\" prefix=\", \"/>\n            <text variable=\"issue\" prefix=\", \"/>\n          </group>\n        </else>\n      </choose>\n      <group delimiter=\", \" prefix=\" (=\" suffix=\")\">\n        <text variable=\"original-title\" font-style=\"italic\"/>\n        <text variable=\"original-publisher-place\"/>\n        <text variable=\"original-publisher\"/>\n        <date variable=\"original-date\">\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n      <text macro=\"locators\" prefix=\", \"/>\n      <text macro=\"access\" prefix=\", \"/>\n    </layout>\n  </bibliography>\n</style>\n"