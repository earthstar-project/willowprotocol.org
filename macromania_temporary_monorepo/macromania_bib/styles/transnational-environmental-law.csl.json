"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" and=\"symbol\" page-range-format=\"minimal\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Transnational Environmental Law</title>\n    <title-short>TEL</title-short>\n    <id>http://www.zotero.org/styles/transnational-environmental-law</id>\n    <link href=\"http://www.zotero.org/styles/transnational-environmental-law\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/elsevier-with-titles\" rel=\"template\"/>\n    <link href=\"https://www.cambridge.org/core/journals/transnational-environmental-law/information/instructions-contributors\" rel=\"documentation\"/>\n    <link href=\"https://www.cambridge.org/core/services/aop-file-manager/file/5f8435072aae1a2bdc987dac/TEL-IFC.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"note\"/>\n    <category field=\"law\"/>\n    <issn>2047-1025</issn>\n    <eissn>2047-1033</eissn>\n    <updated>2021-05-07T11:59:01+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\".\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\".\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group prefix=\"(\" suffix=\")\">\n      <text variable=\"publisher\" suffix=\", \"/>\n      <text macro=\"year-date\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <choose>\n      <if variable=\"locator\">\n        <choose>\n          <if match=\"any\" position=\"subsequent\">\n            <group delimiter=\" \">\n              <label variable=\"locator\" form=\"short\"/>\n              <text variable=\"locator\"/>\n            </group>\n          </if>\n          <else>\n            <choose>\n              <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n                <group delimiter=\" \" prefix=\", \">\n                  <text term=\"at\"/>\n                  <label variable=\"page\" form=\"short\"/>\n                  <text variable=\"locator\"/>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\" \" prefix=\", \">\n                  <text term=\"at\"/>\n                  <text variable=\"locator\"/>\n                </group>\n              </else>\n            </choose>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <citation>\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid\">\n          <text term=\"ibid\"/>\n        </if>\n        <else-if match=\"any\" position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"author\"/>\n            <group delimiter=\" \">\n              <text value=\"n.\"/>\n              <text variable=\"first-reference-note-number\"/>\n              <text value=\"above\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"author\" suffix=\", \"/>\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\" \">\n                <text variable=\"title\" font-style=\"italic\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <text variable=\"title\" quotes=\"true\" suffix=\", \"/>\n              <text term=\"in\" suffix=\" \"/>\n              <text macro=\"editor\" suffix=\", \"/>\n              <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\" suffix=\", \"/>\n              <text macro=\"edition\" suffix=\", \"/>\n              <text macro=\"publisher\" suffix=\", \"/>\n              <group delimiter=\" \">\n                <label variable=\"page\" form=\"short\"/>\n                <text variable=\"page\"/>\n              </group>\n            </else-if>\n            <else-if type=\"patent\">\n              <group delimiter=\", \">\n                <text variable=\"title\"/>\n                <text variable=\"number\"/>\n                <text macro=\"year-date\"/>\n              </group>\n            </else-if>\n            <else-if type=\"thesis\">\n              <group delimiter=\", \">\n                <text variable=\"title\" quotes=\"true\"/>\n                <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                  <text variable=\"genre\"/>\n                  <text variable=\"publisher\"/>\n                  <date date-parts=\"year-month\" form=\"text\" variable=\"issued\"/>\n                </group>\n                <group delimiter=\": \">\n                  <text term=\"available at\"/>\n                  <text variable=\"URL\"/>\n                </group>\n              </group>\n            </else-if>\n            <else-if type=\"webpage post-weblog\" match=\"any\">\n              <group delimiter=\": \">\n                <text term=\"available at\" text-case=\"capitalize-first\"/>\n                <text variable=\"URL\"/>\n              </group>\n            </else-if>\n            <else>\n              <group delimiter=\" \">\n                <text variable=\"title\" quotes=\"true\"/>\n                <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n                <group>\n                  <text variable=\"volume\"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                </group>\n                <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\" suffix=\",\"/>\n                <group delimiter=\" \">\n                  <label variable=\"page\" form=\"short\"/>\n                  <text variable=\"page\" form=\"short\"/>\n                </group>\n              </group>\n            </else>\n          </choose>\n        </else>\n      </choose>\n      <text macro=\"citation-locator\"/>\n    </layout>\n  </citation>\n</style>\n"