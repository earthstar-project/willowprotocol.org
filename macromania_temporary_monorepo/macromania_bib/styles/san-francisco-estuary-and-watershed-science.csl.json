"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>San Francisco Estuary &amp; Watershed Science</title>\n    <title-short>SFEWS</title-short>\n    <id>http://www.zotero.org/styles/san-francisco-estuary-and-watershed-science</id>\n    <link href=\"http://www.zotero.org/styles/san-francisco-estuary-and-watershed-science\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/springer-basic-author-date-no-et-al\" rel=\"template\"/>\n    <link href=\"https://escholarship.org/uc/jmie_sfews/submissionguidelines\" rel=\"documentation\"/>\n    <link href=\"https://www.dropbox.com/s/fdgrumm39tilu1d/SFEWS-Style-Guide-for-Authors_022322LM.pdf?dl=0\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <issn>1546-2366</issn>\n    <updated>2022-12-29T17:07:36+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"available at\">available from</term>\n    </terms>\n  </locale>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label strip-periods=\"true\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n    <choose>\n      <if type=\"post-weblog thesis\" match=\"any\">\n        <text variable=\"genre\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n      <else-if type=\"book\" match=\"any\">\n        <text macro=\"edition\" prefix=\". (\" suffix=\")\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"year-parenth\">\n    <choose>\n      <if type=\"webpage post post-weblog\" match=\"any\">\n        <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n      </if>\n      <else>\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n      <else>\n        <group delimiter=\". \">\n          <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"year\" suffix=\" \"/>\n              <date-part name=\"month\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n              <date-part name=\"day\"/>\n            </date>\n          </group>\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"accessed-date\">\n    <group delimiter=\" \" prefix=\"[\" suffix=\"]\">\n      <text term=\"accessed\"/>\n      <date variable=\"accessed\">\n        <date-part name=\"year\"/>\n        <date-part name=\"month\" prefix=\" \"/>\n        <date-part name=\"day\" form=\"numeric-leading-zeros\" prefix=\" \"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-pages\"/>\n      <label plural=\"never\" variable=\"number-of-pages\" form=\"short\"/>\n    </group>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"author\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"year-parenth\"/>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <names variable=\"editor\">\n                  <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n                  <label strip-periods=\"true\" prefix=\", \"/>\n                </names>\n              </group>\n              <text variable=\"container-title\"/>\n              <text macro=\"edition\"/>\n            </group>\n            <group delimiter=\": \">\n              <text variable=\"publisher-place\"/>\n              <text variable=\"publisher\"/>\n            </group>\n            <group delimiter=\" \">\n              <label plural=\"never\" strip-periods=\"false\" variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </if>\n          <else-if type=\"article-journal\">\n            <group delimiter=\". \">\n              <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n              <group delimiter=\";\">\n                <text macro=\"accessed-date\"/>\n                <group delimiter=\":\">\n                  <text variable=\"volume\" suffix=\":\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n              <text macro=\"number-of-pages\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\". \">\n              <text prefix=\"In: \" variable=\"container-title\" form=\"short\"/>\n              <group delimiter=\": \">\n                <text term=\"available at\" text-case=\"capitalize-first\"/>\n                <text variable=\"URL\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\", \">\n              <text variable=\"genre\" text-case=\"capitalize-first\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"accessed-date\"/>\n              <text macro=\"number-of-pages\"/>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <group delimiter=\":\">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\". \"/>\n    </layout>\n  </bibliography>\n</style>\n"