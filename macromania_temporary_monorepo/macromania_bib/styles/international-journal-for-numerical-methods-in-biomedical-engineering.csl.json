"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>International Journal for Numerical Methods in Biomedical Engineering</title>\n    <id>http://www.zotero.org/styles/international-journal-for-numerical-methods-in-biomedical-engineering</id>\n    <link href=\"http://www.zotero.org/styles/international-journal-for-numerical-methods-in-biomedical-engineering\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/wireless-communications-and-mobile-computing\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1002/(ISSN)2040-7947/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>2040-7939</issn>\n    <eissn>2040-7947</eissn>\n    <updated>2017-06-22T10:52:52+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"anon\">\n    <text term=\"anonymous\" form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n  </macro>\n  <macro name=\"author\">\n    <group suffix=\".\">\n      <names variable=\"author\">\n        <name name-as-sort-order=\"all\" sort-separator=\" \" initialize-with=\"\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n        <label form=\"short\" prefix=\" \" strip-periods=\"true\"/>\n        <substitute>\n          <names variable=\"editor\"/>\n          <text variable=\"title\"/>\n          <text macro=\"anon\"/>\n        </substitute>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\"/>\n      <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n          <group suffix=\".\" prefix=\" \" delimiter=\" \">\n            <text macro=\"edition\"/>\n            <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text prefix=\" \" macro=\"publisher\"/>\n          <group prefix=\"; \" suffix=\".\">\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <text macro=\"editor\" suffix=\". \"/>\n            <group delimiter=\"; \">\n              <group delimiter=\". \">\n                <group delimiter=\", \">\n                  <text variable=\"container-title\" font-style=\"italic\"/>\n                  <text variable=\"volume\" prefix=\"vol \"/>\n                  <text macro=\"edition\"/>\n                </group>\n                <text macro=\"publisher\"/>\n              </group>\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\". \"/>\n          <choose>\n            <if variable=\"URL\">\n              <group delimiter=\". \" suffix=\".\">\n                <text variable=\"URL\"/>\n                <group prefix=\"Published \">\n                  <date variable=\"issued\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n                <group>\n                  <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n                  <date variable=\"accessed\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\":\" suffix=\".\">\n                <group>\n                  <date variable=\"issued\">\n                    <date-part name=\"month\" suffix=\" \"/>\n                    <date-part name=\"day\" suffix=\", \"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n                <text variable=\"page\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"article-journal\" match=\"any\">\n          <text macro=\"editor\" prefix=\" \" suffix=\".\"/>\n          <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n          <group delimiter=\":\" prefix=\" \" suffix=\".\">\n            <group delimiter=\"; \" prefix=\" \">\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <group>\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n          <choose>\n            <if match=\"any\" variable=\"DOI\">\n              <text variable=\"DOI\" prefix=\" doi:\"/>\n            </if>\n            <else>\n              <group>\n                <text variable=\"URL\"/>\n                <text term=\"accessed\" text-case=\"capitalize-first\"/>\n                <date form=\"text\" variable=\"accessed\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <group delimiter=\" \">\n              <text variable=\"container-title\"/>\n              <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n            </group>\n            <text variable=\"URL\"/>\n            <group delimiter=\" \">\n              <text term=\"accessed\" text-case=\"capitalize-first\"/>\n              <date form=\"text\" variable=\"accessed\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"editor\" prefix=\" \" suffix=\".\"/>\n          <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n          <group delimiter=\":\" prefix=\" \" suffix=\".\">\n            <group delimiter=\"; \" prefix=\" \">\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n              <group>\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n              </group>\n            </group>\n            <text variable=\"page\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"