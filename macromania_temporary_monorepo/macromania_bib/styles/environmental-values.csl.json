"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" and=\"text\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Environmental Values</title>\n    <id>http://www.zotero.org/styles/environmental-values</id>\n    <link href=\"http://www.zotero.org/styles/environmental-values\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/r-and-d-management\" rel=\"template\"/>\n    <link href=\"http://www.whpress.co.uk/EV/EVinst.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"humanities\"/>\n    <issn>0963-2719</issn>\n    <updated>2021-04-12T05:24:41+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n      </if>\n      <else-if type=\"article-journal\" match=\"any\">\n        <text variable=\"title\" text-case=\"capitalize-first\" quotes=\"true\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name delimiter-precedes-last=\"never\" initialize-with=\".\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"collection-title\">\n    <text variable=\"collection-title\"/>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"paper-conference\" match=\"any\">\n        <text variable=\"container-title\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" font-style=\"italic\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"translator\">\n    <names variable=\"translator\">\n      <label form=\"short\" suffix=\". \" text-case=\"capitalize-first\"/>\n      <name sort-separator=\", \" initialize-with=\"\" delimiter=\"; \" delimiter-precedes-last=\"always\"/>\n    </names>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <name sort-separator=\", \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\"; \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"volume\" font-weight=\"bold\"/>\n          <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case motion_picture report song chapter paper-conference\" match=\"any\">\n        <text macro=\"edition\" suffix=\" \"/>\n        <group delimiter=\". \" suffix=\".\">\n          <group>\n            <choose>\n              <if variable=\"volume\">\n                <text macro=\"volume\" prefix=\"v.\"/>\n              </if>\n              <else>\n                <text variable=\"number-of-volumes\" suffix=\" v.\"/>\n              </else>\n            </choose>\n          </group>\n          <group>\n            <choose>\n              <if variable=\"page\">\n                <label variable=\"page\" suffix=\" \" form=\"short\"/>\n                <text variable=\"page\"/>\n              </if>\n            </choose>\n          </group>\n          <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n            <text variable=\"collection-title\"/>\n            <text variable=\"collection-number\" prefix=\"no. \"/>\n          </group>\n          <text macro=\"publisher\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <text variable=\"number-of-pages\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\" text-case=\"capitalize-first\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n      <text term=\"accessed\"/>\n      <date variable=\"accessed\" delimiter=\" \">\n        <date-part name=\"day\"/>\n        <date-part name=\"month\"/>\n        <date-part name=\"year\"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <number variable=\"edition\" form=\"numeric\" suffix=\" ed.\"/>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"month\"/>\n        </date>\n      </if>\n      <else>\n        <text value=\"n.d.\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <group delimiter=\" \">\n          <date variable=\"original-date\" form=\"text\" date-parts=\"year\" prefix=\"[\" suffix=\"]\"/>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else-if variable=\"status\">\n        <text variable=\"status\"/>\n      </else-if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"author-citation\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"volume\">\n    <text variable=\"volume\" font-weight=\"bold\"/>\n  </macro>\n  <macro name=\"doi\">\n    <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" collapse=\"year\">\n    <sort>\n      <key variable=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\": \">\n        <group delimiter=\", \">\n          <choose>\n            <if variable=\"author\">\n              <text macro=\"author-citation\"/>\n            </if>\n            <else>\n              <text macro=\"title\"/>\n            </else>\n          </choose>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"_____\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author-short\"/>\n      <key macro=\"year-date\" sort=\"ascending\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <choose>\n        <if type=\"bill book\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"author-short\"/>\n            <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n            <text macro=\"title\"/>\n            <text macro=\"translator\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"locators\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter\">\n          <group delimiter=\" \">\n            <text macro=\"author-short\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\" quotes=\"true\" suffix=\".\"/>\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text macro=\"container-author\"/>\n            <group delimiter=\", \" suffix=\",\">\n              <text macro=\"editor\"/>\n              <text macro=\"container-title\"/>\n            </group>\n            <text macro=\"locators\"/>\n          </group>\n          <group display=\"block\">\n            <text macro=\"doi\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <text macro=\"author-short\" suffix=\". \"/>\n          <text macro=\"year-date\" suffix=\". \"/>\n          <text macro=\"title\" suffix=\". \"/>\n          <text variable=\"genre\" suffix=\". \"/>\n          <text variable=\"page\" prefix=\"Págs. \" suffix=\". \"/>\n          <text macro=\"publisher\" suffix=\". \"/>\n          <text macro=\"number-of-pages\" suffix=\" p.\"/>\n        </else-if>\n        <else-if type=\"article-newspaper\">\n          <text macro=\"author-short\" suffix=\". \"/>\n          <text macro=\"year-date\" suffix=\". \"/>\n          <text macro=\"title\" suffix=\". \"/>\n          <group suffix=\". \">\n            <group delimiter=\", \">\n              <text macro=\"container-title\"/>\n              <text macro=\"publisher\"/>\n              <date variable=\"issued\">\n                <date-part name=\"month\" form=\"short\" suffix=\". \"/>\n                <date-part name=\"day\"/>\n              </date>\n            </group>\n            <text variable=\"page\" prefix=\":\"/>\n          </group>\n        </else-if>\n        <else-if type=\"speech report paper-conference\" match=\"any\">\n          <choose>\n            <if variable=\"event\">\n              <text variable=\"event\" suffix=\". \"/>\n              <group delimiter=\", \" prefix=\"(\" suffix=\"). \">\n                <text macro=\"volume\"/>\n                <text macro=\"year-date\"/>\n              </group>\n              <text macro=\"year-date\" suffix=\". \"/>\n              <text macro=\"title\" suffix=\". \"/>\n              <text macro=\"author-short\" suffix=\". \"/>\n              <text macro=\"publisher\" suffix=\". \"/>\n              <group delimiter=\", \" suffix=\". \">\n                <text macro=\"volume\" prefix=\"v.\"/>\n                <text variable=\"page\" suffix=\" p\"/>\n              </group>\n            </if>\n            <else>\n              <text macro=\"author-short\" suffix=\". \"/>\n              <text macro=\"year-date\" suffix=\". \"/>\n              <text macro=\"title\" suffix=\". \"/>\n              <text macro=\"publisher\" suffix=\". \"/>\n              <group delimiter=\", \" suffix=\". \">\n                <text macro=\"volume\" prefix=\"v.\"/>\n                <text variable=\"page\" suffix=\" p\"/>\n              </group>\n              <group display=\"block\">\n                <text macro=\"doi\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article\" match=\"any\">\n          <text macro=\"author-short\" suffix=\". \"/>\n          <text macro=\"year-date\" suffix=\". \"/>\n          <group delimiter=\" \" suffix=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"note\" prefix=\"(En \" suffix=\")\" text-case=\"lowercase\"/>\n          </group>\n          <group delimiter=\" \">\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text macro=\"collection-title\" prefix=\"(Serie \" suffix=\") \"/>\n            <text macro=\"locators\" suffix=\": \"/>\n            <text variable=\"page\"/>\n          </group>\n          <group display=\"block\">\n            <text macro=\"doi\"/>\n          </group>\n        </else-if>\n        <else-if type=\"map\" match=\"any\">\n          <group delimiter=\". \">\n            <text macro=\"author-short\"/>\n            <text macro=\"year-date\"/>\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text variable=\"page\" prefix=\"Págs. \"/>\n            <text macro=\"publisher\"/>\n            <text variable=\"scale\"/>\n            <text macro=\"number-of-pages\" suffix=\" p.\"/>\n          </group>\n        </else-if>\n        <else-if type=\"song motion_picture broadcast musical_score graphic\" match=\"any\">\n          <text macro=\"author-short\" suffix=\". \"/>\n          <text macro=\"year-date\" suffix=\". \"/>\n          <group suffix=\". \">\n            <text macro=\"title\"/>\n            <text variable=\"note\" prefix=\" (\" suffix=\")\"/>\n          </group>\n          <group delimiter=\". \" suffix=\".\">\n            <text macro=\"editor\"/>\n            <text macro=\"edition\"/>\n            <group delimiter=\", \">\n              <text macro=\"publisher\"/>\n            </group>\n            <text variable=\"medium\" text-case=\"capitalize-first\"/>\n            <text macro=\"collection-title\" prefix=\"(\" suffix=\")\"/>\n            <text macro=\"container-title\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <text macro=\"author-short\" suffix=\". \"/>\n          <choose>\n            <if variable=\"issued\">\n              <date variable=\"issued\" suffix=\". \">\n                <date-part name=\"year\"/>\n              </date>\n            </if>\n            <else>\n              <date variable=\"accessed\" suffix=\". \">\n                <date-part name=\"year\"/>\n              </date>\n            </else>\n          </choose>\n          <text macro=\"title\" quotes=\"true\"/>\n          <text macro=\"edition\" suffix=\". \"/>\n          <text macro=\"container-title\" font-style=\"italic\" suffix=\".\"/>\n          <group delimiter=\" \" prefix=\" \">\n            <text term=\"online\" text-case=\"capitalize-first\"/>\n            <text term=\"at\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <text macro=\"publisher\" suffix=\". \"/>\n          <text variable=\"note\" suffix=\". \"/>\n          <text macro=\"access\" prefix=\" \"/>\n        </else-if>\n        <else>\n          <text macro=\"author-short\" suffix=\". \"/>\n          <text macro=\"year-date\" prefix=\"(\" suffix=\")\"/>\n          <text macro=\"title\" prefix=\" &quot;\" suffix=\"&quot;.\"/>\n          <group prefix=\" \" delimiter=\" \">\n            <text term=\"in\" text-case=\"capitalize-first\"/>\n            <text variable=\"container-title\" font-style=\"italic\" suffix=\". \"/>\n            <text macro=\"editor\" suffix=\". \"/>\n            <text macro=\"locators\" suffix=\",\"/>\n            <text macro=\"date\" suffix=\". \"/>\n            <text variable=\"page\" prefix=\"Págs. \" suffix=\". \"/>\n            <group suffix=\". \">\n              <text macro=\"publisher\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"