"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Frontiers medical journals</title>\n    <id>http://www.zotero.org/styles/frontiers-medical-journals</id>\n    <link href=\"http://www.zotero.org/styles/frontiers-medical-journals\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/frontiers\" rel=\"template\"/>\n    <link href=\"https://www.frontiersin.org/guidelines/author-guidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Chris Forden</name>\n      <email>cforden@comcast.net</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <summary>Style for the Open Access Frontiers in ... Journals</summary>\n    <updated>2024-04-26T13:34:45+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name prefix=\" \" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name delimiter-precedes-last=\"always\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi: \"/>\n      </if>\n      <else-if variable=\"URL\">\n        <text variable=\"URL\"/>\n        <group prefix=\" [\" suffix=\"]\">\n          <text term=\"accessed\" text-case=\"capitalize-first\" suffix=\" \"/>\n          <date variable=\"accessed\">\n            <date-part name=\"month\" suffix=\" \"/>\n            <date-part name=\"day\" suffix=\", \"/>\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <text variable=\"title\" font-style=\"italic\" suffix=\".\"/>\n      </if>\n      <else-if type=\"chapter\">\n        <text variable=\"title\" quotes=\"true\" suffix=\",\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"11\" et-al-use-first=\"10\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\".\"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \" suffix=\".\">\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"editor\"/>\n            <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n            <group delimiter=\". \">\n              <text macro=\"publisher\"/>\n              <date variable=\"issued\" prefix=\"(\" suffix=\")\">\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n            <group delimiter=\" \">\n              <text variable=\"number-of-pages\"/>\n              <label plural=\"never\" variable=\"number-of-pages\" form=\"short\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\". \" prefix=\" \">\n            <text macro=\"title\"/>\n            <group delimiter=\": \">\n              <text term=\"in\" text-case=\"capitalize-first\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"collection-title\" suffix=\".\"/>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <date date-parts=\"year\" form=\"text\" variable=\"issued\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group delimiter=\" \">\n              <label plural=\"never\" variable=\"page\" form=\"short\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n          <group delimiter=\" \" prefix=\" \">\n            <text variable=\"container-title\" form=\"short\" strip-periods=\"true\" font-style=\"italic\"/>\n            <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n              <date-part name=\"year\"/>\n            </date>\n            <text variable=\"volume\" suffix=\":\"/>\n          </group>\n          <text variable=\"page\" suffix=\".\"/>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"