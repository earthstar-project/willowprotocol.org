"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" default-locale=\"en-US\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\">\n  <info>\n    <title>Southern African Journal of Critical Care</title>\n    <title-short>SAJCC</title-short>\n    <id>http://www.zotero.org/styles/southern-african-journal-of-critical-care</id>\n    <link href=\"http://www.zotero.org/styles/southern-african-journal-of-critical-care\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/springer-basic-brackets\" rel=\"template\"/>\n    <link href=\"http://www.sajcc.org.za/index.php/SAJCC/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <updated>2019-10-23T13:21:05+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"et-al\">et al</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\" strip-periods=\"true\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\">\n        <group delimiter=\", \">\n          <text variable=\"title\"/>\n          <text macro=\"edition\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\". \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n      </group>\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \" prefix=\". \">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\":\"/>\n            <names variable=\"editor\">\n              <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n              <label form=\"short\" strip-periods=\"false\" prefix=\", \"/>\n            </names>\n            <group delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text macro=\"edition\"/>\n            </group>\n          </group>\n          <group prefix=\". \" delimiter=\", \">\n            <text variable=\"publisher\"/>\n            <text variable=\"publisher-place\"/>\n            <group delimiter=\":\">\n              <text macro=\"year-date\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"article-journal\">\n          <choose>\n            <if variable=\"page volume\" match=\"any\">\n              <group prefix=\". \" delimiter=\". \">\n                <group delimiter=\" \">\n                  <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n                  <group delimiter=\";\">\n                    <text macro=\"year-date\"/>\n                    <group delimiter=\":\">\n                      <group>\n                        <text variable=\"volume\"/>\n                        <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                      </group>\n                      <text variable=\"page\"/>\n                    </group>\n                  </group>\n                </group>\n                <text prefix=\"https://doi.org/\" variable=\"DOI\"/>\n              </group>\n            </if>\n            <else>\n              <group prefix=\". \" delimiter=\". \">\n                <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n                <text prefix=\"https://doi.org/\" variable=\"DOI\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\", \">\n            <group delimiter=\": \" prefix=\". \">\n              <text variable=\"publisher\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post-weblog\" match=\"any\">\n          <text variable=\"container-title\" form=\"short\" prefix=\". \"/>\n          <group prefix=\". \" delimiter=\". \">\n            <text variable=\"URL\"/>\n            <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n              <text term=\"accessed\"/>\n              <date variable=\"accessed\">\n                <date-part name=\"day\" suffix=\" \"/>\n                <date-part name=\"month\" strip-periods=\"true\" suffix=\" \"/>\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group prefix=\". \" delimiter=\", \">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n            <text variable=\"publisher\"/>\n          </group>\n        </else-if>\n        <else>\n          <group prefix=\". \" delimiter=\" \">\n            <text variable=\"container-title\" form=\"short\"/>\n            <group delimiter=\":\">\n              <text variable=\"volume\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"