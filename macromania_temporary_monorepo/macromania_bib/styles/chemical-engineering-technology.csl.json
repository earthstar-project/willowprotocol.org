"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Chemical Engineering &amp; Technology</title>\n    <id>http://www.zotero.org/styles/chemical-engineering-technology</id>\n    <link href=\"http://www.zotero.org/styles/chemical-engineering-technology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/chemie-ingenieur-technik\" rel=\"template\"/>\n    <link href=\"https://onlinelibrary.wiley.com/page/journal/15214125/homepage/2044_forauthors.html\" rel=\"documentation\"/>\n    <link href=\"https://onlinelibrary.wiley.com/pb-assets/assets/15214125/CET%20Instructions%20for%20Authors-1645006746923.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <category field=\"engineering\"/>\n    <issn>0930-7516</issn>\n    <eissn>1521-4125</eissn>\n    <updated>2022-04-26T08:51:32+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\">accessed on</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" suffix=\": \"/>\n      <name delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <group font-weight=\"bold\">\n      <choose>\n        <if variable=\"issued\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </if>\n        <else>\n          <text term=\"no date\" form=\"short\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"DOI: https://doi.org/\"/>\n      </if>\n      <else-if type=\"webpage post-weblog post\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\" text-case=\"capitalize-all\"/>\n      <text variable=\"publisher-place\" text-case=\"title\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <number variable=\"edition\" form=\"ordinal\"/>\n      <text term=\"edition\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n      <number variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"11\" et-al-use-first=\"10\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n              <text macro=\"volume\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <text macro=\"year-date\"/>\n          </group>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text term=\"in\"/>\n                <text variable=\"container-title\" form=\"short\" text-case=\"title\" font-style=\"italic\"/>\n                <text macro=\"editor\" strip-periods=\"false\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text macro=\"edition\"/>\n              <text macro=\"volume\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"patent\">\n          <group delimiter=\", \">\n            <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n            <group delimiter=\" \">\n              <text variable=\"authority\"/>\n              <text variable=\"number\"/>\n            </group>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"thesis\">\n          <group delimiter=\" \">\n            <group delimiter=\", \">\n              <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n              <text variable=\"genre\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else-if type=\"webpage post post-weblog\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"title\" quotes=\"true\"/>\n            <text macro=\"access\"/>\n            <text macro=\"year-date\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <group delimiter=\", \">\n              <text macro=\"year-date\"/>\n              <group delimiter=\" \">\n                <text variable=\"volume\" font-style=\"italic\"/>\n                <text variable=\"issue\" font-style=\"italic\" prefix=\"(\" suffix=\")\"/>\n              </group>\n              <text variable=\"page\" form=\"short\"/>\n            </group>\n            <text macro=\"access\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"