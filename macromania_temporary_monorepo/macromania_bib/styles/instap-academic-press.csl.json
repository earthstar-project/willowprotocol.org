"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>INSTAP Academic Press</title>\n    <id>http://www.zotero.org/styles/instap-academic-press</id>\n    <link href=\"http://www.zotero.org/styles/instap-academic-press\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/ecology\" rel=\"template\"/>\n    <link href=\"http://instappress.com/submitting-manuscripts/style-guide/\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n      <email>obrienpat86@gmail.com</email>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"history\"/>\n    <summary>Reference style for INSTAP manuscripts, based on the 3rd edition (2014) of the guidelines.</summary>\n    <updated>2019-07-18T17:31:08+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"presented at\">paper read at the</term>\n      <term name=\"translator\" form=\"short\">\n        <single>trans.</single>\n        <multiple>trans.</multiple>\n      </term>\n    </terms>\n  </locale>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"container-title\" match=\"none\">\n        <choose>\n          <if variable=\"event\">\n            <choose>\n              <if variable=\"genre\" match=\"none\">\n                <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n                <text variable=\"event\"/>\n              </if>\n              <else>\n                <group delimiter=\" \">\n                  <text variable=\"genre\" text-case=\"capitalize-first\"/>\n                  <text term=\"presented at\"/>\n                  <text variable=\"event\"/>\n                </group>\n              </else>\n            </choose>\n          </if>\n          <else-if type=\"speech\">\n            <text variable=\"genre\" text-case=\"capitalize-first\"/>\n          </else-if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-contributors\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\".\"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"secondary-contributors\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name and=\"text\" initialize-with=\". \" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"always\" initialize-with=\".\" name-as-sort-order=\"first\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\", \" suffix=\",\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <text variable=\"URL\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"article-journal article-magazine chapter paper-conference\" match=\"none\">\n          <choose>\n            <if type=\"thesis\" match=\"any\">\n              <group delimiter=\", \">\n                <text variable=\"publisher\"/>\n              </group>\n            </if>\n            <else>\n              <text variable=\"genre\"/>\n              <text variable=\"publisher-place\"/>\n            </else>\n          </choose>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <group prefix=\" \" suffix=\".\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <choose>\n            <if type=\"article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"none\">\n              <date variable=\"issued\">\n                <date-part prefix=\", \" name=\"month\"/>\n                <date-part prefix=\" \" name=\"day\"/>\n              </date>\n            </if>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <text prefix=\" (\" term=\"no date\" suffix=\").\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued-year\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"bill book chapter graphic legal_case legislation motion_picture paper-conference report song\" match=\"any\">\n        <choose>\n          <if is-numeric=\"edition\">\n            <group delimiter=\" \" suffix=\".\">\n              <number variable=\"edition\" form=\"ordinal\"/>\n              <text term=\"edition\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"edition\"/>\n          </else>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\" \">\n          <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n          <group delimiter=\", \">\n            <choose>\n              <if match=\"any\" variable=\"volume\">\n                <group delimiter=\" \">\n                  <text variable=\"volume\" prefix=\" \"/>\n                  <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  <date form=\"text\" variable=\"original-date\" prefix=\"[\" suffix=\"]\"/>\n                </group>\n              </if>\n              <else>\n                <text macro=\"issued-year\"/>\n              </else>\n            </choose>\n            <text macro=\"page\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <group delimiter=\", \" suffix=\", \">\n          <text macro=\"secondary-contributors\"/>\n          <text variable=\"container-title\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text term=\"in\" font-style=\"normal\" suffix=\" \"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <text macro=\"collection\"/>\n            </group>\n            <text macro=\"container-contributors\"/>\n          </group>\n          <text variable=\"publisher-place\"/>\n          <text macro=\"page\"/>\n        </group>\n      </else-if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"event\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else-if>\n      <else-if type=\"webpage post-weblog post\" match=\"any\">\n        <text variable=\"container-title\" font-style=\"italic\" suffix=\",\"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" suffix=\",\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"citation-locator\">\n    <text variable=\"locator\"/>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\">\n        <group delimiter=\" \" suffix=\", \">\n          <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n          <text macro=\"collection\"/>\n          <text macro=\"volumes\"/>\n        </group>\n      </if>\n      <else-if type=\"paper-conference\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\" suffix=\". \"/>\n      </else-if>\n      <else>\n        <choose>\n          <if match=\"any\" variable=\"reviewed-author\">\n            <group delimiter=\", \" suffix=\", \">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text macro=\"reviewed-author\"/>\n            </group>\n          </if>\n          <else>\n            <text variable=\"title\" text-case=\"title\" quotes=\"true\" suffix=\", \"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"collection\">\n    <group delimiter=\" \">\n      <text variable=\"volume\"/>\n      <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n        <text variable=\"collection-title\" font-style=\"italic\"/>\n        <text variable=\"collection-number\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"volumes\">\n    <text variable=\"number-of-volumes\"/>\n  </macro>\n  <macro name=\"reviewed-author\">\n    <names variable=\"reviewed-author\">\n      <label form=\"verb\"/>\n      <name prefix=\" \" initialize-with=\".\"/>\n    </names>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued-year\"/>\n        </group>\n        <text macro=\"citation-locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&#8212;&#8212;&#8212;\" entry-spacing=\"1\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <text macro=\"author\" suffix=\".\"/>\n      <text macro=\"issued\" suffix=\" \"/>\n      <text macro=\"title\" quotes=\"false\"/>\n      <text macro=\"locators\"/>\n      <group delimiter=\". \">\n        <text macro=\"edition\" suffix=\".\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"