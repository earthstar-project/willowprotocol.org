"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Photochemistry and Photobiology</title>\n    <id>http://www.zotero.org/styles/photochemistry-and-photobiology</id>\n    <link href=\"http://www.zotero.org/styles/photochemistry-and-photobiology\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/nucleic-acids-research-web-server-issue\" rel=\"template\"/>\n    <link href=\"https://onlinelibrary.wiley.com/page/journal/17511097/homepage/forauthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien, PhD</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"biology\"/>\n    <category field=\"chemistry\"/>\n    <issn>0031-8655</issn>\n    <eissn>1751-1097</eissn>\n    <updated>2018-08-23T15:13:17+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\">accessed on</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \" prefix=\"(\" suffix=\")\">\n      <label form=\"verb\" text-case=\"capitalize-first\" strip-periods=\"false\"/>\n      <name prefix=\" \" initialize-with=\". \" name-as-sort-order=\"all\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n      <et-al font-style=\"italic\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <choose>\n        <if type=\"thesis\" match=\"none\">\n          <text variable=\"publisher-place\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage post-weblog\" match=\"any\">\n        <group delimiter=\". \">\n          <group delimiter=\": \">\n            <text term=\"available at\" text-case=\"capitalize-first\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <group delimiter=\" \">\n            <text term=\"accessed\" text-case=\"capitalize-first\"/>\n            <date form=\"text\" variable=\"accessed\"/>\n          </group>\n        </group>\n      </if>\n      <else>\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"volume\">\n    <group delimiter=\" \">\n      <label text-case=\"capitalize-first\" variable=\"volume\" form=\"short\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography and=\"text\">\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text variable=\"citation-number\"/>\n        <group display=\"block\">\n          <text macro=\"author\"/>\n          <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n          <choose>\n            <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n              <group delimiter=\". \" prefix=\" \">\n                <text macro=\"title\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"publisher\"/>\n              </group>\n            </if>\n            <else-if type=\"chapter paper-conference\" match=\"any\">\n              <group delimiter=\". \" prefix=\" \">\n                <text macro=\"title\" suffix=\".\"/>\n                <group suffix=\".\" delimiter=\", \">\n                  <group delimiter=\" \">\n                    <group delimiter=\" \">\n                      <text term=\"in\" text-case=\"capitalize-first\"/>\n                      <text variable=\"container-title\" font-style=\"italic\"/>\n                      <text macro=\"volume\" prefix=\"Vol. \"/>\n                    </group>\n                    <text macro=\"editor\"/>\n                    <text variable=\"collection-title\" prefix=\" \"/>\n                  </group>\n                  <group delimiter=\" \">\n                    <label variable=\"page\" form=\"short\"/>\n                    <text variable=\"page\"/>\n                  </group>\n                </group>\n                <text macro=\"publisher\" prefix=\" \"/>\n              </group>\n            </else-if>\n            <else-if type=\"thesis\" match=\"any\">\n              <group delimiter=\". \">\n                <text macro=\"title\"/>\n                <group delimiter=\", \">\n                  <text variable=\"genre\"/>\n                  <text macro=\"publisher\"/>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <text macro=\"title\" prefix=\" \" suffix=\".\"/>\n              <group delimiter=\" \" prefix=\" \">\n                <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n                <group delimiter=\", \">\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </else>\n          </choose>\n        </group>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"