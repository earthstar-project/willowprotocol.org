"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" and=\"text\" et-al-min=\"4\" et-al-use-first=\"1\" initialize=\"false\" initialize-with=\".\" page-range-format=\"minimal\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Tsaqafah</title>\n    <id>http://www.zotero.org/styles/tsaqafah</id>\n    <link href=\"http://www.zotero.org/styles/tsaqafah\" rel=\"self\"/>\n    <link href=\"http://ejournal.unida.gontor.ac.id/index.php/tsaqafah/about/submissions#authorGuidelines\" rel=\"documentation\"/>\n    <author>\n      <email>lpp@unida.gontor.ac.id</email>\n      <name>Tim LPP</name>\n    </author>\n    <contributor>\n      <name>Lalu Ganda</name>\n      <email>laluganda@unida.gontor.ac.id</email>\n    </contributor>\n    <category citation-format=\"note\"/>\n    <category field=\"theology\"/>\n    <issn>1411-0334</issn>\n    <eissn>2460-0008</eissn>\n    <summary>note with bibliography (adapted-modified from jurnal al-jami'ah)</summary>\n    <updated>2017-10-06T14:58:06+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <style-options punctuation-in-quote=\"false\"/>\n    <terms>\n      <term name=\"month-01\" form=\"short\">Jan</term>\n      <term name=\"month-02\" form=\"short\">Feb</term>\n      <term name=\"month-03\" form=\"short\">Mar</term>\n      <term name=\"month-04\" form=\"short\">Apr</term>\n      <term name=\"month-05\" form=\"short\">May</term>\n      <term name=\"month-06\" form=\"short\">Jun</term>\n      <term name=\"month-07\" form=\"short\">Jul</term>\n      <term name=\"month-08\" form=\"short\">Aug</term>\n      <term name=\"month-09\" form=\"short\">Sep</term>\n      <term name=\"month-10\" form=\"short\">Oct</term>\n      <term name=\"month-11\" form=\"short\">Nov</term>\n      <term name=\"month-12\" form=\"short\">Dec</term>\n      <term name=\"collection-number\">collection</term>\n    </terms>\n  </locale>\n  <macro name=\"Author\">\n    <names variable=\"author\">\n      <name/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"collection-editor composer container-author director editor editorial-director illustrator reviewed-author original-author translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"Author-sort\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"first\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"collection-editor composer container-author director editor editorial-director illustrator reviewed-author original-author translator\"/>\n        <text macro=\"title-sort\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title-sort\">\n    <group delimiter=\", \">\n      <choose>\n        <if match=\"any\" variable=\"container-title\">\n          <text variable=\"title\" quotes=\"true\"/>\n          <text variable=\"container-title\" font-style=\"italic\"/>\n        </if>\n        <else>\n          <text variable=\"title\" font-style=\"italic\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if match=\"any\" variable=\"container-title\">\n        <group delimiter=\", \">\n          <choose>\n            <if type=\"chapter\" match=\"any\">\n              <group>\n                <text variable=\"title\" quotes=\"true\"/>\n                <text term=\"in\" prefix=\", \"/>\n                <text variable=\"container-title\" font-style=\"italic\" prefix=\" \"/>\n              </group>\n            </if>\n            <else-if type=\"legislation\" match=\"any\">\n              <group delimiter=\", \">\n                <text variable=\"title\" quotes=\"false\" font-style=\"italic\"/>\n                <group>\n                  <text variable=\"container-title\" font-style=\"normal\"/>\n                  <text variable=\"volume\" form=\"short\" prefix=\" No. \"/>\n                  <date date-parts=\"year\" form=\"numeric\" variable=\"issued\" prefix=\"/\"/>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <text variable=\"title\" quotes=\"true\"/>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n            </else>\n          </choose>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <choose>\n            <if type=\"paper-conference\" match=\"any\">\n              <text variable=\"title\" quotes=\"true\"/>\n              <group delimiter=\" \">\n                <text term=\"presented at\" form=\"verb-short\"/>\n                <text variable=\"event\" quotes=\"false\"/>\n              </group>\n            </if>\n            <else-if type=\"thesis\" match=\"any\">\n              <text variable=\"title\" quotes=\"true\"/>\n              <text variable=\"genre\"/>\n            </else-if>\n            <else-if type=\"report\" match=\"any\">\n              <text variable=\"title\" font-style=\"italic\"/>\n              <text variable=\"genre\"/>\n              <group delimiter=\" \">\n                <label variable=\"issue\" form=\"short\"/>\n                <text variable=\"number\"/>\n              </group>\n            </else-if>\n            <else-if type=\"interview\" match=\"any\">\n              <group delimiter=\", \">\n                <text variable=\"title\" quotes=\"true\"/>\n              </group>\n              <group>\n                <choose>\n                  <if match=\"any\" variable=\"medium\">\n                    <group delimiter=\" \">\n                      <text variable=\"medium\"/>\n                      <text term=\"interview\"/>\n                    </group>\n                  </if>\n                  <else>\n                    <text term=\"interview\"/>\n                  </else>\n                </choose>\n              </group>\n            </else-if>\n            <else-if type=\"manuscript\" match=\"any\">\n              <group delimiter=\", \">\n                <text variable=\"title\" quotes=\"true\"/>\n                <text variable=\"genre\"/>\n              </group>\n              <choose>\n                <if match=\"any\" variable=\"archive_location\">\n                  <text variable=\"source\"/>\n                  <group delimiter=\" \">\n                    <text variable=\"archive_location\" font-style=\"normal\"/>\n                    <text term=\"collection-number\"/>\n                  </group>\n                </if>\n                <else>\n                  <text variable=\"publisher-place\"/>\n                </else>\n              </choose>\n            </else-if>\n            <else-if type=\"personal_communication\" match=\"any\">\n              <group delimiter=\", \">\n                <choose>\n                  <if match=\"any\" variable=\"title\">\n                    <text variable=\"title\" quotes=\"true\"/>\n                  </if>\n                </choose>\n                <group>\n                  <text value=\"personal communication\"/>\n                  <choose>\n                    <if match=\"any\" variable=\"recipient\">\n                      <names variable=\"recipient\" prefix=\" with \"/>\n                    </if>\n                  </choose>\n                </group>\n              </group>\n            </else-if>\n            <else>\n              <text variable=\"title\" font-style=\"italic\"/>\n            </else>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <group delimiter=\", \">\n          <group>\n            <label suffix=\" \" variable=\"volume\" form=\"short\"/>\n            <text variable=\"volume\"/>\n          </group>\n          <group>\n            <label suffix=\" \" variable=\"issue\" form=\"short\"/>\n            <text variable=\"issue\" form=\"short\"/>\n          </group>\n        </group>\n      </if>\n      <else-if type=\"broadcast\" match=\"any\">\n        <choose>\n          <if match=\"any\" variable=\"publisher\">\n            <group>\n              <text value=\"broadcast\"/>\n              <text variable=\"publisher\" prefix=\" on \"/>\n            </group>\n          </if>\n          <else>\n            <text value=\"broadcast\"/>\n          </else>\n        </choose>\n        <text variable=\"publisher-place\" text-case=\"title\" prefix=\", \"/>\n      </else-if>\n      <else-if type=\"legislation\" match=\"any\"/>\n      <else>\n        <group delimiter=\", \">\n          <choose>\n            <if match=\"any\" variable=\"volume\">\n              <group delimiter=\" \">\n                <label variable=\"volume\" form=\"short\"/>\n                <text variable=\"volume\" form=\"short\"/>\n              </group>\n            </if>\n          </choose>\n          <choose>\n            <if match=\"any\" variable=\"edition\">\n              <group delimiter=\" \">\n                <choose>\n                  <if match=\"any\" is-numeric=\"edition\">\n                    <number variable=\"edition\" form=\"ordinal\"/>\n                  </if>\n                  <else>\n                    <text variable=\"edition\" form=\"short\"/>\n                  </else>\n                </choose>\n                <label variable=\"edition\"/>\n              </group>\n            </if>\n          </choose>\n          <choose>\n            <if match=\"all\" variable=\"author editor\">\n              <group delimiter=\", \">\n                <names variable=\"editor\">\n                  <label form=\"verb-short\" suffix=\" \"/>\n                  <name/>\n                </names>\n                <choose>\n                  <if match=\"any\" variable=\"translator\">\n                    <names variable=\"translator\">\n                      <label form=\"verb-short\" suffix=\" \"/>\n                      <name/>\n                    </names>\n                  </if>\n                </choose>\n              </group>\n            </if>\n            <else-if match=\"any\" variable=\"author editor\">\n              <choose>\n                <if match=\"any\" variable=\"translator\">\n                  <names variable=\"translator\">\n                    <label form=\"verb-short\" suffix=\" \"/>\n                    <name/>\n                  </names>\n                </if>\n              </choose>\n            </else-if>\n          </choose>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-newspaper interview paper-conference personal_communication speech webpage broadcast bill post-weblog post\" match=\"any\">\n        <date delimiter=\" \" variable=\"issued\">\n          <date-part name=\"day\" range-delimiter=\"-\"/>\n          <date-part name=\"month\" form=\"short\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else-if type=\"legislation\" match=\"any\"/>\n      <else>\n        <date date-parts=\"year\" form=\"text\" variable=\"issued\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \">\n      <text variable=\"publisher-place\"/>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"journal-page\">\n    <choose>\n      <if type=\"article-journal chapter\" match=\"any\">\n        <choose>\n          <if match=\"any\" variable=\"page\">\n            <label suffix=\" \" variable=\"page\" form=\"short\"/>\n            <text variable=\"page\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"url\">\n    <group delimiter=\", \">\n      <text variable=\"URL\"/>\n      <group delimiter=\" \">\n        <text term=\"accessed\"/>\n        <date delimiter=\" \" variable=\"accessed\">\n          <date-part name=\"day\"/>\n          <date-part name=\"month\" form=\"short\"/>\n          <date-part name=\"year\"/>\n        </date>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"locator-subsequent\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <label suffix=\" \" variable=\"locator\" form=\"short\"/>\n        <text variable=\"locator\" form=\"short\"/>\n      </if>\n      <else>\n        <choose>\n          <if match=\"any\" variable=\"volume\">\n            <group delimiter=\": \">\n              <text variable=\"volume\"/>\n              <text variable=\"locator\" form=\"short\"/>\n            </group>\n          </if>\n          <else>\n            <label suffix=\" \" variable=\"locator\" form=\"short\"/>\n            <text variable=\"locator\" form=\"short\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"Author-Short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"original-author editor collection-editor translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title-short\">\n    <choose>\n      <if match=\"any\" variable=\"container-title\">\n        <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n      </if>\n      <else>\n        <choose>\n          <if type=\"paper-conference thesis speech manuscript\" match=\"any\">\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </if>\n          <else-if type=\"personal_communication\" match=\"any\">\n            <group delimiter=\", \">\n              <choose>\n                <if match=\"any\" variable=\"title\">\n                  <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n                </if>\n              </choose>\n              <text value=\"personal communication\"/>\n            </group>\n            <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </else-if>\n          <else-if type=\"interview\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"title\" quotes=\"true\"/>\n              <group>\n                <choose>\n                  <if match=\"any\" variable=\"medium\">\n                    <text variable=\"medium\" suffix=\" \"/>\n                    <text term=\"interview\"/>\n                  </if>\n                  <else>\n                    <text term=\"interview\"/>\n                  </else>\n                </choose>\n              </group>\n            </group>\n            <date delimiter=\" \" variable=\"issued\" prefix=\" (\" suffix=\")\">\n              <date-part name=\"day\" range-delimiter=\"-\"/>\n              <date-part name=\"month\" form=\"short\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else>\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <group delimiter=\" \">\n      <label variable=\"locator\" form=\"short\"/>\n      <text variable=\"locator\" form=\"short\"/>\n    </group>\n  </macro>\n  <citation>\n    <layout delimiter=\"; \" suffix=\".\">\n      <choose>\n        <if match=\"any\" position=\"ibid-with-locator\">\n          <group delimiter=\", \">\n            <text term=\"ibid\" font-style=\"italic\"/>\n            <text macro=\"locator-subsequent\"/>\n          </group>\n        </if>\n        <else-if match=\"any\" position=\"ibid\">\n          <text term=\"ibid\" font-style=\"italic\" suffix=\".\"/>\n        </else-if>\n        <else-if match=\"any\" position=\"subsequent\">\n          <group delimiter=\", \">\n            <text macro=\"Author-Short\"/>\n            <text macro=\"title-short\"/>\n            <choose>\n              <if match=\"any\" variable=\"locator\">\n                <text macro=\"locator-subsequent\"/>\n              </if>\n            </choose>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"Author\"/>\n            <text macro=\"title\"/>\n            <text macro=\"edition\"/>\n          </group>\n          <group delimiter=\", \" prefix=\" (\" suffix=\")\">\n            <choose>\n              <if type=\"manuscript\" match=\"none\">\n                <group>\n                  <choose>\n                    <if type=\"broadcast personal_communication\" match=\"none\">\n                      <text macro=\"publisher\"/>\n                    </if>\n                  </choose>\n                </group>\n              </if>\n            </choose>\n            <text macro=\"issued\"/>\n          </group>\n          <group prefix=\", \">\n            <choose>\n              <if match=\"none\" variable=\"locator\">\n                <text macro=\"journal-page\"/>\n              </if>\n              <else>\n                <group delimiter=\" \">\n                  <text macro=\"locator\"/>\n                </group>\n              </else>\n            </choose>\n          </group>\n          <text macro=\"url\" prefix=\", \"/>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"____\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"Author-sort\"/>\n      <key variable=\"issued\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\". \">\n        <text macro=\"Author-sort\"/>\n        <text macro=\"edition\"/>\n        <choose>\n          <if type=\"manuscript\" match=\"none\">\n            <group delimiter=\", \">\n              <text macro=\"issued\"/>\n              <text macro=\"title\"/>\n              <choose>\n                <if type=\"broadcast personal_communication interview\" match=\"none\">\n                  <text macro=\"publisher\"/>\n                </if>\n              </choose>\n            </group>\n          </if>\n        </choose>\n        <text macro=\"journal-page\"/>\n        <choose>\n          <if match=\"none\" variable=\"DOI\">\n            <text macro=\"url\" prefix=\" \"/>\n          </if>\n        </choose>\n      </group>\n      <text variable=\"DOI\" prefix=\" [https://doi.org/\" suffix=\"]\"/>\n    </layout>\n  </bibliography>\n</style>\n"