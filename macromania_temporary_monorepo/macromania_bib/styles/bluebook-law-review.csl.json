"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"note\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Bluebook Law Review</title>\n    <id>http://www.zotero.org/styles/bluebook-law-review</id>\n    <link href=\"http://www.zotero.org/styles/bluebook-law-review\" rel=\"self\"/>\n    <link href=\"https://www.legalbluebook.com/\" rel=\"documentation\"/>\n    <author>\n      <name>Bruce D'Arcus</name>\n      <email>bdarcus@gmail.com</email>\n    </author>\n    <author>\n      <name>Nancy Sims</name>\n      <email>nsims@umich.edu</email>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"note\"/>\n    <category field=\"law\"/>\n    <summary>The Bluebook legal citation style for law reviews.</summary>\n    <updated>2023-06-28T09:05:37+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <terms>\n      <term name=\"editor\" form=\"verb-short\">ed.</term>\n      <term name=\"ibid\">id.</term>\n      <term name=\"translator\" form=\"verb-short\">trans.</term>\n    </terms>\n  </locale>\n  <macro name=\"name-macro\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter=\", \" delimiter-precedes-last=\"never\"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <choose>\n      <if type=\"legal_case\">\n        <choose>\n          <if variable=\"title-short\">\n            <text macro=\"name-short-macro\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text macro=\"name-short-macro\"/>\n          </else>\n        </choose>\n      </if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <text macro=\"name-macro\"/>\n      </else-if>\n      <else-if type=\"book graphic motion_picture report song\" match=\"any\">\n        <text macro=\"name-short-macro\" font-variant=\"small-caps\"/>\n      </else-if>\n      <else>\n        <text macro=\"name-short-macro\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"name-short-macro\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <label form=\"verb-short\" prefix=\", \"/>\n      <substitute>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <choose>\n      <if type=\"bill legal_case legislation\" match=\"any\">\n        <text macro=\"name-macro\"/>\n      </if>\n      <else-if type=\"book graphic motion_picture report song\" match=\"any\">\n        <text macro=\"name-macro\" font-variant=\"small-caps\"/>\n      </else-if>\n      <else>\n        <text macro=\"name-macro\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor-translator\">\n    <names variable=\"editor translator\" delimiter=\", \">\n      <name and=\"symbol\" delimiter=\", \"/>\n      <label form=\"short\" strip-periods=\"false\" prefix=\" \"/>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n            <text value=\"last visited\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"month\" form=\"short\" suffix=\" \" strip-periods=\"true\"/>\n              <date-part name=\"day\" suffix=\", \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"source\">\n    <choose>\n      <if type=\"article-journal\" match=\"any\">\n        <group delimiter=\" \">\n          <group delimiter=\", \">\n            <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n            <group delimiter=\" \">\n              <text variable=\"volume\"/>\n              <text macro=\"container\"/>\n              <text variable=\"page-first\"/>\n            </group>\n            <text variable=\"locator\"/>\n          </group>\n          <text macro=\"issuance\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </if>\n      <else-if type=\"legal_case\">\n        <group delimiter=\" \">\n          <group delimiter=\", \">\n            <text variable=\"title\" text-case=\"title\"/>\n            <text variable=\"volume\"/>\n          </group>\n          <text macro=\"container\"/>\n          <group delimiter=\", \">\n            <text variable=\"page-first\"/>\n            <text variable=\"locator\"/>\n          </group>\n          <text macro=\"issuance\" prefix=\"(\" suffix=\")\"/>\n        </group>\n      </else-if>\n      <else-if type=\"article-newspaper article-magazine thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n          <group delimiter=\" \">\n            <text variable=\"volume\"/>\n            <text macro=\"container\"/>\n          </group>\n          <text macro=\"issuance\"/>\n          <group delimiter=\" \">\n            <text value=\"at\"/>\n            <text variable=\"page-first\"/>\n          </group>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n        <group prefix=\", \" delimiter=\" \" suffix=\" \">\n          <text variable=\"volume\"/>\n          <text macro=\"container\"/>\n        </group>\n        <text variable=\"page-first\"/>\n        <text variable=\"locator\" prefix=\", \"/>\n        <text macro=\"issuance\" prefix=\" (\" suffix=\")\"/>\n      </else-if>\n      <else-if type=\"book\" match=\"any\">\n        <text variable=\"title\" text-case=\"title\" font-variant=\"small-caps\"/>\n        <text variable=\"locator\" prefix=\" \"/>\n        <text macro=\"issuance\" prefix=\" (\" suffix=\")\"/>\n      </else-if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n          <group delimiter=\" \">\n            <text variable=\"volume\"/>\n            <text macro=\"container\"/>\n            <text variable=\"page-first\"/>\n            <text variable=\"locator\"/>\n            <text macro=\"issuance\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"issuance\">\n    <choose>\n      <if match=\"any\" variable=\"status\">\n        <group delimiter=\" \">\n          <text variable=\"status\" text-case=\"capitalize-first\"/>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </if>\n      <else>\n        <choose>\n          <if type=\"article-journal article-magazine article-newspaper broadcast interview manuscript map patent personal_communication song speech thesis webpage post-weblog\" match=\"any\">\n            <group>\n              <choose>\n                <if type=\"article-newspaper thesis post-weblog\" match=\"any\">\n                  <group suffix=\", \">\n                    <date variable=\"issued\">\n                      <date-part name=\"month\" form=\"short\"/>\n                      <date-part name=\"day\" prefix=\" \"/>\n                    </date>\n                  </group>\n                </if>\n                <else-if type=\"article-magazine\">\n                  <date variable=\"issued\">\n                    <date-part name=\"month\" suffix=\" \" form=\"short\"/>\n                  </date>\n                </else-if>\n              </choose>\n              <date variable=\"issued\">\n                <date-part name=\"year\"/>\n              </date>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\", \">\n              <text macro=\"editor-translator\"/>\n              <group delimiter=\" \">\n                <group delimiter=\" \">\n                  <text variable=\"edition\"/>\n                  <label variable=\"edition\" form=\"short\"/>\n                </group>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"at_page\">\n    <group delimiter=\" \">\n      <text value=\"at\"/>\n      <text variable=\"locator\"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <group>\n          <text term=\"in\" font-style=\"italic\" prefix=\" \"/>\n          <text variable=\"container-title\" font-variant=\"small-caps\" prefix=\" \"/>\n        </group>\n      </if>\n      <else-if type=\"legal_case\">\n        <text variable=\"container-title\" form=\"short\" prefix=\" \"/>\n      </else-if>\n      <else-if type=\"article-journal\">\n        <text variable=\"container-title\" form=\"short\" font-variant=\"small-caps\" prefix=\" \"/>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" font-variant=\"small-caps\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"4\" et-al-use-first=\"1\">\n    <layout suffix=\".\" delimiter=\"; \">\n      <choose>\n        <if position=\"ibid-with-locator\">\n          <group delimiter=\" \">\n            <text term=\"ibid\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n            <text macro=\"at_page\"/>\n          </group>\n        </if>\n        <else-if position=\"ibid\">\n          <text term=\"ibid\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n        </else-if>\n        <else-if position=\"subsequent\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <choose>\n                <if type=\"book\" match=\"any\">\n                  <text variable=\"volume\"/>\n                </if>\n              </choose>\n              <text macro=\"author-short\"/>\n            </group>\n            <group delimiter=\" \">\n              <text value=\"supra\" font-style=\"italic\"/>\n              <text value=\"note\"/>\n              <text variable=\"first-reference-note-number\"/>\n              <text macro=\"at_page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <choose>\n                <if type=\"book\" match=\"any\">\n                  <text variable=\"volume\"/>\n                </if>\n              </choose>\n              <text macro=\"author\"/>\n            </group>\n            <text macro=\"source\"/>\n            <text macro=\"access\"/>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </citation>\n</style>\n"