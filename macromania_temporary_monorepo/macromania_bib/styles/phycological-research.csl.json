"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" page-range-format=\"minimal\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Phycological Research</title>\n    <title-short>Phycol. Res.</title-short>\n    <id>http://www.zotero.org/styles/phycological-research</id>\n    <link href=\"http://www.zotero.org/styles/phycological-research\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/journal-of-phycology\" rel=\"template\"/>\n    <link href=\"http://onlinelibrary.wiley.com/journal/10.1111/%28ISSN%291440-1835/homepage/ForAuthors.html\" rel=\"documentation\"/>\n    <author>\n      <name>Philipp Zumstein</name>\n      <uri>https://github.com/zuphilip</uri>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"botany\"/>\n    <issn>1322-0829</issn>\n    <eissn>1440-1835</eissn>\n    <updated>2014-11-03T22:59:27+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"accessed\">last accessed</term>\n      <term name=\"online\">Published online</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\" \">\n      <group delimiter=\"; \" prefix=\"[\" suffix=\"].\">\n        <group delimiter=\" \">\n          <text value=\"updated:\"/>\n          <date form=\"text\" variable=\"accessed\"/>\n        </group>\n        <group delimiter=\" \">\n          <text value=\"cited:\"/>\n          <date form=\"text\" variable=\"accessed\"/>\n        </group>\n      </group>\n      <group delimiter=\": \">\n        <text term=\"available at\" text-case=\"title\"/>\n        <text variable=\"URL\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publication-details\">\n    <group delimiter=\". \">\n      <text macro=\"edition\"/>\n      <text macro=\"version\"/>\n      <group delimiter=\", \">\n        <text variable=\"publisher\"/>\n        <text variable=\"publisher-place\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"number-of-pages\">\n    <group delimiter=\" \">\n      <text variable=\"number-of-pages\"/>\n      <label variable=\"number-of-pages\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"version\">\n    <choose>\n      <if is-numeric=\"version\">\n        <group delimiter=\" \">\n          <text term=\"version\" text-case=\"capitalize-first\"/>\n          <text variable=\"version\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"version\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key macro=\"issued\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography delimiter-precedes-et-al=\"never\" delimiter-precedes-last=\"never\" et-al-min=\"8\" et-al-use-first=\"3\" entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group delimiter=\". \" suffix=\".\">\n        <group delimiter=\" \">\n          <text macro=\"author\"/>\n          <text macro=\"issued\"/>\n        </group>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"article\" match=\"any\">\n            <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n            <choose>\n              <if match=\"any\" variable=\"volume\">\n                <group delimiter=\": \">\n                  <text variable=\"volume\" font-weight=\"bold\"/>\n                  <text variable=\"page\"/>\n                </group>\n              </if>\n              <else>\n                <group delimiter=\"; \">\n                  <group delimiter=\": \">\n                    <text term=\"online\"/>\n                    <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n                  </group>\n                  <choose>\n                    <if match=\"any\" variable=\"DOI\">\n                      <group delimiter=\": \">\n                        <text value=\"DOI\"/>\n                        <text variable=\"DOI\"/>\n                      </group>\n                    </if>\n                    <else>\n                      <text variable=\"URL\"/>\n                    </else>\n                  </choose>\n                </group>\n              </else>\n            </choose>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\" \">\n              <text term=\"in\" text-case=\"capitalize-first\" font-style=\"italic\"/>\n              <group delimiter=\" \">\n                <names variable=\"editor\" delimiter=\", \">\n                  <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \" name-as-sort-order=\"all\"/>\n                  <label form=\"short\" text-case=\"capitalize-first\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n                </names>\n                <text variable=\"container-title\" text-case=\"title\" font-style=\"italic\"/>\n              </group>\n            </group>\n            <group delimiter=\", \">\n              <text macro=\"publication-details\"/>\n              <text macro=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"collection-title\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"number\" prefix=\"No. \"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage\" match=\"any\">\n            <text macro=\"access\"/>\n          </else-if>\n          <else-if type=\"thesis\" match=\"any\">\n            <group delimiter=\", \">\n              <text variable=\"genre\"/>\n              <text macro=\"publication-details\"/>\n              <text macro=\"number-of-pages\"/>\n            </group>\n          </else-if>\n          <else>\n            <text macro=\"publication-details\"/>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"