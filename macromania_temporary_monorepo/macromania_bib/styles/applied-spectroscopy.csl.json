"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Applied Spectroscopy</title>\n    <id>http://www.zotero.org/styles/applied-spectroscopy</id>\n    <link href=\"http://www.zotero.org/styles/applied-spectroscopy\" rel=\"self\"/>\n    <link href=\"http://www.s-a-s.org/journal/authguide/\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <category field=\"science\"/>\n    <issn>0003-7028</issn>\n    <eissn>1943-3530</eissn>\n    <summary>Style for Applied Spectroscopy</summary>\n    <updated>2012-10-25T21:15:26+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <style-options punctuation-in-quote=\"false\"/>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <choose>\n      <if type=\"chapter paper-conference\" match=\"any\">\n        <text term=\"in\" suffix=\": \" text-case=\"capitalize-first\"/>\n      </if>\n    </choose>\n    <names variable=\"editor\">\n      <name initialize-with=\".\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"article-journal chapter article-magazine speech webpage\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </if>\n      <else-if type=\"thesis patent\" match=\"any\">\n        <group delimiter=\". \">\n          <text variable=\"title\"/>\n          <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n          <text variable=\"number\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"patent\">\n            <text value=\"Issued \"/>\n          </if>\n        </choose>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"speech\">\n        <group delimiter=\": \">\n          <choose>\n            <if variable=\"genre\">\n              <group delimiter=\" \">\n                <text variable=\"genre\" text-case=\"capitalize-first\"/>\n                <text term=\"presented at\"/>\n              </group>\n            </if>\n            <else>\n              <text term=\"presented at\" prefix=\"Paper \"/>\n            </else>\n          </choose>\n          <group delimiter=\"; \">\n            <group delimiter=\". \">\n              <text variable=\"event\"/>\n              <text variable=\"publisher-place\"/>\n            </group>\n            <date variable=\"issued\" form=\"text\"/>\n          </group>\n        </group>\n      </if>\n      <else>\n        <text variable=\"publisher\" suffix=\", \"/>\n        <text variable=\"publisher-place\" suffix=\", \"/>\n        <text macro=\"year-date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"location\">\n    <group delimiter=\": \">\n      <choose>\n        <if type=\"article-journal article-magazine\" match=\"any\">\n          <group>\n            <text variable=\"volume\"/>\n            <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n          </group>\n        </if>\n      </choose>\n      <group delimiter=\" \">\n        <choose>\n          <if type=\"chapter paper-conference\" match=\"any\">\n            <label variable=\"page\" form=\"short\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n        <text variable=\"page\"/>\n      </group>\n    </group>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"container-title\" form=\"short\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <group delimiter=\". \">\n      <text variable=\"DOI\"/>\n      <choose>\n        <if type=\"webpage\">\n          <group delimiter=\" \">\n            <text variable=\"URL\"/>\n            <date variable=\"accessed\" prefix=\"[\" suffix=\"]\" delimiter=\" \">\n              <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n              <date-part name=\"day\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\",\" vertical-align=\"sup\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"7\" et-al-use-first=\"6\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <text macro=\"editor\"/>\n        <text macro=\"container-title\"/>\n        <text macro=\"edition\"/>\n        <text macro=\"publisher\"/>\n        <text macro=\"location\"/>\n        <text macro=\"access\"/>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"