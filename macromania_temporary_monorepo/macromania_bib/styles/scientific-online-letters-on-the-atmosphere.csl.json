"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Scientific Online Letters on the Atmosphere</title>\n    <title-short>SOLA</title-short>\n    <id>http://www.zotero.org/styles/scientific-online-letters-on-the-atmosphere</id>\n    <link href=\"http://www.zotero.org/styles/scientific-online-letters-on-the-atmosphere\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/limnology-and-oceanography\" rel=\"template\"/>\n    <link href=\"https://www.metsoc.jp/sola/submission.html\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"geology\"/>\n    <eissn>1349-6476</eissn>\n    <updated>2022-10-05T12:46:33+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"available at\">available online at</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name and=\"text\" delimiter=\", \" initialize-with=\".\">\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n        <name-part name=\"given\" text-case=\"capitalize-first\"/>\n      </name>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\", \"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" name-as-sort-order=\"first\" sort-separator=\", \" delimiter=\", \" delimiter-precedes-last=\"always\" initialize-with=\". \"/>\n      <label form=\"short\" text-case=\"capitalize-first\" prefix=\" \"/>\n      <et-al term=\"and others\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text variable=\"title\" form=\"short\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <text variable=\"publisher\"/>\n  </macro>\n  <macro name=\"year-date\">\n    <date variable=\"issued\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"day-month\">\n    <date variable=\"issued\">\n      <date-part name=\"month\"/>\n      <date-part name=\"day\" prefix=\" \"/>\n    </date>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <label variable=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year-suffix\">\n    <sort>\n      <key variable=\"issued\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <text variable=\"locator\" prefix=\"p. \"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" line-spacing=\"2\" hanging-indent=\"true\" et-al-min=\"9\" et-al-use-first=\"3\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group>\n        <group delimiter=\". \">\n          <text macro=\"author\"/>\n          <text macro=\"year-date\" suffix=\": \"/>\n        </group>\n        <choose>\n          <if type=\"article-newspaper article-magazine\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" form=\"short\"/>\n                <text macro=\"day-month\"/>\n                <text variable=\"edition\"/>\n              </group>\n            </group>\n          </if>\n          <else-if type=\"thesis\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"bill book graphic legal_case motion_picture song\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"edition\" font-style=\"italic\"/>\n              </group>\n              <text macro=\"editor\"/>\n              <group delimiter=\", \">\n                <text macro=\"publisher\"/>\n                <group delimiter=\" \">\n                  <text variable=\"number-of-pages\"/>\n                  <label variable=\"number-of-pages\" form=\"short\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"report\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text variable=\"genre\"/>\n              <text variable=\"number\"/>\n              <text macro=\"edition\"/>\n              <text variable=\"number\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"paper-conference\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\". \">\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <text variable=\"event\" prefix=\"Proceedings of the \"/>\n                <text variable=\"publisher\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"chapter\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <group delimiter=\", \">\n                  <group delimiter=\", \">\n                    <text macro=\"editor\"/>\n                    <text variable=\"container-title\"/>\n                  </group>\n                  <text macro=\"publisher\"/>\n                  <text macro=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else-if type=\"webpage post post-weblog\" match=\"any\">\n            <group delimiter=\". \">\n              <text variable=\"title\"/>\n              <group delimiter=\", \" prefix=\"(\" suffix=\")\">\n                <group delimiter=\": \">\n                  <text term=\"available at\" text-case=\"capitalize-first\"/>\n                  <text variable=\"URL\"/>\n                </group>\n                <group delimiter=\" \">\n                  <text term=\"accessed\"/>\n                  <date form=\"text\" variable=\"accessed\"/>\n                </group>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"editor\"/>\n              <group delimiter=\", \">\n                <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n                <text variable=\"DOI\" prefix=\"doi:\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"