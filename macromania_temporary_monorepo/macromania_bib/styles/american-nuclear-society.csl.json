"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" default-locale=\"en-US\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"expanded\">\n  <info>\n    <title>American Nuclear Society</title>\n    <title-short>ANS</title-short>\n    <id>http://www.zotero.org/styles/american-nuclear-society</id>\n    <link href=\"http://www.zotero.org/styles/american-nuclear-society\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/spie-journals\" rel=\"template\"/>\n    <link href=\"http://cdn.ans.org/pubs/journals/fst/docs/fst-ref-style.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Patrick O'Brien</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"physics\"/>\n    <category field=\"engineering\"/>\n    <updated>2017-09-19T14:22:43+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"presented at\">presented at</term>\n      <term name=\"accessed\">current as of</term>\n    </terms>\n  </locale>\n  <macro name=\"type-number\">\n    <group delimiter=\" \">\n      <choose>\n        <if type=\"patent\" match=\"none\">\n          <text variable=\"genre\"/>\n        </if>\n      </choose>\n      <text variable=\"number\"/>\n    </group>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group prefix=\", \" delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name initialize-with=\". \" delimiter=\", \" and=\"text\"/>\n      <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name initialize-with=\". \" delimiter=\", \" and=\"text\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label form=\"short\" prefix=\", \" text-case=\"title\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"report\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n        <text macro=\"edition\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"publisher\"/>\n      </if>\n      <else>\n        <group delimiter=\", \">\n          <text variable=\"publisher\"/>\n          <text variable=\"publisher-place\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"; https://doi.org/\"/>\n      </if>\n      <else-if type=\"webpage\">\n        <text variable=\"URL\"/>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\">\n      <date-part name=\"day\" suffix=\" \"/>\n      <date-part name=\"month\" form=\"long\" suffix=\" \"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"issued-year\">\n    <date variable=\"issued\" prefix=\" (\" suffix=\")\">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"accessed\">\n    <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n      <text term=\"accessed\"/>\n      <date variable=\"accessed\">\n        <date-part name=\"month\" form=\"short\"/>\n        <date-part name=\"day\" prefix=\" \"/>\n        <date-part name=\"year\" prefix=\", \"/>\n      </date>\n    </group>\n  </macro>\n  <macro name=\"page\">\n    <choose>\n      <if type=\"article-journal\">\n        <text variable=\"page-first\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"page\" form=\"short\"/>\n          <text variable=\"page\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"container-prefix\">\n    <choose>\n      <if type=\"webpage article-journal\" match=\"none\">\n        <text term=\"in\" suffix=\" \"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"locators\">\n    <group delimiter=\" \">\n      <text variable=\"volume\" prefix=\" \" font-weight=\"bold\"/>\n      <text variable=\"issue\"/>\n    </group>\n  </macro>\n  <macro name=\"container\">\n    <choose>\n      <if variable=\"container-title\">\n        <text macro=\"container-prefix\"/>\n        <choose>\n          <if type=\"article-journal\">\n            <text variable=\"container-title\" form=\"short\"/>\n          </if>\n          <else>\n            <text variable=\"container-title\"/>\n          </else>\n        </choose>\n        <text macro=\"locators\"/>\n        <text macro=\"edition\"/>\n      </if>\n      <else-if type=\"paper-conference\">\n        <group delimiter=\" \">\n          <text term=\"presented at\"/>\n          <text variable=\"event\"/>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"1\" entry-spacing=\"0\" second-field-align=\"flush\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\".\"/>\n      <text macro=\"author\" suffix=\", \"/>\n      <choose>\n        <if type=\"webpage\">\n          <group delimiter=\"; \">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"container\"/>\n            </group>\n            <text macro=\"issued\"/>\n            <text macro=\"access\"/>\n            <text macro=\"accessed\"/>\n          </group>\n        </if>\n        <else-if type=\"report\">\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"type-number\"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"page\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n          <text macro=\"access\"/>\n        </else-if>\n        <else-if type=\"paper-conference\">\n          <choose>\n            <if variable=\"container-title\">\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <text macro=\"editor\"/>\n                <text macro=\"page\"/>\n                <text macro=\"publisher\"/>\n              </group>\n              <text macro=\"issued-year\"/>\n            </if>\n            <else>\n              <group delimiter=\", \">\n                <text macro=\"title\"/>\n                <text macro=\"container\"/>\n                <text macro=\"issued\"/>\n                <text variable=\"event-place\"/>\n                <text variable=\"page\"/>\n                <text variable=\"publisher\"/>\n              </group>\n            </else>\n          </choose>\n          <text macro=\"access\"/>\n        </else-if>\n        <else-if type=\"interview\">\n          <group delimiter=\", \">\n            <text variable=\"note\"/>\n            <text variable=\"medium\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <text macro=\"title\"/>\n            <text macro=\"type-number\"/>\n            <text macro=\"container\"/>\n            <text macro=\"editor\"/>\n            <text macro=\"page\"/>\n            <text macro=\"publisher\"/>\n          </group>\n          <text macro=\"issued-year\"/>\n          <text macro=\"access\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"