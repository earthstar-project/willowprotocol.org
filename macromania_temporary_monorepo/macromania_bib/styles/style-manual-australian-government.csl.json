"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Style Manual - Australian Government (author-date)</title>\n    <id>http://www.zotero.org/styles/style-manual-australian-government</id>\n    <link href=\"http://www.zotero.org/styles/style-manual-australian-government\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/harvard-cite-them-right\" rel=\"template\"/>\n    <link href=\"https://www.stylemanual.gov.au/style-rules-and-conventions/referencing-and-attribution/author-date\" rel=\"documentation\"/>\n    <author>\n      <name>jstedman</name>\n      <email>jessica.stedman@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"generic-base\"/>\n    <summary>Snooks style manual Australia Harvard author-date style</summary>\n    <updated>2022-07-04T07:48:19+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"edition\" form=\"short\">edn</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor translator\">\n      <name and=\"text\" initialize-with=\"\"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\"\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" strip-periods=\"true\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"doi:\"/>\n      </if>\n      <else-if match=\"any\" variable=\"URL\">\n        <group delimiter=\", \">\n          <text variable=\"URL\"/>\n          <group delimiter=\" \">\n            <text term=\"accessed\"/>\n            <date variable=\"accessed\">\n              <date-part name=\"day\" suffix=\" \"/>\n              <date-part name=\"month\" suffix=\" \"/>\n              <date-part name=\"year\"/>\n            </date>\n          </group>\n        </group>\n      </else-if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case legislation motion_picture report song thesis webpage\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\" quotes=\"true\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <choose>\n          <if type=\"article-newspaper article-magazine paper-conference speech\" match=\"any\">\n            <date form=\"text\" date-parts=\"year-month-day\" variable=\"issued\"/>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-names=\"true\" disambiguate-add-givenname=\"true\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <layout delimiter=\"; \" prefix=\"(\" suffix=\")\">\n      <group delimiter=\":\">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <choose>\n            <if match=\"none\" locator=\"page\">\n              <label variable=\"locator\" form=\"short\"/>\n            </if>\n          </choose>\n          <text variable=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography subsequent-author-substitute=\"&#8212;&#8212;\">\n    <sort>\n      <key macro=\"author\"/>\n      <key variable=\"issued\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\" prefix=\" (\" suffix=\")\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference speech\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <group delimiter=\" \">\n                <text term=\"in\"/>\n                <text macro=\"editor\"/>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n              </group>\n              <text variable=\"collection-title\"/>\n              <text variable=\"event\" font-style=\"italic\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"thesis\">\n            <group delimiter=\", \">\n              <group delimiter=\" \">\n                <text macro=\"title\"/>\n                <text variable=\"genre\" prefix=\"[\" suffix=\"]\"/>\n              </group>\n              <text macro=\"publisher\"/>\n            </group>\n          </else-if>\n          <else-if type=\"article-magazine article-newspaper\" match=\"any\">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text variable=\"container-title\"/>\n              <group delimiter=\" \">\n                <text term=\"accessed\"/>\n                <date form=\"text\" variable=\"accessed\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <group>\n                <text variable=\"container-title\" font-style=\"italic\"/>\n                <group delimiter=\":\">\n                  <group prefix=\", \">\n                    <text variable=\"volume\"/>\n                    <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n                  </group>\n                  <text variable=\"page\"/>\n                </group>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text prefix=\", \" macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"