"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>International Atomic Energy Agency</title>\n    <title-short>IAEA</title-short>\n    <id>http://www.zotero.org/styles/international-atomic-energy-agency</id>\n    <link href=\"http://www.zotero.org/styles/international-atomic-energy-agency\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/multidisciplinary-digital-publishing-institute\" rel=\"template\"/>\n    <link href=\"http://www-pub.iaea.org/mtcd/publications/pdf/iaea_stylemanual_06.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"science\"/>\n    <summary>Manual of Style for the IAEA</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <style-options punctuation-in-quote=\"false\"/>\n    <terms>\n      <term name=\"open-quote\">“</term>\n      <term name=\"close-quote\">”</term>\n      <term name=\"open-inner-quote\">‘</term>\n      <term name=\"close-inner-quote\">’</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\", \" initialize-with=\".\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\", \" initialize-with=\".\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\">\n        <name-part name=\"family\" text-case=\"uppercase\"/>\n      </name>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"chapter speech\" match=\"any\">\n        <text variable=\"title\" quotes=\"true\"/>\n      </if>\n      <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"title\"/>\n      </else-if>\n      <else>\n        <text variable=\"title\" text-case=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <choose>\n      <if variable=\"volume\">\n        <group delimiter=\" \">\n          <text term=\"volume\" text-case=\"capitalize-first\" form=\"short\"/>\n          <text variable=\"volume\"/>\n        </group>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <text variable=\"number-of-volumes\"/>\n          <text term=\"volume\" form=\"short\" strip-periods=\"true\" plural=\"true\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"series\">\n    <group delimiter=\" \">\n      <text variable=\"collection-title\"/>\n      <text variable=\"collection-number\"/>\n    </group>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if variable=\"page\">\n        <label variable=\"page\" form=\"short\" suffix=\" \"/>\n        <text variable=\"page\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <text variable=\"number-of-pages\"/>\n          <text term=\"page\" plural=\"true\" form=\"short\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"book-container\">\n    <text variable=\"container-title\" text-case=\"title\"/>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if type=\"article-magazine article-newspaper\" match=\"any\">\n        <text macro=\"full-issued\"/>\n      </if>\n      <else>\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"full-issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"day\"/>\n      <date-part name=\"month\" form=\"short\"/>\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout delimiter=\", \" prefix=\"[\" suffix=\"]\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" second-field-align=\"flush\" et-al-min=\"6\" et-al-use-first=\"1\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <group delimiter=\", \">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report thesis song\" match=\"any\">\n            <text macro=\"editor\" prefix=\" \"/>\n            <group delimiter=\" \">\n              <text variable=\"genre\"/>\n              <text variable=\"number\"/>\n            </group>\n            <text macro=\"series\"/>\n            <text macro=\"edition\"/>\n            <text macro=\"volume\"/>\n            <group delimiter=\" \" prefix=\" \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\" \">\n              <group delimiter=\", \">\n                <text macro=\"book-container\"/>\n                <text macro=\"series\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"volume\"/>\n              </group>\n              <text macro=\"editor\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group delimiter=\" \">\n              <text macro=\"publisher\"/>\n              <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n              <text variable=\"page\"/>\n            </group>\n          </else-if>\n          <else-if type=\"webpage\">\n            <text variable=\"container-title\"/>\n            <text variable=\"URL\"/>\n          </else-if>\n          <else-if type=\"speech\">\n            <group delimiter=\" \">\n              <text variable=\"genre\"/>\n              <text term=\"presented at\"/>\n              <text variable=\"event\"/>\n            </group>\n            <text variable=\"event-place\"/>\n            <text macro=\"issued\"/>\n          </else-if>\n          <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n            <group delimiter=\" \">\n              <text variable=\"container-title\" form=\"short\"/>\n              <text variable=\"volume\" font-weight=\"bold\"/>\n              <text variable=\"issue\"/>\n              <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n              <text variable=\"page-first\"/>\n            </group>\n          </else-if>\n          <else-if type=\"patent\">\n            <group delimiter=\" \">\n              <text variable=\"authority\"/>\n              <text variable=\"number\"/>\n            </group>\n            <date variable=\"issued\" delimiter=\" \">\n              <date-part name=\"month\" form=\"short\"/>\n              <date-part name=\"year\"/>\n            </date>\n          </else-if>\n          <else>\n            <group delimiter=\" \">\n              <group delimiter=\", \">\n                <text variable=\"container-title\" font-style=\"italic\" text-case=\"title\"/>\n                <text macro=\"edition\"/>\n                <text macro=\"volume\"/>\n                <text variable=\"genre\"/>\n                <text macro=\"series\"/>\n                <text macro=\"publisher\"/>\n              </group>\n              <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n              <text macro=\"pages\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n    </layout>\n  </bibliography>\n</style>\n"