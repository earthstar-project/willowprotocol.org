"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\" default-locale=\"en-US\">\n  <info>\n    <title>Zeitschrift f√ºr Papyrologie und Epigraphik</title>\n    <title-short>ZPE</title-short>\n    <id>http://www.zotero.org/styles/zeitschrift-fur-papyrologie-und-epigraphik</id>\n    <link href=\"http://www.zotero.org/styles/zeitschrift-fur-papyrologie-und-epigraphik\" rel=\"self\"/>\n    <link href=\"http://www.zotero.org/styles/politische-vierteljahresschrift\" rel=\"template\"/>\n    <link href=\"https://ifa.phil-fak.uni-koeln.de/zeitschriften-reihen/zeitschrift-fuer-papyrologie-und-epigraphik-zpe/wie-sollen-die-manuskripte-eingereicht-werden\" rel=\"documentation\"/>\n    <author>\n      <name>John D. Muccigrosso</name>\n    </author>\n    <category citation-format=\"author-date\"/>\n    <category field=\"social_science\"/>\n    <issn>0084-5388</issn>\n    <eissn>2747-6448</eissn>\n    <summary>Style meeting the fairly minimal requirements of the journal</summary>\n    <updated>2021-07-09T10:20:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale>\n    <date form=\"numeric\">\n      <date-part name=\"day\" form=\"numeric\" suffix=\".\"/>\n      <date-part name=\"month\" form=\"numeric\" suffix=\".\"/>\n      <date-part name=\"year\"/>\n    </date>\n    <terms>\n      <term name=\"page-range-delimiter\">&#8211;</term>\n      <term name=\"et-al\">et al.</term>\n    </terms>\n  </locale>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter-precedes-last=\"always\" delimiter=\"; \"/>\n      <label form=\"short\" prefix=\" (\" text-case=\"capitalize-first\" suffix=\")\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" delimiter-precedes-last=\"never\" delimiter=\"; \" delimiter-precedes-et-al=\"never\" initialize=\"false\" initialize-with=\". \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" delimiter=\"; \" and=\"text\" delimiter-precedes-last=\"never\" initialize-with=\". \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\"; \">\n          <group delimiter=\": \">\n            <text term=\"in\"/>\n            <text variable=\"URL\"/>\n          </group>\n          <date variable=\"accessed\" form=\"numeric\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\" text-case=\"title\" font-style=\"italic\"/>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <choose>\n        <if type=\"speech\" match=\"none\">\n          <text variable=\"publisher-place\"/>\n        </if>\n      </choose>\n      <text macro=\"status\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <group prefix=\"(\" suffix=\")\">\n      <choose>\n        <if variable=\"issued\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </if>\n        <else>\n          <text term=\"no date\" form=\"short\"/>\n        </else>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"series\">\n    <group prefix=\"(\" suffix=\")\">\n      <choose>\n        <if type=\"book chapter paper-conference\" match=\"any\">\n          <group delimiter=\", \">\n            <text variable=\"collection-title\"/>\n            <group delimiter=\" \">\n              <text term=\"volume\"/>\n              <text variable=\"collection-number\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"thesis\">\n          <group delimiter=\": \">\n            <text variable=\"publisher\"/>\n            <text variable=\"genre\"/>\n          </group>\n        </else-if>\n        <else-if type=\"report\">\n          <choose>\n            <if variable=\"genre\">\n              <group delimiter=\" \">\n                <text variable=\"genre\"/>\n                <text variable=\"number\"/>\n              </group>\n            </if>\n            <else>\n              <group delimiter=\" \">\n                <text variable=\"collection-title\"/>\n                <text variable=\"collection-number\"/>\n              </group>\n            </else>\n          </choose>\n        </else-if>\n        <else-if type=\"speech\">\n          <group delimiter=\", \">\n            <group delimiter=\" \">\n              <text variable=\"genre\"/>\n              <text variable=\"event\"/>\n            </group>\n            <text variable=\"event-place\"/>\n            <date variable=\"issued\" form=\"numeric\"/>\n          </group>\n        </else-if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"status\">\n    <choose>\n      <if type=\"manuscript\">\n        <text variable=\"genre\"/>\n      </if>\n      <else>\n        <text variable=\"status\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"locator\">\n    <choose>\n      <if locator=\"page\">\n        <text variable=\"locator\"/>\n      </if>\n      <else>\n        <group delimiter=\" \">\n          <label variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"book-volume\">\n    <group delimiter=\" \">\n      <text term=\"volume\"/>\n      <text variable=\"volume\"/>\n    </group>\n  </macro>\n  <macro name=\"date-newspaper\">\n    <choose>\n      <if type=\"article-newspaper article-magazine\" match=\"any\">\n        <date variable=\"issued\" form=\"numeric\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if variable=\"issue\" type=\"article-newspaper\" match=\"any\">\n        <text variable=\"page\" prefix=\", \"/>\n      </if>\n      <else>\n        <text variable=\"page\" prefix=\", \"/>\n      </else>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" disambiguate-add-names=\"true\" givenname-disambiguation-rule=\"by-cite\" disambiguate-add-givenname=\"true\" collapse=\"year\" cite-group-delimiter=\"; \">\n    <layout delimiter=\"; \">\n      <group delimiter=\": \">\n        <group delimiter=\" \">\n          <text macro=\"author-short\"/>\n          <text macro=\"year-date\"/>\n        </group>\n        <group>\n          <text macro=\"locator\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"true\" entry-spacing=\"1\" line-spacing=\"1\" subsequent-author-substitute=\"&#8211;\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"year-date\"/>\n      <key variable=\"title\"/>\n    </sort>\n    <layout suffix=\".\">\n      <group delimiter=\" \" suffix=\":\">\n        <text macro=\"author\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <choose>\n        <if type=\"bill book graphic legal_case legislation motion_picture report speech thesis song\" match=\"any\">\n          <group prefix=\" \" delimiter=\" \">\n            <group delimiter=\", \">\n              <text macro=\"title\"/>\n              <text macro=\"book-volume\"/>\n              <text macro=\"edition\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <text macro=\"series\"/>\n          </group>\n          <text prefix=\", \" macro=\"publisher\"/>\n        </if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <text macro=\"title\" prefix=\" \"/>\n          <group prefix=\", \" delimiter=\": \">\n            <text term=\"in\" text-case=\"lowercase\"/>\n            <text macro=\"editor\"/>\n            <group prefix=\" \" delimiter=\", \">\n              <text variable=\"container-title\"/>\n              <text macro=\"book-volume\"/>\n              <text macro=\"edition\"/>\n              <text variable=\"event\"/>\n              <text macro=\"publisher\"/>\n              <text variable=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"webpage\">\n          <group delimiter=\", \" prefix=\" \">\n            <text macro=\"title\"/>\n            <text variable=\"container-title\"/>\n            <text prefix=\", \" macro=\"access\"/>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\", \">\n            <group prefix=\" \" delimiter=\" \">\n              <text macro=\"title\"/>\n              <text macro=\"editor\"/>\n            </group>\n            <group delimiter=\" \">\n              <text term=\"in\" text-case=\"lowercase\" suffix=\":\"/>\n              <text variable=\"container-title\"/>\n              <group delimiter=\": \">\n                <text variable=\"volume\"/>\n                <text variable=\"issue\"/>\n              </group>\n            </group>\n            <text macro=\"date-newspaper\"/>\n          </group>\n          <text macro=\"pages\"/>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"