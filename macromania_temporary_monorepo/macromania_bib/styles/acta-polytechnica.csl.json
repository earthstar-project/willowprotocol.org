"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-GB\">\n  <info>\n    <title>Acta Polytechnica</title>\n    <id>http://www.zotero.org/styles/acta-polytechnica</id>\n    <link href=\"http://www.zotero.org/styles/acta-polytechnica\" rel=\"self\"/>\n    <link href=\"http://www.cvut.cz/en/facilities/ph/ap/resolveuid/a1a4420631dd2b61eff7a823f4b4f949\" rel=\"documentation\"/>\n    <author>\n      <name>Libor Ansorge</name>\n      <email>libor.ansorge@gmail.com</email>\n    </author>\n    <category citation-format=\"numeric\"/>\n    <category field=\"engineering\"/>\n    <issn>1210-2709</issn>\n    <eissn>1805-2363</eissn>\n    <summary>Style for journal Acta Polytechnica</summary>\n    <updated>2013-05-09T23:17:00+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\", \" initialize-with=\".\" delimiter-precedes-last=\"never\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\" \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"container-author\">\n    <names variable=\"container-author\">\n      <label form=\"long\" suffix=\": \" text-case=\"capitalize-first\" strip-periods=\"true\"/>\n      <name and=\"text\" delimiter=\" \" initialize-with=\". \" delimiter-precedes-last=\"never\">\n        <name-part name=\"given\"/>\n        <name-part name=\"family\" text-case=\"capitalize-first\"/>\n      </name>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\" form=\"long\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"page\">\n    <group delimiter=\" \">\n      <text term=\"page\" form=\"short\"/>\n      <text variable=\"page\"/>\n    </group>\n  </macro>\n  <macro name=\"issue\">\n    <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n      <text variable=\"issue\"/>\n    </group>\n  </macro>\n  <citation collapse=\"citation-number\" after-collapse-delimiter=\"; \">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\", \">\n      <text variable=\"citation-number\"/>\n      <group delimiter=\" \">\n        <label variable=\"locator\" form=\"short\" prefix=\" \"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"1\" second-field-align=\"flush\" et-al-min=\"3\" et-al-use-first=\"1\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout>\n      <!-- Citation Number -->\n      <text variable=\"citation-number\" prefix=\"[\" suffix=\"]\"/>\n      <!-- Author(s) -->\n      <group delimiter=\", \" suffix=\": \">\n        <text macro=\"author\"/>\n      </group>\n      <!-- Rest of Citation -->\n      <choose>\n        <!-- Specific Formats -->\n        <if type=\"book report\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <text variable=\"number\"/>\n            <group delimiter=\", \">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n              <text macro=\"year-date\"/>\n            </group>\n          </group>\n        </if>\n        <else-if type=\"paper-conference chapter\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <group delimiter=\" \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text variable=\"container-title\"/>\n              </group>\n              <text variable=\"event\"/>\n              <text macro=\"container-author\" prefix=\"(\" suffix=\")\"/>\n            </group>\n            <group delimiter=\", \">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n              <text macro=\"year-date\"/>\n              <text macro=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else-if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n          <group delimiter=\". \" suffix=\". \">\n            <text variable=\"title\"/>\n            <group delimiter=\", \">\n              <text variable=\"container-title\" form=\"short\" font-style=\"italic\"/>\n              <group delimiter=\" \">\n                <text variable=\"volume\" font-weight=\"bold\"/>\n                <text macro=\"issue\"/>\n              </group>\n              <text macro=\"year-date\"/>\n              <text macro=\"page\"/>\n            </group>\n          </group>\n        </else-if>\n        <else>\n          <group delimiter=\". \" suffix=\". \">\n            <text variable=\"title\" font-style=\"italic\"/>\n            <group delimiter=\", \">\n              <group delimiter=\": \">\n                <text variable=\"publisher-place\"/>\n                <text variable=\"publisher\"/>\n              </group>\n              <text macro=\"year-date\"/>\n              <text variable=\"volume\"/>\n              <text macro=\"page\"/>\n            </group>\n          </group>\n        </else>\n      </choose>\n    </layout>\n  </bibliography>\n</style>\n"