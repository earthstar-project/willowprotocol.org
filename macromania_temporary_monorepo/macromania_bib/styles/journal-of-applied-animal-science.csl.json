"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\">\n  <!-- Polyglot; journal publishes in English and Thai\n       https://github.com/citation-style-language/styles/commit/58bf05953a62688ded19599330a6e08c15603b3d#commitcomment-1546725 -->\n  <info>\n    <title>Journal of Applied Animal Science</title>\n    <id>http://www.zotero.org/styles/journal-of-applied-animal-science</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-applied-animal-science\" rel=\"self\"/>\n    <link href=\"http://www.vs.mahidol.ac.th/jaas\" rel=\"documentation\"/>\n    <author>\n      <name>Dusit Laohasinnarong</name>\n      <email>vsdlh@mahidol.ac.th</email>\n      <uri>http://www.vs.mahidol.ac.th/dusit</uri>\n    </author>\n    <contributor>\n      <name>Michael Berkowitz</name>\n      <email>mberkowi@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Sean Takats</name>\n      <email>stakats@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Rintze Zelle</name>\n      <uri>http://twitter.com/rintzezelle</uri>\n    </contributor>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"medicine\"/>\n    <category field=\"science\"/>\n    <category field=\"zoology\"/>\n    <issn>1906-2257</issn>\n    <summary>This Style Requirements for Manuscript Submitted to Journal of Applied Animal Science</summary>\n    <published>2011-11-11T11:11:11+00:00</published>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <!--Citation for Thai article-->\n  <locale xml:lang=\"th\">\n    <terms>\n      <term name=\"and\"> และ</term>\n      <term name=\"cited\">อ้างถึง</term>\n      <term name=\"edition\">\n      </term>\n      <term name=\"et-al\"> และคณะ</term>\n      <term name=\"from\"> จาก</term>\n      <term name=\"in\"> ใน </term>\n      <term name=\"internet\">อินเตอร์เน็ต</term>\n      <term name=\"retrieved\"> เข้าถึง </term>\n      <term name=\"section\">\n      </term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"retrieved\">available</term>\n      <term name=\"section\" form=\"short\">sect.</term>\n    </terms>\n  </locale>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name name-as-sort-order=\"all\" sort-separator=\" \" delimiter=\", \" initialize-with=\"\" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <group delimiter=\": \">\n      <choose>\n        <if type=\"chapter paper-conference\" match=\"any\">\n          <text term=\"in\" text-case=\"capitalize-first\"/>\n        </if>\n      </choose>\n      <names variable=\"editor\" suffix=\".\">\n        <name delimiter=\", \" sort-separator=\" \" name-as-sort-order=\"all\" initialize-with=\"\" delimiter-precedes-last=\"always\"/>\n        <label form=\"long\" prefix=\", \"/>\n      </names>\n    </group>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"text\" delimiter-precedes-last=\"never\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <!--names variable=\"translator\"/-->\n        <text variable=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if variable=\"URL\">\n        <group delimiter=\": \">\n          <group delimiter=\" \">\n            <text term=\"retrieved\" text-case=\"capitalize-first\"/>\n            <text term=\"from\"/>\n          </group>\n          <text variable=\"URL\" suffix=\".\"/>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"accessed-date\">\n    <choose>\n      <if variable=\"URL\">\n        <group prefix=\"[\" suffix=\"]\" delimiter=\" \">\n          <text term=\"cited\" text-case=\"lowercase\"/>\n          <date variable=\"accessed\">\n            <date-part name=\"year\"/>\n            <date-part name=\"month\" prefix=\" \" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\" prefix=\" \"/>\n          </date>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"container-title\">\n    <choose>\n      <if type=\"article-journal article-magazine chapter paper-conference article-newspaper\" match=\"any\">\n        <group suffix=\".\" delimiter=\" \">\n          <text variable=\"container-title\" form=\"short\" strip-periods=\"true\"/>\n          <choose>\n            <if variable=\"URL\">\n              <text term=\"internet\" prefix=\"[\" suffix=\"]\" text-case=\"capitalize-first\"/>\n            </if>\n          </choose>\n        </group>\n        <text macro=\"edition\" prefix=\" \"/>\n      </if>\n      <!--add event-name and event-place once they become available-->\n      <else-if type=\"bill legislation\" match=\"any\">\n        <group delimiter=\", \">\n          <group delimiter=\". \">\n            <text variable=\"container-title\" form=\"short\"/>\n            <group delimiter=\" \">\n              <text term=\"section\" form=\"short\" text-case=\"capitalize-first\"/>\n              <text variable=\"section\"/>\n            </group>\n          </group>\n          <text variable=\"number\"/>\n        </group>\n      </else-if>\n      <else>\n        <text variable=\"container-title\" suffix=\".\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <text variable=\"title\"/>\n    <choose>\n      <if type=\"article-journal article-magazine chapter paper-conference article-newspaper\" match=\"none\">\n        <choose>\n          <if variable=\"URL\">\n            <text term=\"internet\" prefix=\" [\" suffix=\"]\" text-case=\"capitalize-first\"/>\n          </if>\n        </choose>\n        <text macro=\"edition\" prefix=\". \"/>\n      </if>\n    </choose>\n    <choose>\n      <if type=\"thesis\">\n        <text variable=\"genre\" prefix=\" [\" suffix=\"]\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\": \" suffix=\";\">\n      <choose>\n        <if type=\"thesis\">\n          <text variable=\"publisher-place\" prefix=\"[\" suffix=\"]\"/>\n        </if>\n        <else>\n          <text variable=\"publisher-place\"/>\n        </else>\n      </choose>\n      <text variable=\"publisher\"/>\n    </group>\n  </macro>\n  <macro name=\"year-date\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <date variable=\"accessed\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal\">\n        <group suffix=\";\" delimiter=\" \">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <text macro=\"accessed-date\"/>\n        </group>\n      </if>\n      <else-if type=\"article-magazine article-newspaper\" match=\"any\">\n        <group suffix=\";\" delimiter=\" \">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"year\"/>\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n          <text macro=\"accessed-date\"/>\n        </group>\n      </else-if>\n      <else-if type=\"bill legislation\" match=\"any\">\n        <group delimiter=\", \">\n          <date variable=\"issued\" delimiter=\" \">\n            <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n            <date-part name=\"day\"/>\n          </date>\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n        </group>\n      </else-if>\n      <else-if type=\"report\">\n        <date variable=\"issued\" delimiter=\" \">\n          <date-part name=\"year\"/>\n          <date-part name=\"month\" form=\"short\" strip-periods=\"true\"/>\n        </date>\n      </else-if>\n      <else>\n        <group suffix=\".\">\n          <date variable=\"issued\">\n            <date-part name=\"year\"/>\n          </date>\n          <text macro=\"accessed-date\" prefix=\" \"/>\n        </group>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"pages\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <text variable=\"page\" prefix=\":\"/>\n      </if>\n      <else>\n        <text variable=\"page\" prefix=\" p. \"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"journal-location\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"volume\"/>\n        <text variable=\"issue\" prefix=\"(\" suffix=\")\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"report-details\">\n    <choose>\n      <if type=\"report\">\n        <text variable=\"number\" prefix=\"Report No.: \"/>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-year-suffix=\"true\" collapse=\"year\">\n    <sort>\n      <key macro=\"year-date\"/>\n      <key macro=\"author-short\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\" \">\n        <text macro=\"author-short\"/>\n        <text macro=\"year-date\"/>\n      </group>\n      <group>\n        <label variable=\"locator\" form=\"short\"/>\n        <text variable=\"locator\"/>\n      </group>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"7\" et-al-use-first=\"6\" entry-spacing=\"0\" hanging-indent=\"true\" line-spacing=\"2\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"date\"/>\n      <key macro=\"title\"/>\n    </sort>\n    <layout>\n      <text macro=\"author\" suffix=\". \"/>\n      <text macro=\"title\" suffix=\". \"/>\n      <group delimiter=\" \" suffix=\". \">\n        <text macro=\"editor\"/>\n        <text macro=\"container-title\"/>\n        <text macro=\"publisher\"/>\n        <group>\n          <text macro=\"date\"/>\n          <text macro=\"journal-location\"/>\n          <text macro=\"pages\"/>\n        </group>\n      </group>\n      <text macro=\"report-details\" suffix=\". \"/>\n      <text macro=\"access\"/>\n    </layout>\n  </bibliography>\n</style>\n"