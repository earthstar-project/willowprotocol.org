"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" page-range-format=\"minimal\" default-locale=\"en-GB\">\n  <info>\n    <title>BMJ</title>\n    <id>http://www.zotero.org/styles/bmj</id>\n    <link href=\"http://www.zotero.org/styles/bmj\" rel=\"self\"/>\n    <link href=\"http://group.bmj.com/products/journals/instructions-for-authors/formatting#references\" rel=\"documentation\"/>\n    <link href=\"https://authors.bmj.com/writing-and-formatting/formatting-your-paper/\" rel=\"documentation\"/>\n    <author>\n      <name>Sebastian Karcher</name>\n    </author>\n    <contributor>\n      <name>Sean Takats</name>\n      <email>stakats@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Michael Berkowitz</name>\n      <email>mberkowi@gmu.edu</email>\n    </contributor>\n    <contributor>\n      <name>Patrick O'Brien</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"medicine\"/>\n    <issn>0959-8138</issn>\n    <eissn>1756-1833</eissn>\n    <summary>Vancouver style adapted for BMJ journals</summary>\n    <updated>2023-09-08T10:29:40+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"long\" prefix=\", \"/>\n      <et-al term=\"et-al\" font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"editor\">\n    <names variable=\"editor\">\n      <name sort-separator=\" \" initialize-with=\"\" name-as-sort-order=\"all\" delimiter=\", \" delimiter-precedes-last=\"always\"/>\n      <label form=\"short\" prefix=\", \"/>\n      <et-al term=\"et-al\" font-style=\"italic\"/>\n    </names>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\" \">\n      <group delimiter=\": \" suffix=\" \">\n        <text variable=\"publisher-place\"/>\n        <text variable=\"publisher\"/>\n      </group>\n      <text macro=\"date\"/>\n    </group>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"article-journal\">\n        <!--only use DOI for e-journals-->\n        <choose>\n          <if variable=\"number\">\n            <text variable=\"DOI\" prefix=\"doi: \"/>\n          </if>\n          <else-if variable=\"page\" match=\"none\">\n            <text variable=\"DOI\" prefix=\"doi: \"/>\n          </else-if>\n        </choose>\n      </if>\n      <else>\n        <choose>\n          <if variable=\"DOI\">\n            <!-- use modern DOI format for preprint, data, and presumably everywhere else-->\n            <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n          </if>\n          <else-if variable=\"URL\">\n            <group delimiter=\" \">\n              <text variable=\"URL\"/>\n              <group delimiter=\" \" prefix=\"(\" suffix=\")\">\n                <text term=\"accessed\"/>\n                <date variable=\"accessed\">\n                  <date-part name=\"day\" suffix=\" \"/>\n                  <date-part name=\"month\" strip-periods=\"false\" suffix=\" \"/>\n                  <date-part name=\"year\"/>\n                </date>\n              </group>\n            </group>\n          </else-if>\n        </choose>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"journal-title\">\n    <choose>\n      <if type=\"article-journal article-magazine\" match=\"any\">\n        <text variable=\"container-title\" form=\"short\" font-style=\"italic\" strip-periods=\"true\"/>\n      </if>\n      <else>\n        <text variable=\"container-title\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"date\">\n    <choose>\n      <if type=\"article-journal\">\n        <choose>\n          <if variable=\"volume page\" match=\"none\">\n            <choose>\n              <if variable=\"DOI URL\" match=\"any\">\n                <group prefix=\"Published Online First: \">\n                  <date variable=\"issued\" delimiter=\" \">\n                    <date-part name=\"day\"/>\n                    <date-part name=\"month\" form=\"long\" text-case=\"capitalize-first\"/>\n                    <date-part name=\"year\"/>\n                  </date>\n                </group>\n              </if>\n              <else>\n                <date variable=\"issued\">\n                  <date-part name=\"year\"/>\n                </date>\n              </else>\n            </choose>\n          </if>\n          <else>\n            <date variable=\"issued\">\n              <date-part name=\"year\"/>\n            </date>\n          </else>\n        </choose>\n      </if>\n      <else>\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"book thesis\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout prefix=\"[\" suffix=\"]\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography et-al-min=\"4\" et-al-use-first=\"3\" second-field-align=\"flush\">\n    <layout>\n      <text variable=\"citation-number\" suffix=\" \"/>\n      <group delimiter=\". \" suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"title\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <group delimiter=\". \">\n              <text macro=\"edition\"/>\n              <text macro=\"publisher\"/>\n            </group>\n          </if>\n          <else-if type=\"chapter paper-conference\" match=\"any\">\n            <group delimiter=\". \">\n              <group delimiter=\": \">\n                <text term=\"in\" text-case=\"capitalize-first\"/>\n                <text macro=\"editor\"/>\n              </group>\n              <text variable=\"container-title\" font-style=\"italic\"/>\n              <group delimiter=\":\">\n                <text macro=\"publisher\"/>\n                <text variable=\"page\" suffix=\".\"/>\n              </group>\n            </group>\n          </else-if>\n          <else>\n            <text macro=\"journal-title\"/>\n            <group>\n              <text macro=\"date\"/>\n              <group prefix=\";\" delimiter=\":\">\n                <text variable=\"volume\"/>\n                <text variable=\"page\"/>\n              </group>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\" \"/>\n    </layout>\n  </bibliography>\n</style>\n"