"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"sort-only\" default-locale=\"en-US\">\n  <info>\n    <title>Journal of The Electrochemical Society</title>\n    <title-short>JES</title-short>\n    <id>http://www.zotero.org/styles/journal-of-the-electrochemical-society</id>\n    <link href=\"http://www.zotero.org/styles/journal-of-the-electrochemical-society\" rel=\"self\"/>\n    <link href=\"http://www.electrochem.org/dl/support/assets/jes_inst.pdf\" rel=\"documentation\"/>\n    <author>\n      <name>Julian Onions</name>\n      <email>julian.onions@gmail.com</email>\n    </author>\n    <contributor>\n      <name>Sebastian Karcher</name>\n    </contributor>\n    <category citation-format=\"numeric\"/>\n    <category field=\"chemistry\"/>\n    <issn>0013-4651</issn>\n    <eissn>1945-7111</eissn>\n    <summary>Style for JES</summary>\n    <updated>2012-09-27T22:06:38+00:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <macro name=\"editor\">\n    <names variable=\"editor\" delimiter=\", \">\n      <name initialize-with=\". \" delimiter=\", \" and=\"text\"/>\n      <label form=\"long\" prefix=\", \" text-case=\"capitalize-first\"/>\n    </names>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name sort-separator=\", \" initialize-with=\". \" and=\"text\" delimiter=\", \"/>\n      <label form=\"short\" prefix=\", \" text-case=\"capitalize-first\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <text variable=\"URL\"/>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n        <text variable=\"title\" font-style=\"italic\"/>\n      </if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"volume\">\n    <text variable=\"volume\" prefix=\"vol. \"/>\n  </macro>\n  <macro name=\"pages\">\n    <text variable=\"page\" form=\"short\" prefix=\"p. \"/>\n  </macro>\n  <macro name=\"issued\">\n    <date variable=\"issued\" delimiter=\" \">\n      <date-part name=\"year\"/>\n    </date>\n  </macro>\n  <macro name=\"edition\">\n    <choose>\n      <if is-numeric=\"edition\">\n        <group delimiter=\" \">\n          <number variable=\"edition\" form=\"ordinal\"/>\n          <text term=\"edition\" form=\"short\"/>\n        </group>\n      </if>\n      <else>\n        <text variable=\"edition\" suffix=\".\"/>\n      </else>\n    </choose>\n  </macro>\n  <citation collapse=\"citation-number\">\n    <sort>\n      <key variable=\"citation-number\"/>\n    </sort>\n    <layout vertical-align=\"sup\" delimiter=\",\">\n      <text variable=\"citation-number\"/>\n    </layout>\n  </citation>\n  <bibliography hanging-indent=\"false\" et-al-min=\"6\" et-al-use-first=\"1\">\n    <layout suffix=\".\">\n      <text variable=\"citation-number\" suffix=\". \"/>\n      <text macro=\"author\" suffix=\",\"/>\n      <choose>\n        <if type=\"thesis\">\n          <text value=\" thesis, \"/>\n          <text macro=\"publisher\"/>\n          <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n        </if>\n        <else-if type=\"bill book graphic legal_case motion_picture report song\" match=\"any\">\n          <group>\n            <text macro=\"title\" prefix=\" \"/>\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\", \" delimiter=\", \">\n            <text macro=\"edition\"/>\n            <text variable=\"number-of-pages\" prefix=\"p. \"/>\n            <text macro=\"publisher\"/>\n            <text macro=\"issued\" prefix=\"(\" suffix=\")\"/>\n          </group>\n          <text macro=\"pages\" prefix=\", \"/>\n        </else-if>\n        <else-if type=\"chapter paper-conference\" match=\"any\">\n          <group prefix=\" \">\n            <text term=\"in\"/>\n            <text variable=\"container-title\" font-style=\"italic\" prefix=\" \" suffix=\",\"/>\n            <text variable=\"collection-title\" prefix=\" \" suffix=\".\"/>\n            <text macro=\"editor\" prefix=\" \"/>\n            <group delimiter=\", \" prefix=\", \">\n              <text macro=\"volume\"/>\n              <text macro=\"pages\"/>\n              <text macro=\"publisher\"/>\n            </group>\n            <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n          </group>\n        </else-if>\n        <else>\n          <group suffix=\".\">\n            <text macro=\"editor\" prefix=\" \"/>\n          </group>\n          <group prefix=\" \" delimiter=\", \">\n            <text form=\"short\" variable=\"container-title\" font-style=\"italic\"/>\n            <text variable=\"volume\" font-weight=\"bold\"/>\n            <text variable=\"page\"/>\n            <!-- should only have starting page -->\n          </group>\n          <text macro=\"issued\" prefix=\" (\" suffix=\")\"/>\n        </else>\n      </choose>\n      <text prefix=\" \" macro=\"access\" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"