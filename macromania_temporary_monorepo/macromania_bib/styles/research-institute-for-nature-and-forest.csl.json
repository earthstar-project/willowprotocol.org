"<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<style xmlns=\"http://purl.org/net/xbiblio/csl\" class=\"in-text\" version=\"1.0\" demote-non-dropping-particle=\"never\">\n  <info>\n    <title>Research Institute for Nature and Forest (Instituut voor Natuur- en Bosonderzoek)</title>\n    <title-short>INBO</title-short>\n    <id>http://www.zotero.org/styles/research-institute-for-nature-and-forest</id>\n    <link href=\"http://www.zotero.org/styles/research-institute-for-nature-and-forest\" rel=\"self\"/>\n    <link href=\"https://inbomd-examples.netlify.app/citation_style/nl/index.html\" rel=\"documentation\"/>\n    <author>\n      <name>Maarten Stevens</name>\n      <uri>https://www.mendeley.com/profiles/maarten-stevens/</uri>\n    </author>\n    <author>\n      <name>Floris Vanderhaeghe</name>\n      <uri>https://github.com/florisvdh</uri>\n    </author>\n    <contributor>\n      <name>Thierry Onkelinx</name>\n      <uri>https://www.mendeley.com/profiles/thierry-onkelinx/</uri>\n    </contributor>\n    <contributor>\n      <name>Els Lommelen</name>\n      <uri>https://github.com/elslommelen</uri>\n    </contributor>\n    <category citation-format=\"author-date\"/>\n    <category field=\"biology\"/>\n    <category field=\"botany\"/>\n    <category field=\"zoology\"/>\n    <updated>2021-12-21T18:20:00+01:00</updated>\n    <rights license=\"http://creativecommons.org/licenses/by-sa/3.0/\">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>\n  </info>\n  <locale xml:lang=\"en\">\n    <terms>\n      <term name=\"edition\" form=\"short\">edn.</term>\n      <term name=\"no date\" form=\"short\">s.d.</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"nl\">\n    <terms>\n      <term name=\"et-al\">et al.</term>\n      <term name=\"editor\" form=\"short\">\n        <single>ed.</single>\n        <multiple>eds.</multiple>\n      </term>\n      <term name=\"edition\" form=\"short\">ed.</term>\n      <term name=\"no date\" form=\"short\">s.d.</term>\n    </terms>\n  </locale>\n  <locale xml:lang=\"fr\">\n    <terms>\n      <term name=\"no date\" form=\"short\">s.d.</term>\n    </terms>\n  </locale>\n  <macro name=\"container\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"none\">\n        <choose>\n          <if type=\"paper-conference\" variable=\"container-title\" match=\"any\">\n            <text term=\"in\" text-case=\"capitalize-first\" suffix=\": \"/>\n            <names variable=\"editor translator\" delimiter=\", \" suffix=\". \">\n              <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n              <label text-case=\"lowercase\" form=\"short\" prefix=\" (\" suffix=\")\"/>\n            </names>\n          </if>\n        </choose>\n      </if>\n    </choose>\n    <group delimiter=\", \">\n      <text variable=\"container-title\"/>\n      <text variable=\"collection-title\"/>\n      <choose>\n        <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n          <!-- show nothing -->\n        </if>\n        <else-if variable=\"volume\">\n          <group delimiter=\" \">\n            <text term=\"volume\" form=\"short\" text-case=\"capitalize-first\"/>\n            <number variable=\"volume\"/>\n          </group>\n        </else-if>\n        <else-if variable=\"collection-number\">\n          <group delimiter=\" \">\n            <text term=\"issue\" form=\"short\" text-case=\"capitalize-first\"/>\n            <number variable=\"collection-number\"/>\n          </group>\n        </else-if>\n        <else-if variable=\"number\">\n          <group delimiter=\" \">\n            <text term=\"issue\" form=\"short\" text-case=\"capitalize-first\"/>\n            <text variable=\"number\"/>\n          </group>\n        </else-if>\n      </choose>\n      <choose>\n        <if type=\"paper-conference\" variable=\"container-title\" match=\"any\">\n          <text macro=\"edition\"/>\n        </if>\n      </choose>\n    </group>\n  </macro>\n  <macro name=\"author\">\n    <names variable=\"author\">\n      <name and=\"symbol\" delimiter-precedes-last=\"never\" initialize-with=\".\" name-as-sort-order=\"all\" sort-separator=\" \"/>\n      <label form=\"short\" prefix=\" (\" suffix=\")\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <text macro=\"title\"/>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"author-short\">\n    <names variable=\"author\">\n      <name form=\"short\" and=\"symbol\" et-al-min=\"3\" et-al-use-first=\"1\" initialize-with=\". \"/>\n      <et-al font-style=\"italic\"/>\n      <substitute>\n        <names variable=\"editor\"/>\n        <names variable=\"translator\"/>\n        <choose>\n          <if type=\"bill book graphic legal_case legislation motion_picture report song\" match=\"any\">\n            <text variable=\"title\" form=\"short\" font-style=\"italic\"/>\n          </if>\n          <else>\n            <text variable=\"title\" form=\"short\" quotes=\"true\"/>\n          </else>\n        </choose>\n      </substitute>\n    </names>\n  </macro>\n  <macro name=\"access\">\n    <choose>\n      <if type=\"webpage\">\n        <group delimiter=\" \">\n          <text variable=\"URL\"/>\n          <group prefix=\"(\" suffix=\").\">\n            <text term=\"accessed\" suffix=\" \"/>\n            <date variable=\"accessed\" form=\"text\"/>\n          </group>\n        </group>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"title\">\n    <choose>\n      <if type=\"webpage\">\n        <text variable=\"title\"/>\n      </if>\n      <else-if variable=\"container-title\" match=\"none\">\n        <text variable=\"title\"/>\n        <text macro=\"edition\" prefix=\". \"/>\n      </else-if>\n      <else>\n        <text variable=\"title\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"publisher\">\n    <group delimiter=\", \">\n      <text variable=\"publisher\"/>\n      <text variable=\"publisher-place\"/>\n    </group>\n  </macro>\n  <macro name=\"event\">\n    <choose>\n      <if variable=\"event\">\n        <text term=\"presented at\" text-case=\"capitalize-first\" suffix=\" \"/>\n        <text variable=\"event\"/>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"issued\">\n    <choose>\n      <if variable=\"issued\">\n        <date variable=\"issued\">\n          <date-part name=\"year\"/>\n        </date>\n      </if>\n      <else>\n        <text term=\"no date\" form=\"short\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"edition\">\n    <group delimiter=\" \">\n      <choose>\n        <if is-numeric=\"edition\">\n          <number variable=\"edition\" form=\"ordinal\"/>\n        </if>\n        <else>\n          <text variable=\"edition\"/>\n        </else>\n      </choose>\n      <text term=\"edition\" form=\"short\"/>\n    </group>\n  </macro>\n  <macro name=\"locators\">\n    <choose>\n      <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n        <group delimiter=\": \">\n          <group>\n            <text variable=\"volume\"/>\n            <choose>\n              <if match=\"any\" variable=\"issue\">\n                <number prefix=\" (\" suffix=\")\" variable=\"issue\"/>\n              </if>\n            </choose>\n          </group>\n          <text variable=\"page\"/>\n        </group>\n      </if>\n      <else-if type=\"bill book graphic legal_case legislation motion_picture report song thesis\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n          <text variable=\"number-of-pages\" suffix=\" p\"/>\n        </group>\n      </else-if>\n      <else-if type=\"chapter paper-conference\" variable=\"page\" match=\"any\">\n        <group delimiter=\", \">\n          <text macro=\"event\"/>\n          <text macro=\"publisher\"/>\n          <text variable=\"page\" prefix=\"p. \"/>\n        </group>\n      </else-if>\n      <else-if type=\"patent\">\n        <text variable=\"number\"/>\n      </else-if>\n      <else>\n        <text macro=\"publisher\"/>\n      </else>\n    </choose>\n  </macro>\n  <macro name=\"url\">\n    <choose>\n      <if type=\"webpage\" variable=\"DOI\" match=\"none\">\n        <choose>\n          <if match=\"any\" variable=\"URL\">\n            <text variable=\"URL\"/>\n          </if>\n        </choose>\n      </if>\n    </choose>\n  </macro>\n  <macro name=\"doi\">\n    <choose>\n      <if match=\"any\" variable=\"DOI\">\n        <text variable=\"DOI\" prefix=\"https://doi.org/\"/>\n      </if>\n    </choose>\n  </macro>\n  <citation et-al-min=\"3\" et-al-use-first=\"1\" disambiguate-add-givenname=\"false\" disambiguate-add-year-suffix=\"true\" collapse=\"year\" cite-group-delimiter=\", \">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\"/>\n    </sort>\n    <layout prefix=\"(\" suffix=\")\" delimiter=\"; \">\n      <group delimiter=\", \">\n        <text macro=\"author-short\" font-style=\"normal\"/>\n        <text macro=\"issued\"/>\n        <group delimiter=\" \">\n          <label font-style=\"normal\" variable=\"locator\" form=\"short\"/>\n          <text variable=\"locator\" form=\"long\" font-style=\"normal\"/>\n        </group>\n      </group>\n    </layout>\n  </citation>\n  <bibliography entry-spacing=\"0\" hanging-indent=\"true\">\n    <sort>\n      <key macro=\"author\"/>\n      <key macro=\"issued\" sort=\"ascending\"/>\n    </sort>\n    <layout>\n      <group suffix=\".\">\n        <text macro=\"author\"/>\n        <text macro=\"issued\" prefix=\" (\" suffix=\"). \"/>\n        <choose>\n          <if type=\"article-journal article-magazine article-newspaper\" match=\"any\">\n            <group delimiter=\" \">\n              <text macro=\"title\" suffix=\".\"/>\n              <text macro=\"container\"/>\n              <text macro=\"locators\"/>\n            </group>\n          </if>\n          <else>\n            <group delimiter=\". \">\n              <text macro=\"title\"/>\n              <text macro=\"container\"/>\n              <text macro=\"locators\"/>\n            </group>\n          </else>\n        </choose>\n      </group>\n      <text macro=\"access\" prefix=\" \" suffix=\".\"/>\n      <text macro=\"url\" prefix=\" \" suffix=\".\"/>\n      <text macro=\"doi\" prefix=\" \" suffix=\".\"/>\n    </layout>\n  </bibliography>\n</style>\n"